<!--
	cdn.socialtwist.com is hosted at s3.amazonaws.com/cdn.socialtwist.com/, but has a valid cert.

-->
<ruleset name="SocialTwist">

	<target host="socialtwist.com" />
	<target host="*.socialtwist.com" />


	<!--	Observed subdomains:

			- blog
			- cdn
			- content
			- images
			- nospam-taf
			- sales
			- secure
			- taf
			- tellafriend
			- www
					-->
	<rule from="^http://([\w\-]+\.)?socialtwist\.com/"
		to="https://$1socialtwist.com/" />

</ruleset>
