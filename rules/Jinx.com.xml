<!--
	Some pages redirect to http.

-->
<ruleset name="Jinx.com (partial)" platform="mixedcontent">

	<target host="jinx.com" />
	<target host="www.jinx.com" />
		<exclusion pattern="^http://www\.jinx\.com/(?!create\.|design_contest(?:_info)?\.|log(?:in|out)\.).+\.aspx" />


	<rule from="^http://(www\.)?jinx\.com/home\.aspx($|\?.*)"
		to="https://$1jinx.com/$2" />

	<rule from="^http://(www\.)?jinx\.com/"
		to="https://$1jinx.com/" />

</ruleset>
