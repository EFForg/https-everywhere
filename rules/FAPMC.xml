<!--
	Federal Agency on Press and Mass Communications of the Russian Federation


	Cert only matches www.

-->
<ruleset name="FAPMC" default_off="self-signed">

	<target host="fapmc.ru" />
	<target host="www.fapmc.ru" />


	<rule from="^http://(?:www\.)?fapmc\.ru/"
		to="https://www.fapmc.ru/" />

</ruleset>
