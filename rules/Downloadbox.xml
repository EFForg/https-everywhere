<ruleset name="Downloadbox" default_off="self-signed" platform="mixedcontent">

	<target host="downloadbox.me" />
	<target host="*.downloadbox.me" />


	<securecookie host="^\.downloadbox\.me$" name=".+" />


	<rule from="^http://(?:www\.)?downloadbox\.me/"
		to="https://downloadbox.me/" />

</ruleset>
