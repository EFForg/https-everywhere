<!--
	www.raam.org: Mismatched

-->
<ruleset name="Raam.org">

	<!--	Direct rewrites:
				-->
	<target host="raam.org" />

	<!--	Complications:
				-->
	<target host="www.raam.org" />


	<rule from="^http://www\.raam\.org/"
		to="https://raam.org/" />

	<rule from="^http:"
		to="https:" />

</ruleset>
