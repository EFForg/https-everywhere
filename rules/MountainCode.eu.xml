<!--
	Insecure cookies are set for these domains:

		- .mountaincode.eu

-->
<ruleset name="MountainCode.eu">

	<!--	Direct rewrites:
				-->
	<target host="mountaincode.eu" />
	<target host="www.mountaincode.eu" />


	<!--	CloudFlare cookies:
					-->
	<!--securecookie host="^\.mountaincode\.eu$" name="^(?:__cfduid|cf_clearance)$" /-->

	<securecookie host="^\.mountaincode\.eu$" name=".+" />


	<rule from="^http:"
		to="https:" />

</ruleset>
