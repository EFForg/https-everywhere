<!--
	For rules that are on by default, see Database.com.xml.

-->
<ruleset name="Database.com (problematic)" default_off="mismatched">

	<target host="database.com" />
	<target host="docs.database.com" />
	<target host="www.database.com" />


	<rule from="^http://(?:(docs\.)|www\.)?database\.com/"
		to="https://$1database.com/" />

</ruleset>
