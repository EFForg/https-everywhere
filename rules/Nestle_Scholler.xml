<!--
	For other Nestlé coverage, see Nestle.xml.


	Problematic subdomains:

		- ^	(mismatched, CN: hosting.nestle.com)

-->
<ruleset name="Nestlé Schöller (partial)">

	<target host="schoeller.de" />
	<target host="www.schoeller.de" />


	<!--	At least some pages redirect to http.
							-->
	<rule from="^http://(?:www\.)?schoeller\.de/(NR|schoeller/Images)/"
		to="https://www.schoeller.de/$1/" />

</ruleset>
