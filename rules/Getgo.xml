<!--
	For other Eventim coverage, see Eventim.xml.


	Problematic domains:

		 getgo.de

-->
<ruleset name="getgo">

	<target host="getgo.de" />
	<target host="*.getgo.de" />


	<securecookie host="^.*\.getgo\.de$" name=".+" />


	<rule from="^http://(?:www\.)?getgo\.de/"
		to="https://www.getgo.de/" />

	<rule from="^http://secure\.getgo\.de/"
		to="https://secure.getgo.de/" />

</ruleset>
