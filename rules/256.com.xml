<!--
	Problematic subdomains:

		- ^ ยน
		- www ยน	(cert only matches ^256.com)

	ยน Expired

-->
<ruleset name="256.com" default_off="expired">

	<!--	Direct rewrites:
				-->
	<target host="256.com" />

	<!--	Complications:
				-->
	<target host="www.256.com" />


	<rule from="^http://www\.256\.com/"
		to="https://256.com/" />

	<rule from="^http:"
		to="https:" />

</ruleset>
