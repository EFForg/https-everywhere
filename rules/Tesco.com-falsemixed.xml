<!--
	For rules not causing false/broken MCB, see Tesco.xml.

-->
<ruleset name="Tesco.com (false MCB)" platform="mixedcontent">

	<target host="tesco.com" />
	<target host="www.tesco.com" />


	<rule from="^http://(?:www\.)?tesco\.com/groceries/"
		to="https://secure.tesco.com/groceries/" />

</ruleset>
