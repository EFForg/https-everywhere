<!--
	www.hostbillapp.com: Mismatched

-->
<ruleset name="HostBill (partial)">

	<!--	Direct rewrites:
				-->
	<target host="hostbillapp.com" />

	<!--	Complications:
				-->
	<target host="www.hostbillapp.com" />


	<rule from="^http://www\.hostbillapp\.com/"
		to="https://hostbillapp.com/" />

	<rule from="^http:"
		to="https:" />

</ruleset>
