<!--
	Problematic subdomains:

		-(www.)		(no https)
		- freemasonry	(cert only matches www.freemasonry)

-->
<ruleset name="Grand Lodge of British Columbia">

	<target host="bcy.ca" />
	<target host="*.bcy.ca" />


	<rule from="^http://(?:www\.)?(?:freemasonry\.)?bcy\.ca/"
		to="https://www.freemasonry.bcy.ca/" />

</ruleset>