<!--
	includeSubDomains is sent in the HSTS header.


	www.ohsocool.org: Mismatched

-->
<ruleset name="Oh So Cool.org">

	<target host="ohsocool.org" />
	<target host="*.ohsocool.org" />

		<test url="http://www.ohsocool.org/" />


	<rule from="^http://www\.ohsocool\.org/"
		to="https://ohsocool.org/" />

	<rule from="^http:"
		to="https:" />

</ruleset>
