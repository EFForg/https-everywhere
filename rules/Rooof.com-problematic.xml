<!--
	For rules that are on by default, see Rooof.xml.

-->
<ruleset name="Rooof.com (mismatched)" default_off="mismatched">

	<!--	Direct rewrites:
				-->
	<target host="rooof.com" />
	<target host="www.rooof.com" />


	<securecookie host="(?:www)?\.roof\.com$" name=".+" />


	<rule from="^http:"
		to="https:" />

</ruleset>
