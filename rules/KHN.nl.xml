<!--
	Koninklijke Horeca Nederland


	^: Handshake fails
	www: Refused

-->
<ruleset name="KHN.nl (partial)">

	<!--target host="khn.nl" /-->
	<target host="*.khn.nl" />


	<securecookie host="^ox\.khn\.nl$" name=".+" />


	<!--	^ redirects to www over http,
		so copy that behavior:
					-->
	<!--rule from="^http://(?:www\.)?khn\.nl/"
		to="https://www.khn.nl/" /-->

	<rule from="^http://ox\.khn\.nl/"
		to="https://ox.khn.nl/" />

</ruleset>
