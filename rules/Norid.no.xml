<!--
	^: Cert only matches www


	Fully covered subdomains:

		- (www.)?	(^ â†’ www)
		- pid

-->
<ruleset name="Norid.no">

	<target host="norid.no" />
	<target host="*.norid.no" />


	<rule from="^http://(?:www\.)?norid\.no/"
		to="https://www.norid.no/" />

	<rule from="^http://pid\.norid\.no/"
		to="https://pid.norid.no/" />

</ruleset>
