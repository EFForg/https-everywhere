<!--
	Nonfunctional subdomains:

		- cup		(cert: linguistlist.org; shows that domain)
		- listserv
		- odin		(times out)

-->
<ruleset name="LINGUIST List (partial)">

	<target host="linguistlist.org" />
	<target host="www.linguistlist.org" />


	<securecookie host="^linguistlist\.org$" name=".+" />


	<!--	Cert is only valid for ^linguistlist.org.
								-->
	<rule from="^http://(?:www\.)?linguistlist\.org/"
		to="https://linguistlist.org/" />

</ruleset>
