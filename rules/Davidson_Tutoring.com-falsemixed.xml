<!--
	For rules not causing false/broken MCB, see Davidson_Tutoring.com.xml.

-->
<ruleset name="Davidson Tutoring (false MCB)" platform="mixedcontent">

	<target host="davidsontutoring.com" />
	<target host="*.davidsontutoring.com" />
		<!--
			Handled in Davidson_Tutoring.com.xml:
								-->
		<!--exclusion pattern="^http://(www\.)?davidsontutoring.com/(assets/|favicon\.ico)" /-->


	<securecookie host="^(?:www)?\.davidsontutoring\.com$" name=".+" />


	<!--	!www is configured for rc4 only.
						-->
	<rule from="^http://(?:www\.)?davidsontutoring\.com/(?!assets/|favicon\.ico)"
		to="https://www.davidsontutoring.com/" />

</ruleset>
