<!--
	Cert only matches www.

-->
<ruleset name="Barbican">

	<target host="barbican.org.uk" />
	<target host="www.barbican.org.uk" />


	<securecookie host="^www\.barbican\.org\.uk$" name=".+" />


	<rule from="^http://(?:www\.)?barbican\.org\.uk/"
		to="https://www.barbican.org.uk/" />

</ruleset>
