<!--
	Problematic subdomains:

		- ^	(mismatched, CN: *.web4a.de)

-->
<ruleset name="Webspace4All">

	<target host="webspace4all.eu" />
	<target host="www.webspace4all.eu" />


	<securecookie host="^www\.webspace4all\.eu$" name=".+" />


	<rule from="^http://(?:www\.)?webspace4all\.eu/"
		to="https://www.webspace4all.eu/" />

</ruleset>