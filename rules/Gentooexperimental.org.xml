<!--
	Problematic subdomains:

		- ^	(cert only matches *.gentooexperimental.org)

-->
<ruleset name="gentooexperimental.org (partial)" default_off="self-signed">

	<target host="gentooexperimental.org" />
	<target host="www.gentooexperimental.org" />
		<!--
			$ shows empty tree
						-->
		<!--exclusion pattern="^http://(www\.)?gentooexperimental\.org/+(?!~)" /-->


	<rule from="^http://(?:www\.)?gentooexperimental\.org/~"
		to="https://www.gentooexperimental.org/~" />

</ruleset>
