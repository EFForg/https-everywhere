<!--
	Other Init7 rulesets:

		- Fiber7.ch.xml

-->
<ruleset name="Init7" platform="cacert">

	<!--	Direct rewrites:
				-->
	<target host="www.init7.net" />

	<!--	Complications:
				-->
	<target host="init7.net" />


	<!--	Cert only matches www.init7.net	-->

	<rule from="^http://init7\.net/"
		to="https://www.init7.net/" />

	<rule from="^http:"
		to="https:" />

</ruleset>
