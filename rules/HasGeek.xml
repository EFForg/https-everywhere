<!--
	Nonfunctional domains:

		- hasgeek.com	(301s to http)
		- hasgeek.tv	(cert: *.hasgeek.com; 302s to http)

-->
<ruleset name="HasGeek (partial)">

	<target host="jobs.hasgeek.com" />


	<!--	At least some pages 301 to http.
							-->
	<rule from="^http://jobs\.hasgeek\.com/(new(?:/?$|\?)|static/)"
		to="https://jobs.hasgeek.com/$1" />

</ruleset>
