<ruleset name="Paxetia (partial)">

	<target host="patexia.com" />
	<target host="www.patexia.com" />


	<rule from="^http://patexia\.com/"
		to="https://patexia.com/" />

	<!--	Some paths redirect to http, e.g.:

			- $
			- blog/$
			- feed/$
			- glossary.html$
			- nova$
			- users$
				-->
	<rule from="^http://www\.patexia\.com/(css|data|fonts|images|users?)/"
		to="https://www.patexia.com/$1/" />

</ruleset>
