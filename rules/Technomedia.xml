<!--
	Problematic domains:

		- technomedia.com	(cert only matches www)

-->
<ruleset name="Technomedia">

	<target host="technomedia.com" />
	<target host="www.technomedia.com" />


	<securecookie host="^www\.technomedia\.com$" name=".+" />


	<rule from="^http://(?:www\.)?technomedia\.com/"
		to="https://www.technomedia.com/" />

</ruleset>
