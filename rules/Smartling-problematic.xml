<ruleset name="Smartling (problematic)" default_off="expired, mismatched">

	<target host="smartlingsource.com" />
	<target host="www.smartlingsource.com" />


	<rule from="^http://(?:www\.)?smartlingsource\.com/"
		to="https://smartlingsource.com/" />

</ruleset>
