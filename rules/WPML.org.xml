<!--
	www.wpml.org: Mismatched

-->
<ruleset name="WPML.org">

	<!--	Direct rewrites:
				-->
	<target host="wpml.org" />
	<target host="tp.wpml.org" />

	<!--	Complications:
				-->
	<target host="www.wpml.org" />


	<rule from="^http://www\.wpml\.org/"
		to="https://wpml.org/" />

	<rule from="^http:"
		to="https:" />

</ruleset>
