<!--
	For rules that are on by default, see Tradelab.xml.

-->
<ruleset name="Tradelab (problematic)" default_off="expired, mismatched, self-signed">

	<target host="tradelab.fr" />
	<target host="www.tradelab.fr" />


	<rule from="^http://(?:www\.)?tradelab\.fr/"
		to="https://www.tradelab.fr/" />

</ruleset>
