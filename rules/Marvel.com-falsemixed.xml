<!--
	For rules not causing false/broken MCB, see Marvel.com.xml.

-->
<ruleset name="Marvel.com (false MCB)" platform="mixedcontent">

	<!--	Direct rewrites:
				-->
	<target host="marvel.com" />
	<target host="www.marvel.com" />


	<rule from="^http://marvel\.com:81/"
		to="https://marvel.com/" />

		<test url="http://marvel.com:81/" />

	<rule from="^http:"
		to="https:" />

</ruleset>
