<!--
	www.quup.com: Cert only matches ^quup.com

-->
<ruleset name="quup.com">

	<!--	Direct rewrites:
				-->
	<target host="quup.com" />

	<!--	Complications:
				-->
	<target host="www.quup.com" />


	<securecookie host="." name="." />


	<rule from="^http://www\.quup\.com/"
		to="https://quup.com/" />

	<rule from="^http:"
		to="https:" />

</ruleset>
