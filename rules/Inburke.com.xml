<!--
	Problematic subdomains:

		- (www.)? *
		- www.kev *

	* Mismatched, CN: *.webfaction.com

-->
<ruleset name="inburke.com">

	<target host="inburke.com" />
	<target host="*.inburke.com" />


	<rule from="^http://(?:www\.)?(?:kev\.)?inburke\.com/"
		to="https://kev.inburke.com/" />

</ruleset>
