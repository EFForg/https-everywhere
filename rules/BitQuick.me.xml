<!--
	For other BitQuick coverage, see BitQuick.xml.


	Server sends no certificate chain *

	* See https://whatsmychaincert.com


	Fully covered domains:

		- (www.)?bitquick.me

-->
<ruleset name="BitQuick.me" default_off="missing certificate chain">

	<!--	Direct rewrites:
				-->
	<target host="bitquick.me" />
	<target host="www.bitquick.me" />


	<rule from="^http:"
		to="https:" />

</ruleset>
