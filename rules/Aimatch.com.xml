	<rule from="^http://(?:www\.)?aimatch\.com/[^?]*"
		to="https://www.sas.com/software/customer-intelligence/intelligent-advertising.html" />

		<test url="http://aimatch.com/?" />
		<test url="http://www.aimatch.com/?" />

	<rule from="^http:"
		to="https:" />

</ruleset>
