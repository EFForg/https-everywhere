<!--
	Problematic subdomains:

		- ^	(404)

-->
<ruleset name="v-identity.com">

	<target host="v-identity.com" />
	<target host="*.v-identity.com" />


	<rule from="^http://(?:www\.)?v-identity\.com/"
		to="https://www.v-identity.com/" />

	<rule from="^http://cdn\.v-identity\.com/"
		to="https://cdn.v-identity.com/" />

</ruleset>