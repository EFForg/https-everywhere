<ruleset name="Redbox.com" platform="mixedcontent">
  <target host="redbox.com" />
  <target host="www.redbox.com" />
  <target host="images.redbox.com" />

  <rule from="^http://(?:www\.)?redbox\.com/" to="https://www.redbox.com/" />
  <rule from="^http://images\.redbox\.com/" to="https://images.redbox.com/" />
</ruleset>
