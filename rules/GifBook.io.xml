<!--
	For other levels.io coverage, see Levels.io.xml.


	www.gifbook.io: Redirects to http$

-->
<ruleset name="GifBook.io">

	<!--	Direct rewrites:
				-->
	<target host="gifbook.io" />

	<!--	Complications:
				-->
	<target host="www.gifbook.io" />


	<rule from="^http://www\.gifbook\.io/"
		to="https://gifbook.io/" />

	<rule from="^http:"
		to="https:" />

</ruleset>
