<ruleset name="Common Short Code Administration (partial)">

	<target host="usshortcodes.com" />
	<target host="www.usshortcodes.com" />


	<!--	Some pages redirect to http, others 404.
							-->
	<rule from="^http://(www\.)?usshortcodes\.com/(content|csc|csclogin|images|Includes)/"
		to="https://$1usshortcodes.com/$2/" />

</ruleset>
