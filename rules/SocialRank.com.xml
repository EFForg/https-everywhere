<!--
	Nonfunctional subdomains:

		- blog *

	* Show socialrank.co


	These altnames don't exist:

		- www.i.socialrank.com


	(www.)?....co is a simple redirect to .com

-->
<ruleset name="SocialRank.com (partial)">

	<target host="socialrank.co" />
	<target host="www.socialrank.co" />
	<target host="socialrank.com" />
	<target host="i.socialrank.com" />
	<target host="www.socialrank.com" />


	<!--	Not secured by server:
					-->
	<!--securecookie host="^(?:www\.)?socialrank\.com$" name="^(?:_csrf|referrer)$" /-->
	<!--securecookie host="^\.socialrank\.com$" name="socialrank\.sid$" /-->

	<securecookie host="^(?:\.|www\.)?socialrank\.com$" name="" />


	<rule from="^http:"
		to="https:" />

</ruleset>
