<ruleset name="Say Media (mismatches)" default_off="mismatch">

	<!--	cert: *.sixapart.com	-->
	<target host="blogs.com" />
	<target host="www.blogs.com" />


	<rule from="^http://(?:www\.)?blogs\.com/"
		to="https://blogs.com/" />

</ruleset>
