<!--
	Server sends no certificate chain *

	* See https://whatsmychaincert.com


	Mixed content:

		- Images from ^waag.org

-->
<ruleset name="Waag.org" default_off="missing certificate chain">

	<!--	Direct rewrites:
				-->
	<target host="waag.org" />
	<target host="www.waag.org" />


	<rule from="^http:"
		to="https:" />

</ruleset>
