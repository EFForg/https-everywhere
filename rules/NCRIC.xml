<ruleset name="NCRIC">

	<target host="ncric.org" />
	<target host="www.ncric.org" />


	<securecookie host="^ncric\.org$" name=".+" />


	<rule from="^http://ncric\.org/"
		to="https://ncric.org/" />

	<!--	Server drops paths, sans trailing slash:
								-->
	<rule from="^http://www\.ncric\.org/.*"
		to="https://ncric.org/" />

</ruleset>