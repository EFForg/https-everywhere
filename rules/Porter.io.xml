<!--
	Insecure cookies are set for these domains:

		- .porter.io

-->
<ruleset name="Porter.io">

	<!--	Direct rewrites:
				-->
	<target host="porter.io" />
	<target host="www.porter.io" />


	<!--	CloudFlare cookies:
					-->
	<!--securecookie host="^\.porter\.io$" name="^(__cfduid|cf_clearance)$" /-->

	<securecookie host="^\.porter\.io$" name=".+" />


	<rule from="^http:"
		to="https:" />

</ruleset>
