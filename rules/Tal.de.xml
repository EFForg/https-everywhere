<ruleset name="tal.de">
  <target host="tal.de" />
  <target host="*.tal.de" />

  <securecookie host="^(?:api|backup|mysql\d\d|service|webmail|winbackup|xvp)\.tal\.de$" name=".+" />

  <rule from="^http://(api|backup|mysql\d\d|service|webmail|winbackup|xvp)\.tal\.de/" to="https://$1.tal.de/" />
</ruleset>
