<!--
	Insecure cookies are set for these domains:

		- .cobalt.io

-->
<ruleset name="Cobalt.io">

	<!--	Direct rewrites:
				-->
	<target host="cobalt.io" />
	<target host="www.cobalt.io" />


	<!--	CloudFlare cookies:
					-->
	<!--securecookie host="^\.cobalt\.io$" name="^(?:__cfduid|cf_clearance)$" /-->

	<securecookie host="^\.cobalt\.io$" name=".+" />


	<rule from="^http:"
		to="https:" />

</ruleset>
