<!--
	For rules that are on by default, see NetApp.xml.

-->
<ruleset name="NetApp.com (mismatches)" default_off="mismatched">

	<!--	Direct rewrites:
				-->
	<target host="media.netapp.com" />


	<securecookie host="^\.netapp\.com$" name=".+" />


	<rule from="^http:"
		to="https:" />

</ruleset>
