<ruleset name="Uptilo">

	<target host="uptilo.com" />
	<target host="*.uptilo.com" />


	<securecookie host="^(?:.*\.)?uptilo\.com$" name=".+" />


	<rule from="^http://(esm\.|www\.)?uptilo\.com/"
		to="https://$1uptilo.com/" />

	<rule from="^http://assets\.uptilo\.com/"
		to="https://d3rvd86gsdneth.cloudfront.net/" />

</ruleset>
