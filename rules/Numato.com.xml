<ruleset name="Numato.com (partial)">

	<target host="numato.com" />
	<target host="www.numato.com" />


	<rule from="^http://(www\.)?numato\.com/(?=[\w-]+\.(?:gif|jpg|png)|cart/checkout(?:$|\?.*|/\??$)|misc/|sites/)"
