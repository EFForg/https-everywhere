<!--
	Fully covered subdomains:

		- (www.)
		- blog
		- cvs
		- git
		- svn
		- toccata
		- web
		- wiki

-->
<ruleset name="ens-lyon.org" platform="cacert">

	<target host="ens-lyon.org" />
	<target host="*.ens-lyon.org" />


	<rule from="^http://((?:blog|cvs|git|svn|toccata|web|wiki|www)\.)?ens-lyon\.org/"
		to="https://$1ens-lyon.org/" />

</ruleset>
