<!--
	Mismatch:
		serverbase.ch:
			- stats
			- status
			- vm*
		*serverbase-status.net
-->
<ruleset name="ServerBase">
	<target host="serverbase.ch"/>
	<target host="www.serverbase.ch"/>
	<target host="my.serverbase.ch"/>
	<target host="support.serverbase.ch"/>

	<securecookie host=".*\.serverbase\.ch$" name=".+" />

	<rule from="^http:"
		to="https:"/>
</ruleset>
