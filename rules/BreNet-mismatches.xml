<!--
	For rules that are on by default, see BreNet.xml.

-->
<ruleset name="BreNet (mismatches)" default_off="mismatch, self-signed">

	<!--	Self-signed	-->
	<target host="vz46.brenet.de" />
	<!--	Cert: www.brenet.de	-->
	<target host="webmail.brenet.de" />


	<!--	Cookies are handled in the main ruleset.	-->


	<rule from="^http://(vz46|webmail)\.brenet\.de/"
		to="https://$1.brenet.de/" />

</ruleset>
