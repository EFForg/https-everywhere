<ruleset name="S-pankki">
	<target host="s-pankki.fi"/>
	<target host="www.s-pankki.fi"/>
	<target host="online.s-pankki.fi"/>

	<securecookie host="^(?:www|online)\.s-pankki\.fi$" name=".+" />

	<!-- Cert matches www -->
	<rule from="^http://(?:www\.)?s-pankki\.fi/"
		to="https://www.s-pankki.fi/"/>

	<rule from="^http://online\.s-pankki\.fi/"
		to="https://online.s-pankki.fi/" />

</ruleset>
