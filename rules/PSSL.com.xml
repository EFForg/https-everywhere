<!--
	Problematic subdomains:

		- ^	(mismatched)

-->
<ruleset name="PSSL.com">

	<target host="pssl.com" />
	<target host="*.pssl.com" />


	<securecookie host=".+\.pssl\.com$" name=".+" />


	<rule from="^http://(?:www\.)?pssl\.com/"
		to="https://www.pssl.com/" />

	<rule from="^http://espanol\.pssl\.com/"
		to="https://espanol.pssl.com/" />

</ruleset>