<!--
	^sphere.com: Mismatched

-->
<ruleset name="Sphere.com">

	<!--	Direct rewrites:
				-->
	<target host="www.sphere.com" />

	<!--	Complications:
				-->
	<target host="sphere.com" />


	<rule from="^http://sphere\.com/"
		to="https://www.sphere.com/" />

	<rule from="^http:"
		to="https:" />

</ruleset>
