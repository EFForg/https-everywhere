<!-- Nonfunctional subdomains:
	- db02.jumptap.com
	- dine.jumptap.com
	- reporting.jumptap.com
-->
<ruleset name="Jumptap">
	<target host="*.jumptap.com" />

	<exclusion pattern="^http://(?:db02|dine|reporting).jumptap.com" />
	<rule from="^http://([^@:/]*)\.jumptap\.com/" to="https://$1.jumptap.com/" />
</ruleset>
