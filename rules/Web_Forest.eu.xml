<!--
	Problematic subdomains:

		- www	(cert only matches ^webforest.eu)

-->
<ruleset name="Web Forest.eu">

	<target host="webforest.eu" />
	<target host="www.webforest.eu" />


	<securecookie host="^webforest\.eu$" name=".+" />


	<rule from="^http://(?:www\.)?webforest\.eu/"
		to="https://webforest.eu/" />

</ruleset>