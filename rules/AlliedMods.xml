<ruleset name="AlliedModders">
	<target host="alliedmods.net" />
	<target host="*.alliedmods.net" />

	<securecookie host="^(?:.*\.)?alliedmods\.net$" name=".+" />

	<rule from="^http://([^/@:\.]+)\.alliedmods\.net/"
		to="https://$1.alliedmods.net/" />

	<rule from="^http://alliedmods\.net/" to="https://alliedmods.net/" />
</ruleset>
