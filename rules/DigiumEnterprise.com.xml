<!-- On-line survey service; this rule only redirects actual survey responses,
     because not all URLs appear to support HTTPS.  -->
<ruleset name="DigiumEnterprise.com">
  <target host="digiumenterprise.com" />

  <rule from="^http://digiumenterprise\.com/answer/" to="https://digiumenterprise.com/answer/" />
</ruleset>
