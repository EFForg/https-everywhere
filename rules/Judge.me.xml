<ruleset name="Judge.me" default_off="mismatch">

	<!--	Cert: *.heroku.com	-->
	<target host="judge.me" />
	<target host="www.judge.me" />


	<securecookie host="^www\.judge\.me$" name=".*" />


	<!--	!www redirects to www.	-->
	<rule from="^http://(?:www\.)?judge\.me/"
		to="https://www.judge.me/" />

</ruleset>
