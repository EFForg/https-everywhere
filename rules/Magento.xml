	<target host="golinks.magento.com" />

	<!--	Complications:
				-->
	<target host="www.magento.com" />

		<test url="http://go.magento.com/migration" />


	<rule from="^http://www\.magento\.com/"
		to="https://magento.com/" />

	<rule from="^http:"
		to="https:" />

</ruleset>
