<!--
	for Zendesk clients which otherwise have no rules.

-->
<ruleset name="Zendesk.com clients (partial)">

	<target host="support.photobucket.com" />


	<rule from="^http://support\.photobucket\.com/(assets|external|generated|images|registration|system)/"
		to="https://photobucket.zendesk.com/$1/" />

</ruleset>
