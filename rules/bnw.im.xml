<ruleset name="bnw.im">

	<!--	Direct rewrites:
				-->
  <target host="bnw.im" />
  <target host="meow.bnw.im" />

	<!--	Complications:
				-->
  <target host="www.bnw.im" />

  <securecookie host="^bnw\.im$" name=".*" />
  <rule from="^http://www\.bnw\.im/" to="https://bnw.im/" />
  <rule from="^http:" to="https:" />
</ruleset>
