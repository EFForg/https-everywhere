<ruleset name="Patterns in the Void.net" default_off="self-signed">

	<target host="patternsinthevoid.net" />
	<target host="*.patternsinthevoid.net" />


	<!--	Not secured by server:
					-->
	<!--securecookie host="^(blog|code|www)\.patternsinthevoid\.net$" name="^TRACKID$" /-->

	<securecookie host="^(?:blog|code|www)\.patternsinthevoid\.net$" name=".+" />


	<rule from="^http://(?:www\.)?patternsinthevoid\.net/"
		to="https://www.patternsinthevoid.net/" />

	<rule from="^http://(blog|code)\.patternsinthevoid\.net/"
		to="https://$1.patternsinthevoid.net/" />

</ruleset>
