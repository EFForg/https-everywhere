<!--
	Fully covered hosts in *equityzen.com:

		- (www.)?
		- dev
		- staging

-->
<ruleset name="EquityZen.com">

	<!--	Direct rewrites:
				-->
	<target host="equityzen.com" />
	<target host="dev.equityzen.com" />
	<target host="staging.equityzen.com" />
	<target host="www.equityzen.com" />


	<rule from="^http:"
		to="https:" />

</ruleset>
