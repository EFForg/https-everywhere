<ruleset name="Brian Smith.org">

	<target host="briansmith.org" />
	<target host="*.briansmith.org" />


	<securecookie host="^\.briansmith\.org$" name=".+" />


	<!--	www redirects to ^ over http,
		so copy that behavior:
					-->
	<rule from="^http://(?:www\.)?briansmith\.org/"
		to="https://briansmith.org/" />

</ruleset>
