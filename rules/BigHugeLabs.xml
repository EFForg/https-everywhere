<!--
	Cert only matches ^bighugelabs.com

-->
<ruleset name="BigHugeLabs">

	<target host="bighugelabs.com" />
	<target host="*.bighugelabs.com" />


	<securecookie host="^\.bighugelabs\.com$" name=".+" />


	<rule from="^http://(?:www\.)?bighugelabs\.com/"
		to="https://bighugelabs.com/" />

</ruleset>
