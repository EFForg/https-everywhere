<!--
	Nonfunctional subdomains:

		- support	(cert: *.assistly.com; redirects to http)

-->
<ruleset name="Tea Party Patriots">

	<target host="teapartypatriots.org" />
	<target host="www.teapartypatriots.org" />


	<securecookie host="^www\.teapartypatriots\.org$" name=".*" />


	<!--	Cert only matches www.	-->
	<rule from="^http://(?:www\.)?teapartypatriots\.org/"
		to="https://www.teapartypatriots.org/" />

</ruleset>
