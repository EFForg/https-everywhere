<!--
	Problematic subdomains:

		- ^	(recursive redirect)

-->
<ruleset name="Privacy Not Prism.org.uk">

	<target host="privacynotprism.org.uk" />
	<target host="www.privacynotprism.org.uk" />


	<securecookie host="^www\.privacynotprism\.org\.uk$" name=".+" />


	<rule from="^http://(?:www\.)?privacynotprism\.org\.uk/"
		to="https://www.privacynotprism.org.uk/" />

</ruleset>
