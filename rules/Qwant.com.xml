<!--
	STS header includes includeSubDomains.

-->
<ruleset name="Qwant.com">

	<!--	Direct rewrites:
				-->
	<target host="qwant.com" />
	<target host="*.qwant.com" />

		<test url="http://blog.qwant.com/" />
		<test url="http://lite.qwant.com/" />
		<test url="http://m.qwant.com/" />
		<test url="http://pressroom.qwant.com/" />
		<test url="http://s.qwant.com/" />
		<test url="http://stats.qwant.com/" />
		<test url="http://www.qwant.com/" />


	<rule from="^http:"
		to="https:" />

</ruleset>
