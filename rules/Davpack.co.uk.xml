<!--
	CDN buckets:

		- davpack.s3.amazonaws.com


	Cert only matches www.

-->
<ruleset name="Davpack.co.uk">

	<target host="davpack.co.uk" />
	<target host="www.davpack.co.uk" />


	<rule from="^http://(?:www\.)?davpack\.co\.uk/(?=(?:checkout|Signin)\.aspx|favicon\.ico|(?:Script|Web)Resource\.axd)"
		to="https://www.davpack.co.uk/" />

</ruleset>
