<ruleset name="Bendigo Bank">
  <target host="bendigobank.com.au" />
  <target host="*.bendigobank.com.au" />

  <rule from="^http://bendigobank\.com\.au/"
          to="https://www.bendigobank.com.au/" />
  <rule from="^http://(edroom|m|shop|www)\.bendigobank\.com\.au/"
          to="https://$1.bendigobank.com.au/" />
</ruleset>