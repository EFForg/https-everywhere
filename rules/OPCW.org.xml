<!--
	Organisation for the Prohibition of Chemical Weapons

	http redirect:
		- ^

	Mixed content:
		- Images from $self *

	* Secured by us
-->
<ruleset name="OPCW.org">
	<target host="opcw.org" />
	<target host="www.opcw.org" />

	<rule from="^http://opcw\.org/"
		to="https://www.opcw.org/" />
	<rule from="^http:"
		to="https:" />
</ruleset>
