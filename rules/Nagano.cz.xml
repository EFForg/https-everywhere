<ruleset name="Nagano.cz" default_off="expired, mismatch, self-signed">

	<target host="nagano.cz"/>
	<target host="*.nagano.cz"/>

	<rule from="^http://(?:www\.)?nagano\.cz/"
		to="https://nagano.cz/"/>

	<rule from="^http://([^w]\w*|w[^w]\w*)\.nagano\.cz/"
		to="https://$1.nagano.cz/"/>

</ruleset>
