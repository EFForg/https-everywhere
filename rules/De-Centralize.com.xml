<!--
	Mixed content:

		- css from synduimages.synduweb.netdna-cdn.com

		- Images, from:

			- *.cloudfront.net
			- *.images-amazon.com
			- *.netdna-cdn.com
			- synduimages.synduweb.netdna-cdn.com

		- Ads, from:

			- ws-\w\w.amazon-adsystem.com
			- ir-\w\w.amazon-adsystem.com

-->
<ruleset name="De-Centralize.com" default_off="expired, mismatched" platform="mixedcontent">

	<target host="de-centralize.com" />
	<target host="www.de-centralize.com" />


	<rule from="^http://(?:www\.)?de-centralize\.com/"
		to="https://www.de-centralize.com/" />

</ruleset>
