<!--
	For other AOL coverage, see AOL.xml.


	Problematic domains:

		- aim.com
		- cdn.aim.com		(akamai)

-->
<ruleset name="AIM.com">

	<!--	Direct rewrites:
				-->
	<target host="www.aim.com" />

	<!--	Complications:
				-->
	<target host="aim.com" />
	<target host="cdn.aim.com" />


	<rule from="^http://(?:cdn\.)?aim\.com/"
		to="https://www.aim.com/" />

	<rule from="^http:"
		to="https:" />

</ruleset>
