<ruleset name="Artemis.net">

	<target host="artemis.net" />
	<target host="*.artemis.net" />
	<target host="artemisinternet.com" />
	<target host="*.artemisinternet.com" />


	<securecookie host="^\.artemis(?:\.net|internet\.com)$" name=".+" />


	<rule from="^http://(www\.)?artemis(\.net|internet\.com)/"
		to="https://$1artemis$2/" />

</ruleset>
