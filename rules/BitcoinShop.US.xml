<!--
	Other BitcoinShop.US rulesets:

		- BTCS.com.xml

-->
<ruleset name="BitcoinShop.US" default_off="mismatched">

	<!--	Direct rewrites:
				-->
	<target host="bitcoinshop.us" />
	<target host="www.bitcoinshop.us" />


	<securecookie host="^\.(?:www\.)?bitcoinshop\.us$" name=".+" />


	<rule from="^http:"
		to="https:" />

</ruleset>
