<ruleset name="Geoclub.de (partial)">

	<target host="geoclub.de" />
	<target host="www.geoclub.de" />

	<securecookie host="^\.geoclub\.de$" name=".+" />

	<rule from="^http:"
		to="https:" />

</ruleset>
