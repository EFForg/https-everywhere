<!--
	Other levels.io rulesets:

		- GifBook.io.xml


	www.levels.io: Redirects to http$

-->
<ruleset name="levels.io">

	<!--	Direct rewrites:
				-->
	<target host="levels.io" />

	<!--	Complications:
				-->
	<target host="www.levels.io" />


	<rule from="^http://www\.levels\.io/"
		to="https://levels.io/" />

	<rule from="^http:"
		to="https:" />

</ruleset>
