<!--
	For rules that are on by default, see CARI.net.xml.

-->
<ruleset name="CARI.net (self-signed)" default_off="self-signed">

	<target host="webmail.cari.net" />


	<!--	Cookies are handled in CARI.net.xml.	-->


	<rule from="^http://webmail\.cari\.net/"
		to="https://webmail.cari.net/" />

</ruleset>
