<!--
	CDN buckets:

		- skitch-img.s3.amazonaws.com

-->
<ruleset name="Skitch">
  <target host="skitch.com" />
  <target host="*.skitch.com" />

	<securecookie host="^(?:.*\.)?skitch\.com$" name=".*"/>

  <rule from="^http://(?:www\.)?skitch\.com/"
          to="https://skitch.com/" />
  <rule from="^http://img\.skitch\.com/"
          to="https://img.skitch.com/" />
</ruleset>
