<!-- already enforces SSL. adding to prevent ssl strip attacks -->
<ruleset name="Nordnet.se" platform="mixedcontent">
  <target host="www.nordnet.se" />
  <target host="nordnet.se" />
  <rule from="^http://www\.nordnet\.se/" to="https://www.nordnet.se/"/>
  <rule from="^http://nordnet\.se/" to="https://www.nordnet.se/"/>
</ruleset>

