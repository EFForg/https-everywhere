<!--
	Informational Democracy Foundation


	Fully covered subdomains:

		- (www.)
		- css
		- js

-->
<ruleset name="ROI.ru">

	<target host="roi.ru" />
	<target host="*.roi.ru" />


	<securecookie host="^\.roi\.ru$" name=".+" />


	<rule from="^http://((?:css|js|www)\.)?roi\.ru/"
		to="https://$1roi.ru/" />

</ruleset>