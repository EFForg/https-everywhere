<ruleset name="get Shine.com">

	<target host="getshine.com" />
	<target host="*.getshine.com" />


	<securecookie host="^\.getshine\.com$" name=".+" />


	<!--	^ redirects to www over http,
		so copy that behavior:
					-->
	<rule from="^http://(?:www\.)?getshine\.com/"
		to="https://www.getshine.com/" />

</ruleset>
