<ruleset name="Money Advice Service (UK)">
    <target host="moneyadviceservice.org.uk" />
    <target host="*.moneyadviceservice.org.uk" />

    <rule from="^http://moneyadviceservice\.org\.uk/"
        to="https://www.moneyadviceservice.org.uk/" />
