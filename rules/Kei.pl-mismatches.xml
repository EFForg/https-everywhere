<!--	See Kri.pl.xml also
-->
<ruleset name="Kei.pl (mismatches)" default_off="expired, mismatch, self-signed">

	<target host="default.kei.pl"/>
	<target host="kreator.kei.pl"/>

	<rule from="^http://(default|kreator)\.kei\.pl/"
		to="https://$1.kei.pl/"/>

	<securecookie host="^kreator\.kei\.pl$" name=".*"/>

</ruleset>
