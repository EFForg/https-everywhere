<!--
	See CDS-Global.xml for rules that are on by default.

-->
<ruleset name="CDS Global (mismatches)" default_off="mismatch">

	<!--	Cert: w1.buysub.com	-->
	<target host="buysub.com"/>


	<securecookie host="^buysub\.com$" name=".*" />


	<rule from="^http://buysub\.com/"
		to="https://buysub.com/" />

</ruleset>
