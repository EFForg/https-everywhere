<ruleset name="NZ Herald.co.nz (problematic)" default_off="mismatched">

	<target host="nzherald.co.nz" />
	<target host="www.nzherald.co.nz" />


	<securecookie host="^www\.nzherald\.co\.nz$" name=".+" />


	<rule from="^http://(?:www\.)?nzherald\.co\.nz/"
		to="https://www.nzherald.co.nz/" />

</ruleset>