<!--
	Mismatch:
		- 1.8.swiss
		- ^nic.swiss
-->
<ruleset name=".swiss">
	<target host="dot.swiss" />
	<target host="*.dot.swiss" />
	<target host="nic.swiss" />
	<target host="*.nic.swiss" />

	<test url="http://www.nic.swiss/" />
	<test url="http://www.nic.swiss/nic/en/home.html" />
	<test url="http://www.dot.swiss/" />
	<test url="http://dot.swiss/registrieren/" />

	<rule from="^http://nic\.swiss/"
		to="https://www.nic.swiss/" />
	<rule from="^http:"
		to="https:" />
</ruleset>
