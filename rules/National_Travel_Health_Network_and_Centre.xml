<ruleset name="National Travel Health Network and Centre (NaTHNaC)">
    <target host="nathnac.org" />
    <target host="*.nathnac.org" />

    <rule from="^http://nathnac\.org/"
        to="https://www.nathnac.org/" />
    <rule from="^http://([^/:@]+)?\.nathnac\.org/"
        to="https://$1.nathnac.org/" />
</ruleset>
