<!--
	Nonfunctional subdomains:

		- (www.)


	Fully covered subdomains:

		- llm
		- lsaclookup
		- os

-->
<ruleset name="Law School Admission Council (partial)">

	<target host="*.lsac.org" />


	<securecookie host="^.+\.lsac\.org$" name=".+" />


	<rule from="^http://(llm|lsaclookup|os)\.lsac\.org/"
		to="https://$1.lsac.org/" />

</ruleset>