<!--
	Problematic subdomains:

		- ^	(cert only maches *.thompson.com)

-->
<ruleset name="Thompson.com">

	<target host="thompson.com" />
	<target host="www.thompson.com" />


	<securecookie host="^www\.thompson\.com$" name=".+" />


	<rule from="^http://(?:www\.)?thompson\.com/"
		to="https://www.thompson.com/" />

</ruleset>