<ruleset name="Nasuni.com (partial)">

	<target host="nasuni.com"/>
	<target host="account.nasuni.com"/>
	<target host="www.nasuni.com"/>

	<rule from="^http://(www\.)?nasuni\.com/(account|evaluate|fonts/|img/|writable/)" to="https://www.nasuni.com/$2"/>

	<rule from="^http://account\.nasuni\.com/" to="https://account.nasuni.com/"/>

	<securecookie host="^account\.nasuni\.com$" name=".*"/>

</ruleset>
