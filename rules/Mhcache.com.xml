<!--
    Other MyHeritage rulesets:
	    - Myheritage
	    - Myheritageimages.com
-->
<ruleset name="MyHeritage Cache">
    <target host="mhcache.com" />
    <target host="www.mhcache.com" />
    <target host="d.mhcache.com" />

    <securecookie host="^(www\.|d\.)?mhcache.com" name=".+" />

    <rule from="^http:"
            to="https:" />
</ruleset>
