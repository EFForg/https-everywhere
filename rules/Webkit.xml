<!--	!functional:
		- piwik.wk1.net		(cert: pageloc.com; shows that domain's data)
-->
<ruleset name="Webkit" default_off="shows default page">

	<target host="pageloc.com"/>
	<target host="www.pageloc.com"/>

	<securecookie host="^pageloc\.com$" name=".*"/>

	<rule from="^http:"
		to="https:" />

</ruleset>
