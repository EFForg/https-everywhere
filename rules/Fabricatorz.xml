<!--
	Problematic subdomains:

		- ^	(mismatched)

-->
<ruleset name="Fabricatorz">

	<target host="fabricatorz.com" />
	<target host="*.fabricatorz.com" />


	<securecookie host="^fabricatorz\.com$" name=".+" />


	<rule from="^http://(?:(private\.)|www\.)?fabricatorz\.com/"
		to="https://$1fabricatorz.com/" />

</ruleset>
