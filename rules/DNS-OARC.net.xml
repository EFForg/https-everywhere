<ruleset name="DNS-OARC.net">

	<!--	Direct rewrites:
				-->
	<target host="indico.dns-oarc.net" />
	<target host="lists.dns-oarc.net" />
	<target host="www.dns-oarc.net" />

	<!--	Special cases:
				-->
	<target host="dns-oarc.net" />


	<rule from="^http://dns-oarc\.net/"
		to="https://www.dns-oarc.net/" />

	<rule from="^http:"
		to="https:" />

</ruleset>
