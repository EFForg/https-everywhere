<ruleset name="Shurgard">
    <target host="shurgardpayment.eu" />
    <target host="*.shurgardpayment.eu" />

    <rule from="^http://shurgardpayment\.eu/"
        to="https://www.shurgardpayment.eu/"/>
    <rule from="^http://([^/:@]+)?\.shurgardpayment\.eu/"
        to="https://$1.shurgardpayment.eu/" />
</ruleset>
