	<target host="static.infnx.com"/>

		<test url="http://idn.infnx.com/fixunix/fixunix-logo.png" />


	<rule from="^http://(idn|static)\.infnx\.com/"
		to="https://s3.amazonaws.com/$1.infnx.com/"/>

</ruleset>
