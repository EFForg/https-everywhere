<!--
        Functional domain:
                - www.mobile01.com/login.php
-->
<ruleset name="Mobile01.com">

	<target host="www.mobile01.com" />

  	<exclusion pattern="^http://www\.mobile01\.com/(?!login\.php)" />

		<test url="http://www.mobile01.com/" />

	<rule from="^http:" to="https:" />
	
		<test url="http://www.mobile01.com/login.php" />

</ruleset>
