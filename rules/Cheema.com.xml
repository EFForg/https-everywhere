<ruleset name="cheema.com" default_off="expired, self-signed">

	<target host="cheema.com" />
	<target host="www.cheema.com" />


	<securecookie host="^cheema\.com$" name=".*" />


	<!--	Cert only matches //cheema.com.	-->
	<rule from="^http://(?:www\.)?cheema\.com/"
		to="https://cheema.com/" />

</ruleset>
