<!--
	Problematic subdomains:

		- ^	(404, valid cert)

-->
<ruleset name="Roskilde University (partial)">

	<target host="ruc.dk" />
	<target host="www.ruc.dk" />


	<securecookie host="^www\.ruc\.dk$" name=".+" />


	<rule from="^http://(?:www\.)?ruc\.dk/"
		to="https://www.ruc.dk/" />

</ruleset>