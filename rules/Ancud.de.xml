<!--
	Ancud IT-Beratung


	Cert only matches www

-->
<ruleset name="Ancud.de" default_off="self-signed">

	<target host="ancud.de" />
	<target host="www.ancud.de" />


	<securecookie host="^www\.ancud\.de$" name=".+" />


	<rule from="^http://(?:www\.)?ancud\.de/"
		to="https://www.ancud.de/" />

</ruleset>