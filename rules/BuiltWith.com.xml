<!--
	Fully covered subdomains:

		- (www.)?
		- api
		- blog
		- kb
		- s-orig
		- trends
		- trendspro


	These altnames don't exist:

		- www.kb.builtwith.com


	Mixed content:

		- Image on blog from $self *

	* Secured by us

-->
<ruleset name="BuiltWith.com">

	<target host="builtwith.com" />
	<target host="*.builtwith.com" />


	<rule from="^http://((?:api|blog|kb|s-orig|trends|trendspro|www)\.)?builtwith\.com/"
		to="https://$1builtwith.com/" />

</ruleset>
