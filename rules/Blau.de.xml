<!--
	Observed subdomains:

		- (www.)
		- handel
		- handyshop
		- images
		- konto
		- mein
		- meinkonto

-->
<ruleset name="Blau.de">

	<target host="blau.de" />
	<target host="*.blau.de" />


	<securecookie host="^(?:.+\.)?blau\.de$" name=".+" />


	<rule from="^http://([^/:@]+\.)?blau\.de/"
		to="https://$1blau.de/" />

</ruleset>
