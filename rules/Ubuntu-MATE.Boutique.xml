	<!--	CloudFlare cookies:
					-->
	<!--securecookie host="^\.ubuntu-mate\.boutique$" name="^(?:__cfduid|cf_clearance)$" /-->

	<securecookie host="^\.ubuntu-mate\.boutique$" name=".+" />


	<rule from="^http:"
		to="https:" />

</ruleset>
