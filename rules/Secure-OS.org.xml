<!--
	www.secure-os.org: Mismatched

-->
<ruleset name="Secure-OS.org">

	<!--	Direct rewrites:
				-->
	<target host="secure-os.org" />

	<!--	Complications:
				-->
	<target host="www.secure-os.org" />


	<rule from="^http://www\.secure-os\.org/"
		to="https://secure-os.org/" />

	<rule from="^http:"
		to="https:" />

</ruleset>
