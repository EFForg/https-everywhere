<!--
	Fully covered subdomains:

		- (www.)


	Observed cookie domains:

		- www

-->
<ruleset name="Yopto.com" default_off="mismatched">

	<!--	Direct rewrites:
				-->
	<target host="yopto.com" />
	<target host="www.yopto.com" />


	<securecookie host="^(?:www)\.yopto\.com$" name=".+" />


	<rule from="^http:"
		to="https:" />

</ruleset>
