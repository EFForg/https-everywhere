<ruleset name="Bloodhound Gang.com">

	<target host="bloodhoundgang.com" />
	<target host="*.bloodhoundgang.com" />


	<securecookie host="^\.(?:stufftobuy\.)?bloodhoundgang\.com$" name=".+" />


	<rule from="^http://(?:www\.)?bloodhoundgang\.com/"
