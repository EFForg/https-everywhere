<!--
	www.sysdig.com: Expired cert at redirect destination

-->
<ruleset name="Sysdig.com">

	<!--	Direct rewrites:
				-->
	<target host="sysdig.com" />

	<!--	Complications:
				-->
	<target host="www.sysdig.com" />


	<securecookie host="^\w" name="." />


	<rule from="^http://www\.sysdig\.com/"
		to="https://sysdig.com/" />

	<rule from="^http:"
		to="https:" />

</ruleset>
