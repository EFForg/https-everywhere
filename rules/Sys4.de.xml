<!--
	Other sys4 rulesets:

		- Automx.org.xml


	Problematic hosts in *sys4.de:

		- blog *

	* Mismatched

-->
<ruleset name="sys4.de">

	<target host="sys4.de" />
	<target host="www.sys4.de" />

	<!--	Complications:
				-->
	<target host="blog.sys4.de" />


	<rule from="^http://blog\.sys4\.de/.*"
		to="https://sys4.de/blog/" />

		<test url="http://blog.sys4.de//home.php" />

	<rule from="^http:"
		to="https:" />

</ruleset>
