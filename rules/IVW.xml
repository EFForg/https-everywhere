<!--
	Nonfunctional subdomains:

		- ^ *		(shows heft)
		- daten *	(redirects to http)
		- www		(shows heft)

	* Mismatched, CN: heft.ivw.eu
	


-->
<ruleset name="IVW (partial)">

	<target host="heft.ivw.eu" />


	<securecookie host="^heft\.ivw\.eu$" name=".+" />


	<rule from="^http://heft\.ivw\.eu/"
		to="https://heft.ivw.eu/" />

</ruleset>