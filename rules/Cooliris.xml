<!--
	Problematic subdomains:

		- ^	(times out)

-->
<ruleset name="Cooliris">

	<target host="cooliris.com" />
	<target host="*.cooliris.com" />



	<rule from="^http://(?:www\.)?cooliris\.com/"
		to="https://www.cooliris.com/" />

	<rule from="^http://apps\.cooliris\.com/"
		to="https://apps.cooliris.com/" />

</ruleset>