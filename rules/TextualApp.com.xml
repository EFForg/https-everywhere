<!--
	^textualapp.com: 522


	STS header includes includeSubdomains

-->
<ruleset name="TextualApp.com">

	<!--	Direct rewrites:
				-->
	<target host="*.textualapp.com" />

	<!--	Complications:
				-->
	<target host="textualapp.com" />

		<test url="http://www.textualapp.com/" />


	<securecookie host="." name="." />


	<rule from="^http://textualapp\.com/"
		to="https://www.textualapp.com/" />

	<rule from="^http:"
		to="https:" />

</ruleset>
