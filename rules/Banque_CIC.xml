<ruleset name="Banque CIC">
    <target host="cic.fr" />
    <target host="*.cic.fr" />

    <securecookie host="^(?:.*\.)?cic\.fr$" name=".+" />

    <rule from="^http://cic\.fr/"
        to="https://www.cic.fr/"/>
    <rule from="^http://([^/:@]+)?\.cic\.fr/"
        to="https://$1.cic.fr/" />
</ruleset>
