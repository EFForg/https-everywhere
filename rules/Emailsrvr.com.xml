<!--
	www times out over http.


	Fully covered subdomains:

		- ^

		- autodiscover.*:

			mex07a

-->
<ruleset name="emailsrvr.com">

	<target host="emailsrvr.com" />
	<target host="*.emailsrvr.com" />


	<rule from="^http://(autodiscover\.\w+\.)?emailsrvr\.com/"
		to="https://$1emailsrvr.com/" />

</ruleset>
