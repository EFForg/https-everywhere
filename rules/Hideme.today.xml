<!--
	^: Expired
	www: cert only matches ^hideme.today

-->
<ruleset name="hideme.today" default_off="expired">

	<!--	Direct rewrites:
				-->
	<target host="www.hideme.today" />

	<!--	Complications:
				-->
	<target host="hideme.today" />


	<rule from="^http://www\.hideme\.today/"
		to="https://hideme.today/" />

	<rule from="^http:"
		to="https:" />

</ruleset>
