<ruleset name="Commission Junction" platform="mixedcontent">

	<target host="www.afcyhf.com" />
	<target host="www.anrdoezrs.net" />
	<target host="www.apmebf.com" />
	<target host="www.awltovhc.com" />
	<target host="www.awxibrm.com" />
	<target host="cj.com" />
	<target host="*.cj.com" />
	<target host="www.commission-junction.com" />
	<target host="www.cualbr.com" />
	<target host="www.dpbolvw.net" />
	<target host="www.emjcd.com" />
	<target host="www.ftjcfx.com" />
	<target host="www.jdoqocy.com" />
	<target host="www.kdukvh.com" />
	<target host="www.kqzyfj.com" />
	<target host="www.lduhtrp.net"/>
	<target host="www.pkracv.com" />
	<target host="www.rnsfpw.net" />
	<target host="www.qksrv.net" />
	<target host="www.qksz.net" />
	<target host="www.tkqlhce.com" />
	<target host="www.tqlkg.com" />
	<target host="www.vofzpwh.com" />
	<target host="www.yceml.net" />


	<securecookie host="^(?:.*\.)?(?:apmebf|cj)\.com$" name=".+" />


	<rule from="^http://www\.(afcyhf|anrdoezrs|apmebf|awltovhc|commission-junction|dpbolvw|emjcd|ftjcfx|jdoqocy|kdukvh|kqzyfj|lduhtrp|pkracv|tkqlhce|tqlkg)\.(com|net)/"
		to="https://www.$1.$2/" />

	<rule from="^http://www\.(?:awxibrm|cualbr|rnsfpw|vofzpwh|yceml)\.(?:com|net)/"
		to="https://www.apmebf.com/" />

	<rule from="^http://(?:www\.)?cj\.com/"
		to="https://www.cj.com/" />

	<rule from="^http://(members|signup)\.cj\.com/"
		to="https://$1.cj.com/" />

	<rule from="^http://www\.qks(?:rv|z)\.net/"
		to="https://www.qksrv.net/" />

</ruleset>
