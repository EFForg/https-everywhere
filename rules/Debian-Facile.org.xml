<!--
	www.debian-facile.org: Mismatched

-->
<ruleset name="Debian-Facile.org">

	<!--	Direct rewrites:
				-->
	<target host="debian-facile.org" />

	<!--	Complications:
				-->
	<target host="www.debian-facile.org" />


	<rule from="^http://www\.debian-facile\.org/"
		to="https://debian-facile.org/" />

	<rule from="^http:"
		to="https:" />

</ruleset>
