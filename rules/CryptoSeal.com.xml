<!--
	Fully covered subdomains:

		- (www.)
		- connect

-->
<ruleset name="CryptoSeal.com">

	<target host="cryptoseal.com" />
	<target host="connect.cryptoseal.com" />
	<target host="www.cryptoseal.com" />


	<securecookie host="^\.cryptoseal\.com$" name=".+" />


	<rule from="^http:"
		to="https:" />

</ruleset>
