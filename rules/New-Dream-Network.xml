<!--
	Other New Dream Network rulesets:

		- DreamHost.xml

-->
<ruleset name="New Dream Network">

	<target host="newdream.net" />
	<target host="*.newdream.net" />


	<!--	www: Cert only matches ^newdream.net.
							-->
	<rule from="^http://(?:(secure\.)|www\.)?newdream\.net/"
		to="https://$1newdream.net/" />

</ruleset>