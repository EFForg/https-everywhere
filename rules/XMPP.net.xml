<!--
	www: mismatched, CN: beta.xmpp.net

-->
<ruleset name="XMPP.net">

	<!--	Direct rewrites:
				-->
	<target host="xmpp.net" />
	<target host="beta.xmpp.net" />

	<!--	Complications:
				-->
	<target host="www.xmpp.net" />


	<rule from="^http://www\.xmpp\.net/"
		to="https://xmpp.net/" />

	<rule from="^http:"
		to="https:" />

</ruleset>
