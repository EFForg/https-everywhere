<!--
	Problematic subdomains:

		- ^	(cert only matches www)

-->
<ruleset name="Domreg.lt">

	<target host="domreg.lt" />
	<target host="www.domreg.lt" />


	<securecookie host="^www\.domreg\.lt$" name=".+" />


	<rule from="^http://(?:www\.)?domreg\.lt/"
		to="https://www.domreg.lt/" />

</ruleset>