<ruleset name="Suomi.fi">

	<!-- Has a few subdomains which have cookies too -->

	<target host="suomi.fi" />
	<target host="*.suomi.fi" />


	<securecookie host="^[^@:/\.]+\.suomi\.fi" name=".+" />


	<rule from="^http://((?:www|asiointitili|tunnistus)\.)?suomi\.fi/"
		to="https://$1suomi.fi/" />

</ruleset>
