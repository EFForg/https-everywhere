<!--
	Problematic subdomains:

		- ^	(cert only matches www)


	Some pages redirect to http.

-->
<ruleset name="Uline.com (partial)">

	<target host="uline.com" />
	<target host="www.uline.com" />


	<rule from="^http://(?:www\.)?uline\.com/(?=favicon\.ico|FilesCombiner\.ashx|[iI]mages/|Product/ContainerBrowseListing\.aspx|(?:Script|Web)Resource\.axd|Signin/|WebServices/)"
		to="https://www.uline.com/" />

</ruleset>
