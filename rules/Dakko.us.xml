<!--
	Nonfunctional subdomains:

		- b	(cert: www-s.dakko.us; shows that domain's data)

-->
<ruleset name="dakko.us (partial)">

	<target host="dakko.us" />
	<target host="www.dakko.us" />
	<target host="www-s.dakko.us" />


	<rule from="^http://(www-s\.)?dakko\.us/"
		to="https://$1dakko.us/" />

	<rule from="^http://www\.dakko\.us/"
		to="https://www-s.dakko.us/" />

</ruleset>
