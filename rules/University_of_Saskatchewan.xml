<!--
	Nonfunctional subdomains:

		- aboriginal	(redirects to http, valid cert)


	!www: no https

-->
<ruleset name="University of Saskatchewan (partial)">

	<target host="usask.ca" />
	<target host="www.usask.ca" />


	<rule from="^http://(?:www\.)?usask\.ca/"
		to="https://www.usask.ca/" />

</ruleset>