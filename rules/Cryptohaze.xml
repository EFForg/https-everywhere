<ruleset name="Cryptohaze" default_off="connection refused">

	<target host="cryptohaze.com" />
	<target host="webtables.cryptohaze.com" />
	<target host="www.cryptohaze.com" />


	<securecookie host="^(?:.+\.)?cryptohaze\.com$" name=".+" />


	<rule from="^http:"
		to="https:" />

</ruleset>
