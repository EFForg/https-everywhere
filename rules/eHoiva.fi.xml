<ruleset name="eHoiva">

	<!--	Direct rewrites:
				-->
	<target host="ehoiva-fi.directo.fi" />

	<!--	Complications:
				-->
	<target host="www.ehoiva.fi" />
	<target host="ehoiva.fi" />


	<rule from="^http://(?:www\.)?ehoiva\.fi/"
		to="https://ehoiva-fi.directo.fi/" />

	<rule from="^http:"
		to="https:" />

</ruleset>
