<ruleset name="Cybertip.ca">

	<target host="cyberaide.ca" />
	<target host="www.cyberaide.ca" />
	<target host="cybertip.ca" />
	<target host="*.cybertip.ca" />


	<rule from="^http://(www\.)?cyberaide\.ca/"
		to="https://$1cyberaide.ca/" />

	<rule from="^http://((?:cwa|lea|www)\.)?cybertip\.ca/"
		to="https://$1cybertip.ca/" />

</ruleset>
