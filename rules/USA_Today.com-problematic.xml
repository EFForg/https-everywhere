<!--
	For rules that are on by default, see USA_Today.xml.

-->
<ruleset name="USA Today.com (mismatched)" default_off="mismatched" platform="mixedcontent">

	<target host="usatoday.com" />
	<target host="static.usatoday.com" />
	<target host="www.usatoday.com" />


	<rule from="^http://(?:www\.)?usatoday\.com/"
		to="https://www.usatoday.com/" />

	<rule from="^http://static\.usatoday\.com/"
		to="https://static.usatoday.com/" />

</ruleset>
