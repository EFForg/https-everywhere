<!--
	Nonfunctional subdomains:

		- ^		(mismatched, CN: *.kreativmedia.ch)
		- www		(404, valid cert)

-->
<ruleset name="Kreativ Media (partial)">

	<target host="secure.kreativmedia.ch" />


	<securecookie host="^secure\.kreativmedia\.ch$" name=".+" />


	<rule from="^http://secure\.kreativmedia\.ch/"
		to="https://secure.kreativmedia.ch/" />

</ruleset>