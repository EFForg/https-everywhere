<!--
	For other NPD Group coverage, see NPD_Group.xml.


	Cert only matches www.

-->
<ruleset name="DecisionKey">

	<target host="npddecisionkey.com" />
	<target host="www.npddecisionkey.com" />


	<securecookie host="^www\.npddecisionkey\.com$" name=".+" />


	<rule from="^http://(?:www\.)?npddecisionkey\.com/"
		to="https://www.npddecisionkey.com/" />

</ruleset>
