<!--
	Problematic domains:

		- www.ukwebsolutionsdirect.co.uk *
		- (www.)ukwebsolutionsdirect.com *

	* Mismatched


	.com redirects to .co.uk

-->
<ruleset name="UK Web Solutions Direct">

	<target host="ukwebsolutionsdirect.co.uk" />
	<target host="www.ukwebsolutionsdirect.co.uk" />
	<target host="ukwebsolutionsdirect.com" />
	<target host="www.ukwebsolutionsdirect.com" />


	<securecookie host="^ukwebsolutionsdirect\.co\.uk$" name=".+" />


	<rule from="^http://(?:www\.)?ukwebsolutionsdirect\.co(?:\.uk|m)/"
		to="https://ukwebsolutionsdirect.co.uk/" />

</ruleset>
