<!--
	^mnot.net: "TLS SNI Required"

-->
<ruleset name="mnot.net">

	<!--	Direct rewrites:
				-->
	<target host="www.mnot.net" />

	<!--	Complications:
				-->
	<target host="mnot.net" />


	<rule from="^http://mnot\.net/"
		to="https://www.mnot.net/" />

	<rule from="^http:"
		to="https:" />

</ruleset>
