<ruleset name="Panoramio (problematic)" default_off="mismatched">

	<target host="panoramio.com" />
	<target host="www.panoramio.com" />


	<rule from="^http://(?:www\.)?panoramio\.com/"
		to="https://www.panoramio.com/" />

</ruleset>
