<!--
	^allmend.ch: mismatched
	www.allmend.ch: redirects to ^allmend.ch

-->
<ruleset name="Allmend.ch" default_off="mismatched" platform="cacert">

	<target host="allmend.ch" />
	<target host="www.allmend.ch" />


	<securecookie host="^\w" name="." />


	<rule from="^http:"
		to="https:" />

</ruleset>
