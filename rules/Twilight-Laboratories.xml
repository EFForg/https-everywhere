<ruleset name="Twilight Laboratories" default_off="mismatch, self-signed">

	<!--	advel.cz	-->
	<target host="twibright.com"/>
	<target host="*.twibright.com"/>

	<rule from="^http://twibright\.com/"
		to="https://www.twibright.com/"/>

	<rule from="^http://(\w+)\.twibright\.com/"
		to="https://$1.twibright.com/"/>

</ruleset>
