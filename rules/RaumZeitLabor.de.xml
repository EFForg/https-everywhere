	<securecookie host="^wiki\.raumzeitlabor\.de$" name=".+" />


	<rule from="^http://((?:lists|wiki|www)\.)?raumzeitlabor\.de/"
		to="https://$1raumzeitlabor.de/" />

</ruleset>