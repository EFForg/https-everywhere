<!--
	Clients have unique subdomains.

-->
<ruleset name="sslcert14.com">

	<target host="sslcert14.com" />
	<target host="*.sslcert14.com" />


	<securecookie host="^(?:.+\.)?sslcert14\.com$" name=".+" />


	<rule from="^http://([\w-]+\.)?sslcert14\.com/"
		to="https://$1sslcert14.com/" />

</ruleset>