<!--
	Nonfunctional subdomains:

		- ^		(valid cert; shows ssl)
		- api		(cert: ssl; shows ssl)
		- c		(ditto)
		- marketplace	(ditto)
		- www		(ditto)

-->
<ruleset name="ATbar (partial)">

	<target host="ssl.atbar.org" />


	<rule from="^http://ssl\.atbar\.org/"
		to="https://ssl.atbar.org/" />

</ruleset>