<!--
	Fully covered subdomains:

		- (www.)
		- auto
		- cars
		- coches
		- media
		- static
		- voitures

-->
<ruleset name="Volotea.com">

	<target host="volotea.com" />
	<target host="*.volotea.com" />


	<securecookie host="^(?:www)?\.volotea\.com$" name=".+" />


	<rule from="^http://((?:auto|cars|coches|media|static|voitures|www)\.)?volotea\.com/"
		to="https://$1volotea.com/" />

</ruleset>
