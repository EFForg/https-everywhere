	<target host="deals.techdirt.com" />
	<target host="ii.techdirt.com" />
	<target host="news.techdirt.com" />
	<target host="rtb.techdirt.com" />
	<target host="www.techdirt.com" />


	<!--	Not secured by server:
					-->
	<!--securecookie host="^\.techdirt\.com$" name="^(__cfduid|__qca|cf_clearance|tdukey$" /-->

	<securecookie host="^(?:www)?\.techdirt\.com$" name=".+" />


	<rule from="^http:"
		to="https:" />

</ruleset>
