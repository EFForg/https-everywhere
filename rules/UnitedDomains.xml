<ruleset name="United Domains">
	<target host="uniteddomains.com" />
	<target host="www.uniteddomains.com" />
	<target host="www.united-domains.de" />

	<rule from="^http://(?:www\.)?uniteddomains\.com/" to="https://www.uniteddomains.com/"/>
	<rule from="^http://www\.united-domains\.de/" to="https://www.united-domains.de/"/>
</ruleset>
