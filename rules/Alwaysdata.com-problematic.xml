<!--
	For rules that are on by default, see Alwaysdata.com.xml.

-->
<ruleset name="alwaysdata.com (problematic, false MCB)" default_off="mismatched" platform="mixedcontent">

	<target host="blog.alwaysdata.com" />
	<target host="*.blog.alwaysdata.com" />
	<target host="forum.alwaysdata.com" />


	<securecookie host="^(?:\.?blog|forum)\.alwaysdata\.com$" name=".+" />


	<rule from="^http://(blog|forum)\.alwaysdata\.com/"
		to="https://$1.alwaysdata.com/" />

</ruleset>
