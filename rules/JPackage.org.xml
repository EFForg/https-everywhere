<!--
	For other zarb coverage, see Zarb.org.xml.


	Problematic subdomains:

		- ^ ¹
		- www ²

	¹ Mismatched
	² Self-signed

-->
<ruleset name="JPackage.org" default_off="self-signed">

	<target host="jpackage.org" />
	<target host="www.jpackage.org" />


	<rule from="^http://(?:www\.)?jpackage\.org/"
		to="https://www.jpackage.org/" />

</ruleset>
