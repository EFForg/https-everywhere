  <target host="www.Avanza.se" />
  <target host="Avanza.se" />
  <rule from="^http://www\.avanza\.se/" to="https://www.avanza.se/"/>
  <rule from="^http://avanza\.se/" to="https://www.avanza.se/"/>
</ruleset>

