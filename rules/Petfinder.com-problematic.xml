<!--
	For rules that are on by default, see Petfinder.com.xml.


	Server sends no certificate chain *

	* See https://whatsmychaincert.com

-->
<ruleset name="Petfinder.com (problematic)" default_off="missing certificate chain, untrusted root">

	<!--	Direct rewrites:
				-->
	<target host="profiles.petfinder.com" />


	<securecookie host="^profiles\.petfinder\.com$" name=".+" />


	<rule from="^http:"
		to="https:" />

</ruleset>
