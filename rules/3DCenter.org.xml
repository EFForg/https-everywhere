<ruleset name="3DCenter.org (cacert)" platform="cacert">
	<target host="3dcenter.org" />
	<target host="www.3dcenter.org" />
	<target host="m.3dcenter.org" />

	<securecookie host="^(www|m)?\.3dcenter\.org$" name=".+" />

	<rule from="^http://3dcenter\.org/" to="https://www.3dcenter.org/" />
	<rule from="^http:" to="https:" />
</ruleset>
