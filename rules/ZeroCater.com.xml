<!--
	Nonfunctional hosts in *zerocater.com:

		- blog *

	* WP Engine

-->
<ruleset name="ZeroCater.com (partial)">

	<!--	Direct rewrites:
				-->
	<target host="www.zerocater.com" />

	<!--	Complications:
				-->
	<target host="zerocater.com" />


	<rule from="^http://zerocater\.com/"
		to="https://www.zerocater.com/" />

	<rule from="^http:"
		to="https:" />

</ruleset>
