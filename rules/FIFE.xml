<ruleset name="FIFE" default_off="mismatch, self-signed">

	<!--	cert: webserver.ispgateway.de	-->
	<target host="fifengine.de"/>
	<target host="www.fifengine.de"/>
	<target host="fifengine.net"/>
	<target host="www.fifengine.net"/>

	<!--	data is the same, so minimize exceptions needed		-->
	<rule from="^http://(?:www\.)?fifengine\.(?:de|net)/"
		to="https://fifengine.net/"/>

</ruleset>
