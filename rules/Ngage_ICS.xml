<!--
	 domains:

		- ngageics.com		(shows messenger, valid cert)
		- ngagelive.com		(503)
		- www.ngagelive.com	(http reply)

-->
<ruleset name="Ngage ICS (partial)">

	<target host="messenger.ngageics.com" />
	<target host="secure.ngagelive.com" />


	<securecookie host="^messenger\.ngageics\.com$" name=".+" />


	<rule from="^http://messenger\.ngageics\.com/"
		to="https://messenger.ngageics.com/" />

	<rule from="^http://secure\.ngagelive\.com/"
		to="https://secure.ngagelive.com/" />

</ruleset>