<!--
	Other Virtkick rulesets:

		- VirtKick.io.xml


	Fully covered hosts in *virtkick.com:

		- (www.)?
		- demo

-->
<ruleset name="Virtkick.com">

	<!--	Direct rewrites:
				-->
	<target host="virtkick.com" />
	<target host="demo.virtkick.com" />
	<target host="www.virtkick.com" />


	<rule from="^http:"
		to="https:" />

</ruleset>
