<!--
	Nonfunctional subdomains:

		- babelwiki *
		- blog *

	* Shows www

-->
<ruleset name="BabelZilla" default_off="expired">

	<target host="babelzilla.org" />
	<target host="*.babelzilla.org" />


	<securecookie host="^\.?babelzilla\.org$" name=".+" />


	<rule from="^http://(?:www\.)?babelzilla\.org/"
		to="https://babelzilla.org/" />

</ruleset>
