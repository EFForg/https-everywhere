<!--
	Other Avis rulesets:

		- Avisservices.com.xml


	Observed cookie subdomains:

		- .
		- www
		- .www

-->
<ruleset name="Avis.com">

	<target host="avis.com" />
	<target host="www.avis.com" />


	<securecookie host=".*\.avis\.com$" name=".+" />


	<rule from="^http:" to="https:" />

</ruleset>