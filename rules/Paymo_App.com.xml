<!--
	Other Paymo rulesets:

		- Paymo.xml


	Fully covered hosts in *paymoapp.com:

		- (www.)?
		- app
		- wpecdn

-->
<ruleset name="Paymo App.com">

	<!--	Direct rewrites:
				-->
	<target host="paymoapp.com" />
	<target host="app.paymoapp.com" />
	<target host="wpecdn.paymoapp.com" />
	<target host="www.paymoapp.com" />


	<rule from="^http:"
		to="https:" />

</ruleset>
