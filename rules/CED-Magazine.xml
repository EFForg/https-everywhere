<ruleset name="CED Magazine" default_off="expired, mismatch">

	<!--	Cert: *.acquia-sites.com	-->
	<target host="cedmagazine.com" />
	<target host="www.cedmagazine.com" />


	<!--	!www redirects to www.	-->
	<rule from="^http://(?:www\.)?cedmagazine\.com/"
		to="https://www.cedmagazine.com/" />

</ruleset>
