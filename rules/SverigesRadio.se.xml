<ruleset name="SverigesRadio">

	<target host="sr.se" />
	<target host="www.sr.se" />
	<target host="sverigesradio.se" />
	<target host="www.sverigesradio.se" />


	<securecookie host="^sverigesradio\.se$" name=".+" />


	<!--	Cert only matches ^sverigesradio.se.
							-->
	<rule from="^http://(?:www\.)?s(?:r|verigesradio)\.se/"
		to="https://sverigesradio.se/" />

</ruleset>

