<!--
	^: Cert only matches www

-->
<ruleset name="MARE-system.de">

	<!--	Direct rewrites:
				-->
	<target host="www.mare-system.de" />

	<!--	Complications:
				-->
	<target host="mare-system.de" />


	<rule from="^http://mare-system\.de/"
		to="https://www.mare-system.de/" />

	<rule from="^http:"
		to="https:" />

</ruleset>
