<ruleset name="OpenPGPjs.org" default_off="mismatched, self-signed">

	<target host="openpgpjs.org" />
	<target host="docs.openpgpjs.org" />
	<target host="tickets.openpgpjs.org" />
	<target host="www.openpgpjs.org" />


	<rule from="^http:"
		to="https:" />

</ruleset>
