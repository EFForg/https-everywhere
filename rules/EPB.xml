<ruleset name="EPB">

	<target host="epb.net" />
	<target host="*.epb.net" />
	<target host="epbfi.com" />
	<target host="*.epbfi.com" />


	<securecookie host="^(?:.*\.)?epb(?:\.net|fi\.com)$" name=".*" />


	<rule from="^http://(livehelp\.|www\.)?epb\.net/"
		to="https://$1epb.net/" />

	<rule from="^http://((?:mail|livehelp|phone|www)\.)?epbfi\.com/"
		to="https://$1epbfi.com/" />

</ruleset>
