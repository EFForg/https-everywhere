<ruleset name="BlueZ (partial)" default_off="expired, self-signed">

	<target host="bluez.org"/>
	<target host="www.bluez.org"/>
		<!--	homepage redirects to http	-->
		<exclusion pattern="^http://www\.bluez\.org/$"/>

	<rule from="^http://(?:www\.)?bluez\.org/"
		to="https://www.bluez.org/"/>

</ruleset>
