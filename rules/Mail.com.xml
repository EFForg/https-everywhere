<!--
	For other United Internet coverage, see United-Internet.xml.


	Nonfunctional subdomains:

		- search


	Fully covered subdomains:

		- (www.)
		- dl
		- www.faq
		- help
		- sec-i0
		- service

-->
<ruleset name="Mail.com">

	<!--	Direct rewrites:
				-->
	<target host="mail.com" />
	<target host="dl.mail.com" />
	<target host="www.faq.mail.com" />
	<target host="help.mail.com" />
	<target host="sec-i0.mail.com" />
	<target host="service.mail.com" />
	<target host="www.mail.com" />


	<securecookie host=".*\.mail\.com$" name=".+" />


	<rule from="^http:"
		to="https:" />

</ruleset>
