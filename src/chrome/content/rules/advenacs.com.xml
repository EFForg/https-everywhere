<!--
	advenacs.com is the primary operating domain of the Advena Network Protection Service.
-->

<ruleset name="Advena">
        <target host="advenacs.com" />
        <target host="secure.advenacs.com" />
        <target host="verify.advenacs.com" />
        <target host="*.advenacs.com" />

        <rule from="^http:"
                to="https:" />
</ruleset>
