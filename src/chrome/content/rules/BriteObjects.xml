<!--
	CDN buckets:

		- wac.4C33.edgecastcdn.net/??4C33/

			- cdn


	Nonfunctional subdomains:

		- cdn	(404; mismatched, CN: gp1.wac.edgecastcdn.net)


	CN: *.gridserver.com

-->
<ruleset name="BriteObjects" default_off="mismatched">

	<target host="briteobjects.com" />
	<target host="www.briteobjects.com" />


	<rule from="^http://(?:www\.)?briteobjects\.com/"
		to="https://www.briteobjects.com/" />

</ruleset>
