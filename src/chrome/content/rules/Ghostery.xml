<!--
	For other Evidon coverage, see Evidon.xml.


	^: dropped


	Fully covered subdomains:

		- ba-cdn
		- purplebox

-->
<ruleset name="Ghostery.com">

	<!--	Direct rewrites:
				-->
	<target host="ba-cdn.ghostery.com" />
	<target host="purplebox.ghostery.com" />
	<target host="www.ghostery.com" />

	<!--	Complications:
				-->
	<target host="ghostery.com" />


	<rule from="^http://ghostery\.com/"
		to="https://www.ghostery.com/" />

	<rule from="^http:"
		to="https:" />

</ruleset>
