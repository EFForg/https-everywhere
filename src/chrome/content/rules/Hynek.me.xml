<!--
	Problematic domains:

		- ox.cx		(dropped)


	Mixed content:

		- Web bugs from stats.ox.cx *

	* Secured by us

-->
<ruleset name="Hynek.me">

	<target host="hynek.me" />
	<target host="www.hynek.me" />
	<target host="ox.cx" />
	<target host="*.ox.cx" />


	<rule from="^http://(www\.)?hynek\.me/"
		to="https://$1hynek.me/" />

	<rule from="^http://(?:www\.)?ox\.cx/"
		to="https://www.ox.cx/" />

	<rule from="^http://stats\.ox\.cx/"
		to="https://stats.ox.cx/" />

</ruleset>
