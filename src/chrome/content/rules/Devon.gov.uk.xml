	<!--	Complications:
				-->
	<target host="devon.gov.uk" />

		<!--	Sets cookies without Secure:
							-->
		<!--test url="http://www.devon.gov.uk/library_catalogue" /-->


	<!--	Not secured by server:
					-->
	<!--securecookie host="^www\.devon\.gov\.uk$" name="^(?:menustate|rpages)$" /-->

