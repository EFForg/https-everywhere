		- .codementor.io
		- www.codementor.io

-->
<ruleset name="Codementor.io">

	<!--	Direct rewrites:
				-->
	<target host="codementor.io" />
	<target host="cdn.codementor.io" />
	<target host="www.codementor.io" />


	<!--	Not secured by server:
					-->
	<!--securecookie host="^\.codementor\.io$" name="^_session_id$" /-->
