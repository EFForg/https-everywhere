<!--

	For other GMO coverage, see GMO_Internet.xml.

	Nonfunctional subdomains:

		- m	(connection refused)
		- techblog	(times out)

	Problematic subdomains:

		- jword	(invalid cert)

-->
<ruleset name="GMO Adpartners">

	<target host="*.gmo-ap.jp" />

	<test url="http://saiyo.gmo-ap.jp/css/style.css" />
	<test url="http://www.gmo-ap.jp/css/core.css" />
	<test url="http://www.gmo-ap.jp/images/common/header_gnav_05_on.jpg" />
	<test url="http://www.gmo-ap.jp/js/lib/jquery.min.js" />

	<rule from="^http://saiyo\.gmo-ap\.jp/"
		to="https://saiyo.gmo-ap.jp/" />

	<rule from="^http://www\.gmo-ap\.jp/(css|images|js)"
		to="https://www.gmo-ap.jp/$1" />

</ruleset>
