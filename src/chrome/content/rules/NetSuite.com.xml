<ruleset name="NetSuite.com (partial)">

	<target host="*.netsuite.com" />


	<!--	Not secured by server:
					-->
	<!--securecookie host="^\.netsuite\.com$" name="^mbox$" /-->
	<!--securecookie host="^checkout\.netsuite\.com$" name="^JSESSIONID$" /-->
