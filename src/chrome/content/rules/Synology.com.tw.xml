<!--
	For other Synology coverage, see


	Nonfunctional domains:

		- blog.synology.com
		- wiki.synology.com

-->
<ruleset name="Synology.com.tw" default_off="mismatched">

	<target host="synology.com.tw" />
	<target host="www.synology.com.tw" />


	<securecookie host="^.*\.synology\.com\.tw$" name=".+" />


	<rule from="^http:"
		to="https:" />

</ruleset>
