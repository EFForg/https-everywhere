<ruleset name="Healthcare.gov">
    <target host="healthcare.gov" />
    <target host="*.healthcare.gov" />

    <securecookie host="^(?:.*\.)?healthcare\.gov$" name=".+" />

    <rule from="^http://healthcare\.gov/"
        to="https://www.healthcare.gov/" />
    <rule from="^http://([^/:@]+)?\.healthcare\.gov/"
        to="https://$1.healthcare.gov/" />
</ruleset>
