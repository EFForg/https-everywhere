<!--
	For other Invoca coverage, see Invoca.com.xml.


	Fully covered subdomains:

		- (www.)
		- js10
		- json10

-->
<ruleset name="RingRevenue.com">

	<target host="ringrevenue.com" />
	<target host="*.ringrevenue.com" />


	<rule from="^http://((?:js10|json10|www)\.)?ringrevenue\.com/"
		to="https://$1ringrevenue.com/" />

</ruleset>
