<!--
	Non-functional hosts
		SSL peer certificate was not OK:
		- www.lookback.io
-->
<ruleset name="Lookback.io">
	<target host="lookback.io" />
	<target host="www.lookback.io" />

	<rule from="^http://www\.lookback\.io/"
			to="https://lookback.io/" />

	<rule from="^http:" 
			to="https:" />
</ruleset>
