<!--
	Invalid certificate:
		ftp.iers.org
		ww2.iers.org

	Refused:
		webserver.iers.org
-->
<ruleset name="IERS.org">

	<target host="www.iers.org" />
	<target host="data.iers.org" />
	<target host="datacenter.iers.org" />

	<securecookie host=".+" name=".+" />

	<rule from="^http:" to="https:" />

</ruleset>
