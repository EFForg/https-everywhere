<!--
	For rules not causing false/broken MCB, see 101domain.com.xml.

-->
<ruleset name="101domain.com (false MCB)" platform="mixedcontent">

	<target host="portuguese.101domain.com" />
	<target host="spanish.101domain.com" />


	<rule from="^http://(portuguese|spanish)\.101domain\.com/"
		to="https://$1.101domain.com/" />

</ruleset>
