<!--
	For rules that are on by default, see Livefyre.xml.

-->
<ruleset name="Livefyre (mismatches)" default_off="mismatched">

	<target host="blog.livefyre.com" />
	<target host="press.livefyre.com" />
		<exclusion pattern="^http://press\.livefyre\.com/$" />


	<!--	- At least some pages redirect to http
		- Cert: gridserver.com
					-->
	<rule from="^http://blog\.livefyre\.com/wp-content/"
		to="https://blog.livefyre.com/wp-content/" />

	<!--	Cert: totemapp.com
					-->
	<rule from="^http://press\.livefyre\.com/"
		to="https://press.livefyre.com/" />

</ruleset>
