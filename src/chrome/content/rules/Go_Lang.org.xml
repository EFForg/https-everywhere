<!--
	For other Google coverage, see GoogleServices.xml.


	Fully covered domains:

		- golang.com

		- \w+.golang.com:

			- blog
			- http2
			- tour
			- www

-->
<ruleset name="Go Lang.org">

	<target host="golang.org" />
	<target host="*.golang.org" />


  <rule from="^http://(\w+\.)?golang\.org/"
    to="https://$1golang.org/" />

</ruleset>
