<!--
	Other related rulesets:

		- Etiqa.com.my.xml
		- KETrade.com.sg.xml
		- Maybank.co.id.xml
		- Maybank.com.ph.xml
		- Maybank.com.sg.xml
		- Maybank2e.com.xml
		- Maybank2e.net.xml
		- Maybank2u.com.kh.xml
		- Maybank2u.com.my.xml
		- Maybank2u.com.ph.xml
		- MaybankHeart.com.xml
		- Maybank-IB.com.xml
		- Maybank-KE.co.th.xml
		- Maybank-KE.com.xml


	Non-functional subdomains:

		- www.maybank.com	(m)
		- eappstore	(t)
		- ev	(i)
		- mailserv	(i)
		- mayproof03	(t)
		- ms2	(t)
		- myaccessportal	(i)
		- www.mymdm01	(t)
		- rmt	(t)
		- surveys	(e)
		- swift2fa	(r)
		- uc	(t)
		- webmail	(r)

	e: expired certificate
	h: http redirect
	i: invalid certificate chain
	m: certificate mismatch
	r: connection refused
	s: self-signed certificate
	t: timeout on https
-->
<ruleset name="Maybank.com">

	<target host="cards.maybank.com" />
	<target host="connect.maybank.com" />
	<target host="ebpgcards.maybank.com" />
	<target host="mkemobile.maybank.com" />
	<target host="mpos.maybank.com" />
	<target host="myaccessportal01.maybank.com" />
	<target host="mycampus.maybank.com" />
	<target host="mycampusreporting.maybank.com" />
	<target host="mycampussocial.maybank.com" />
	<target host="mymdm01.maybank.com" />
	<target host="myvpn.maybank.com" />
	<target host="sconnect.maybank.com" />

	<securecookie host=".+" name=".+" />

	<rule from="^http:"
		to="https:" />
</ruleset>
