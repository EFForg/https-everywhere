<!--
	Nonfunctional hosts in *.eldarya.fr:

	h: http redirect
	m: certificate mismatch
	r: connection refused
	s: self-signed certificate
	t: timeout on https
-->
<ruleset name="Eldarya.fr">
	<target host="eldarya.fr" />
	<target host="www.eldarya.fr" />

	<rule from="^http:" to="https:" />
</ruleset>
