<ruleset name="Etrade.com">

	<target host="etrade.com" />
	<target host="*.etrade.com" />
	<target host="cdn.etrade.net" />


	<rule from="^http://((?:about|career|global|us|www)\.)?etrade\.com/"
		to="https://$1etrade.com/" />

	<rule from="^http://cdn\.etrade\.net/"
		to="https://cdn.etrade.net/" />

</ruleset>
