<ruleset name="ch.ch">
    <target host="ch.ch" />
    <target host="*.ch.ch" />

    <securecookie host="^(?:www\.)?\.ch\.ch$" name=".+" />

    <rule from="^http://ch\.ch/"
        to="https://www.ch.ch/" />
    <rule from="^http://([^/:@]+)?\.ch\.ch/"
        to="https://$1.ch.ch/" />
</ruleset>
