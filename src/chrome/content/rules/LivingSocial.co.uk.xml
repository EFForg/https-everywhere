<!--
	For other LivingSocial coverage, see LivingSocial.xml.


	Fully covered hosts in *livingsocial.co.uk:

		- (www.)?
		- help

-->
<ruleset name="LivingSocial.co.uk">

	<!--	Direct rewrites:
				-->
	<target host="livingsocial.co.uk" />
