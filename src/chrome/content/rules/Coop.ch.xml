<ruleset name="Coop">
	<!--
	mixedcontent:
		<target host="coop.ch"/>  
		<target host="www.coop.ch"/>
	-->
	<target host="onlinebanking.bankcoop.ch"/>
	<target host="supercard.ch"/>
