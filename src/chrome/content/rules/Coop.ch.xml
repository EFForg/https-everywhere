<ruleset name="Coop">
	<target host="onlinebanking.bankcoop.ch"/>
	<target host="supercard.ch"/>
	<target host="www.supercard.ch"/>
	<target host="coopzeitung.ch"/>
	<target host="www.coopzeitung.ch"/>
	<target host="cooperazione.ch"/>
