<!--
	For other SRG SSR coverage, see srg-ssr.xml.

	Mismatch:
		- cicr.rsi.ch
		- live.rsi.ch

	Wrong content:
		- portalemedia.rsi.ch

	HTTP redirect:
		- mediaww.rsi.ch

-->
<ruleset name="RSI.ch">
	<target host="rsi.ch"/>
	<target host="www.rsi.ch"/>
	<target host="cdn.rsi.ch"/>
	<target host="giochi.rsi.ch"/>
	<target host="shop.rsi.ch"/>
	<target host="webseries.rsi.ch"/>

	<rule from="^http:"
		to="https:"/>
</ruleset>
