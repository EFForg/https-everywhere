<ruleset name="ghacks.net">
	<target host="ghacks.net"/>
	<target host="www.ghacks.net"/>
	<target host="cdn.ghacks.net/wp-content/*"/>
	<target host="cdn.ghacks.net/wp-includes/*"/>
	<target host="deals.ghacks.net"/>
	<test url="http://www.ghacks.net/"/>
	<test url="http://deals.ghacks.net/"/>
	<securecookie host=".+" name=".+" />
<rule from="^http:" to="https:"/>
</ruleset>
