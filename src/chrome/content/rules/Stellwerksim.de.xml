<ruleset name="Stellwerksim.de">

    <target host="stellwerksim.de" />
    <target host="www.stellwerksim.de" />
    <target host="doku.stellwerksim.de" />
    <target host="stitz.stellwerksim.de" />

	<rule from="^http:" to="https:" />
</ruleset>
