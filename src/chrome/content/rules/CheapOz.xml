<!--
	For cheapoz.com.au

		Works:

			- www
			- $

		Doesn't Work:
			nil

		Notes:
			www redirects to $
-->
<ruleset name="CheapOz">
  <target host="cheapoz.com.au" />
  <target host="*.cheapoz.com.au" />

  <rule from="^http://(?:www\.)?cheapoz\.com\.au/"
          to="https://cheapoz.com.au/" />
</ruleset>