<ruleset name="Úřad průmyslového vlastnictví">
	<target host="upv.cz" />
	<target host="isdv.upv.cz" />
	<target host="www.upv.cz" />

    <!-- Disabled tests
	<test url="http://upv.cz" />
	<test url="http://isdv.upv.cz" />
	<test url="http://www.upv.cz" />
    -->

	<rule from="^http://upv\.cz/"
		to="https://www.upv.cz/" />

	<rule from="^http://(\w+)\.upv\.cz/"
		to="https://$1.upv.cz/" />
</ruleset>
