<!--
	For rules that are on by default, see Liftdna.com.xml.

-->
<ruleset name="Liftdna.com (problematic)" default_off="mismatched">

	<target host="liftdna.com" />
	<target host="www.liftdna.com" />
		<!--
			Handled in Liftdna.com.xml:
							-->
		<!--exclusion pattern="^http://(www\.)?liftdna\.com/(\w+\.js|css/|images/|js/)" /-->


	<rule from="^http://(?:www\.)?liftdna\.com/(?!\w+\.js|css/|images/|js/)"
		to="https://www.liftdna.com/" />

</ruleset>
