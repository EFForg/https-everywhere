<!--

	REQUIRES https:
		- api (will return apache page if attempted access over http)

	Nonfunctional domains (refused):
		- ar
		- blogs

	Self signed domains:
		- imagebank
		- www.imagebank

	- study (redirect, cert for *.studyinsweden.se)

-->
<ruleset name="Sweden.se">
	<target host="sweden.se" />
	<target host="www.sweden.se" />
	<target host="api.sweden.se" />
	<target host="work.sweden.se" />

	<rule from="^http:" to="https:" />
</ruleset>
