<!--
	Fully covered hosts:

		- (www.)?


	Insecure cookies are set for these hosts:

		- xmpp.jp
		- www.xmpp.jp

-->
<ruleset name="XMPP.jp">

	<!--	Direct rewrites:
				-->
	<target host="xmpp.jp" />
	<target host="www.xmpp.jp" />


	<!--	Not secured by server:
					-->
	<!--securecookie host="^(www\.)?xmpp\.jp$" name="^session$" /-->

	<securecookie host="^(?:www\.)?xmpp\.jp$" name=".+" />


	<rule from="^http:"
		to="https:" />

</ruleset>
