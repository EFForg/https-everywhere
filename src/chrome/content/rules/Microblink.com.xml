<!--
	Other microblink rulesets:

		- PhotoMath.net.xml
		- PhotoPay.net.xml

-->
<ruleset name="microblink.com">

	<target host="microblink.com" />
	<target host="*.microblink.com" />


	<securecookie host="^\.microblink\.com$" name=".+" />


	<rule from="^http://(www\.)?microblink\.com/"
		to="https://$1microblink.com/" />

</ruleset>
