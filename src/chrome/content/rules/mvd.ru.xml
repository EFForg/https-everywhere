<!--
	Remark: *.mvd.ru has a wildcard DNS records and serve and wildcard certificates.
-->
<ruleset name="mvd.ru">
	<target host="mvd.ru" />
	<target host="*.mvd.ru" />
		<test url="http://www.mvd.ru/" />
		<test url="http://static.mvd.ru/upload/site1/05IWFr3NZU.png" />
		<test url="http://static.mvd.ru/media/mvd-2015/css/flexlist.css" />

	<rule from="^http:" to="https:" />
</ruleset>
