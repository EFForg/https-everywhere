<!--
	Remark: *.mvd.ru has a wildcard DNS records and serve and wildcard certificates.
-->
<ruleset name="mvd.ru">
	<target host="mvd.ru" />
	<target host="*.mvd.ru" />
		<test url="http://www.mvd.ru/" />
		<test url="http://image.mvd.ru/" />
		<test url="http://static.mvd.ru/" />

	<rule from="^http:" to="https:" />
</ruleset>
