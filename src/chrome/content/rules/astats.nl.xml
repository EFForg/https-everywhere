	Invalid certificate:
		autodiscover.astats.nl
		ftp.astats.nl

	Refused:
		localhost.astats.nl
-->
<ruleset name="astats.nl">
	<target host="astats.nl" />
	<target host="www.astats.nl" />
	<target host="astats.astats.nl" />
	<target host="cpanel.astats.nl" />
	<target host="cpcalendars.astats.nl" />
	<target host="cpcontacts.astats.nl" />
	<target host="ipv6.astats.nl" />
	<target host="mail.astats.nl" />
	<target host="webdisk.astats.nl" />
	<target host="webmail.astats.nl" />

	<rule from="^http:" to="https:" />
</ruleset>
