<!--
	www.curo.co.za: Mismatched

-->
<ruleset name="Curo.co.za" default_off="expired">

	<!--	Direct rewrites:
				-->
	<target host="curo.co.za" />

	<!--	Complications:
				-->
	<target host="www.curo.co.za" />


	<securecookie host="^\w" name="." />


	<rule from="^http://www\.curo\.co\.za/"
		to="https://curo.co.za/" />

	<rule from="^http:"
		to="https:" />

</ruleset>
