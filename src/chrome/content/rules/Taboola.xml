<!--
	Other Taboola rulesets:

		- Taboolasyndication.com.xml


	CDN buckets:

		- orig-1000[23].taboola.cotcdn.net

	Nonfunctional subdomains:

		- blog	(refused)
		- mb	(times out)
		- signup	(400)

	* akamai

	Problematic subdomains:

		- cannes2016	(invalid cert; mismatch)
		- jobs	(invalid cert; mismatch)
		- store	(invalid cert; mismatch)
		- vrc	(expired)

-->
<ruleset name="Taboola">

	<target host="api.taboola.com" />
	<target host="backstage.taboola.com" />
	<target host="cdn-yjp.taboola.com" />
	<target host="cdn.taboola.com" />
	<target host="go.taboola.com" />
	<target host="images.taboola.com" />
	<target host="netstorage.taboola.com" />
	<target host="popup.taboola.com" />
	<!-- target host="signup.taboola.com" / -->
	<target host="taboola.com" />
	<target host="trc.taboola.com" />
	<target host="www.taboola.com" />

	<securecookie host="^www\.taboola\.com$" name=".*" />

	<test url="http://cdn-yjp.taboola.com/libtrc/yahoojp-thepage/loader.js" />
	<test url="http://cdn.taboola.com/libtrc/efinancialcareers-efinancialcareers/loader.js" />
	<test url="http://images.taboola.com/taboola/image/fetch/f_jpg%2Cq_80%2Ch_192%2Cw_231%2Cc_fill%2Cg_faces%2Ce_sharpen/http%3A//cdn.taboolasyndication.com/libtrc/static/thumbnails/9044c567dd6b62e2dce4b882b2b5c8c5.png" />
	<test url="http://netstorage.taboola.com/libtrc/common/cross-check.js" />

	<rule from="^http:"
		to="https:" />

</ruleset>
