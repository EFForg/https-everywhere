<ruleset name="socialbakers.com" platform="mixedcontent">

	<target host="socialbakers.com"/>
	<target host="*.socialbakers.com"/>

	<securecookie host="^(?:.*\.)?socialbakers\.com$" name=".+" />

	<!--	encountered subdomains:
			- analytics
			- cms		-->
	<rule from="^http://(\w+\.)?socialbakers\.com/"
		to="https://$1socialbakers.com/"/>

</ruleset>
