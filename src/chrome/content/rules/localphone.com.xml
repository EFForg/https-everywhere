<ruleset name="Localphone">
  <target host="localphone.com" />
  <target host="www.localphone.com" />
  <target host="static.localphone.com" />

  <rule from="^http://localphone\.com/" to="https://www.localphone.com/" />
  <rule from="^http://(www|static)\.localphone\.com/" to="https://$1.localphone.com/" />
</ruleset>
