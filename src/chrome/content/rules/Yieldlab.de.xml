<!--
	Other Yieldlab rulesets:

		- Yieldlab.net.xml


	- ^: Self-signed
	- www: Mismatched


	Mixed content:

		- Images from $self
		- Bug from www.facebook.com

-->
<ruleset name="Yieldlab.de" default_off="expired, self-signed">

	<target host="yieldlab.de" />
	<target host="www.yieldlab.de" />


	<securecookie host="^www\.yieldlab\.de$" name=".+" />


	<rule from="^http:"
		to="https:" />

</ruleset>
