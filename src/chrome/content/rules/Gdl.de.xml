<!--
	Nonfunctional hosts in *.gdl.de:

	h: http redirect
	m: certificate mismatch
	r: connection refused
	s: self-signed certificate
	t: timeout on https
-->
<ruleset name="Gdl.de">
	<target host="gdl.de" />
	<target host="www.gdl.de" />
	<target host="uploads.gdl.de" />
	
	<securecookie host=".+" name=".+" />
	
	<rule from="^http:" to="https:" />
</ruleset>
