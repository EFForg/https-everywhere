<!--
	www.cryptosms.org: Mismatched

-->
<ruleset name="CryptoSMS.org (CAcert)" platform="cacert" default_off="expired">

	<!--	Direct rewrites:
				-->
  <target host="cryptosms.org" />

	<!--	Complications:
				-->
  <target host="www.cryptosms.org" />

	<rule from="^http://www\.cryptosms\.org/"
		to="https://cryptosms.org/" />

	<rule from="^http:"
		to="https:" />
</ruleset>
