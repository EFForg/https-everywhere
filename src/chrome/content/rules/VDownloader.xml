<!--
	Invalid certificate:
		vdownloader.es
		www.vdownloader.es
		mail.vdownloader.es

-->
<ruleset name="VDownloader">

	<target host="vdownloader.es" />
	<target host="www.vdownloader.es" />

	<!--	es redirects to com	-->
	<rule from="^http://(www\.)?vdownloader\.es/"
		to="https://vdownloader.com/" />

</ruleset>
