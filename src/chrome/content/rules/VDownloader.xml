<ruleset name="VDownloader">

	
	<!--	* for cross-domain cookie.	-->
	
	<target host="vdownloader.es" />
	


	<securecookie host="^\.?vdownloader\.com$" name=".*" />


	<!--	es redirects to com	-->
	<rule from="^http://(www\.)?vdownloader\.(?:com|es)/"
		to="https://$1vdownloader.com/" />

</ruleset>
