<ruleset name="Buycraft.net">

	<target host="buycraft.net" />
	<target host="*.buycraft.net" />


	<!--	Server doesn't set Secure for:
						-->
	<securecookie host="^(?:server\.|www\.|\.)?buycraft\.net$" name=".+" />


	<rule from="^http://(server\.|www\.)?buycraft\.net/"
		to="https://$1buycraft.net/" />

</ruleset>
