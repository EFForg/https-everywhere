<!--
	(www.)?: 403

	Problematic subdomains:

		- www	(akamai)

-->
<ruleset name="ushmm.org" default_off="403">

	<target host="ushmm.org" />
	<target host="www.ushmm.org" />


	<securecookie host="^ushmm\.org$" name=".+" />


	<rule from="^http://(?:www\.)?ushmm\.org/"
		to="https://ushmm.org/" />

</ruleset>
