<!--
	Ruleset for storebot.me.

	Not supporting HTTPS or using an invalid certificate:
		* dev.storebot.me
		* email.storebot.me
		* mail.storebot.me
-->
<ruleset name="storebot.me">

	<target host="storebot.me" />
	<target host="www.storebot.me" />

	<rule from="^http:"
		to="https:" />

</ruleset>
