<!--
	Ruleset for storebot.me.

	Invalid certificate (mismatch):
		* dev.storebot.me
		* email.storebot.me
		* idea.storebot.me

	Refused:
		* mail.storebot.me
-->
<ruleset name="storebot.me">

	<target host="storebot.me" />
	<target host="www.storebot.me" />

	<rule from="^http:"
		to="https:" />

</ruleset>
