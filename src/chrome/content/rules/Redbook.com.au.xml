<!--
	Non-functional subdomains:
		- beta		(r)
		- certificate	(m)
		- secure.liveimages		(r)
		- tfnsw		(m)

	e: expired certificate
	h: http redirect
	i: invalid certificate chain
	m: certificate mismatch
	r: connection refused
	s: self-signed certificate
	t: timeout on https
-->
<ruleset name="Redbook.com.au">

	<target host="redbook.com.au" />
	<target host="www.redbook.com.au" />
	<target host="airbagrecall.redbook.com.au" />
	<target host="express.redbook.com.au" />
	<target host="m.redbook.com.au" />
	<target host="mobicert.redbook.com.au" />
	<target host="mobile.redbook.com.au" />
	<target host="nevdis.redbook.com.au" />
	<target host="payment.redbook.com.au" />
	<target host="tmr.redbook.com.au" />
	<target host="vicroads.redbook.com.au" />

	<securecookie host=".+" name=".+" />

	<rule from="^http:"
		to="https:" />
</ruleset>
