<!--
	This domain contains a wildcard DNS record.

	Invalid certificate:
		www.codeblocks.org

	Mismatch:
		forums.codeblocks.org
		wiki.codeblocks.org

	Timeout:
		staging.codeblocks.org
-->
<ruleset name="Codeblocks.org (partial)">

	<target host="codeblocks.org" />

	<securecookie host=".+" name=".+" />

	<rule from="^http:" to="https:" />

</ruleset>
