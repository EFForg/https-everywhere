<!--
	For ingdirect.com.au

		Works:

			- www

		Doesn't Work:
			- $ (cert mismatch)
-->
<ruleset name="ING Direct (AU)">
  <target host="ingdirect.com.au" />
  <target host="*.ingdirect.com.au" />

  <rule from="^http://(?:www\.)?ingdirect\.com\.au/"
          to="https://www.ingdirect.com.au/" />
</ruleset>