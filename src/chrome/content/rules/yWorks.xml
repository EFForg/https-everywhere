<!--
	Problematic domains:
		- yworks.de (TLS handshake error)
		- www.yworks.de (TLS handshake error)
-->

<ruleset name="yWorks">
	<target host="yworks.com" />
	<target host="www.yworks.com" />
	<target host="docs.yworks.com" />
	<target host="kb.yworks.com" />
	<target host="live.yworks.com" />
	<target host="yed.yworks.com" />

	<target host="yworks.de" />
	<target host="www.yworks.de" />

	<securecookie host=".+" name=".+" />

	<rule from="^http://(www\.)?yworks\.de/" to="https://www.yworks.com/" />
	<rule from="^http:" to="https:" />
</ruleset>
