<ruleset name="Discordapp.com">
	<target host="discordapp.com" />
	<target host="*.discordapp.com" />
	<test url="http://www.discordapp.com/" />
	<test url="http://support.discordapp.com/" />
	<test url="http://status.discordapp.com/" />
	<test url="http://blog.discordapp.com/" />
	<test url="http://merch.discordapp.com/" />
	<test url="http://i18n.discordapp.com/" />
	<rule from="^http:" to="https:" />
</ruleset>
