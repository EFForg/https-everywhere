<!--
	Problematic subdomains:

		 - www	(cert only matches ^openid.net)

-->
<ruleset name="OpenID">

	<target host="openid.net" />
	<target host="www.openid.net" />


	<securecookie host="^openid\.net$" name=".+" />


	<rule from="^http://(?:www\.)?openid\.net/"
		to="https://openid.net/" />

</ruleset>