<!--
	Insecure cookies are set for these hosts:

		- openid.net

-->
<ruleset name="OpenID.net">

	<!--	Direct rewrites:
				-->
	<target host="openid.net" />
	<target host="www.openid.net" />
	<target host="lists.openid.net" />


	<!--	Not secured by server:
					-->
	<securecookie host="^openid\.net$" name=".+" />

	<rule from="^http:"
		to="https:" />

</ruleset>
