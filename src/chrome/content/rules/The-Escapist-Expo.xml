<ruleset name="The Escapist Expo">

	<target host="escapistexpo.com" />
	<target host="*.escapistexpo.com" />
	<target host="www.sec.escapistexpo.com" />


	<!--	Cert only matches (www.)sec.	-->
	<rule from="^http://(?:(www\.)?sec\.|www\.)?escapistexpo\.com/"
		to="https://$1sec.escapistexpo.com/" />

</ruleset>
