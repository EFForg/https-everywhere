<!--
	For other Rose Web Services coverage, see RoseHosting.com.xml.


	^: cert only matches www

-->
<ruleset name="CoolWebScripts.com" default_off="self-signed">

	<target host="coolwebscripts.com" />
	<target host="www.coolwebscripts.com" />


	<rule from="^http://(?:www\.)?coolwebscripts\.com/"
		to="https://www.coolwebscripts.com/" />

</ruleset>
