<ruleset name="Stadtwerke Ingolstadt" platform="mixedcontent">
<target host="*.sw-i.de"/>
<target host="*.sw-in.de"/>


<rule from="^http://www\.sw\-i\.de/" to="https://www.sw-i.de/"/>
<rule from="^http://portal\.sw\-in\.de/" to="https://portal.sw-in.de/"/>

<test url="http://www.sw-i.de/" />
<test url="http://portal.sw-in.de/" />

</ruleset>
