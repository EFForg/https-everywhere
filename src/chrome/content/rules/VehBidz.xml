<!--
	!www: cert only matches www


	Some pages redirect to http.

-->
<ruleset name="VehBidz (partial)" platform="mixedcontent">

	<target host="vehbidz.com" />
	<target host="www.vehbidz.com" />


	<rule from="^http://(?:www\.)?vehbidz\.com/((?:login|members_area|register)\.php|themes/|uplimg/)"
		to="https://www.vehbidz.com/$2" />

</ruleset>