<!--
	Mismatch:
		- www
-->
<ruleset name="Comprehensive C Archive Network" platform="cacert">
	<target host="ccodearchive.net" />
	<target host="www.ccodearchive.net" />

	<rule from="^http://www\.ccodearchive\.net/"
		to="https://ccodearchive.net/" />
	<rule from="^http:"
		to="https:" />
</ruleset>
