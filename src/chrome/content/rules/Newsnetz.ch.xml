<!--
	Nonfunctional hosts in *.newsnetz.ch:
		- (www.)newsnetz.ch (m)

		Each newspaper has its own subdomainin, but https is not yet widly supported.
		files.newsnetz.ch is used as CDN for various newspapers.

	h: http redirect
	m: certificate mismatch
	r: connection refused
	s: self-signed certificate
	t: timeout on https
-->
<ruleset name="files.newsnetz.ch">
	<target host="files.newsnetz.ch" />

	<rule from="^http:" to="https:" />
</ruleset>
