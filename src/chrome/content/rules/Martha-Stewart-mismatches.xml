	<securecookie host="^\.community\.wholeliving\.com$" name=".+" />


	<!--	Cert: storefront.amazon.com	-->
	<rule from="^http:" to="https:" />

	<!--	Cert: *.ning.com	-->
	

	<!--	Expired, self-signed	-->
	

</ruleset>
