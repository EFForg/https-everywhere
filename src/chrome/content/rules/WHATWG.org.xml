<!--
	Fully covered subdomains:

		- (www.)
		- developers
		- html-differences
		- images
		- resources
		- 
		- \w+.spec:

			- dom
			- encoding
			- fetch
			- html
			- javascript
			- notifications
			- xhr

		- wiki


	Mixed content:

		- css on developers from fonts.googleapis.com *

	* Secured by us

-->
<ruleset name="WHATWG.org">

	<target host="whatwg.org" />
	<target host="*.whatwg.org" />


	<rule from="^http://((?:developers|html-differences|images|resources|\w+\.spec|wiki|www)\.)?whatwg\.org/"
		to="https://$1whatwg.org/" />

</ruleset>
