<!--
	For other Tilda coverage, see Tilda.cc.xml.

	Tilda.ws has both a wildcard DNS record and a wildcard certificate
-->

<ruleset name="Tilda.ws">
	<target host="tilda.ws" />
	<target host="*.tilda.ws" />

	<securecookie host=".+" name=".+" />

	<rule from="^http:" to="https:" />

	<test url="http://www.tilda.ws/" />
	<test url="http://100proectov.tilda.ws/" />
	<test url="http://1cits.tilda.ws/" />
	<test url="http://2017pilgrimage.tilda.ws/" />
</ruleset>
