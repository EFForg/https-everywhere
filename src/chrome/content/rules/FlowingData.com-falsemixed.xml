<!--
	For rules not causing false/broken MCB, see FlowingData.com.xml.

-->
<ruleset name="FlowingData.com (false MCB)" platform="mixedcontent">

	<target host="flowingdata.com" />
	<target host="*.flowingdata.com" />


	<securecookie host="^\.flowingdata\.com$" name=".+" />


	<rule from="^http://(www\.)?flowingdata\.com/"
		to="https://$1flowingdata.com/" />

</ruleset>
