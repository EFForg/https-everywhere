<ruleset name="HJCMS.de (CAcert)" platform="cacert">

	<target host="hjcms.de"/>
	<target host="*.hjcms.de"/>

		<test url="http://www.hjcms.de/" />
		<test url="http://www.hjcms.de//" />

	<securecookie host="^(?:.*\.)?hjcms\.de$" name=".*"/>

	<rule from="^http://(\w+\.)?hjcms\.de/"
		to="https://$1hjcms.de/"/>

</ruleset>
