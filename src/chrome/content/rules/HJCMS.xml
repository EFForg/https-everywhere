<ruleset name="HJCMS (CAcert)" platform="cacert">

	<target host="hjcms.de"/>
	<target host="*.hjcms.de"/>
	<!--	* for cross-domain cookie	-->
	<target host="*.qx11grab.hjcms.de"/>

	<securecookie host="^(.*\.)?hjcms\.de$" name=".*"/>

	<rule from="^http://(\w+\.)?hjcms\.de/"
		to="https://$1hjcms.de/"/>

</ruleset>
