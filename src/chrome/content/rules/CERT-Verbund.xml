<ruleset name="CERT-Verbund">
    <target host="cert-verbund.de" />
    <target host="*.cert-verbund.de" />

    <securecookie host="^(.*\.)?cert-verbund\.de$" name=".+" />

    <rule from="^https?://cert-verbund\.de/"
        to="https://www.cert-verbund.de/" />
    <rule from="^http://([^/:@]+)?\.cert-verbund\.de/"
        to="https://$1.cert-verbund.de/" />
</ruleset>
