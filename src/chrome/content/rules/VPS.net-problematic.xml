<!--
	For rules that are on by default, see VPS.net.xml.

-->
<ruleset name="VPS.net (missing certificate chain)" default_off="cert-chain">

	<!--	Direct rewrites:
				-->
	<target host="vps.net"/>
	<target host="www.vps.net"/>


	<securecookie host="^(?:www\.)?vps\.net$" name=".+" />


	<rule from="^http:"
		to="https:" />

</ruleset>
