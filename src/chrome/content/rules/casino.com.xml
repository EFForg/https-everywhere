<!--
	Nonfunctional domains:

    - act.casino.com
    - affiliates.casino.com
    - cache.casino.com
    - cacheimg.casino.com
    - cmo.casino.com
    - live.casino.com
    - mobile.casino.com
    - support.casino.com

-->
<ruleset name="casino.com" platform="mixedcontent">
	<target host="casino.com"/>
	<target host="*.casino.com"/>
	<target host="lesacasino.com"/>
	<target host="*.lesacasino.com"/>

	<rule from="^http://(?:www\.)?casino\.com/"
			to="https://$1.casino.com/"/>
	<rule from="^http://deutschland\.casino\.com/"
			to="https://deutschland.casino.com/"/>
	<rule from="^http://(?:www\.)?lesacasino\.com/"
			to="https://$1.lesacasino.com/"/>
</ruleset>
