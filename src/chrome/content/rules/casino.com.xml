	<target host="lesacasino.com"/>
	<target host="*.lesacasino.com"/>
	<target host="mansioncasino.com"/>
	<target host="*.mansioncasino.com"/>
	<target host="club777.com"/>
	<target host="*.club777.com"/>
	<target host="slotsheaven.com"/>
	<target host="*.slotsheaven.com"/>
	<target host="iaffiliates.com"/>
	<target host="*.iaffiliates.com"/>

	<rule from="^http://(?:www\.)?casino\.com/"
		to="https://www.casino.com/"/>
	<rule from="^http://deutschland\.casino\.com/"
