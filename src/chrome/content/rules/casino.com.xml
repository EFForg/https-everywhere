		to="https://www.mansioncasino.com/"/>
	<rule from="^http://(?:www\.)?club777\.com/"
		to="https://www.club777.com/"/>
	<rule from="^http://(?:www\.)?slotsheaven\.com/"
		to="https://www.slotsheaven.com/"/>
	<rule from="^http://affiliates\.iaffiliates\.com/"
		to="https://affiliates.iaffiliates.com/"/>
	<rule from="^http://(?:www\.)?iaffiliates\.com/"
		to="https://www.iaffiliates.com/"/>
