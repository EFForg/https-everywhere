<!--

	Other County of Santa Clara rulesets:

		- County-of-Santa-Clara-California.xml

-->
<ruleset name="County Assessor, County of Santa Clara, California">

	<target host="sccassessor.org" />
	<target host="efile.sccassessor.org" />
	<target host="www.sccassessor.org" />

	<securecookie host="(^|\.)(efile|www)\.sccassessor\.org$"
			name=".+" />

	<rule from="^http:"
		to="https:" />

</ruleset>