<!--
	For other Avis related rulesets, see Avis.com.xml


	Non-functional subdomains:
		- $host		(m)
		- locations		(m)

	e: expired certificate
	h: http redirect
	i: invalid certificate chain
	m: certificate mismatch
	r: connection refused
	s: self-signed certificate
	t: timeout on https
-->
<ruleset name="Avisservices.com">

	<target host="avisservices.com" />
	<target host="www.avisservices.com" />
	<target host="meetingsandgroups.avisservices.com" />
	<target host="persona.avisservices.com" />
	<target host="snap.avisservices.com" />

	<securecookie host=".+" name=".+" />

	<rule from="^http:"
		to="https:" />
</ruleset>