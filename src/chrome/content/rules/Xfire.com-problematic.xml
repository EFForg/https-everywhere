<!--
	For rules that are on by default, see Xfire.com.xml.


	Server sends no certificate chain *

	* See https://whatsmychaincert.com

-->
<ruleset name="Xfire.com (missing certificate chain)" default_off="missing certificate chain">

	<target host="xfire.com"/>
	<target host="www.xfire.com"/>


	<securecookie host="^(?:www\.)?xfire\.com$" name=".+" />


	<rule from="^http:"
		to="https:" />

</ruleset>
