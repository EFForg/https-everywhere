<ruleset name="Invoca.net">

	<target host="invoca.net" />
	<target host="*.invoca.net" />


	<!--	Some, but not all, secured by server:
							-->
	<securecookie host="^\.invoca\.net$" name=".+" />


	<rule from="^http://(www2?\.)?invoca\.net/"
		to="https://$1invoca.net/" />

</ruleset>
