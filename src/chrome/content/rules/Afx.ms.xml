<!--
	For other Microsoft coverage, see Microsoft.xml.


	Fully covered domains:

		- dub*.afx.ms:

			- dub002
			- dub10[1-9]
			- dub11\d
			- dub12[0-4]

-->
<ruleset name="afx.ms">

	<target host="*.afx.ms" />

		<!--	Direct rewrites:
					-->
		<test url="http://dub002.afx.ms/" />
		<test url="http://dub101.afx.ms/" />
		<test url="http://dub120.afx.ms/" />


	<rule from="^http://dub(\d+)\.afx\.ms/"
		to="https://dub$1.afx.ms/" />

</ruleset>
