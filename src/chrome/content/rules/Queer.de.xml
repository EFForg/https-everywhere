<!-- 
	redirect to http:
		- queer.de toplevel folders

	Cert mismatch on:
		- ad.queer.de
		- ads.queer.de
		- en.queer.de
		- mobil.queer.de
		- wap.queer.de
		- mail.queercom.de
-->
<ruleset name="Queer.de">

	<target host="queer.de" />
	<target host="www.queer.de" />
	<target host="queercom.de" />
	<target host="www.queercom.de" />

	<rule from="^http:" to="https:"/>
		<exclusion pattern="^http://(www\.)?queer\.de/$" />



</ruleset>
