<!--	not functional subdomains:
		- www.parkes.atnf	(404, "You're Lost")
		- www.narrabri.atnf (cert mismatch)
-->
<ruleset name="CSIRO (partial)">
	<target host="csiro.au" />
	<target host="*.csiro.au" />

	<!-- Australia Telescope National Facility -->
	<rule from="^http://(?:www\.)?atnf\.csiro\.au/"
		to="https://www.atnf.csiro.au/" />

	<!-- Australia Telescope Outreach and Education -->
	<rule from="^http://(?:www\.)?outreach\.atnf\.csiro\.au/"
		to="https://outreach.atnf.csiro.au/" />

</ruleset>
