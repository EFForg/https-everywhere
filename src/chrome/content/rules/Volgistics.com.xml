<!--
	Invalid certificate:
		volgistics.com

	Connection reset:
		blog.volgistics.com

	Login required:
		promo.volgistics.com

-->
<ruleset name="Volgistics">

	<target host="volgistics.com" />
	<target host="www.volgistics.com" />
	<target host="sample.volgistics.com" />

	<rule from="^http://volgistics\.com/"
		to="https://www.volgistics.com/" />

	<rule from="^http:"
		to="https:" />

</ruleset>
