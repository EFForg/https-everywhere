<ruleset name="Coverity.com (partial)">

	<target host="coverity.com" />
	<target host="*.coverity.com" />


	<!--	Not secured by server:
					-->
	<!--securecookie host="^(uat\.|apps\.uat\.)?communities\.coverity\.com$" name="^BIGipServer\+-11-pool$" /-->
	<!--securecookie host="^communities\.coverity\.com$" name="^(JSESSIONID|jive\.server\.info)$" /-->
	<!--securecookie host="^scan\.coverity\.com$" name="^(_session_id|request_method)$" /-->

	<securecookie host="^(?:(?:(?:apps\.)?uat\.)?communities|(?:www\.)?scan)\.coverity\.com$" name=".+" />

	<exclusion pattern="^http://(autodiscover|blog|.*\.communities|go|softwareintegrity)\.coverity\.com/"/>

		<test url="http://autodiscover.coverity.com/" />
		<test url="http://apps.communities.coverity.com/" />
		<test url="http://apps.uat.communities.coverity.com/" />
		<test url="http://blog.coverity.com/" />
		<test url="http://go.coverity.com/" />
		<test url="http://softwareintegrity.coverity.com/" />

	<!-- cert error -->
	<rule from="^http://coverity\.com/"
		to="https://www.coverity.com/" />

		<test url="http://www.coverity.com/" />

	<rule from="^http:" 
		to="https:" />

</ruleset>
