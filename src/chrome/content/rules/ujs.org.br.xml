<ruleset name="ujs.org.br">

	<target host="ujs.org.br" />
	<target host="www.ujs.org.br" />
  <target host="m.ujs.org.br" />

	<rule from="^http://ujs.org.br/"
		to="https://ujs.org.br/" />

	<rule from="^http://www.ujs.org.br/"
		to="https://www.ujs.org.br" />
    
    <rule from="^http:" to="https:"/>
</ruleset>
