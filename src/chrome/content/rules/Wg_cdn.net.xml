<!--
	Other Wengo rulesets:

		- Astrocentro.com.xml


	Problematic subdomains:

		- www[12] *

	* 403; mismatched, CN: secure.wengo.com

-->
<ruleset name="Wg cdn.net">

	<target host="*.wgcdn.net" />


	<rule from="^http://(?:www\d|secure)\.wgcdn\.net/"
		to="https://secure.wgcdn.net/" />

</ruleset>
