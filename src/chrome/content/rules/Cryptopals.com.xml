<!--
	Non-functional hosts
		SSL peer certificate was not OK:
			 - www.cryptopals.com
-->
<ruleset name="Cryptopals.com">
	<target host="cryptopals.com" />
	<target host="www.cryptopals.com" />

	<rule from="^http://www\.cryptopals\.com/"
			to="https://cryptopals.com/" />

	<rule from="^http:"
			to="https:" />
</ruleset>
