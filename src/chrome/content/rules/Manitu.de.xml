<!--
	Problematic domains:

		- manitu.net *

	* Mismatched

-->
<ruleset name="manitu.de">

	<target host="manitu.de" />
	<target host="*.manitu.de" />
	<target host="manitu.net" />
	<target host="www.manitu.net" />


	<rule from="^http://(status\.|www\.)?manitu\.de/"
		to="https://$1manitu.de/" />

	<!--	Simple redirect to .de:
					-->
	<rule from="^http://(?:www\.)?manitu\.net/"
		to="https://www.manitu.net/" />

</ruleset>
