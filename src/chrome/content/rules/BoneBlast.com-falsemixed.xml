<!--
	For rules not causing false/broken MCB, see BoneBlast.com.xml.

-->
<ruleset name="BoneBlast.com (false MCB)" platform="mixedcontent">

	<target host="boneblast.com" />
	<target host="*.boneblast.com" />


	<securecookie host="^\.boneblast\.com$" name=".+" />


	<rule from="^http://(www\.)?boneblast\.com/"
		to="https://$1boneblast.com/" />

</ruleset>
