<ruleset name="telenet">
    <target host="telenet.be" />
    <target host="*.telenet.be" />

    <test url="http://www2.telenet.be/" />
    <test url="http://webmail.telenet.be/" />
    <test url="http://snap.telenet.be/" />

    <!--
        https://www.telenet.be redirects to http://www.telenet.be before redirecting to https://www2.telenet.be
    -->
    <rule from="^http://www\.telenet\.be/"
            to="https://www2.telenet.be/" />

            <test url="http://www.telenet.be/" />

    <rule from="^http:"
            to="https:" />
    
</ruleset>
