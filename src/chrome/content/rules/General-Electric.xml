<!--
	Invalid certificate:
		*.files.geblogs.com
		files.*.geblogs.com

	Other subdomains are mismatched or refused.

-->
<ruleset name="GEBlogs.com (partial)">

	<target host="*.geblogs.com"/>

	<rule from="^http://files\.([\w-]+)\.geblogs\.com/"
		to="https://s3.amazonaws.com/files.$1.geblogs.com/"/>

	<test url="http://files.jeff.geblogs.com/wp-content/uploads/2014/10/MMAnalystNotes1.pdf" />
	<test url="http://files.publicaffairs.geblogs.com/files/2015/01/20416_GE_ICO_WHITE-PAPER_V7.pdf" />
	<test url="http://files.tenyears.geblogs.com/tenyears/files/2011/.../JRIRemarks_Chicago3-23-2011.pdf" />

	<rule from="^http://careers\.files\.geblogs\.com/"
		to="https://s3.amazonaws.com/careers.files.geblogs.com/"/>

	<test url="http://careers.files.geblogs.com/careers/files/2013/11/ge_retiree_employmentvalidation2.pdf" />

</ruleset>
