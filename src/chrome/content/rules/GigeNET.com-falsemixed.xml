<!--
	For rules not causing false/broken MCB, see GigeNET.com.xml.

-->
<ruleset name="GigeNET.com (false MCB)" platform="mixedcontent">

	<target host="gigenet.com" />
	<target host="www.gigenet.com" />
		<!--
			Handled in GigeNET.com.xml:
							-->
		<!--exclusion pattern="^http://(www\.)?gigenet\.com/+(favicon\.ico|wp-content/|wp-includes/)" /-->


	<rule from="^http://(www\.)?gigenet\.com/(?!favicon\.ico|wp-content/|wp-includes/)"
		to="https://$1gigenet.com/" />

</ruleset>
