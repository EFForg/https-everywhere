<ruleset name="Asbury Park Press (partial)">

	<target host="app.com" />
	<target host="*.app.com" />


	<securecookie host="^www\.app\.com$" name=".+" />


	<rule from="^http://(?:cmsimg\.|www\.)?app\.com/"
		to="https://www.app.com/" />

</ruleset>
