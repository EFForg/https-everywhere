<!--
	Other 20min rulesets:
		- Jakpost.net.xml

	Nonfunctional hosts in *.thejakartapost.com:
		- thejakartapost.com (h)
		- www.thejakartapost.com (h)
		- epapaer.thejakartapost.com (m)
		- jobs.thejakartapost.com (mixed content)

	h: http redirect
	m: certificate mismatch
	r: connection refused
	s: self-signed certificate
	t: timeout on https
-->
<ruleset name="Thejakartapost.com (partial)">
	<target host="jobs.thejakartapost.com" />

	<rule from="^http:" to="https:" />
</ruleset>
