<!--
	For rules not causing false/broken MCB, see Plusme.net.xml.

-->
<ruleset name="plusme.net (false MCB)" platform="mixedcontent">

	<target host="plusme.net" />
	<target host="*.plusme.net" />


	<!--	Not secured by server:
					-->
	<securecookie host="^(?:w*\.)?plusme\.net$" name=".+" />


	<rule from="^http://(www\.)?plusme\.net/"
		to="https://$1plusme.net/" />

</ruleset>
