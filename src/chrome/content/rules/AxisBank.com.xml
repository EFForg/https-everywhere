<ruleset name="AxisBank.com (partial)">

	<target host="axisbank.com" />
	<target host="www.axisbank.com" />
	<target host="branch.axisbank.com" />

	<securecookie host=".+" name=".+" />

	<rule from="^http:" to="https:" />

</ruleset>
