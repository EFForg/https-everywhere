<!--
Disabled by https-everywhere-checker because:
Fetch error: http://ipqualityscore.com/ => https://ipqualityscore.com/: (60, 'SSL certificate problem: certificate has expired')
Fetch error: http://www.ipqualityscore.com/ => https://www.ipqualityscore.com/: (60, 'SSL certificate problem: certificate has expired')
-->
<ruleset name="IPQualityScore.com">

	<target host="ipqualityscore.com" />
	<target host="www.ipqualityscore.com" />


	<securecookie host="^(?:www\.)?ipqualityscore\.com$" name=".+" />


	<rule from="^http:" to="https:" />

</ruleset>