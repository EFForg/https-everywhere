<!--
	For rules causing false/broken MCB, see Plantappstore.com-falsemixed.xml.

-->
<ruleset name="plantappstore.com (partial)">

	<target host="plantappstore.com" />
	<target host="www.plantappstore.com" />
		<!--
			Avoid false/broken MCB:
						-->
		<!--exclusion pattern="^http://(www\.)?plantappstore\.com/+(?!catalog/|favicon\.ico|images/)" /-->


	<rule from="^http://(www\.)?plantappstore\.com/(?=catalog/|favicon\.ico|images/)"
		to="https://$1plantappstore.com/" />

</ruleset>
