<!-- HTTPS supported on all subdomains (has HSTS with includeSubdomains) -->

<ruleset name="OpenBlox">
	<target host="openblox.org" />
	<target host="*.openblox.org" />
	<test url="http://git.openblox.org/" />
	<test url="http://lists.openblox.org/" />
	<test url="http://wiki.openblox.org/" />
	<test url="http://bugs.openblox.org/" />
	<test url="http://cloud.openblox.org/" />

	<rule from="^http:" to="https:" />
</ruleset>
