<!--
	CDN buckets:

		- pictos.s3.amazonaws.com


	Nonfunctional hosts in *pictos.cc:

		- (www.)? ʰ
		- www.get ᵃ

	ᵃ Shows another domain
	ʰ Redirects to http


	Problematic hosts in *pictos.cc:

		- my ᵉ

	ᵉ Expired

-->
<ruleset name="Pictos.cc (partial)">

	<target host="get.pictos.cc" />


	<securecookie host="^\w" name=".+" />


	<rule from="^http:"
		to="https:" />

</ruleset>
