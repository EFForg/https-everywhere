<!--

    Fotki does not use cachefly anymore

    Most of subdomains should just work now with https

-->
<ruleset name="Fotki (partial)">

	<target host="*.fotki.com" />

	<rule from="^http:"
		to="https:" />

	<test url="http://www.fotki.com/" />
	<test url="http://images.fotki.com/pixel.gif" />

</ruleset>
