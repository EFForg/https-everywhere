<!--
	Invalid certificate:
		fr.nakedcitizens.eu

-->
<ruleset name="Naked citizens.eu" default_off="expired">

	<target host="nakedcitizens.eu" />
	<target host="www.nakedcitizens.eu" />

	<securecookie host="^(?:www\.)?nakedcitizens\.eu$" name=".+" />

	<rule from="^http:"
		to="https:" />

</ruleset>
