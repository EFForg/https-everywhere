<!--
	Other imgix rulesets:

		- Imgix.net.xml

-->
<ruleset name="imgix.com">

	<target host="imgix.com" />
	<target host="blog.imgix.com" />
	<target host="docs.imgix.com" />
	<target host="webapp.imgix.com" />
	<target host="www.imgix.com" />


	<securecookie host="^\w" name=".+" />


	<rule from="^http:"
		to="https:" />

</ruleset>
