<ruleset name="Hertz UK">
	<target host="hertz.co.uk" />
	<target host="www.hertz.co.uk" />
	<target host="www2.hertz.co.uk" />
	<target host="hertz247.co.uk" />
	<target host="www.hertz247.co.uk" />
	<target host="hertzcarsales.co.uk" />
	<target host="www.hertzcarsales.co.uk" />
	<target host="hertzvans.co.uk" />
	<target host="www.hertzvans.co.uk" />

	<!-- hostname mismatch -->
	<rule from="^http://hertzcarsales\.co\.uk/"
		to="https://www.hertzcarsales.co.uk/" />

	<rule from="^http:"
		to="https:" />
</ruleset>
