<!--
	Problematic subdomains:

		- ^	(expired 2012-12-09)
		- www	(mismatched, CN: srv0.eu)

-->
<ruleset name="DotBIT" default_off="expired">

	<target host="dot-bit.org" />
	<target host="*.dot-bit.org" />


	<securecookie host="^\.dot-bit\.org$" name=".+" />


	<rule from="^http://(?:www\.)?dot-bit\.org/"
		to="https://dot-bit.org/" />

</ruleset>

	