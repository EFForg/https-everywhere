<ruleset name="ING.nl">
  <target host="ing.nl" />
  <target host="www.ing.nl" />
  <target host="mijn.ing.nl" />
  <target host="bankieren.mijn.ing.nl" />
  <target host="mijnzakelijk.ing.nl" />
  <target host="bankieren.mijnzakelijk.ing.nl" />
  <target host="trade.ing.nl" />
  <target host="ondernemerspanel.ing.nl" />
  
  <!-- Almost all cookies not secured. -->
  <securecookie host="^bankieren\.mijnzakelijk\.ing\.nl$" name=".+" />
  <securecookie host="^bankieren\.mijn\.ing\.nl$" name=".+" />
  <securecookie host="^.*\.ing\.nl$" name=".+" />
  
  <rule from="^http:" to="https:" />
</ruleset>
