<!--
  For Hertfordshire Constabulary

    Works:

      - $
      - www

    Mismatch:

      - snt.herts.police.uk

-->
<ruleset name="Hertfordshire Constabulary">
  <target host="herts.police.uk" />
  <target host="www.herts.police.uk" />

  <rule from="^http://(www\.)?herts\.police\.uk/" to="https://www.herts.police.uk/" />
</ruleset>
