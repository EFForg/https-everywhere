<!-- Do not change -> approved by 1and1 staff -->
<ruleset name="web.de">

	<target host="web.de" />
	<target host="*.web.de" />

	<!-- Cookies -->

	<securecookie host="^(?:.*\.)?web\.de$" name=".+" />

	<!-- Rules -->

	<rule from="^http://i([0-9])\.web\.de/"
		  to="https://sec-i$1.web.de/" />
	<rule from="^http:"
		  to="https:"/>

	<!-- Tests -->

	<test url="http://www.web.de" />
	<test url="http://produkte.web.de" />
	<test url="http://i0.web.de/" />

</ruleset>