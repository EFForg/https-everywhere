<!--
	xplosion interactive


	Fully covered hosts in *xplosion.de:

		- (www.)?
		- pimages

-->
<ruleset name="xplosion.de">

	<!--	Direct rewrites:
				-->
	<target host="xplosion.de" />
	<target host="pimages.xplosion.de" />
	<target host="www.xplosion.de" />


	<securecookie host=".+\.xplosion\.de$" name=".+" />


	<rule from="^http:"
		to="https:" />

</ruleset>
