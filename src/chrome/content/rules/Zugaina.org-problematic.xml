<!--
	For rules that are on by default, see Zugaina.xml.


	^zugaina.org: Mismatched

-->
<ruleset name="Zugaina.org (expired)" platform="cacert" default_off="expired">

	<!--	Direct rewrites:
				-->
	<target host="blog.zugaina.org" />
	<target host="gentoo-overlays.zugaina.org" />
	<target host="www.zugaina.org" />

	<!--	Complications:
				-->
	<target host="zugaina.org" />


	<rule from="^http://zugaina\.org/"
		to="https://www.zugaina.org/" />

	<rule from="^http:"
		to="https:" />

</ruleset>
