<!--
	Nonfunctional hosts in *.eldarya.pl:

	h: http redirect
	m: certificate mismatch
	r: connection refused
	s: self-signed certificate
	t: timeout on https
-->
<ruleset name="Eldarya.pl">
	<target host="eldarya.pl" />
	<target host="www.eldarya.pl" />

	<rule from="^http:" to="https:" />
</ruleset>
