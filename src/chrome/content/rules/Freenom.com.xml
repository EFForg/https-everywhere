<!--
	Timed out:
		- resellers.freenom.com
	
	Expired, weak certificate:
		- partners.freenom.com
-->

<ruleset name="Freenom.com" platform="mixedcontent">
	<target host="freenom.com" />
	<target host="www.freenom.com" />
	<target host="api.freenom.com" />
	<target host="my.freenom.com" />
	<target host="register.freenom.com" />
	<target host="whois.freenom.com" />

	<rule from="^http:" to="https:" />
</ruleset>
