	<target host="msgsafe.io" />
	<target host="www.msgsafe.io" />
	<target host="api.msgsafe.io" />
	<target host="api1.msgsafe.io" />
	<target host="api2.msgsafe.io" />
	<target host="api3.msgsafe.io" />
	<target host="api4.msgsafe.io" />
