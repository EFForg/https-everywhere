<!--
	For rules not causing false/broken MCB, see Mirror_Bingo.com.xml.

-->
<ruleset name="Mirror Bingo.com (false MCB)" platform="mixedcontent">

	<target host="mirrorbingo.com" />
	<target host="*.mirrorbingo.com" />


	<securecookie host="^\.mirrorbingo\.com$" name=".+" />


	<rule from="^http://(games\.|www\.)?mirrorbingo\.com/"
		to="https://$1mirrorbingo.com/" />

</ruleset>
