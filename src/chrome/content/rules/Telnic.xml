<ruleset name="Telnic">
        <target host="www.nic.tel"/>
        <target host="telnic.net"/>
        <target host="www.telnic.net"/>
        <target host="telnic.com"/>
        <target host="www.telnic.com"/>
        <target host="telnic.org"/>
        <target host="www.telnic.org"/>
        <target host="a0.webproxy.nic.tel"/>
        <target host="a1.webproxy.nic.tel"/>
        <target host="d0.webproxy.nic.tel"/>
        <target host="d1.webproxy.nic.tel"/>
        <target host="s0.webproxy.nic.tel"/>
        <target host="s1.webproxy.nic.tel"/>
        <!-- webproxy redirects -->
        <rule from="^http://a0\.webproxy\.nic\.tel/"
                to="https://a0.webproxy.nic.tel/" />
        <rule from="^http://a1\.webproxy\.nic\.tel/"
                to="https://a1.webproxy.nic.tel/" />
        <rule from="^http://d0\.webproxy\.nic\.tel/"
                to="https://d0.webproxy.nic.tel/" />
        <rule from="^http://d1\.webproxy\.nic\.tel/"
                to="https://d1.webproxy.nic.tel/" />
        <rule from="^http://s0\.webproxy\.nic\.tel/"
                to="https://s0.webproxy.nic.tel/" />
        <rule from="^http://s1\.webproxy\.nic\.tel/"
                to="https://s1.webproxy.nic.tel/" />
        <!-- registry/corporate site
             cert valid for telnic.org and www.telnic.org -->
        <rule from="^http://(?:www\.)?(?:nic\.tel|telnic\.org|telnic\.com|telnic\.net)/" to="https://www.telnic.org/"/>
</ruleset>
