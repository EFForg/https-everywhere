<!--
	For other Samsung coverage, see Samsung.com.xml.

	Check error:Non-2xx HTTP code: http://samsung.cn/ (200) => https://www.samsung.com/cn/ (403)

	Problematic domains:

		- (www.)samsung.cn	(â†’ www.samsung.com/cn)
		
	Mismatch:
		- content.samsung.cn
-->

<ruleset name="Samsung_CN">

	<target host="www.samsung.cn" />
	<rule from="^http://www\.samsung\.cn/"
		to="https://www.samsung.com/cn/" />
		
	<target host="samsung.com.cn" />
	<target host="www.samsung.com.cn" />
	<rule from="^http://(www\.)?samsung\.com\.cn/"
		to="https://www.samsung.com/cn/" />
		
	<target host="res.samsungshop.com.cn" />
	<target host="www.samsungshop.com.cn" />
	
	<rule from="^http://(res|www)\.samsungshop\.com\.cn/" to="https://$1.samsungshop.com.cn/" />

</ruleset>
