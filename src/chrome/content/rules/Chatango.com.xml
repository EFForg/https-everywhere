<!--
	Ads.


	Fully covered domains:

		- chatango.com
		- (www.)secure.chatango.com

		- [\w-]+.chatango.com:

			- st
			- www
			- [\w-]+ *

	* Per-client domains

-->
<ruleset name="Chatango.com">

	<target host="chatango.com" />
	<target host="*.chatango.com" />


	<securecookie host="^\.chatango\.com$" name=".+" />


	<rule from="^http://([\w-]+\.|(?:www\.)?secure\.)?chatango\.com/"
		to="https://$1chatango.com/" />

</ruleset>
