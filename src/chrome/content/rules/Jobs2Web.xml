<!--
	Fully covered domains:

		- *.jobs2web.com:

			- (www.)
			- careers
			- \w+		(per-client subdomains)

-->
<ruleset name="Jobs2Web">

	<target host="jobs2web.com"/>
	<target host="*.jobs2web.com"/>

	<securecookie host="^(.*\.)?jobs2web\.com$" name=".*"/>


	<rule from="^http://([\w-]+\.)?jobs2web\.com/"
		to="https://$1jobs2web.com/"/>

</ruleset>
