<!--
	For rules not causing false/broken MCB, see Fishwrapper.com.xml.

-->
<ruleset name="Fishwrapper.com (false MCB)" platform="mixedcontent">

	<target host="fishwrapper.com" />
	<target host="www.fishwrapper.com" />
	<target host="*.www.fishwrapper.com" />


	<securecookie host="^\.www\.fishwrapper.com$" name=".+" />


	<rule from="^http://(www\.)?fishwrapper\.com/"
		to="https://$1fishwrapper.com/" />

</ruleset>
