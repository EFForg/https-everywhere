<!--
	Problematic subdomains:

		- ^	(mismatched, CN: www.medicinescomplete.com)


	Some pages redirect to http

-->
<ruleset name="British National Formulary (partial)">

	<target host="bnf.org" />
	<target host="www.bnf.org" />


	<rule from="^http://(?:www\.)?bnf\.org/bnf/((?:account|login|password|request_registration)\.htm|images/|style/|theme/)"
		to="https://www.bnf.org/bnf/$1" />

</ruleset>