<ruleset name="Ask Ubuntu.com">

	<target host="askubuntu.com" />
	<target host="*.askubuntu.com" />


	<securecookie host="^\.(?:meta\.)?askubuntu\.com$" name=".+" />


	<rule from="^http://(meta\.|www\.)?askubuntu\.com/"
		to="https://$1askubuntu.com/" />

</ruleset>
