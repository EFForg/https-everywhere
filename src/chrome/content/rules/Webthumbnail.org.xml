<!--
	Server sends no certificate chain *

	* See https://whatsmychaincert.com

-->
<ruleset name="Webthumbnail.org" default_off="missing certificate chain">

	<!--	Direct rewrites:
				-->
	<target host="webthumbnail.org" />
	<target host="www.webthumbnail.org" />


	<rule from="^http:"
		to="https:" />

</ruleset>
