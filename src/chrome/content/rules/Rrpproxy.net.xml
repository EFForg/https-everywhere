<!--
	Nonfunctional hosts in *.rrpproxy.net:

	Certificate mismatch
        pdf.rrpproxy.net
        www.wi2.rrpproxy.net

	Connection refused
        mail-in.registry-auto.rrpproxy.net
        mailrobot.rrpproxy.net
        namestore-epp.rrpproxy.net
        rrpgw01.rrpproxy.net
        secretary.rrpproxy.net
        sms.rrpproxy.net
        whois.rrpproxy.net

    Does not resolve
        www.api-alipay-ote.rrpproxy.net
        www.beta-wi-ote.rrpproxy.net
        www.beta-wi.rrpproxy.net
        www.claimnotification.rrpproxy.net
-->
<ruleset name="Rrpproxy.net">
    <target host="rrpproxy.net" />
    <target host="www.rrpproxy.net" />
    <target host="api-alipay-ote.rrpproxy.net" />
    <target host="beta-wi-ote.rrpproxy.net" />
    <target host="beta-wi.rrpproxy.net" />
    <target host="claimnotification.rrpproxy.net" />
    <target host="legacy-wi-ote.rrpproxy.net" />
    <target host="legacy-wi.rrpproxy.net" />
    <target host="wi2.rrpproxy.net" />
    <target host="wiki-beta.rrpproxy.net" />
    <target host="wiki.rrpproxy.net" />
    <target host="wi-ote.rrpproxy.net" />
    <target host="wi.rrpproxy.net" />

	<rule from="^http:" to="https:" />
</ruleset>
