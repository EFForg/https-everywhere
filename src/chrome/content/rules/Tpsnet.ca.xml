<!--
	Nonfunctional hosts in *.tpsnet.ca:

	h: http redirect
	m: certificate mismatch
	r: connection refused
	s: self-signed certificate
	t: timeout on https
-->
<ruleset name="Tpsnet.ca">
	<target host="tpsnet.ca" />
	<target host="www.tpsnet.ca" />

	<rule from="^http:"
		to="https:" />
</ruleset>
