<!--
	Fully covered domains:

		- (www.)linux-sunxi.org
		- (www.)sunxi.org

-->
<ruleset name="linux-sunxi.org">

	<!--	Direct rewrites:
				-->
	<target host="linux-sunxi.org" />
	<target host="www.linux-sunxi.org" />
	<target host="sunxi.org" />
	<target host="www.sunxi.org" />


	<securecookie host="^(?:linux-)?sunxi\.org$" name=".+" />


	<rule from="^http:"
		to="https:" />

</ruleset>
