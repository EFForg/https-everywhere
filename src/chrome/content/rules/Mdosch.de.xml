<!--
	www.mdosch.de: Self-signed

-->
<ruleset name="mdosche.de">

	<!--	Direct rewrites:
				-->
	<target host="mdosche.de" />
	<target host="blog.mdosche.de" />

	<!--	Complications:
				-->
	<target host="www.mdosche.de" />


	<securecookie host="." name="." />


	<rule from="^http://www\.mdosche\.de/"
		to="https://mdosche.de/" />

	<rule from="^http:"
		to="https:" />

</ruleset>
