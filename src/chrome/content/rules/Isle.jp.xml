<!--
	For other GMO coverage, see GMO_Internet.xml.


	(www.): refused


	Fully covered subdomains:

		- (www.)	(-> shared.gmocloud.com)
		- help
		- set

-->
<ruleset name="Isle.jp">

	<target host="isle.jp" />
	<target host="*.isle.jp" />


	<!--	Redirects as so:
				-->
	<rule from="^http://(?:www\.)?isle\.jp/+"
		to="https://shared.gmocloud.com/" />

	<rule from="^http://(home|set)\.isle\.jp/"
		to="https://$1.isle.jp/" />

</ruleset>
