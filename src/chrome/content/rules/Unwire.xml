<!--
	Not covered here:

		- unwire.dk *
		- editor.portal.unwire.com *
		- unwire.com **

	 * Bad certificate
	** Serves https but style sheets fail to load on "mixedcontent" platforms.
-->
<ruleset name="Unwire">

	<target host="portal.unwire.dk" />
	<target host="image.unwire.com" />

	<securecookie host="^portal\.unwire\.dk$" name=".+" />

	<rule from="^http:"
		to="https:"/>

</ruleset>
