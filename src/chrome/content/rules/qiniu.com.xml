	<!--
		qbox.me is a https CDN provided by QiNiu.

		Sometimes it will try to get contents from a source where is set by
		its customer, so there are some inevitable MCB. However, only few
		people will visit pages on *.qbox.me instead of original domains.
	-->
	<target host="*.qbox.me" />
		<test url="http://dn-growing.qbox.me/vds.js" />
		<test url="http://dn-pili-static.qbox.me/images/loading.gif" />

		<exclusion pattern="^http://dn-geekpark-new\.qbox\.me/(topics|users|videos)/" />
			<test url="http://dn-geekpark-new.qbox.me/topics/210279" />
			<test url="http://dn-geekpark-new.qbox.me/topics/210313" />
			<test url="http://dn-geekpark-new.qbox.me/users/215004" />
			<test url="http://dn-geekpark-new.qbox.me/videos/210537" />

	<target host="*.qnssl.com" />
		<test url="http://dn-dji-videos.qnssl.com/" />
		<test url="http://itisatest.qnssl.com/" />
		<test url="http://nzer51d3l.qnssl.com/" />

	<securecookie host=".+" name=".+" />

	<rule from="^http:" to="https:" />
</ruleset>
