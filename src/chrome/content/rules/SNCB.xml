<ruleset name="SNCB">
    <target host="b-europe.com" />
    <target host="*.b-europe.com" />

    <securecookie host=".+" name=".+"/>

    <rule from="^http://b-europe\.com/"
        to="https://www.b-europe.com/" />
    <rule from="^http://([^/:@]+)?\.b-europe\.com/"
        to="https://$1.b-europe.com/" />
</ruleset>
