<ruleset name="Talking French">
    <target host="talkingfrench.com" />
    <target host="www.talkingfrench.com" />

    <rule from="^http://talkingfrench\.com/"
        to="https://www.talkingfrench.com/" />
    <rule from="^http://([^/:@]+)?\.talkingfrench\.com/"
        to="https://$1.talkingfrench.com/" />
</ruleset>
