<!--
	400:
		www.coin-hive.com
	Unable to Connect:
		www.cnhv.co
-->
<ruleset name="coin-hive.com">
	<target host="coin-hive.com" />
	<target host="www.coin-hive.com" />
	<target host="api.coin-hive.com" />

	<target host="cnhv.co" />
	<target host="www.cnhv.co" />

	<securecookie host=".+" name=".+" />

	<rule from="^http://www\.coin-hive\.com/" to="https://coin-hive.com/" />

	<rule from="^http://www\.cnhv\.co/" to="https://cnhv.co/" />

	<rule from="^http:" to="https:" />
</ruleset>
