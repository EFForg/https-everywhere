<ruleset name="Get.It">

	<target host="get.it" />
	<target host="media.get.it" />
	<target host="www.get.it" />
	<target host="detect.get.it" />


	<securecookie host="^\.get.it$" name=".+" />



	<rule from="^http://detect\.get\.it/"
		to="https://s3.amazonaws.com/detect.get.it/" />

	<rule from="^http:" to="https:" />
</ruleset>
