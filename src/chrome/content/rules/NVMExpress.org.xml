<!--
	Invalid certificate:
		- www.nvmexpress.org
-->

<ruleset name="NVMExpress.org">
	<target host="nvmexpress.org" />
	<target host="www.nvmexpress.org" />
	<target host="workspace.nvmexpress.org" />

	<rule from="^http://www\.nvmexpress\.org/"
		to="https://nvmexpress.org/" />
	<rule from="^http:" to="https:" />
</ruleset>
