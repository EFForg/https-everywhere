<!--
	Amsterdam Internet Exchange

-->
<ruleset name="AMS-IX.net">

	<!--	Direct rewrites:
				-->
	<target host="ams-ix.net" />
	<target host="my.ams-ix.net" />
	<target host="stats.ams-ix.net" />
	<target host="www.ams-ix.net" />


	<securecookie host="^(?:w*\.)?ams-ix\.net$" name=".+" />


	<rule from="^http:"
		to="https:" />

</ruleset>
