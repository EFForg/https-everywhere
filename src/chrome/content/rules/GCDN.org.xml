<!--
	For other Edoceo coverage, see Edoceo.com.xml.


	www.gcdn.org: Cert only matches ^gcdn.org

-->
<ruleset name="gCDN.org">

	<!--	Direct rewrites:
				-->
	<target host="gcdn.org" />

	<!--	Complications:
				-->
	<target host="www.gcdn.org" />


	<rule from="^http://www\.gcdn\.org/"
		to="https://gcdn.org/" />

	<rule from="^http:"
		to="https:" />

</ruleset>
