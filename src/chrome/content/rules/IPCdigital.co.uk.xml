<!--
	Other IPC Media rulesets:

		- Marie_Claire.co.uk.xml


	CDN buckets:

		- ipccdn.co.uk.edgesuite.net

			- analytics.assets

		- assets.ipcdigital.co.uk.edgesuite.net
		- yui.ipcdigital.co.uk.edgesuite.net
		- zardoz.ipcdigital.co.uk.edgesuite.net


	FUlly covered subdomains:

		- *.media:

			- marieclaire

		- *.secure.media:

			- default


	Nonfunctional subdomains:

		- assets *
		- analytics.assets *
		- yui *
		- zardoz *

	* 404, akamai

-->
<ruleset name="IPCdigital.co.uk (partial)">

	<target host="*.media.ipcdigital.co.uk" />


	<rule from="^http://([\w-]+)(?:\.secure)?\.media\.ipcdigital\.co\.uk/"
		to="https://$1.secure.media.ipcdigital.co.uk/" />

</ruleset>