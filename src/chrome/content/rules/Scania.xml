<!--
	Nonfunctional subdomains:

		- (www.)
		- campaign
		- newsroom	(record_too_long)
		- se
		- webshop
		- www[23]	(shows www4; mismatched, CN: www4.scania.com)

-->
<ruleset name="Scania (partial)">

	<target host="*.scania.com" />


	<securecookie host="^.+\.scania\.com$" name=".+" />


	<rule from="^http://(static|webapp\d+|www4)\.scania\.com/"
		to="https://$1.scania.com/" />

</ruleset>