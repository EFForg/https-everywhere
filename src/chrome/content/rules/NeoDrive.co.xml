<!--
	www.neodrive.co: Mismatched

-->
<ruleset name="NeoDrive.co">

	<!--	Direct rewrites:
				-->
	<target host="neodrive.co" />

	<!--	Complications:
				-->
	<target host="www.neodrive.co" />


	<rule from="^http://www\.neodrive\.co/"
		to="https://neodrive.co/" />

	<rule from="^http:"
		to="https:" />

</ruleset>
