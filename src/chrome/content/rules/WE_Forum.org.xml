<!--
	World Economic Forum


	Nonfunctional subdomains:

		- chinese *
		- www *

	* Redirects to http


	Fully covered subdomains:

		- ^
		- agenda
		- toplink

-->
<ruleset name="WE Forum.org (partial)">

	<!--	Direct rewrites:
				-->
	<target host="weforum.org" />
	<target host="agenda.weforum.org" />
	<target host="toplink.weforum.org" />
		<!--
			Redirects to http:
						-->
		<!--exclusion pattern="^http://(chinese|www)\.weforum\.org/($|sites/)" /-->


	<securecookie host="^\.weforum\.org$" name="^__cfduid$" />


	<rule from="^http:"
		to="https:" />

</ruleset>
