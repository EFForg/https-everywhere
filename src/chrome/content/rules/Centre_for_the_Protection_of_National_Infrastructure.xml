<ruleset name="Centre for the Protection of National Infrastructure (CPNI)">
    <target host="cpni.gov.uk" />
    <target host="*.cpni.gov.uk" />

    <securecookie host="^(?:.*\.)?cpni\.gov\.uk$" name=".+" />

    <rule from="^http://cpni\.gov\.uk/"
        to="https://www.cpni.gov.uk/" />
    <rule from="^http://([^/:@]+)?\.cpni\.gov\.uk/"
        to="https://$1.cpni.gov.uk/" />
</ruleset>
