<ruleset name="MyMoustache.net">
	<target host=    "mymoustache.net" />
	<target host="www.mymoustache.net" />

  	<securecookie host="^.*\.mymoustache\.net$" name=".+" />

	<!-- connection timeout -->
	<rule from="^http://mymoustache\.net/"
		to="https://www.mymoustache.net/" />

	<rule from="^http:"
		to="https:" />
</ruleset>
