<!--
	For other Europcar coverage, see Europcar.com.xml


	Non-functional subdomains:
		- aurigny	(r)
		- autotrader	(r)
		- beatyoursummerblues	(m)
		- blog	(m)
		- eurohotelsearch	(r)
		- gastro-guide	(r)
		- humbersideairport	(r)
		- media	(r)
		- microsite	(r)
		- mod	(r)
		- roadtrip	(m)
		- secure	(r)
		- services	(r)
		- tourops	(r)
		- wtm	(h)
		- www2	(m)

	e: expired certificate
	h: http redirect
	i: invalid certificate chain
	m: certificate mismatch
	r: connection refused
	s: self-signed certificate
	t: timeout on https
-->
<ruleset name="Europcar.co.uk">

	<target host="europcar.co.uk" />
	<target host="www.europcar.co.uk" />
	<target host="biz.europcar.co.uk" />
	<target host="careers.europcar.co.uk" />
	<target host="corporate.europcar.co.uk" />
	<target host="corporatevanbrochure.europcar.co.uk" />
	<target host="deliveryandcollection.europcar.co.uk" />
	<target host="drivingforbusiness.europcar.co.uk" />
	<target host="faq.europcar.co.uk" />
	<target host="faq-test.europcar.co.uk" />
	<target host="greyfleetcalculator.europcar.co.uk" />
	<target host="hourlyhire.europcar.co.uk" />
	<target host="longterm.europcar.co.uk" />
	<target host="longtermhire.europcar.co.uk" />
	<target host="m.europcar.co.uk" />
	<target host="memberstravelpack.europcar.co.uk" />
	<target host="mobile.europcar.co.uk" />
	<target host="my-europcar.europcar.co.uk" />
	<target host="news.europcar.co.uk" />
	<target host="one.europcar.co.uk" />

	<securecookie host=".+" name=".+" />

	<rule from="^http:"
		to="https:" />
</ruleset>
