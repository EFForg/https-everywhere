<!--
	www.screenshot.ru: Mismatched

-->
<ruleset name="Screenshot.ru" default_off="expired, untrusted root">

	<!--	Direct rewrites:
				-->
	<target host="screenshot.ru" />

	<!--	Complications:
				-->
	<target host="www.screenshot.ru" />


	<rule from="^http://www\.screenshot\.ru/"
		to="https://screenshot.ru/" />

	<rule from="^http:"
		to="https:" />

</ruleset>
