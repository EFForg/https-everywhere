<!--
	Other Levo League rulesets:

		- Levo_League.com.xml

	Invalid certificate:
		horizon.levo.com
		link.levo.com

	Login required:
		leaders.levo.com

	Time out:
		ttd.levo.com

    Redirect to HTTP:
        pages.levo.com ( https://pages.levo.com/guides/ , https://pages.levo.com/jetset/ )

-->
<ruleset name="Levo.com">

	<target host="levo.com" />
	<target host="www.levo.com" />
	<target host="support.levo.com" />

	<rule from="^http:"
		to="https:" />

</ruleset>
