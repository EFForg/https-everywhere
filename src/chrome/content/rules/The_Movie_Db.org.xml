<!--
	Other Movie Database rulesets:

		- TMDb.org


	^themoviedb.org: Mismatched


	Insecure cookies are set for these domains:

		- .www.themoviedb.org

-->
<ruleset name="The Movie Db.org">

	<!--	Direct rewrites:
				-->
	<target host="www.themoviedb.org" />

	<!--	Complications:
				-->
	<target host="themoviedb.org" />


	<!--	Not secured by server:
					-->
	<!--securecookie host="^\.www\.themoviedb\.org$" name="^prefs$" /-->

	<securecookie host="^\.www\.themoviedb\.org$" name=".+" />


	<rule from="^http://themoviedb\.org/"
		to="https://www.themoviedb.org/" />

	<rule from="^http:"
		to="https:" />

</ruleset>
