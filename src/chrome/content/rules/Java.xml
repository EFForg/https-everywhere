<!--
	Non-functional hosts
		Couldn't connect to server:
			 - bugreport.java.com
-->
<ruleset name="Java">
	<target host="java.com" />
	<target host="www.java.com" />
	<target host="go.java.com" />

	<securecookie host="^(www\.)?java\.com$" name=".+"/>

	<rule from="^http:" to="https:" />
</ruleset>
