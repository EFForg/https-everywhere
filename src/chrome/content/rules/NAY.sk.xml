<!--
	Nonfunctional hosts in *.nay.sk:

	certificate mismatch:
		- mail.nay.sk
		- vpn.nay.sk
	connection refused:
		- autodiscover.nay.sk
		- beta.nay.sk
		- cdn.nay.sk
		- cloud.nay.sk
		- www.cloud.nay.sk
		- isasrv.nay.sk
		- ocs.nay.sk
		- pool.nay.sk
		- sapkonzola.nay.sk
		- shp.nay.sk
		- webmail.nay.sk
		- websvc.nay.sk
-->
<ruleset name="NAY.sk">
	<target host="nay.sk" />
	<target host="www.nay.sk" />

	<rule from="^http:" to="https:" />
</ruleset>
