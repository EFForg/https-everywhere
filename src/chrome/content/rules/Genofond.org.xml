	Invalid Certificate:
		www.genofond.org
-->
<ruleset name="Genofond.org">
	<target host="genofond.org" />
	<target host="www.genofond.org" />

	<securecookie host=".+" name=".+" />

	<rule from="^http://www\.genofond\.org/" to="https://genofond.org/" />

	<rule from="^http:" to="https:" />
</ruleset>
