<ruleset name="RESOLV" default_off="mismatch">

	<!--	Cert: *.gridserver.com	-->
	<target host="resolv.org" />
	<target host="www.resolv.org" />


	<securecookie host="^www\.resolv\.org$" name=".*" />


	<!--	!www redirects to www.	-->
	<rule from="^http://(?:www\.)?resolv\.org/"
		to="https://www.resolv.org/" />

</ruleset>
