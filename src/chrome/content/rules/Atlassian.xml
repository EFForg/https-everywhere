<ruleset name="Atlassian">
  <target host="atlassian.com" />
  <target host="*.atlassian.com" />

  <securecookie host="^(.*\.)?atlassian.com$" name=".*" />

  <rule from="^http://(www\.)?atlassian\.com/" to="https://www.atlassian.com/"/>

  <rule from="^http://(blogs|plugins|confluence)\.atlassian\.com/" to="https://$1.atlassian.com/"/>

</ruleset>
