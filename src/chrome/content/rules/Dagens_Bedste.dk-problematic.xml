<!--
	For rules that are on by default, see Dagens_Bedste.dk.xml.

-->
<ruleset name="Dagens Bedste.de" default_off="connection refused">

	<target host="dagensbedste.dk" />
	<target host="www.dagensbedste.dk" />


	<securecookie host="^dagensbedste\.dk$" name=".+" />


	<rule from="^http://(?:www\.)?dagensbedste\.dk/"
		to="https://dagensbedste.dk/" />

</ruleset>
