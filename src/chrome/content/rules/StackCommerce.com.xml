<!--
	Nonfunctional subdomains:

		- blog *

	* wpengine


	Fully covered subdomains:

		- (www.)
		- cdn
		- image-assets
		- stores-assets

-->
<ruleset name="StackCommerce.com (partial)">

	<target host="stackcommerce.com" />
	<target host="*.stackcommerce.com" />


	<rule from="^http://((?:cdn|image-assets|stores-assets|www)\.)?stackcommerce\.com/"
		to="https://$1stackcommerce.com/" />

</ruleset>
