<ruleset name="Real-Debrid.com">

	<target host="real-debrid.com" />
	<target host="*.real-debrid.com" />


	<securecookie host="^\.real-debrid\.com$" name=".+" />


	<!--	www redirects to ^ over http,
		so copy that behavior:
					-->
	<rule from="^http://(?:(cdn\.)|www\.)?real-debrid\.com/"
		to="https://$1real-debrid.com/" />

</ruleset>
