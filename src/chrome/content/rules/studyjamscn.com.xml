<!--
	Invalid cert:
		^studyjamscn.com

	MCB:
		www.studyjamscn.com
			- Break captcha on signup page.
			- Break baidu share js on lots pages.
-->

<ruleset name="studyjamscn.com">
	<target host="studyjamscn.com" />
	<rule from="^http://studyjamscn\.com/" to="https://www.studyjamscn.com/" />

	<target host="www.studyjamscn.com" />
	<exclusion pattern="^http://www\.studyjamscn\.com/member\.php\?mod=newmember" />

	<target host="docs.studyjamscn.com" />

	<rule from="^http:" to="https:" />
</ruleset>
