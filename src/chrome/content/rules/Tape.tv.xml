<ruleset name="Tape.tv">
<target host="*.tape.tv"/>
<target host="tapetv-slowjam.de"/>

<rule from="^http://(blog|connect|metrics|streaming\-url|web202|www)\.tape\.tv/" to="https://$1.tape.tv/"/>
<rule from="^http://tapetv-slowjam\.de/" to="https://tapetv-slowjam.de/"/>

<test url="http://blog.tape.tv/" />
<test url="http://connect.tape.tv/" />
<test url="http://metrics.tape.tv/" />
<test url="http://streaming-url.tape.tv/" />
<test url="http://web202.tape.tv/" />
<test url="http://www.tape.tv/" />
<test url="http://tapetv-slowjam.de" />

</ruleset>
