    <securecookie host="^(www\.)?filecrypt\.cc" name=".+" />

    <test url="http://filecrypt.cc/helper.html"/>

    <exclusion pattern="^http://filecrypt\.cc/helper\.html"/>
    <rule from="^http:" to="https:"/>

</ruleset>
