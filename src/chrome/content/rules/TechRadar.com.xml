<!--
	Nonfunctional hosts in *.techradar.com:

	h: http redirect
	m: certificate mismatch
	r: connection refused
	s: self-signed certificate
	t: timeout on https
-->
<ruleset name="TechRadar.com">
	<target host="www.techradar.com" />

	<rule from="^http:" to="https:" />

	<test url="http://www.techradar.com/how-to/computing/how-to-build-your-own-raspberry-pi-nas-1315968" />
</ruleset>
