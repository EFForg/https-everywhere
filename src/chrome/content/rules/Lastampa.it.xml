<!--
	Nonfunctional hosts in *.lastampa.it:

	h: http redirect
	m: certificate mismatch
	r: connection refused
	s: self-signed certificate
	t: timeout on https
-->
<ruleset name="Lastampa.it">
	<target host="lastampa.it" />
	<target host="www.lastampa.it" />

	<rule from="^http:" to="https:" />
</ruleset>
