<!--
	Problematic subdomains:

		- www *

	* Refused


	Fully covered subdomains:

		- (www.)?	(www â†’ ^)
		- api
		- secure

-->
<ruleset name="Travis-CI.org">

	<target host="travis-ci.org" />
	<target host="*.travis-ci.org" />


	<rule from="^http://(?:(api\.|secure\.)|www\.)?travis-ci\.org/"
		to="https://$1travis-ci.org/" />

</ruleset>
