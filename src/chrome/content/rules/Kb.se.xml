<!--
 	Nonfunctional hosts in *.kb.se:
	    www.libris.kb.se (m)	
-->
<ruleset name="Kb.se">
	<target host="kb.se" />
	<target host="www.kb.se" />
	<target host="libris.kb.se" />
	<target host="librishelp.libris.kb.se" />
	<target host="uppsok.libris.kb.se" />
	<target host="sondera.kb.se" />
	<rule from="^http:" to="https:" />
</ruleset>
