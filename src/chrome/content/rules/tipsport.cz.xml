<ruleset name="Tipsport">
    <target host="tipsport.cz" />
    <target host="www.tipsport.cz" />
    <target host="home.tipsport.cz" />

    <test url="http://tipsport.cz" />
    <test url="http://www.tipsport.cz" />
    <test url="http://home.tipsport.cz" />

    <rule from="^http:"
            to="https:" />
</ruleset>
