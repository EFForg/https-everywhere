<!--
	Nonfunctional subdomains:

		- blog *

	* Shows www


	Fully covered subdomains

		- (www.)?
		- secure

-->
<ruleset name="Siteparc.fr (partial)">

	<target host="siteparc.fr" />
	<target host="*.siteparc.fr" />


	<rule from="^http://(secure\.|www\.)?siteparc\.fr/"
		to="https://$1siteparc.fr/" />

</ruleset>
