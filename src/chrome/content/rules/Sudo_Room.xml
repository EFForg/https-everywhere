<ruleset name="Sudo Room">

	<target host="sudoroom.org" />
	<target host="door.sudoroom.org" />
	<target host="lists.sudoroom.org" />
	<target host="www.sudoroom.org" />

	<securecookie host="^(?:www\.|lists\.|door\.)?sudoroom\.org$" name=".*" />

	<rule from="^http://(?:www\.)?sudoroom\.org/" to="https://sudoroom.org/" />
	<rule from="^http://door\.sudoroom\.org/" to="https://door.sudoroom.org/" />
	<rule from="^http://lists\.sudoroom\.org/" to="https://lists.sudoroom.org/" />

</ruleset>
