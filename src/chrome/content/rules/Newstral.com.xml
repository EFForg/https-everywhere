<!--
	Problematic domains:

		- www ¹
		- blog ²

	¹: Mismatched
	²: Refused
-->
<ruleset name="Newstral.com (partial)">

	<target host="newstral.com" />
	<target host="www.newstral.com" />
	<target host="m.newstral.com" />

	<securecookie host="\.?newstral\.com" name=".*" />

	<rule from="^http://www\.newstral\.com/"
		to="https://newstral.com/" />

	<rule from="^http:"
		to="https:" />

</ruleset>
