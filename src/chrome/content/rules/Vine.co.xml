<!--
	For other Twitter coverage, see Twitter.xml.


	Fully covered hosts in *vine.co:

		- (www.)?
		- mtc.cdn
		- v.cdn

-->
<ruleset name="Vine.co">

	<target host="vine.co" />
	<target host="mtc.cdn.vine.co" />
	<target host="v.cdn.vine.co" />
	<target host="www.vine.co" />


	<rule from="^http:"
		to="https:" />

</ruleset>
