<!--
	Nonfunctional hosts in *.theundefeated.com:

	h: http redirect
	m: certificate mismatch
	r: connection refused
	s: self-signed certificate
	t: timeout on https
-->
<ruleset name="The Undefeated">
	<target host="theundefeated.com" />
	<target host="www.theundefeated.com" />

	<securecookie host="^\w" name=".+" />

	<rule from="^http:" to="https:" />
</ruleset>
