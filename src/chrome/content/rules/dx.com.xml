<ruleset name="dx.com (partial)">
	<target host="www.dx.com" />
	<target host="passport.dx.com" />
	<target host="my.dx.com" />
	<target host="cart.dx.com" />
	<target host="img.dxcdn.com" />
	
	<securecookie host="^\.(cart|www|passport|my)\.dx\.com$" name=".+" />
	<securecookie host="^\.img\.dxcdn\.com$" name=".+" />

	<rule from="^http:"
		to="https:" />
</ruleset>
