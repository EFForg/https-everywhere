<!-- 
	funktionstjanster.se = authenticators and form submissions for SE gov sites etc

	Related: bankid.com.xml
-->

<ruleset name="funktionstjanster.se">
	
	<!-- mNN.LLLL.funktionstjanster.se = authenticator hosts: -->

	<target host="*.idp.funktionstjanster.se" />
	<target host="*.idpat.funktionstjanster.se" />
	<target host="*.idpst.funktionstjanster.se" />
	<target host="*.me.funktionstjanster.se" />
	<target host="*.testidp.funktionstjanster.se" />

	<rule from="^http://(m\d{2}-mg-local\.\w*)\.funktionstjanster\.se/" 
            to="https://$1.funktionstjanster.se/" />

  	<test url="http://m00-mg-local.idp.funktionstjanster.se/" />
  	<test url="http://m01-mg-local.idp.funktionstjanster.se/" />
  	<test url="http://m02-mg-local.idp.funktionstjanster.se/" />

	<test url="http://m00-mg-local.idpat.funktionstjanster.se/" />
	<test url="http://m00-mg-local.idpst.funktionstjanster.se/" />
	<test url="http://m00-mg-local.me.funktionstjanster.se/" />
	<test url="http://m00-mg-local.testidp.funktionstjanster.se/" />
</ruleset>
