<!--
	For Tampermonkey.net

		Works:

			- www
			- $
			- forum
			- safari

		Doesn't Work:
		nil
-->
<ruleset name="Tampermonkey.net">
  <target host="tampermonkey.net" />
  <target host="*.tampermonkey.net" />

  <rule from="^http://tampermonkey\.net/"
          to="https://tampermonkey.net/" />

  <rule from="^http://(www|forum|safari)\.tampermonkey\.net/"
          to="https://$1.tampermonkey.net/" />

  <test url="http://tampermonkey.net/" />
  <test url="http://www.tampermonkey.net/" />
  <test url="http://forum.tampermonkey.net/" />
  <test url="http://safari.tampermonkey.net/" />

  <securecookie host="^forum\.tampermonkey\.net$" name=".+" />
</ruleset>