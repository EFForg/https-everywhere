<!--
	Nonfunctional subdomains:

		- ^ *
		- en *
		- www	(shows login, valid cert)
		- demo.pretarget	(refused)

	* Shows another domain; mismatched, CN: *.sakura.ne.jp

	Problematic subdomains:

		- ie7	(invalid)


	Fully covered subdomains:

		- cnt
		- dsp
		- js
		- sync

-->
<ruleset name="Fout.jp (partial)">

	<target host="ad.fout.jp" />
	<target host="bid.fout.jp" />
	<target host="click.fout.jp" />
	<target host="cnt.fout.jp" />
	<target host="conv.fout.jp" />
	<target host="dsp.fout.jp" />
	<target host="js.fout.jp" />
	<target host="sync.dmp.fout.jp" />
	<target host="sync.fout.jp" />
	<target host="target.fout.jp" />

	<test url="http://sync.dmp.fout.jp/serve/" />

	<!--	Set by cnt:
				-->
	<securecookie host="^\.fout\.jp$" name="^uid$" />

	<rule from="^http:"
		to="https:" />

</ruleset>
