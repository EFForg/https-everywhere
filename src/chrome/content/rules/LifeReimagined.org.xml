<!--
	Problematic hosts in *lifereimagined.org:

		- cms *

	* Server sends no certificate chain, see https://whatsmychaincert.com

-->
<ruleset name="LifeReimagined.org" default_off="cert-chain">

	<!--	Direct rewrites:
				-->
	<target host="cms.lifereimagined.org" />


	<rule from="^http:"
		to="https:" />

</ruleset>
