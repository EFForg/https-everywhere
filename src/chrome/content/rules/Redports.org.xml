<!--
	^redports.org: Mismatched

-->
<ruleset name="redports.org">

	<!--	Direct rewrites:
				-->
	<target host="www.redports.org" />

	<!--	Complications:
				-->
	<target host="redports.org" />


	<securecookie host="^\w" name="." />


	<rule from="^http://redports\.org/"
		to="https://www.redports.org/" />

	<rule from="^http:"
		to="https:" />

</ruleset>
