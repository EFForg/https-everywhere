	<target host="prep.rci.com" />
	<target host="secureftp.rci.com" />

	<rule from="^http://m\.rci\.com/$"
		  	to="https://m.rci.com/rcimobile/" />
	
	<rule from="^http:" 
		  	to="https:" />
</ruleset>
