<ruleset name="Conformal Systems">

	<target host="conformal.com" />
	<target host="*.conformal.com" />
	<target host="cyphertite.com" />
	<target host="*.cyphertite.com" />


	<securecookie host="^(?:.*\.)?c(?:onformal|yphertite)\.com$" name=".+" />


	<rule from="^http://((?:blog|opensource|www)\.)?conformal\.com/"
		to="https://$1conformal.com/" />

	<rule from="^http://(blog\.|www\.)?cyphertite\.com/"
		to="https://$1cyphertite.com/" />

</ruleset>
