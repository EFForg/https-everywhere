<!--
	For other Yandex coverage, see Yandex.xml.

	Problematic subdomains:

		- www.es (wrong cert)
		- ams, msk, rnd, nsk
-->
<ruleset name="Yandex.st">

	<target host="yandex.st" />
	<target host="www.yandex.st" />
	<target host="i.yandex.st" />
	<target host="www.i.yandex.st" />
	<target host="static.yandex.st" />

	<rule from="^http://(?:www\.)?([^.]+)\.yandex\.st/"
		to="https://$1.yandex.st/" />

		<!--test url="http://www.i.yandex.st/" /-->

	<rule from="^http://(?:www\.)?yandex\.st/"
		to="https://yandex.st/" />

</ruleset>
