<ruleset name="CZ.NIC">

	<target host="nic.cz" />
	<target host="*.nic.cz" />


	<securecookie host="^(?:(?:www|labs|git|blog)?\.)?nic\.cz$" name=".+" />


	<rule from="^http://((?:www|labs|git|blog|knihy|(?:gitlab|jenkins)\.labs|lists|piwik|podpora)\.)?nic\.cz/"
		to="https://$1nic.cz/" />

</ruleset>
