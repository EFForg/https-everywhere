<!--
	Clients have unique subdomains.

-->
<ruleset name="Branchable.com">

	<target host="branchable.com" />
	<target host="*.branchable.com" />


	<securecookie host=".+\.branchable\.com$" name=".+" />


	<rule from="^http://([\w-]+\.)?branchable\.com/"
		to="https://$1branchable.com/" />

</ruleset>