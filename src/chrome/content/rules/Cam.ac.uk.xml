<!-- lots of domains lack (valid) https:
	www.cam.ac.uk
	www.sports.cam.ac.uk
	www.lib.cam.ac.uk 
	for exampl -->
<ruleset name="Cam.ac.uk">
	<target host="www.cl.cam.ac.uk" />
	<target host="www.admin.cam.ac.uk" />
	<target host="webservices.admin.cam.ac.uk" />
	<rule from="^http://www\.cl\.cam\.ac\.uk/" to="https://www.cl.cam.ac.uk/"/>
	<rule from="^http://www\.admin\.cam\.ac\.uk/" to="https://www.admin.cam.ac.uk/"/>
	<rule from="^http://webservices\.admin\.cam\.ac\.uk/" to="https://webservices.admin.cam.ac.uk/"/>
</ruleset>

