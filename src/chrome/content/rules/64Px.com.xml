<!--

	Problematic hosts in *64Px.com:

		- blog (mismatch)
		- chrome (self-signed)
		- fan (mismatch)
		- gifts (expired)
		- www.gifts (self-signed)
		- instagram (self-signed)
		- l (mismatch)
		- notifications.extensions (self-signed)
		- s3policy (self-signed)
		- twitter (self-signed)
		- update (http only)

-->
<ruleset name="64Px.com">

	<target host="64px.com" />
	<target host="www.64px.com" />

	<rule from="^http:"
		to="https:" />

</ruleset>
