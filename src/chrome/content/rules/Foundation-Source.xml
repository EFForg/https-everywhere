<ruleset name="Foundation Source">

	<target host="foundationsource.com" />
	<target host="*.foundationsource.com" />


	<securecookie host="^(?:.*\.)?foundationsource\.com$" name=".*" />


	<rule from="^http://((?:access|fsol|online|www)\.)?foundationsource\.com/"
		to="https://$1foundationsource.com/" />

</ruleset>
