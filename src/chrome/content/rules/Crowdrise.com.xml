<!--
	Nonfunctional hosts in *crowdrise.com:

		- support *

	* Zendesk


	Fully covered hosts in *crowdrise.com:

		- (www.)?
		- cdn


	Insecure cookies are set for these domains:

		- .crowdrise.com

-->
<ruleset name="Crowdrise.com (partial)">

	<!--	Direct rewrites:
				-->
	<target host="crowdrise.com" />
	<target host="cdn.crowdrise.com" />
	<target host="www.crowdrise.com" />


	<!--	Not secured by server:
					-->
	<!--securecookie host="^\.crowdrise\.com$" name="^crowdrise_session$" /-->

	<securecookie host="^\.crowdrise\.com$" name=".+" />


	<rule from="^http:"
		to="https:" />

</ruleset>
