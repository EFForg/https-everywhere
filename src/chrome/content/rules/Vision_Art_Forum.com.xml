<!--
	$ redirects to content.php
	content.php redirects to http

-->
<ruleset name="Vision Art Forum.com (partial)">

	<target host="visionartforum.com" />
	<target host="www.visionartforum.com" />
		<exclusion pattern="^http://(?:www\.)?visionartforum\.com/(?:content\.php)?(?:\?.*)?$" />


	<rule from="^http://(www\.)?visionartforum\.com/"
		to="https://$1visionartforum.com/" />

</ruleset>