<!--
	For rules that are on by default, see University-of-Cambridge.xml.

-->
<ruleset name="Cam.ac.uk (mismatched)" default_off="mismatched">

	<target host="www.*.cam.ac.uk" />
	<target host="www.epe.admin.cam.ac.uk" />


	<securecookie host="^www\.(?:epe\.admin|jobs|philanthropy|study)\.cam\.ac\.uk$" name=".+" />


	<rule from="^http://www\.(epe\.admin|jobs|philanthropy|study)\.cam\.ac\.uk/"
		to="https://www.$1.cam.ac.uk/" />

</ruleset>
