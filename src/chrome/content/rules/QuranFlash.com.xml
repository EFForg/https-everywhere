<!--
		Remark: *.quranflash.com have a wildcard DNS Record.

	Invalid Certificate:
		v1.quranflash.com
		v2.quranflash.com
		v3.quranflash.com
-->
<ruleset name="QuranFlash.com" platform="mixedcontent">
	<target host="quranflash.com" />
	<target host="www.quranflash.com" />

	<securecookie host=".+" name=".+" />

	<rule from="^http:" to="https:" />
</ruleset>
