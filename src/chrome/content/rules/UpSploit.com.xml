<!--
	(www.)?: Reset

-->
<ruleset name="upSploit.com" default_off="connection reset">

	<target host="upsploit.com" />
	<target host="*.upsploit.com" />


	<securecookie host="^\.upsploit\.com$" name=".+" />


	<rule from="^http://(www\.)?upsploit\.com/"
		to="https://$1upsploit.com/" />

</ruleset>
