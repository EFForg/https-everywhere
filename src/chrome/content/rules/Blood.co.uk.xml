<!--
	^blood.co.uk: Refused
	www.blood.co.uk: Redirects to http

-->
<ruleset name="Blood.co.uk (partial)">

	<target host="my.blood.co.uk" />
	<target host="secure.blood.co.uk" />


	<securecookie host="^\." name="^_gat?$" />
	<securecookie host="^\w" name="." />


	<rule from="^http:"
		to="https:" />

</ruleset>
