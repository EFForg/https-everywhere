<!--
	Mismatch:
		- cp
		- www

	Wrong content:
		- blog
-->
<ruleset name="EAT.ch">
	<target host="eat.ch" />
	<target host="www.eat.ch" />
	<target host="api.eat.ch" />
	<target host="m.eat.ch" />
	<target host="static.eat.ch" />

	<rule from="^http://www\.eat\.ch/"
		to="https://eat.ch/" />
	<rule from="^http:"
		to="https:" />
</ruleset>
