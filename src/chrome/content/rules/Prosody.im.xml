<!--
	Nonfunctional subdomains:

		- blog	(shows hg)


	Problematic subdomains:

		- www	(shows hg)


	Fully covered subdomains:

		- (www.)	(www â†’ ^)
		- hg

-->
<ruleset name="Prosody.im">

	<!--	Direct rewrites:
				-->
	<target host="prosody.im" />
	<target host="hg.prosody.im" />

	<!--	Complications:
				-->
	<target host="www.prosody.im" />


	<rule from="^http://www\.prosody\.im/"
		to="https://prosody.im/" />

	<rule from="^http:"
		to="https:" />

</ruleset>
