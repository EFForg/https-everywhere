<!--
	This domain contains a wildcard certificate and DNS record.
-->

<ruleset name="Katwarn.de">
	<target host="katwarn.de" />
	<target host="*.katwarn.de" />
		<test url="http://www.katwarn.de/" />
		<test url="http://content.katwarn.de/" />
		<test url="http://geoserver.katwarn.de/" />
		<test url="http://management.katwarn.de/" />
		<test url="http://redsys.katwarn.de/" />
		<test url="http://warnungen.katwarn.de/" />
		<test url="http://web-test.katwarn.de/" />

	<rule from="^http:" to="https:" />
</ruleset>
