<!--
	www.platformadmin.com: Mismatched

-->
<ruleset name="Platform Admin.com">

	<!--	Direct rewrites:
				-->
	<target host="platformadmin.com" />

	<!--	Complications:
				-->
	<target host="www.platformadmin.com" />


	<rule from="^http://www\.platformadmin\.com/"
		to="https://platformadmin.com/" />

	<rule from="^http:"
		to="https:" />

</ruleset>
