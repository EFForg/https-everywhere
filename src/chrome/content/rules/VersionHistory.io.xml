<!--
	meta: Redirects to http

	www: Cert only matches ^versionhistory.io

-->
<ruleset name="VersionHistory.io (partial)">

	<target host="versionhistory.io" />
	<target host="www.versionhistory.io" />


	<rule from="^http://(?:www\.)?versionhistory\.io/"
		to="https://versionhistory.io/" />

</ruleset>
