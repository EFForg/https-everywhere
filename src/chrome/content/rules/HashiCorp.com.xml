<ruleset name="HashiCorp.com">

	<target host="hashicorp.com" />
	<target host="www.hashicorp.com" />
	<target host="atlas.hashicorp.com" />
	<target host="shop.hashicorp.com" />
	<target host="status.hashicorp.com" />


	<securecookie host="^\.hashicorp\.com$" name=".+" />


	<rule from="^http:"
		to="https:" />

</ruleset>
