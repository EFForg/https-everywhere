<!--
	www: cert only matches ^techverse.com.au


	Some pages redirect to http.

-->
<ruleset name="TechVerse (partial)">

	<target host="techverse.com.au" />
	<target host="www.techverse.com.au" />


	<rule from="^http://(?:www\.)?techverse\.com\.au/(chat/|images/|index\.php\?(?:.+&amp;)?_a=(?:login|reg)|language/|skins/)"
		to="https://techverse.com.au/$1" />

</ruleset>