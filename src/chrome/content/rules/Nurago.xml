<ruleset name="Nurago">
	<target host="sensic.net" />
	<target host="mco-pb.sensic.net" />
	<target host="www.sensic.net" />

	<rule from="^http://(mco-pb|www)\.sensic\.net/"
		to="https://$1.sensic.net/" />
	<rule from="^http://sensic\.net/"
		to="https://www.sensic.net/" />

	<test url="http://sensic.net/" />
	<test url="http://www.sensic.net/" />
	<test url="http://mco-pb.sensic.net/" />

</ruleset>
