<!--
	For rules not causing false/broken MCB, see TorrentFunk.com.xml.

-->
<ruleset name="TorrentFunk.com (false MCB)" platform="mixedcontent">

	<target host="*.torrentfunk.com" />


	<securecookie host="^(?:www)?\.torrentfunk\.com$" name=".+" />


	<rule from="^http://www\.torrentfunk\.com/"
		to="https://www.torrentfunk.com/" />

</ruleset>
