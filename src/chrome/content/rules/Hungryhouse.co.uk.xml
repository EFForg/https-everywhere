<!--
	Fully covered subdomains:

		- (www.)?
		- static

-->
<ruleset name="hungryhouse.co.uk">

	<target host="hungryhouse.co.uk" />
	<target host="*.hungryhouse.co.uk" />


	<securecookie host="^\.hungryhouse\.co\.uk$" name="^__cfduid$" />


	<rule from="^http://(static\.|www\.)?hungryhouse\.co\.uk/"
		to="https://$1hungryhouse.co.uk/" />

</ruleset>
