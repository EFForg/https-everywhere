<!--
	For rules that are on by default, see Lighthouse_app.com.xml.

-->
<ruleset name="Lighthouse app.com (mismatches)" default_off="Certificate mismatch">

	<!--	Direct rewrites:
				-->
	<target host="help.lighthouseapp.com" />

	<securecookie host="^help\.lighthouseapp\.com$" name=".*"/>

	<rule from="^http:"
		to="https:" />

</ruleset>
