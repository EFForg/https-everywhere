<!--
	Invalid certificates:
		www.explore.cpdp.co
		www.m.cpdp.co
		explore.cpdb.co
		www.m.cpdb.co
		ms.cpdb.co

	Refused:
		ms.cpdp.co
		staging.cpdp.co
-->
<ruleset name="Citizens Police Data Project">
	<target host="cpdp.co" />
	<target host="www.explore.cpdp.co" />
	<target host="beta.cpdp.co" />
	<target host="www.m.cpdp.co" />

	<target host="cpdb.co" />
	<target host="www.cpdb.co" />
	<target host="beta.cpdb.co" />
	<target host="explore.cpdb.co" />
	<target host="m.cpdb.co" />
	<target host="www.m.cpdb.co" />
	<target host="staging.cpdb.co" />

	<rule from="^http://www\.explore\.cpdp\.co/" to="https://beta\.cpdp\.co/" />
	<rule from="^http://www\.m\.cpdp\.co/" to="https://beta\.cpdp\.co/" />
	<rule from="^http://explore\.cpdb\.co/" to="https://beta\.cpdp\.co/" />
	<rule from="^http://www\.m\.cpdb\.co/" to="https://cpdb\.co/" />
	<rule from="^http:" to="https:" />
</ruleset>
