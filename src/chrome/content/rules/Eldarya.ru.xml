<!--
	Nonfunctional hosts in *.eldarya.ru:

	h: http redirect
	m: certificate mismatch
	r: connection refused
	s: self-signed certificate
	t: timeout on https
-->
<ruleset name="Eldarya.ru">
	<target host="eldarya.ru" />
	<target host="www.eldarya.ru" />

	<rule from="^http:" to="https:" />
</ruleset>
