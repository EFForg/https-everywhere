<!--
	Fully covered domains:

		- helpshift.com

		- [\w-]+.helpshift.com:

			- developers
			- support
			- www
			- [\w-]+ *

	* Per-account domains


	Mixed content:

		- Images on www from $self *

	* Secured by us

-->
<ruleset name="Helpshift.com">

	<target host="helpshift.com" />
	<target host="*.helpshift.com" />


	<rule from="^http://([\w-]+\.)?helpshift\.com/"
		to="https://$1helpshift.com/" />

</ruleset>
