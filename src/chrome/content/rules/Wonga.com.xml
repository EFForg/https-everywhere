<!--
	Problematic subdomains:

		- ^	(cert only matches www)

-->
<ruleset name="Wonga.com">

	<target host="wonga.com" />
	<target host="www.wonga.com" />


	<securecookie host="^www\.wonga\.com$" name=".+" />


	<rule from="^http://(?:www\.)?wonga\.com/"
		to="https://www.wonga.com/" />

</ruleset>