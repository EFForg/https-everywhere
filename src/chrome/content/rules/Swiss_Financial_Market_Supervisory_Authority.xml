<ruleset name="Swiss Financial Market Supervisory Authority (FINMA)">
    <target host="finma.ch" />
    <target host="*.finma.ch" />

    <rule from="^http://finma\.ch/"
        to="https://www.finma.ch/"/>
    <rule from="^http://([^/:@]+)?\.finma\.ch/"
        to="https://$1.finma.ch/" />
</ruleset>
