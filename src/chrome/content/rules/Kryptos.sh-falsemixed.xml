<!--
	For rules not causing false/broken MCB, see Kryptos.sh.xml.

-->
<ruleset name="Kryptos.sh (false MCB)" platform="mixedcontent">

	<target host="*.kryptos.sh" />


	<securecookie host="^(?:\.?:www)?\.kryptos\.sh$" name=".+" />


	<rule from="^http://www\.kryptos\.sh/"
		to="https://www.kryptos.sh/" />

</ruleset>
