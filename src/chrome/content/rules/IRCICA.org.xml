<!--
	404:
		tr.ircica.org
	Unable to Connect:
		e-library.ircica.org
-->
<ruleset name="IRCICA.org">
	<target host="ircica.org" />
	<target host="www.ircica.org" />
	<target host="erp.ircica.org" />
	<target host="library.ircica.org" />

	<securecookie host="^(www|erp|library)\.ircica\.org$" name=".+" />

	<rule from="^http:" to="https:" />
</ruleset>
