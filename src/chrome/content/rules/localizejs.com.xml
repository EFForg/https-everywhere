<ruleset name="Localizejs.com">

	<target host="localizejs.com" />
	<target host="blog.localizejs.com" />
	<target host="cdn.localizejs.com" />
	<target host="help.localizejs.com" />
	<target host="www.localizejs.com" />


	<securecookie host="^\w" name=".+" />


	<rule from="^http:"
		to="https:" />

</ruleset>
