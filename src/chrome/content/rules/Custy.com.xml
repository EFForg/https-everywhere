<!--
	Invalid certificate:
		- custy.com, equivalent to www.custy.com
-->

<ruleset name="Custy.com">
	<target host="custy.com" />
	<target host="www.custy.com" />
	<target host="mailex.custy.com" />

	<rule from="^http://custy\.com/"
		to="https://www.custy.com/" />

	<rule from="^http:" to="https:" />
</ruleset>
