<!--
	Nonfunctional subdomains:

		- jobs	(shows rs; mismatched, CN: rs.broadcastjobs.co.uk)


	- !www times out
	- CN: www.architectsjournal.co.uk

-->
<ruleset name="Screen International" default_off="mismatched">

	<target host="screendaily.com" />
	<target host="www.screendaily.com" />


	<securecookie host="^www\.screendaily\.com$" name=".+" />


	<rule from="^http://(?:www\.)?screendaily\.com/"
		to="https://www.screendaily.com/" />

</ruleset>
