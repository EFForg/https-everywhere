<ruleset name="Buergerserviceportal.de">
	<target host="buergerserviceportal.de" />
	<target host="www.buergerserviceportal.de" />

	<!-- Connection error for: https://buergerserviceportal.de -->
	<rule from="^http://buergerserviceportal\.de/"
		to="https://www.buergerserviceportal.de/" />

	<rule from="^http:" to="https:" />
</ruleset>
