<!--
	Nonfunctional hosts in *.iemoji.com:
		- pix.iemoji.com (m)

	h: http redirect
	m: certificate mismatch
	r: connection refused
	s: self-signed certificate
	t: timeout on https
-->
<ruleset name="Iemoji.com">
	<target host="iemoji.com" />
	<target host="www.iemoji.com" />

	<rule from="^http:" to="https:" />
</ruleset>
