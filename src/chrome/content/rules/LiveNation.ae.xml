<ruleset name="LiveNation.ae (partial)">
	<target host="livenation.ae" />
	<target host="www.livenation.ae" />

	<rule from="^http://livenation\.ae/"
			to="https://www.livenation.ae/" />

	<rule from="^http:"
			to="https:" />
</ruleset>
