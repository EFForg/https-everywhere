<!--
	Invalid certificate:
		www.blueturn.earth

-->
<ruleset name="BlueTurn.Earth">

	<target host="blueturn.earth" />
	<target host="www.blueturn.earth" />
	<target host="app.blueturn.earth" />

	<rule from="^http://www\.blueturn\.earth/"
		to="https://blueturn.earth/" />

	<rule from="^http:"
		to="https:" />

</ruleset>
