<ruleset name="Nimg.jp">
	<target host="appicon.nimg.jp" />
	<test url="http://appicon.nimg.jp/42/icon48.png?1330318224" />

	<target host="secure-dcdn.cdn.nimg.jp" />
	<test url="http://secure-dcdn.cdn.nimg.jp/nicochannel/channelapp/app.html?ch=mssp" />

	<target host="icon.nimg.jp" />
	<target host="icons.nimg.jp" />
	<test url="http://icons.nimg.jp/usericon/s/256/2566558.jpg?1352715615" />

	<target host="res.nimg.jp" />
	<test url="http://res.nimg.jp/img/base/head/icon/nico/000.gif" />

	<target host="ch.res.nimg.jp" />
	<target host="com.res.nimg.jp" />
	<target host="uni.res.nimg.jp" />
	<test url="http://uni.res.nimg.jp/img/zero_index/logo.png" />

	<target host="usericon.nimg.jp" />

	<rule from="^http:" 
			to="https:" />
</ruleset>
