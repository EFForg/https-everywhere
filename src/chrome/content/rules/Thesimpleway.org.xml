<!--
	Nonfunctional hosts in *.thesimpleway.org:

	h: http redirect
	m: certificate mismatch
	r: connection refused
	s: self-signed certificate
	t: timeout on https
-->
<ruleset name="Thesimpleway.org">
	<target host="thesimpleway.org" />
	<target host="www.thesimpleway.org" />

	<rule from="^http:" to="https:" />
</ruleset>
