<!--
	STS header includes includeSubdomains

-->
<ruleset name="Cryptomonkeys.com">

	<target host="cryptomonkeys.com" />
	<target host="*.cryptomonkeys.com" />

		<test url="http://www.cryptomonkeys.com/" />


	<securecookie host=".+" name=".+" />


	<rule from="^http:"
		to="https:" />

</ruleset>
