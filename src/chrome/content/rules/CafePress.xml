<!--
	!functional:
		- blog.cafepress.com		(timeout)
		- images7?.cafepress.com	(reset; redirects to i2.cpcache.com)
		- i[12].cpcache.com		(503, akamai; !at i[12].cafepress.com, content.cafepress.com, www.cafepress.com)
-->
<ruleset name="CafePress (partial)">
	<target host="cafepress.com" />
	<target host="content.cafepress.com" />
	<target host="members.cafepress.com" />
	<target host="shop.cafepress.com" />
	
	<target host="cafepress.co.uk" />
	<target host="members.cafepress.co.uk" />
	
	<target host="content.cpcache.com" />
	<target host="content0.cpcache.com" />
	<target host="content1.cpcache.com" />
	<target host="content2.cpcache.com" />
	<target host="content3.cpcache.com" />
	<target host="content4.cpcache.com" />
	<target host="content5.cpcache.com" />
	<target host="content6.cpcache.com" />
	<target host="content7.cpcache.com" />
	<target host="content8.cpcache.com" />
	<target host="content9.cpcache.com" />

	<!--	Akamai bucket.  Finding it would ideal.	-->
	<rule from="^http://content\d?\.cpcache\.com/"
		to="https://content.cafepress.com/"/>
	
	<rule from="^http:"	to="https:"/>
</ruleset>
