<!--

	Nonfunctional subdomains:

		- img	(times out)

	Problematic subdomains:

		- static-images	(invalid cert)

-->
<ruleset name="app-liv.jp">

	<target host="ad.app-liv.jp" />
	<target host="android.app-liv.jp" />
	<target host="app-liv.jp" />
	<target host="comm.app-liv.jp" />
	<target host="mag.app-liv.jp" />
	<target host="user.app-liv.jp" />

	<exclusion pattern="http://android\.app-liv\.jp/$" />
	<exclusion pattern="http://app-liv\.jp/$" />
	<exclusion pattern="http://user\.app-liv\.jp/$" />

	<rule from="^http:"
		to="https:" />

</ruleset>
