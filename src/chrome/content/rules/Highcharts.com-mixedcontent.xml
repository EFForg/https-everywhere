<!--
	For rules not causing valid MCB, see Highcharts.com.xml.

-->
<ruleset name="Highcharts.com (MCB)" platform="mixedcontent">

	<!--	Direct rewrites:
				-->
	<target host="code.highcharts.com" />


	<securecookie host="^\.highcharts\.com$" name="^(?:__cfduid|cf_clearance)$" />


	<rule from="^http:"
		to="https:" />

</ruleset>
