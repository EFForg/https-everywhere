<!--
	Fully covered hosts:

		- cdn
		- note-to-self
		- policies
		- www


	^baker.org doesn't exist

-->
<ruleset name="Baker.com">

	<target host="cdn.baker.org" />
	<target host="note-to-self.baker.com" />
	<target host="policies.baker.org" />
	<target host="www.baker.org" />


	<rule from="^http:"
		to="https:" />

</ruleset>
