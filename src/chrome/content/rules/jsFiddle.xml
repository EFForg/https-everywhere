<!--
	For jsfiddle.net

		Works:

			- $

		Problematic:
			- www (cert mismatch)
-->
<ruleset name="jsFiddle">
  <target host="jsfiddle.net" />
  <target host="www.jsfiddle.net" />

  <rule from="^http://(?:www\.)?jsfiddle\.net/"
          to="https://jsfiddle.net/" />

  <test url="http://jsfiddle.net/" />
  <test url="http://www.jsfiddle.net/" />
</ruleset>