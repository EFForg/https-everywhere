<!--
	blog.opendz.org is handle in WordPress-clients.xml.

-->
<ruleset name="OpenDZ" default_off="self-signed">

	<target host="opendz.org" />
	<target host="www.opendz.org" />


	<securecookie host="^opendz\.org$" name=".*" />


	<!--	Cert only matches !www.	-->
	<rule from="^http://(?:www\.)?opendz\.org/"
		to="https://opendz.org/" />

</ruleset>
