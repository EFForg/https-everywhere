<ruleset name="Greenhouse.io">

	<target host="greenhouse.io" />
	<target host="*.greenhouse.io" />


	<rule from="^http://(?:www\.)?greenhouse\.io/"
		to="https://www.greenhouse.io/" />

	<rule from="^http://(blog|boards|info)\.greenhouse\.io/"
		to="https://$1.greenhouse.io/" />

</ruleset>
