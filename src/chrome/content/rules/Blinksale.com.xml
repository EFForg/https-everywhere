<!--
	Fully covered hosts in *blinksale.com:

		- (www.)?
		- application

-->
<ruleset name="Blinksale.com">

	<!--	Direct rewrites:
				-->
	<target host="blinksale.com" />
	<target host="application.blinksale.com" />
	<target host="www.blinksale.com" />


	<rule from="^http:"
		to="https:" />

</ruleset>
