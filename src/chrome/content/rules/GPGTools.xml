<!--
	Problematic subdomains:

		- support	(mismatched, CN: *.tenderapp.com)

-->
<ruleset name="GPGTools.org (partial)">

	<target host="gpgtools.org" />
	<target host="*.gpgtools.org" />

		<exclusion pattern="http://support\.gpgtools\.org/" />

			<test url="http://support.gpgtools.org/" />

		<test url="http://www.gpgtools.org/" />


	<rule from="^http:"
		to="https:" />

</ruleset>
