<!--
	^: 404


	Mixed content:

		- iframe from maps.google.com *

	* Secured by us

-->
<ruleset name="aidsmap.com">

	<target host="aidsmap.com" />
	<target host="*.aidsmap.com" />


	<rule from="^http://(?:www\.)?aidsmap\.com/"
		to="https://www.aidsmap.com/" />

	<rule from="^http://images\.aidsmap\.com/"
		to="https://images.aidsmap.com/" />

</ruleset>
