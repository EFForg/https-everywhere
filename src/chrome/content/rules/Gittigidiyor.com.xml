<!--
	For other eBay coverage, see EBay.xml.

	Nonfunctional hosts in *.gittigidiyor.com:
		- blog.gittigidiyor.com (mixedcontnet)
		- kurumsal.gittigidiyor.com (connection reset)

	h: http redirect
	m: certificate mismatch
	r: connection refused
	s: self-signed certificate
	t: timeout on https
-->
<ruleset name="Gittigidiyor.com">
	<target host="gittigidiyor.com" />
	<target host="www.gittigidiyor.com" />
	<target host="aff.gittigidiyor.com" />
	<target host="cm.gittigidiyor.com" />
	<target host="dev.gittigidiyor.com" />
	<target host="dukkanlar.gittigidiyor.com" />
	<target host="m.gittigidiyor.com" />
	<target host="profil.gittigidiyor.com" />
	<target host="urun.gittigidiyor.com" />
	<target host="yardim.gittigidiyor.com" />

	<target host="mcdn01.gittigidiyor.net" />
	<target host="mcdnaff03.gittigidiyor.net" />
	<target host="st2.gittigidiyor.net" />

	<!-- Connection reset on $ -->
	<exclusion pattern="^http://st2.gittigidiyor.net/$" />
	<test url="http://st2.gittigidiyor.net/fred/framework/assets/img/core/main/simple-modal-close.png" />

	<rule from="^http:" to="https:" />
</ruleset>
