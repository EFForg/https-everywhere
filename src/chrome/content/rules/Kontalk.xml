<ruleset name="Kontalk">
    <target host="kontalk.org" />
    <target host="www.kontalk.org" />
    <target host="translate.kontalk.org" />

    <target host="kontalk.net" />
    <target host="www.kontalk.net" />
    <target host="beta.kontalk.net" />

    <rule from="^http:"
            to="https:" />
</ruleset>
