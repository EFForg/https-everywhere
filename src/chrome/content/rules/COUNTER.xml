<ruleset name="COUNTER" default_off="mismatched">

	<!--	Cert: *.iop.org		-->
	<target host="projectcounter.org" />
	<target host="www.projectcounter.org" />

	<!--	!www times out over https.	-->
	<rule from="^http://(?:www\.)?projectcounter\.org/"
		to="https://www.projectcounter.org/" />

</ruleset>
