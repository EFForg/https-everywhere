		www2.ae911truth.org
		writingteamdev.ae911truth.org
	Redirects to HTTP:
		shop.ae911truth.org
-->
<ruleset name="AE911Truth.org" platform="mixedcontent">
	<target host="ae911truth.org" />
	<target host="www.ae911truth.org" />
	<target host="www1.ae911truth.org" />
	<target host="action.ae911truth.org" />
	<target host="beta.ae911truth.org" />

	<securecookie host=".+" name=".+" />

	<rule from="^http:" to="https:" />
</ruleset>
