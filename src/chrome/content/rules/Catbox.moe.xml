<!--
	Invalid Certificate:
		www.catbox.moe
		blog.catbox.moe
		nofun.catbox.moe
-->
<ruleset name="Catbox.moe">
	<target host="catbox.moe" />
	<target host="www.catbox.moe" />
	<target host="files.catbox.moe" />
	<target host="qt.catbox.moe" />

	<rule from="^http://www\.catbox\.moe/" to="https://catbox.moe/" />

	<rule from="^http:" to="https:" />
</ruleset>
