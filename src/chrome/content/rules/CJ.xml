<ruleset name="CJ">
  <target host="cj.com" />
  <target host="www.cj.com" />
  <target host="signup.cj.com" />
  <target host="members.cj.com" />

  <securecookie host="^(.+\.)?cj\.com$" name=".*"/>

  <rule from="^http://(?:www\.)?cj\.com/" to="https://www.cj.com/"/>
  <rule from="^http://(members|signup)\.cj\.com/" to="https://$1.cj.com/"/>
</ruleset>
