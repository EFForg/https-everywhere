<!--
	(www.)?: Refused


	Insecure cookies are set for these domains:

		- .bundle.media

-->
<ruleset name="bundle.media (partial)">

	<!--	Direct rewrites:
				-->
	<target host="production-assets.bundle.media" />


	<!--	CloudFlare cookies:
					-->
	<!--securecookie host="^\.bundle\.media$" name="^(__cfduid|cf_clearance)$" /-->

	<securecookie host="^\.bundle\.media$" name="^(?:__cfduid|cf_clearance)$" />


	<rule from="^http:"
		to="https:" />

</ruleset>
