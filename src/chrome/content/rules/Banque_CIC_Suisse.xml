<ruleset name="Banque CIC (Suisse)">
    <target host="cic.ch" />
    <target host="*.cic.ch" />

    <securecookie host="^(?:.*\.)?cic\.ch$" name=".+" />

    <rule from="^http://cic\.ch/"
        to="https://www.cic.ch/"/>
    <rule from="^http://([^/:@]+)?\.cic\.ch/"
        to="https://$1.cic.ch/" />
</ruleset>
