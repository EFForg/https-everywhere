<!--


	^trafficsafetymarketing.gov: Handshake fails
	www.trafficsafetymarketing.gov: Akamai

-->
<ruleset name="Traffic Safety Marketing.gov" default_off="mismatched">

	<!--	Direct rewrites:
				-->
	<target host="www.trafficsafetymarketing.gov" />

	<!--	Complications:
				-->
	<target host="trafficsafetymarketing.gov" />


	<securecookie host="^\w" name=".+" />


	<rule from="^http://trafficsafetymarketing\.gov/"
		to="https://www.trafficsafetymarketing.gov/" />

	<rule from="^http:"
		to="https:" />

</ruleset>
