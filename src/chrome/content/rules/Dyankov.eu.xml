<!--
	Problematic subdomains:

		- www	(cert only matches ^dyankov.eu)

-->
<ruleset name="Dyankov.eu">

	<target host="dyankov.eu" />
	<target host="www.dyankov.eu" />


	<securecookie host="^dyankov\.eu$" name=".+" />


	<rule from="^http://(?:www\.)?dyankov\.eu/"
		to="https://dyankov.eu/" />

</ruleset>