	<!--	Not secured by server:
					-->
	<!--securecookie host="^app\.shareconnect\.com$" name="^ercVisitor$" /-->

	<securecookie host="^\w" name=".+" />


	<rule from="^http:"
		to="https:" />

</ruleset>
