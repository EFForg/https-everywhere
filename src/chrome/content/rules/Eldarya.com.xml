<!--
	Nonfunctional hosts in *.eldarya.com:

	h: http redirect
	m: certificate mismatch
	r: connection refused
	s: self-signed certificate
	t: timeout on https
-->
<ruleset name="Eldarya.com">
	<target host="eldarya.com" />
	<target host="www.eldarya.com" />

	<rule from="^http:" to="https:" />
</ruleset>
