<ruleset name="SecureList">
  <target host="securelist.com" />
  <target host="www.securelist.com" />

  <securecookie host="^(.+\.)?securelist\.com$" name=".*"/>

	<!--	Cert only matches www.
					-->
  <rule from="^https?://(?:www\.)?securelist\.com/" to="https://www.securelist.com/"/>
</ruleset>
