<ruleset name="SecureList.com">

	<!--	Direct rewrites:
				-->
	<target host="securelist.com" />
	<target host="apt.securelist.com" />
	<target host="cdn.securelist.com" />
	<target host="www.securelist.com" />


	<securecookie host="^(?:.+\.)?securelist\.com$" name=".+" />


	<rule from="^http:"
