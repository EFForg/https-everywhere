<ruleset name="MEBank">
	<target host="mebank.com.au" />
	<target host="www.mebank.com.au" />
	<target host="apply.mebank.com.au" />
	<target host="ib.mebank.com.au" />
	<target host="verify.mebank.com.au" />

	<!-- !www has a mismatch -->
	<rule from="^http://mebank\.com\.au/"
			to="https://www.mebank.com.au/" />

	<rule from="^http:"
			to="https:" />
</ruleset>
