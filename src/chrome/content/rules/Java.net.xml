	<securecookie host="^(?:home|jax-rs-spec|jsr311|wiki\.openjdk)?\.java\.net$" name=".+" />


	<rule from="^http://((?:asset-\d+|glassfish|home|jax-rs-spec|jersey|jsr311|wiki\.openjdk|tyrus|www)\.)?java\.net/"
