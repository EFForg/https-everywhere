<!--
	Different http/https content:
		app.wiz.cn

	Drop:
		^
		bbs.wiz.cn
		blog.wiz.cn

	Redirect to http:
		www.wiz.cn

	Mismatch:
		cdn.wiz.cn
		get.wiz.cn
		release.wiz.cn
		upgrade.wiz.cn
-->

<ruleset name="Wiz.cn">
	<target host="api.wiz.cn" />
	<target host="note.wiz.cn" />
	<target host="url.wiz.cn" />

	<rule from="^http:" to="https:" />
</ruleset>
