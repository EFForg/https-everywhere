<ruleset name="Swissabroad.ch">
    <target host="swissabroad.ch" />
    <target host="*.swissabroad.ch" />

    <rule from="^http://swissabroad\.ch/"
        to="https://www.swissabroad.ch/" />
    <rule from="^http://([^/:@]+)?\.swissabroad\.ch/"
        to="https://$1.swissabroad.ch/" />
</ruleset>
