<!--
	Non-functional subdomains:
		- dns[1-2]	(t)
		- ext-api (t)
		- go  (m)
		- nl[48-49] (SSL connection error)
		- proxy-it	(t)

	e: expired certificate
	h: http redirect
	i: invalid certificate chain
	m: certificate mismatch
	r: connection refused
	s: self-signed certificate
	t: timeout on https
-->
<ruleset name="NordVPN.com">

	<target host="nordvpn.com" />
	<target host="www.nordvpn.com" />
	<target host="affiliates.nordvpn.com" />
	<target host="downloads.nordvpn.com" />
	<target host="join.nordvpn.com" />
	<target host="nanorep.nordvpn.com" />
	<target host="ref.nordvpn.com" />
	<target host="support.nordvpn.com" />
	<target host="ucp.nordvpn.com" />
	<target host="visit.nordvpn.com" />
	<target host="zendesk.nordvpn.com" />

	<securecookie host=".+" name=".+" />

	<rule from="^http:" to="https:" />
</ruleset>
