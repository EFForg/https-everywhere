<!--
	Fully covered subdomains:

		- (www.)?
		- blog


	These altnames don't exist:

		- www.blog.wearewizards.io

-->
<ruleset name="We Are Wizards.io">

	<target host="wearewizards.io" />
	<target host="*.wearewizards.io" />


	<rule from="^http://(blog\.|www\.)?wearewizards\.io/"
		to="https://$1wearewizards.io/" />

</ruleset>
