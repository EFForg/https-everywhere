<!--
	Fully covered subdomains:

		- (www.)
		- help
		- secure
		- webmail

-->
<ruleset name="Brinkster.com">

	<target host="brinkster.com" />
	<target host="*.brinkster.com" />


	<securecookie host="^(?:webmail)?\.brinkster\.com$" name=".+" />


	<rule from="^http://((?:help|secure|webmail|www)\.)?brinkster\.com/"
		to="https://$1brinkster.com/" />

</ruleset>