<!--
	Delivery service Ulozenka.cz
-->
<ruleset name="Ulozenka.cz">
	<target host="ulozenka.cz" />
	<target host="*.ulozenka.cz" />

	<test url="http://www.ulozenka.cz/" />
	<test url="http://partner.ulozenka.cz/" />
	<test url="http://tracking.ulozenka.cz/" />

	<rule from="^http:" to="https:" />
</ruleset>
