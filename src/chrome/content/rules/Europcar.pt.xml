<!--
	For other Europcar coverage, see Europcar.com.xml


	Non-functional subdomains:
		- blog	(m)
		- greenpolicy	(i)
		- movingsamsway	(m)
		- roadtrip	(m)
		- tomydoor	(i)

	e: expired certificate
	h: http redirect
	i: invalid certificate chain
	m: certificate mismatch
	r: connection refused
	s: self-signed certificate
	t: timeout on https
-->
<ruleset name="Europcar.pt">

	<target host="europcar.pt" />
	<target host="www.europcar.pt" />
	<target host="calculadora.europcar.pt" />
	<target host="carreiras.europcar.pt" />
	<target host="chauffeurservice.europcar.pt" />
	<target host="desafiorent.europcar.pt" />
	<target host="entrega-recolha.europcar.pt" />
	<target host="faq.europcar.pt" />
	<target host="faq-test.europcar.pt" />
	<target host="longa-duracao.europcar.pt" />
	<target host="m.europcar.pt" />
	<target host="mobile.europcar.pt" />
	<target host="oficinarent.europcar.pt" />
	<target host="press-release.europcar.pt" />
	<target host="scooters-amd.europcar.pt" />
	<target host="vouchers.europcar.pt" />

	<securecookie host=".+" name=".+" />

	<rule from="^http:"
		to="https:" />
</ruleset>
