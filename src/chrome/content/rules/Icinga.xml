<!--
	Nonfunctional subdomains:

		- classic.demo
		- web.demo

-->
<ruleset name="Icinga">

	<target host="icinga.org" />
	<target host="*.icinga.org" />


	<securecookie host="^(?:.*\.)?icinga\.org$" name=".*" />


	<rule from="^http://((?:dev|translate|wiki|www)\.)?icinga\.org/"
		to="https://$1icinga.org/" />

</ruleset>
