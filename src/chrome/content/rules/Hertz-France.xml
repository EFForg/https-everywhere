-->
<ruleset name="Hertz France">
	<target host="hertz247.fr" />
	<target host="www.hertz247.fr" />
	<target host="www.hertz.fr" />
	<target host="www.hertzrent2buy.fr" />

	<rule from="^http:"
		to="https:" />
</ruleset>
