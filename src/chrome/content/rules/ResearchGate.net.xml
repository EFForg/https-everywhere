<!--
	Other ResearchGate rulesets:

		- RG_static.net.xml


	Some pages redirect to http.

-->
<ruleset name="ResearchGate.net (partial)">

	<target host="researchgate.net" />
	<target host="www.researchgate.net" />
		<!--exclusion pattern="^http://www\.researchgate\.net/+(?!c/|favicon\.ico|i/|images/|signup($|[?.]))" /-->


	<rule from="^http://(www\.)?researchgate\.net/(?=c/|favicon\.ico|i/|images/|signup(?:$|[?.]))"
		to="https://$1researchgate.net/" />

</ruleset>
