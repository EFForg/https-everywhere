<!--
	Nonfunctional subdomains:

		- support *

	* desk.com

-->
<ruleset name="WhisperSystems (partial)">

	<target host="whispersystems.org" />
	<target host="*.whispersystems.org" />


	<rule from="^http://(www\.)?whispersystems\.org/"
		to="https://$1whispersystems.org/" />

	<rule from="^http://support\.whispersystems\.org/(?=favicon\.ico|images/)"
		to="https://openwhispersystems.desk.com/" />

</ruleset>
