<!--
	^tuc.org.uk: refused

-->
<ruleset name="TUC.org.uk">

	<!--	Direct rewrites:
				-->
	<target host="www.tuc.org.uk" />

	<!--	Complications:
				-->
	<target host="tuc.org.uk" />


	<securecookie host="^\." name="^(?:__cfduid$|_ga|cf_clearance)$" />
	<securecookie host="^\w" name="." />


	<rule from="^http://tuc\.org\.uk/"
		to="https://www.tuc.org.uk/" />

	<rule from="^http:"
		to="https:" />

</ruleset>
