<ruleset name="Nike.com (partial)" default_off='failed ruleset test'>

	<target host="nike.com" />
	<target host="*.nike.com" />
		<!--
			At least one stylesheet links resources relative to /
										-->
		<!--exclusion pattern="^http://nikeplus\.nike\.com/plus/static/css/core/($|\?)" /-->
