<!--
  For other UK Police rules, see PoliceUK.xml

  Works:
    - www

  Doesn't Work:
    - $ (gives a 404)
-->

<ruleset name="Ask The Police">
  <target host="askthe.police.uk" />
  <target host="www.askthe.police.uk" />

  <rule from="^http://(?:www\.)?askthe\.police\.uk/" to="https://www.askthe.police.uk/"/>
</ruleset>
