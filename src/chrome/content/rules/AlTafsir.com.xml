<!--
	Wildcard DNS but without wildcard certificate

	Unable to Connect:
		main.altafsir.com
	Invalid Certificate:
		m.altafsir.com
		mobile.altafsir.com
-->
<ruleset name="AlTafsir.com">
	<target host="altafsir.com" />
	<target host="www.altafsir.com" />

	<securecookie host="^(www)\.altafsir\.com$" name=".+" />

	<rule from="^http:" to="https:" />
</ruleset>
