<!--
	Invalid certificate:
		contactforms.53.com
		reo.53.com
		securitiesadvisors.53.com

	Wildcard DNS and certificate
-->
<ruleset name="Fifth Third Bank">

	<target host="53.com" />
	<target host="*.53.com" />

		<test url="http://www.53.com/" />
		<test url="http://commercialcard.53.com/" />
		<test url="http://direct.53.com/" />
		<test url="http://express.53.com/" />
		<test url="http://life360.53.com/" />
		<test url="http://locations.53.com/" />
		<test url="http://pages.offers.53.com/" />
		<test url="http://rewards.53.com/" />
		<test url="http://sdg2.53.com/" />
		<test url="http://secure.53.com/" />

		<test url="http://testurl1.53.com/" />
		<test url="http://testurl2.53.com/" />
		<test url="http://testurl3.53.com/" />
		<test url="http://testurl4.53.com/" />

		<exclusion pattern="^http://(contactforms|reo|securitiesadvisors)\.53\.com/" />

			<test url="http://contactforms.53.com/" />
			<test url="http://reo.53.com/" />
			<test url="http://securitiesadvisors.53.com/" />

	<rule from="^http:"
		to="https:" />

</ruleset>
