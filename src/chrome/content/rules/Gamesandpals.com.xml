<!--
	Nonfunctional hosts in *.gamesandpals.com:

	h: http redirect
	m: certificate mismatch
	r: connection refused
	s: self-signed certificate
	t: timeout on https
-->
<ruleset name="Gamesandpals">
	<target host="gamesandpals.com" />
	<target host="www.gamesandpals.com" />

	<rule from="^http:" to="https:" />
</ruleset>
