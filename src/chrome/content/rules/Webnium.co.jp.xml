<!--
	Other Webnium rulesets:

		- Flagrate.org.xml
		- Yabumi.cc.xml

-->
<ruleset name="Webnium.co.jp">

	<target host="webnium.co.jp" />
	<target host="*.webnium.co.jp" />


	<securecookie host="^\.webnium\.co\.jp$" name=".+" />


	<rule from="^http://(www\.)?webnium\.co\.jp/"
		to="https://$1webnium.co.jp/" />

</ruleset>
