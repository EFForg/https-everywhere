<ruleset name="O2online.de">
  <target host="o2online.de"/>
  <target host="*.o2online.de"/>

  <securecookie host="^(.*\.)?o2online\.de$" name=".*" />

  <rule from="^http://(?:www\.)?o2online\.de/" to="https://www.o2online.de/"/>
  <rule from="^http://o2online\.de/" to="https://www.o2online.de/"/>
</ruleset>
