<ruleset name="Bundesministerium des Innern">
    <!-- Federal Ministry of the Interior. -->

    <target host="bmi.bund.de" />
    <target host="www.bmi.bund.de" />

    <rule from="^http://bmi\.bund\.de/"
        to="https://www.bmi.bund.de/" />
    <rule from="^http://([^/:@]+)?\.bmi\.bund\.de/"
        to="https://$1.bmi.bund.de/" />
</ruleset>
