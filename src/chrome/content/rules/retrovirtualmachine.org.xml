<!--
	Invalid certificate:
		static1.retrovirtualmachine.org
		static2.retrovirtualmachine.org
-->
<ruleset name="retrovirtualmachine.org">
	<target host="retrovirtualmachine.org" />
	<target host="www.retrovirtualmachine.org" />

	<rule from="^http://retrovirtualmachine\.org/" to="https://www.retrovirtualmachine.org/" />
	<rule from="^http:" to="https:" />
</ruleset>
