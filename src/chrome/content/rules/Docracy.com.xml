<!--
    Nonfunctional subdomain:
        - $
        - blog
-->
<ruleset name="Docracy.com">
	<target host="docracy.com" />
	<target host="www.docracy.com" />

	<securecookie host="^www\.docracy\.com$" name=".+" />

	<rule from="^http://docracy\.com/"
		to="https://www.docracy.com/" />

	<rule from="^http:"
		to="https:" />
</ruleset>
