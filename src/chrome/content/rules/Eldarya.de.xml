<!--
	Nonfunctional hosts in *.eldarya.de:

	h: http redirect
	m: certificate mismatch
	r: connection refused
	s: self-signed certificate
	t: timeout on https
-->
<ruleset name="Eldarya.de">
	<target host="eldarya.de" />
	<target host="www.eldarya.de" />

	<rule from="^http:" to="https:" />
</ruleset>
