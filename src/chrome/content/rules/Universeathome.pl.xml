<!--
	Nonfunctional hosts in *.universeathome.pl:

	h: http redirect
	m: certificate mismatch
	r: connection refused
	s: self-signed certificate
	t: timeout on https
-->
<ruleset name="Universeathome.pl">
	<target host="universeathome.pl" />
	<target host="www.universeathome.pl" />

	<rule from="^http:" to="https:" />
</ruleset>
