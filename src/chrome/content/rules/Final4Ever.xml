<!--
	Cert only matches ^final4ever.com

-->
<ruleset name="Final4Ever" default_off="mismatched, self-signed">

	<target host="final4ever.com" />
	<target host="*.final4ever.com" />


	<rule from="^http://(?:cdn\.|(js\.|rss\.)|www\.)?final4ever\.com/"
		to="https://$1final4ever.com/" />

</ruleset>
