<!--
	Other Cloudup rulesets:

		- Cldup.com.xml


	CDN buckets:

		- s3-us-west-1.amazonaws.com/cloudup-assets/


	Fully covered hosts:

		- (www.)?
		- i
		- io6


	Insecure cookies are set for these domains:

		- .cloudup.com

-->
<ruleset name="Cloudup.com">

	<target host="cloudup.com" />
	<target host="i.cloudup.com" />
	<target host="io6.cloudup.com" />
	<target host="www.cloudup.com" />


	<!--	Not secured by server:
					-->
	<!--securecookie host="^\.cloudup\.com$" name="^connect\.sid$" /-->

	<securecookie host="^\.cloudup\.com$" name=".+" />


	<rule from="^http:"
		to="https:" />

</ruleset>
