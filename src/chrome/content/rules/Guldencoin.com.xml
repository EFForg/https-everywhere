<!--
	CDN buckets:

		- s3-eu-west-1.amazonaws.com/guldencoin.com/

-->
<ruleset name="Guldencoin.com">

	<target host="guldencoin.com" />
	<target host="*.guldencoin.com" />


	<securecookie host="^\.guldencoin\.com$" name=".+" />


	<rule from="^http://(www\.)?guldencoin\.com/"
		to="https://$1guldencoin.com/" />

</ruleset>
