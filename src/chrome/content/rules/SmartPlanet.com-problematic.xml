<ruleset name="SmartPlanet.com (mismatched)" default_off="mismatched">

	<target host="*.smartplanet.com" />


	<securecookie host="^(?:www)?\.smartplanet\.com$" name=".+" />


	<rule from="^http://www\.smartplanet\.com/"
		to="https://www.smartplanet.com/" />

</ruleset>
