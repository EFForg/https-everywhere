<!--
	cs.is is handled in Bit.ly_vanity_domains.xml.


	www: cert only matches ^csis.org

-->
<ruleset name="CSIS.org">

	<target host="csis.org" />
	<target host="my.csis.org" />
	<target host="www.csis.org" />


	<rule from="^http://(?:(my\.)|www\.)?csis\.org/"
		to="https://$1csis.org/" />

</ruleset>
