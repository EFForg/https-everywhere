<!--
	^cryptopro.ru: Cert only matches www

-->
<ruleset name="CryptoPro.ru">

	<!--	Direct rewrites:
				-->
	<target host="www.cryptopro.ru" />

	<!--	Complications:
				-->
	<target host="cryptopro.ru" />


	<rule from="^http://cryptopro\.ru/"
		to="https://www.cryptopro.ru/" />

	<rule from="^http:"
		to="https:" />

</ruleset>
