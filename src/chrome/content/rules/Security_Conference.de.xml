<ruleset name="Munich Security Conference">
    <target host="securityconference.de" />
    <target host="www.securityconference.de" />

    <securecookie host="^(?:.*\.)?securityconference\.de$" name=".+" />

    <rule from="^http://securityconference\.de/"
        to="https://www.securityconference.de/" />
    <rule from="^http://([^/:@]+)?\.securityconference\.de/"
        to="https://$1.securityconference.de/" />
</ruleset>
