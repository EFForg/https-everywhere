<!--
	Ruleset for nebrija.com.

	Not supporting HTTPS or using an invalid certificate:
		* list.nebrija.com
		* mail.nebrija.com
		* mobile.nebrija.com
		* mysql.nebrija.com
		* webmail.nebrija.com
-->
<ruleset name="nebrija.com">

	<target host="nebrija.com" />
	<target host="www.nebrija.com" />

	<rule from="^http:"
		to="https:" />

</ruleset>
