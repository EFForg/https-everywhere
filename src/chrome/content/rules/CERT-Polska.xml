<ruleset name="CERT Polska">

	<target host="cert.pl" />
	<!--	*s for cross-domain cookies.	-->
	<target host="*.cert.pl" />
	<target host="*.www.cert.pl" />


	<securecookie host="^(?:.*\.)?cert\.pl$" name=".*" />


	<rule from="^http://(www\.)?cert\.pl/"
		to="https://$1cert.pl/" />

</ruleset>
