<!--
	Nonfunctional subdomains:

		- ^ *
		- forum *
		- review	(refused)
		- stats		(dropped)

	* 522


	Fully covered subdomains:

		- account
		- download
		- jira
		- wiki
		- www

-->
<ruleset name="CyanogenMod.org (partial)">

	<target host="*.cyanogenmod.org" />
	<target host="cyngn.com" />

	<securecookie host="^jira\.cyanogenmod\.org$" name=".+" />


	<rule from="^http://(account|download|jira|wiki|www)\.cyanogenmod\.org/"
		to="https://$1.cyanogenmod.org/" />

	<rule from="^http://cyngn\.com"
		to="https://cyngn.com/" />
</ruleset>
