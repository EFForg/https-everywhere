<!--

	Mismatched hosts in *studivz.net:

		- blog
		- developer
		- mobil
		- mobile
		- play

-->
<ruleset name="StudiVZ" platform="mixedcontent">
	<target host="studivz.net" />
	<target host="www.studivz.net" />
	<target host="ims.studivz.net" />
	<target host="m.studivz.net" />
	<target host="o2.studivz.net" />
	<target host="secure.studivz.net" />

	<rule from="^http:" to="https:" />
</ruleset>
