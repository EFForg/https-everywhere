<ruleset name="VTEC Direct">

	<target host="vtecdirect.com" />
	<!--	* for cross-domain cookie.	-->
	<target host="*.vtecdirect.com" />


	<securecookie host="^\.vtecdirect\.com$" name=".*" />


	<!--	Cert only matches //vtecdirect.com	-->
	<rule from="^http://(?:www\.)?vtecdirect\.com/"
		to="https://vtecdirect.com/" />

</ruleset>
