<!--
	www.screencraft.org: WP Engine

-->
<ruleset name="ScreenCraft.org">

	<!--	Direct rewrites:
				-->
	<target host="screencraft.org" />

	<!--	Complications:
				-->
	<target host="www.screencraft.org" />


	<securecookie host="^\w" name=".+" />


	<rule from="^http://www\.screencraft\.org/"
		to="https://screencraft.org/" />

	<rule from="^http:"
		to="https:" />

</ruleset>
