<ruleset name="The Hacker's Choice (partial)" default_off="mismatch, self-signed">

	<target host="thc.org"/>
	<target host="freeworld.thc.org"/>
	<target host="www.thc.org"/>

	<rule from="^http://(www\.)?thc\.org/"
		to="https://thc.org/"/>

	<rule from="^http://freeworld\.thc\.org/"
		to="https://freeworld.thc.org/"/>

</ruleset>
