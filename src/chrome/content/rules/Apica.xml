<ruleset name="Apica">

	<target host="apicasystem.com" />
	<target host="www.apicasystem.com" />
	<target host="webtracker.apicasystem.com" />


	<securecookie host="^.+\.apicasystem\.com$" name=".+" />


	<rule from="^http://(?:www\.)?apicasystem\.com/"
