<!--
	Problematic subdomains:

		- wwwcache	(akamai)

-->
<ruleset name="WRAL.com">

	<target host="wral.com" />
	<target host="*.wral.com" />


	<securecookie host="^\.wral\.com$" name=".+" />


	<rule from="^http://(www\.)?wral\.com/"
		to="https://$1wral.com/" />

	<rule from="^http://wwwcache\.wral\.com/"
		to="https://www.wral.com/" />

</ruleset>
