<ruleset name="Example">
	<!-- example.com -->
	<target host="example.com" />
	<target host="www.example.com" />

	<!-- example.edu -->
	<target host="example.edu" />
	<target host="www.example.edu" />

	<!-- example.net -->
	<target host="example.net" />
