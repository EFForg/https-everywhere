<ruleset name="Koding">

	<target host="koding.com" />
	<target host="*.koding.com" />


	<securecookie host="^(?:.+\.)?koding\.com$" name=".+" />


	<rule from="^http://(\w+\.)?koding\.com/"
		to="https://$1koding.com/" />


  <exclusion pattern="^http://(?:www\.)?links\.koding\.com/" />

</ruleset>
