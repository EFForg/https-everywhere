<ruleset name="HBoeck.de">

	<target host="hboeck.de" />
	<target host="*.hboeck.de" />


	<!--	Not secured by srver:
					-->
	<!--securecookie host="^blog\.hboeck\.de$" name="^\w+_[0-9a-f]{32}$" /-->


	<securecookie host="^(?:blog\.)?hboeck\.de$" name=".+" />


	<rule from="^http://((?:blog|ssl|[\w-]+\.ssl|svn|www)\.)?hboeck\.de/"
		to="https://$1hboeck.de/" />

</ruleset>
