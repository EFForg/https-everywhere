<ruleset name="Tony Zhaocheng Tan">

	<target host="tonytan.io" />
	<target host="*.tonytan.io" />
	<target host="tonytan.cn" />
	<target host="*.tonytan.cn" />
	<target host="ztan.tk" />
	<target host="*.ztan.tk" />
	
	<!-- ztan.tk redirects to tonytan.io, but it is also used for URL shortening -->

	<rule from="^http:"
		to="https:" />

</ruleset>
