<ruleset name="useso.com">
	<target host="www.useso.com" />
		<exclusion pattern="^http://www\.useso\.com/$" />
		<test url="http://www.useso.com/style.css" />
	<target host="ajax.useso.com" />
	<target host="fonts.useso.com" />
	<target host="libs.useso.com" />
		<exclusion pattern="^http://libs\.useso\.com/$" />
	
	<rule from="^http:" to="https:" />
</ruleset>
