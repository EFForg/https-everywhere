<ruleset name="MP3Ninja">

    <target host="mp3skull.is" />
    <target host="www.mp3skull.is" />

    <target host="mp3skull.ninja" />
    <target host="www.mp3skull.ninja" />

    <securecookie host=".+" name=".+" />

    <rule from="^http://(?:www\.)?mp3skull\.is/"
            to="https://mp3skull.ninja/" />

    <rule from="^http://(?:www\.)?mp3skull\.ninja/"
            to="https://mp3skull.ninja/" />

</ruleset>
