<!--
	For other Stubhub coverage, see Stubhub.com.xml


	Non-functional subdomains:

		- qasecure	(m)
		- qawww	(m)

	e: expired certificate
	h: http redirect
	i: invalid certificate chain
	m: certificate mismatch
	r: connection refused
	s: self-signed certificate
	t: timeout on https
-->
<ruleset name="Stubhub.es">

	<target host="stubhub.es" />
	<target host="www.stubhub.es" />
	<target host="secure.stubhub.es" />
		<test url="http://secure.stubhub.es/robots.txt" />

	<rule from="^http:"
		to="https:" />
</ruleset>
