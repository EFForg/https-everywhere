<!--
	Insecure cookies are set for these domains:

		- .ycombinator.com
		- news.ycombinator.com

-->
<ruleset name="HackerNews">

	<target host="*.ycombinator.com" />


	<!--securecookie host="^\.ycombinator\.com$" name="^__cfduid$" /-->

	<securecookie host="^(?:.*\.)?ycombinator\.com$" name=".+" />


	<rule from="^http://(?:www\.)?news\.ycombinator\.com/"
		to="https://news.ycombinator.com/" />

</ruleset>
