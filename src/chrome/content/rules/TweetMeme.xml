<!--
	tweetmeme.s3.amazonaws.com

-->
<ruleset name="TweetMeme" default_off="expired, mismatch">

	<target host="tweetmeme.com" />
	<target host="*.tweetmeme.com" />


	<!--	Cert: admin.tweetmeme.com	-->
	<rule from="^http://(?:(admin\.)|www\.)?tweetmeme\.com/"
		to="https://$1tweetmeme.com/" />

</ruleset>
