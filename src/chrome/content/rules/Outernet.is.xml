<!--
	^: Reset

-->
<ruleset name="Outernet">
    <target host="outernet.is" />
    <target host="*.outernet.is" />

    <securecookie host="^(?:.*\.)?outernet\.is$" name=".+" />

    <rule from="^http://outernet\.is/"
        to="https://www.outernet.is/" />
    <rule from="^http://([^/:@]+)?\.outernet\.is/"
        to="https://$1.outernet.is/" />
</ruleset>
