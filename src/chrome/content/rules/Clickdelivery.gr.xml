<ruleset name="Clickdelivery.gr">

	<!--	Direct rewrites:
				-->
	<target host="clickdelivery.gr" />
	<target host="www.clickdelivery.gr" />


	<securecookie host="^(?:www\.)?clickdelivery\.gr$" name=".+" />


	<rule from="^http:"
		to="https:" />

</ruleset>
