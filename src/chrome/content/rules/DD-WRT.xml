<ruleset name="DD-WRT">

	<target host="dd-wrt.com" />
	<target host="*.dd-wrt.com" />
	<!--	* for cross-domain cookie.	-->
	<target host="*.secure.dd-wrt.com" />


	<securecookie host="^.*.dd-wrt\.com$" name=".*" />


	<!--	Cert only matches secure.
						-->
	<rule from="^http://(?:secure\.|www\.)?dd-wrt\.com/"
		to="https://secure.dd-wrt.com/" />

</ruleset>
