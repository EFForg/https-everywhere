<!--
	www.pep.foundation.com: mismatched

-->
<ruleset name="pep.Foundation">

	<!--	Direct rewrites:
				-->
	<target host="pep.foundation"/>

	<!--	Complications:
				-->
	<target host="www.pep.foundation"/>


	<rule from="^http://www\.pep\.foundation/"
		to="https://pep.foundation/" />

	<rule from="^http:"
		to="https:" />

</ruleset>
