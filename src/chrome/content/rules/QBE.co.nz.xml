<!--
	For other QBE coverage, see QBE.com.xml


	Non-functional subdomains:
		- brokersuat	(ssl handshake failure)
		- (www.)intermediary	(m)
		- lyncdiscover (ssl connection error)

	e: expired certificate
	h: http redirect
	i: invalid certificate chain
	m: certificate mismatch
	r: connection refused
	s: self-signed certificate
	t: timeout on https
-->
<ruleset name="QBE.co.nz">

	<target host="qbe.co.nz" />
	<target host="www.qbe.co.nz" />
	<target host="brokers.qbe.co.nz" />
	<target host="meet.qbe.co.nz" />
	<target host="metis.qbe.co.nz" />
	<target host="sip.qbe.co.nz" />

	<securecookie host=".+" name=".+" />

	<rule from="^http:"
		to="https:" />
</ruleset>
