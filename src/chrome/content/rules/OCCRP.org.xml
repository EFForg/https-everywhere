<!--
	For other Organized Crime and Corruption Reporting Project coverage, see Reporting_Project.net.xml.

-->
<ruleset name="OCCRP.org (partial)">

	<target host="occrp.org" />
	<target host="*.occrp.org" />
		<!--
			404:
				-->
		<!--exclusion pattern="^http://(www\.)?occrp\.org/+\w" /-->


	<!--	Redirect drops args:
					-->
	<rule from="^http://(?:www\.)?occrp\.org/+(?:$|\?.*)"
		to="https://reportingproject.net/occrp/" />

	<rule from="^http://(www\.)?vis\.occrp\.org/"
		to="https://$1vis.occrp.org/" />

</ruleset>
