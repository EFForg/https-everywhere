	<!--	CloudFlare cookies:
					-->
	<!--securecookie host="^\.iaea\.org$" name="^(?:__cfduid|cf_clearance)$" /-->

	<securecookie host="^\.iaea\.org$" name="^(?:__cfduid|cf_clearance)$" />


	<rule from="^http:"
		to="https:" />

</ruleset>
