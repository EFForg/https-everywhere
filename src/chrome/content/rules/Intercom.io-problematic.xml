<!--
	For rules that are on by default, see Intercom.io.xml.

-->
<ruleset name="Intercom.io (mismatched)" default_off="mismatched">

	<target host="docs.intercom.io" />
	<target host="webinars.intercom.io" />


	<securecookie host="^webinars\.intercom\.io$" name=".+" />


	<rule from="^http://(doc|webinar)s\.intercom\.io/"
		to="https://$1s.intercom.io/" />

</ruleset>
