<ruleset name="LiveNation.se (partial)">
	<target host="livenation.se" />
	<target host="www.livenation.se" />

	<rule from="^http://livenation\.se/"
			to="https://www.livenation.se/" />

	<rule from="^http:"
			to="https:" />
</ruleset>
