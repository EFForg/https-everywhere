<!--
	Fully covered hosts in *awesome-it.de:

		- (www.)?
		- gitlab
		- wiki

-->
<ruleset name="awesome-it.de">

	<!--	Direct rewrites:
				-->
	<target host="awesome-it.de" />
	<target host="gitlab.awesome-it.de" />
	<target host="wiki.awesome-it.de" />
	<target host="www.awesome-it.de" />


	<rule from="^http:"
		to="https:" />

</ruleset>
