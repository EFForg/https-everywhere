<ruleset name="Adap.TV">

	<target host="adap.tv" />
	<target host="*.adap.tv" />


	<securecookie host="^\.adap\.tv$" name=".*" />


	<rule from="^http://((?:ads|my|segments|www)\.)?adap\.tv/"
		to="https://$1adap.tv/" />

	<!--	Akamai	-->
	<rule from="^http://redir\.adap\.tv/"
		to="https://ads.adap.tv/" />

</ruleset>
