<!--
	520 Unknown Error:
		- mrlocksmith.com, equivalent to www.mrlocksmith.com
-->

<ruleset name="Mrlocksmith.com" platform="mixedcontent">
	<target host="mrlocksmith.com" />
	<target host="www.mrlocksmith.com" />

	<rule from="^http://mrlocksmith\.com/"
		  to="https://www.mrlocksmith.com/" />

	<rule from="^http:" to="https:" />
</ruleset>
