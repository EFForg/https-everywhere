<!--
	Nonfunctional hosts in *.scroll.in:

	h: http redirect
	m: certificate mismatch
	r: connection refused
	s: self-signed certificate
	t: timeout on https
-->
<ruleset name="Scroll.in">
	<target host="scroll.in" />
	<target host="www.scroll.in" />
	<target host="shop.scroll.in" />
	<target host="thefield.scroll.in" />
	<target host="thereel.scroll.in" />
	<target host="video.scroll.in" />

	<rule from="^http:" to="https:" />
</ruleset>
