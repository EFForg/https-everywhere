<ruleset name="divShare" platform="mixedcontent">

	<target host="divshare.com" />
	<!--	* for cross-domain cookies.	-->
	<target host="*.divshare.com" />


	<securecookie host="^(?:.*\.)?divshare\.com$" name=".*" />


	<rule from="^http://(www\.)?divshare\.com/"
		to="https://$1divshare.com/" />

</ruleset>
