<!--
	Other News Corporation rulesets:
		- News-Corporation.xml
		- News-Corporation-mismatches.xml

	Non-functional hosts:
		Certificate mismatch:
			- i.marketwatch.com
			- communitycontent.marketwatch.com
			- static.marketwatch.com

		Certificate expire:
			- store.marketwatch.com

		Refused:
			- sm.marketwatch.com

		Timeout:
			- admin.marketwatch.com

  		Redirect to HTTP:
			- blogs.marketwatch.com

		No working 200/3xx URLs:
			- accounts.marketwatch.com
-->
<ruleset name="Marketwatch.com">
	<!-- <target host="marketwatch.com" /> #8123 -->
	<!-- <target host="www.marketwatch.com" /> #8123 -->
	<target host="id.marketwatch.com" />
	<target host="s.marketwatch.com" />
	<target host="secure.marketwatch.com" />
	
	<securecookie host="secure\.marketwatch\.com$" name=".+" />
	
	<!-- <rule from="^http://(www\.)?marketwatch\.com/" to="https://secure.marketwatch.com/" /> #8123 -->
	<rule from="^http:" to="https:" />
</ruleset>
