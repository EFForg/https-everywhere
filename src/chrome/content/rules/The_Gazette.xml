<ruleset name="The Gazette">
    <target host="thegazette.co.uk" />
    <target host="*.thegazette.co.uk" />

    <rule from="^http://thegazette\.co\.uk/"
        to="https://www.thegazette.co.uk/" />
    <rule from="^http://([^/:@]+)\.thegazette\.co\.uk/"
        to="https://$1.thegazette.co.uk/" />
</ruleset>
