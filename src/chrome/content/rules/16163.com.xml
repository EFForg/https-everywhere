<!--
	For other MediaFire coverage, see MediaFire.xml.

	Mismatch:
		^

	MCB:
		cc.16163.com

	Mixed content:
		(gamename).16163.com	( no function issue )
-->

<ruleset name="16163.com">
	<target host="16163.com" />
	<target host="www.16163.com" />
	<target host="m.16163.com" />

	<target host="*.16163.com" />
		<test url="http://kf.16163.com/" />
		<test url="http://ldxy.16163.com/" />
		<test url="http://zm.16163.com/" />

	<exclusion pattern="^http://cc\.16163\.com/" />
		<test url="http://cc.16163.com/" />
		<test url="http://cc.163.com/game/" />

	<rule from="^http://16163\.com/" to="https://www.16163.com/" />

	<rule from="^http:" to="https:" />
</ruleset>
