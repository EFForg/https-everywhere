<!--
	Invalid certificate:
		ca-pca.fr
		assistance.ca-pca.fr

-->
<ruleset name="CA-PCR.fr">

	<target host="ca-pca.fr" />
	<target host="www.ca-pca.fr" />
	<target host="entreprises.ca-pca.fr" />
	<target host="m.ca-pca.fr" />

	<rule from="^http://ca-pca\.fr/" 
		to="https://www.ca-pca.fr/" />

	<rule from="^http:"
		to="https:" />

</ruleset>
