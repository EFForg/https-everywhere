<ruleset name="Soup.io (partial)">

	<target host="soup.io" />
	<target host="*.soup.io" />
		<!--
			Redirect to http:
						-->
		<!--exclusion pattern="^http://www\.soup\.io/+($|\?)" /-->
		<!--
			Exceptions:
					-->
		<exclusion pattern="^http://www\.soup\.io/+(?!favicon\.ico|images/)" />


	<rule from="^http://(static\.|www\.)?soup\.io/"
		to="https://$1soup.io/" />

</ruleset>
