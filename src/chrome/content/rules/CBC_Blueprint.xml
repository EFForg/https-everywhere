<!--
	Problematic subdomains:

		- ^	(cert only matches www)

-->
<ruleset name="CBC Blueprint">

	<target host="cbcblueprint.com" />
	<target host="www.cbcblueprint.com" />


	<securecookie host="^www\.cbcblueprint\.com$" name=".+" />


	<rule from="^http://(?:www\.)?cbcblueprint\.com/"
		to="https://www.cbcblueprint.com/" />

</ruleset>