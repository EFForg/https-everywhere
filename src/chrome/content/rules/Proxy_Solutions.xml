<!--
	Some pages redirect to http.

-->
<ruleset name="Proxy Solutions (partial)">

	<target host="proxysolutions.net" />
	<target host="www.proxysolutions.net" />
		<!--
			Redirects to http:
						-->
		<!--exclusion pattern="^http://www\.proxysolutions\.net/$" /-->
		<!--
			Exceptions:
					-->
		<exclusion pattern="^http://www\.proxysolutions\.net/+(?!(?:affiliates|proxy-services|support|your-proxy-account)(?:$|[?/])|favicon\.ico|images/|wp-content/|wp-content/)" />


	<rule from="^http://(www\.)?proxysolutions\.net/"
		to="https://$1proxysolutions.net/" />

</ruleset>
