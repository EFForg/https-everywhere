<!--
  Raiffeisen.ch does send the correct hsts header which would be needed for preload, 
  but somehow hstspreload.org doesn't accepts it...
-->

<ruleset name="raiffeisen.ch">
        <target host="raiffeisen.ch" />
        <target host="www.raiffeisen.ch" />
        <target host="ebanking.raiffeisen.ch" />
        <target host="memberplus.raiffeisen.ch" />

        <rule from="^http:"
                to="https:" />
</ruleset>
