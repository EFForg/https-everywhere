<!--
	Nonfunctional hosts in *.eldarya.com.br:

	h: http redirect
	m: certificate mismatch
	r: connection refused
	s: self-signed certificate
	t: timeout on https
-->
<ruleset name="Eldarya.com.br">
	<target host="eldarya.com.br" />
	<target host="www.eldarya.com.br" />

	<rule from="^http:" to="https:" />
</ruleset>
