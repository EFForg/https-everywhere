<!--
	Nonfunctional hosts in *.mail.bg:

	certificate mismatch:
		- corp.mail.bg
-->
<ruleset name="Mail.bg">
	<target host="mail.bg" />
	<target host="www.mail.bg" />
	<target host="adsy.mail.bg" />
	<target host="beta.mail.bg" />

	<securecookie host=".+" name=".+" />

	<rule from="^http:" to="https:" />
</ruleset>
