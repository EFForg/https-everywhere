<!--
	Other royalnavy.mod.uk related rulesets:
		+ NavyNews.co.uk.xml

	Non-functional hosts
		Timeout was reached:
			 - prmc.royalnavy.mod.uk
			 - staging-www.royalnavy.mod.uk

		SSL peer certificate was not OK:
			 - careers.royalnavy.mod.uk
			 - email.royalnavy.mod.uk
			 - jackspeak.royalnavy.mod.uk

		Secure connection redirects to plaintext:
			 - royalnavy.mod.uk
			 - www.royalnavy.mod.uk
-->
<ruleset name="Royal Navy.mod.uk (partial)">
	<!-- complications -->
	<target host="www.royalnavy.mod.uk" />
	<exclusion pattern="^http://www\.royalnavy\.mod\.uk/$" />
		<test url="http://www.royalnavy.mod.uk/WebResource.axd?d=RYCQbFARswQ9yDbEYjtwrNQJ_6__UQc_GcZR5VaD1UeP8kDFH5uhs6wWD9b-aJ1Od9L8k7iWoM9gVt040bedhE6oU_g1&amp;t=635802997220000000" />
		<test url="http://www.royalnavy.mod.uk/contact-us/" />
		<test url="http://www.royalnavy.mod.uk/login/" />
		<test url="http://www.royalnavy.mod.uk/profile/" />
		<test url="http://www.royalnavy.mod.uk/rnAssets/css/print.css" />

	<rule from="^http://www\.royalnavy\.mod\.uk/(WebResource\.axd|contact-us|login|profile|rnAssets)"
			to="https://www.royalnavy.mod.uk/$1" />

	<!-- direct rewrite -->
	<target host="authoring.royalnavy.mod.uk" />

	<rule from="^http:" to="https:" />
</ruleset>
