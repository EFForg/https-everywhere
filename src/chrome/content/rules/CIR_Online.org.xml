<ruleset name="CIR Online.org (partial)">

	<target host="cironline.org" />
		<!--
			Redirect to http:
						-->
		<!--exclusion pattern="^http://cironline\.org/($|\?|favicon\.ico)" /-->
		<!--
			Exceptions:
					-->
		<exclusion pattern="^http://cironline\.org/+(?!sites/)" />
	<target host="*.cironline.org" />


	<rule from="^http://(beta\.|www\.)?cironline\.org/"
		to="https://$1cironline.org/" />

</ruleset>
