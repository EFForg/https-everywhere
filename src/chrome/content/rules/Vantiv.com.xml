<!--
	Problematic subdomains:

		- investor *

	* corporate-ir.net


	Fully covered subdomains:

		- (www.)?
		- (www.)?clientconnection
		- sales

-->
<ruleset name="Vantiv.com (partial)">

	<target host="vantiv.com" />
	<target host="*.vantiv.com" />


	<rule from="^http://((?:(?:www\.)?clientconnection|sales|www)\.)?vantiv\.com/"
		to="https://$1vantiv.com/" />

</ruleset>
