<!--
	Active mixed content:
		- avidemux.org
		- bugs.avidemux.org

	Invalid certificate:
		- smtp.avidemux.org, CN=ssl0.ovh.net
-->

<ruleset name="Avidemux.org" platform="mixedcontent">
	<target host="avidemux.org" />
	<target host="www.avidemux.org" />
	<target host="bugs.avidemux.org" />

	<securecookie host=".+" name=".+" />

	<rule from="^http:" to="https:" />
</ruleset>
