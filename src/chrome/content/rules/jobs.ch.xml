<ruleset name="jobs.ch">
	<target host="jobs.ch"/>
	<target host="*.jobs.ch"/>
	<target host="jobcloud.ch"/>
	<target host="*.jobcloud.ch"/>

	<rule from="^http://([^/:@]*)\.jobs\.ch/"
			to="https://$1.jobs.ch/"/>
	<rule from="^http://([^/:@]*)\.jobcloud\.ch/"
			to="https://$1.jobcloud.ch/"/>
</ruleset>
