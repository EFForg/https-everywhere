<ruleset name="Meebo">
  <target host="*.meebo.com" />
  <target host="meebo.com" />


	<securecookie host="^\.meebo\.com$" name=".+" />


  <rule from="^http://(?:www\.)?meebo\.com/" to="https://www.meebo.com/"/>

	<rule from="^http://(cim|dashboard)\.meebo\.com/"
		to="https://$1.meebo.com/" />
</ruleset>
