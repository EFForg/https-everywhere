<!--
	www.bclaconnect.ca: Mismatched

-->
<ruleset name="BCLA Connect.ca">

	<!--	Direct rewrites:
				-->
	<target host="bclaconnect.ca" />

	<!--	Complications:
				-->
	<target host="www.bclaconnect.ca" />


	<securecookie host="^\w" name=".+" />


	<rule from="^http://www\.bclaconnect\.ca/"
		to="https://bclaconnect.ca/" />

	<rule from="^http:"
		to="https:" />

</ruleset>
