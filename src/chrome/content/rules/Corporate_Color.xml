<!--
	!www: cert only matches www
-->
<ruleset name="Corporate Color">

	<target host="saimd.com" />
	<target host="www.saimd.com" />


	<test url="http://www.saimd.com/robots.txt" />


	<rule from="^http://saimd\.com/"
		to="https://www.saimd.com/" />
	<rule from="^http:"
		to="https:" />

</ruleset>
