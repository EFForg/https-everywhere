<!--
	Nonfunctional hosts in *.eldarya.it:

	h: http redirect
	m: certificate mismatch
	r: connection refused
	s: self-signed certificate
	t: timeout on https
-->
<ruleset name="Eldarya.it">
	<target host="eldarya.it" />
	<target host="www.eldarya.it" />

	<rule from="^http:" to="https:" />
</ruleset>
