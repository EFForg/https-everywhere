<!--
	Fully covered subdomains:

		- (www.)?
		- app
		- blog
		- docs
		- status


	Mixed content:

		- Images on blog from $self *

	* Secured by us

-->
<ruleset name="Compose.IO">

	<!--	Direct rewrites:
				-->
	<target host="compose.io" />
	<target host="app.compose.io" />
