<!--
	Nonfunctional subdomains:

		- blog *

	* Redirects to http


	Fully covered subdomains:

		- (www.)?
		- auth


	These altnames don't exist:

		- m.campact.de

-->
<ruleset name="Campact.de (partial)">

	<target host="campact.de" />
	<target host="*.campact.de" />


	<rule from="^http://(auth\.|www\.)?campact\.de/"
		to="https://$1campact.de/" />

</ruleset>
