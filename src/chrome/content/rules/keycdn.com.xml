<!--
        Functional domains:
            keycdn.com
            www.keycdn.com
            cdn.keycdn.com
            app.keycdn.com
            tools.keycdn.com
            logos.keycdn.com
-->

<ruleset name="keycdn.com">

    <target host="keycdn.com" />
    <target host="www.keycdn.com" />
    <target host="cdn.keycdn.com" />
    <target host="app.keycdn.com" />
    <target host="tools.keycdn.com" />
    <target host="logos.keycdn.com" />

    <rule from="^http:" to="https:" />

        <test url="http://keycdn.com/" />
        <test url="http://www.keycdn.com/" />
        <test url="http://cdn.keycdn.com/" />
        <test url="http://app.keycdn.com/" />
        <test url="http://tools.keycdn.com/" />
        <test url="http://logos.keycdn.com/" />

</ruleset>
