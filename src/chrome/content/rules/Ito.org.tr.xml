<ruleset name="Istanbul Chamber of Commerce (partial)">
	<target host="ito.org.tr" />
	<target host="www.ito.org.tr" />
	<target host="galeri.ito.org.tr" />
	<target host="online.ito.org.tr" />
	<target host="satinalma.ito.org.tr" />

    <securecookie host=".+" name="^cookiesession1$" />
	<securecookie host=".+" name="^JSESSIONID$" />
    <rule from="^http:" to="https:" />
</ruleset>