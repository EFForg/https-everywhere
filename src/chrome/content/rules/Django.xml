<!--
	Fully covered hosts in *djangoproject.com:

		- (www.)?
		- code
		- docs

-->
<ruleset name="Django Project.com">

	<!--	Direct rewrites:
				-->
	<target host="djangoproject.com" />
	<target host="code.djangoproject.com" />
	<target host="docs.djangoproject.com" />
	<target host="www.djangoproject.com" />


	<securecookie host="^code\.djangoproject\.com$" name=".*" />


	<rule from="^http:"
		to="https:" />

</ruleset>
