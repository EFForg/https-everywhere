	<target host=                "gmx.info" />
	<target host=                "gmx.li" />
	<target host=                "gmx.lu" />
	<target host=                "gmx.org" />
	<target host=                "gmx.ph" />
	<target host=                "gmx.sg" />
	<target host=                "gmx.tm" />
