<!--
	Insecure cookies are set for these domains:

		- .greatfire.org

-->
<ruleset name="GreatFire.org">
   <target host="greatfire.org"/>
   <target host="en.greatfire.org"/>
   <target host="www.greatfire.org"/>
   <target host="zh.greatfire.org"/>


	<!--	CloudFlare cookies:
					-->
	<!--securecookie host="^\.greatfire\.org$" name="^(__cfduid|cf_clearance)$" /-->

  <securecookie host="^(?:(?:en|www|zh)?\.)?greatfire\.org$" name=".*"/>


  <rule from="^http:" to="https:"/>
</ruleset>
