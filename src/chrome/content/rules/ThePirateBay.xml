<!--
	Non-functional domain:
		- (www.)promobay.org			(hostname mismatch, CN: *.webfaction.com, webfaction.com)
		- www.thepiratebay.(gd|la|mn|vg)	(could not resolve host)
-->

<ruleset name="The Pirate Bay">

	<target host= "thepiratebay.gd" />
	<target host= "thepiratebay.la" />
	<target host= "thepiratebay.mn" />
	<target host= "thepiratebay.vg" />
	<target host=    "piratebay.org" />
	<target host=    "pirates-forum.org" />
	<target host="www.pirates-forum.org" />

  	<securecookie host="^thepiratebay\.(gd|la|mn|vg)$" name=".+" />
  	<securecookie host="^piratebay\.org$" name=".+" />
  	<securecookie host="^pirates-forum\.org$" name=".+" />

	<rule from="^http:"
		to="https:" />

</ruleset>
