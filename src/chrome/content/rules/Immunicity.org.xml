-->
<ruleset name="Immunicity.org">

	<target host="immunicity.org" />
	<target host="www.immunicity.org" />


	<!--	www redirects to ^ over http,
		so copy that behavior:
					-->
	<rule from="^http://(?:www\.)?immunicity\.org/"
		to="https://immunicity.org/" />

</ruleset>
