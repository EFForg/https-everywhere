<ruleset name="Fluendo (partial)" default_off="mismatch">

	<target host="fluendo.com" />
	<target host="www.fluendo.com" />
		<!--exclusion pattern="^http://(www\.)?fluendo\.com(/press)?/$" /-->


	<!--	Many pages redirect to http.	-->
	<rule from="^http://(www\.)?fluendo\.com/(media/|press/\w)"
		to="https://$1fluendo.com/$2" />

</ruleset>
