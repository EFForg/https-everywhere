<!--
	Nonfunctional subdomains:

		- (www.) *

	* Shows milliways, valid cert

-->
<ruleset name="cryptomilk.org (partial)">

	<target host="blog.cryptomilk.org"/>
	<target host="milliways.cryptomilk.org"/>


	<securecookie host="^blog\.cryptomilk\.org$" name=".+" />


	<rule from="^http:"
		to="https:" />

</ruleset>
