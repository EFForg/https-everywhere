	<!--	Not secured by server:
					-->
	<!--securecookie host="^comms\.anz\.co\.nz$" name="^(?:JSESSIONID|prd-aid)$" /-->

	<securecookie host="^\w" name=".+" />


	<rule from="^http://anz\.co\.nz/"
		to="https://www.anz.co.nz/" />

	<rule from="^http:"
