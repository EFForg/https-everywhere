<ruleset name="Twitch.tv (partial)" default_off='broken' >

	<target host="*.jtvnw.net" />
	<target host="*.justin.tv" />	
	<target host="*.twitch.tv" />

	<rule from="^http://static\-cdn\.jtvnw\.net/" to="https://static-cdn.jtvnw.net/"/>
	<rule from="^http://www\-cdn\.jtvnw\.net/" to="https://www-cdn.jtvnw.net/"/>
	<rule from="^http://www\-origin\.justin\.tv/" to="https://www-origin.justin.tv/"/>
	<rule from="^http://api\.twitch\.tv/" to="https://api.twitch.tv/"/>

</ruleset>
