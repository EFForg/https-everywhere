<!--
	Problematic hosts in *voodooping.com:

		- (www.)? *

	* Expired

-->
<ruleset name="Voodoo Ping.com" default_off="expired">

	<target host="voodooping.com" />
	<target host="www.voodooping.com" />


	<securecookie host="^(?:w*\.)?voodooping\.com$" name=".+" />


	<rule from="^http:"
		to="https:" />

</ruleset>
