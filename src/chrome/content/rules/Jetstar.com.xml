<ruleset name="Jetstar.com">

	<target host="book.jetstar.com" />
	<target host="booknow.jetstar.com" />
	<target host="checkin.jetstar.com" />
	<target host="cruises.jetstar.com" />
	<target host="hotels.jetstar.com" />
	<target host="m.jetstar.com" />
	<target host="media1.jetstar.com" />
	<target host="media3.jetstar.com" />
	<target host="roc.jetstar.com" />
	<target host="ssl.jetstar.com" />
	<target host="static.jetstar.com" />
	<target host="travelhub.jetstar.com" />

	<exclusion pattern="^http://media1\.jetstar\.com/$" />
	<exclusion pattern="^http://media3\.jetstar\.com/$" />
	<exclusion pattern="^http://static\.jetstar\.com/$" />

	<test url="http://media1.jetstar.com/assets/20160809153005/version20160809153005/scripts/plugins.js" />
	<test url="http://media3.jetstar.com/assets/scripts/vendors/priority-engine/testjet.pe.js" />
	<test url="http://roc.jetstar.com/wow/Login.aspx" />

	<rule from="^http:"
		to="https:" />

</ruleset>
