<!--
	For rules not causing false/broken MCB, see MyCanvas.com.xml.

-->
<ruleset name="MyCanvas.com (false MCB)" platform="mixedcontent">

	<target host="mycanvas.com" />
	<target host="www.mycanvas.com" />


	<securecookie host="^(?:.*\.)?mycanvas\.com$" name=".+" />


	<rule from="^http://(?:www\.)?mycanvas\.com/"
		to="https://secure.mycanvas.com/" />

</ruleset>
