<!--
	Invalid certificate:
		about.zenodo.org
		blog.zenodo.org
		developers.zenodo.org
		help.zenodo.org

-->
<ruleset name="Zenodo.org">

	<target host="zenodo.org" />
	<target host="www.zenodo.org" />
	<target host="sandbox.zenodo.org" />
	<target host="status.zenodo.org" />

	<securecookie host=".+" name=".+" />

	<rule from="^http:"
		to="https:" />

</ruleset>
