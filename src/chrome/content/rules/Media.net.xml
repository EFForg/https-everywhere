<ruleset name="media.net (partial)">

	<target host="*.media.net" />


	<!--	Can this be secured safely?
						-->
	<!--securecookie host="^\.media\.net$" name="^visitor-id$" /-->
	<!--
		Tracking cookies:
					-->
	<securecookie host="^\.media\.net$" name="^(?:__utm\w|wooTracker)$" />
	<securecookie host="^\.www\.media\.net$" name="^(?:__ar_v4|_te_)$" />


	<rule from="^http://cdn\.media\.net/"
		to="https://cdn.media.net/" />

		<test url="http://cdn.media.net/__media__/pics/da-website/images/shopping1.jpg" />

	<rule from="^http://mycdn\.media\.net/"
		to="https://mycdn.media.net/" />

		<test url="http://mycdn.media.net/__media__/js/util/nrr.js" />

	<rule from="^http://contextual\.media\.net/"
		to="https://contextual.media.net/" />

		<test url="http://contextual.media.net/nmedianet.js" />

	<rule from="^http://qsearch\.media\.net/"
		to="https://qsearch-a.akamaihd.net/" />

		<test url="http://qsearch.media.net/bping.php" />

	<!-- rule from="^http://static\.media\.net/"
