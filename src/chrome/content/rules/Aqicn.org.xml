<!--
	Other related ruleset:
		- Waqi.info.xml


	Non-functional subdomains:
		- www		(m)
		- api		(m)
		- d		(r)
		- feedback		(m)
		- ftp		(r)
		- geo		(m)
		- ios		(m)
		- jp1		(e)
		- jp2		(m)
		- jp3		(m)
		- mail		(m)
		- mapi		(m)
		- mapi3		(m)
		- mapidroid		(m)
		- scs		(m)
		- sg		(c)
		- sg1		(m)
		- stat		(r)
		- static	(m)
		- sw2		(r)
		- sw3		(m)
		- tiles		(e)
		- waqi		(m)
		- wgt		(m)
		- widget		(e)
		- winwgt		(m)

	c: connection error
	e: expired certificate
	h: http redirect
	i: invalid certificate chain
	m: certificate mismatch
	r: connection refused
	s: self-signed certificate
	t: timeout on https
-->
<ruleset name="Aqicn.org">

	<target host="aqicn.org" />
	<target host="www.aqicn.org" />
	<target host="feed.aqicn.org" />
	<target host="res.aqicn.org" />
	<target host="sensor.aqicn.org" />
	<target host="tiles.aqicn.org" />

	<securecookie host=".+" name=".+" />

	<rule from="^http://www\.aqicn\.org/"
		to="https://aqicn.org/" />

	<rule from="^http:"
		to="https:" />
</ruleset>
