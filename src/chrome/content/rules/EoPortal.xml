<!--
	Nonfunctional subdomains:

		- www	(times out)


	directory: Server sends no certificate chain, see https://whatsmychaincert.com


	^eoportal.org doesn't exist.

-->
<ruleset name="eoPortal (partial)" default_off="missing certificate chain">

	<!--	Direct rewrites:
				-->
	<target host="directory.eoportal.org" />


	<securecookie host="^directory\.eoportal\.org$" name=".+" />


	<rule from="^http:"
		to="https:" />

</ruleset>
