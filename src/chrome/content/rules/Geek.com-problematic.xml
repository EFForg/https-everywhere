<!--
	For rules that are on by default, see Geek.com.xml.

-->
<ruleset name="Geek.com (mismatched)" default_off="Certificate mismatch">
	<target host="*.geek.com" />
	<target host="geek.com" />

	<securecookie host="^(?:.*\.)?geek\.com$" name=".*" />

	<rule from="^http://(?:www\.)?geek\.com/" to="https://www.geek.com/"/>
</ruleset>
