<ruleset name="E-junkie (partial)">

	<target host="e-junkie.com" />
	<target host="www.e-junkie.com" />
	<target host="static.e-junkie.com" />
	<target host="ejunkie.com" />
	<target host="www.ejunkie.com" />


	<!--	Some (most?) pages 301 to http.
							-->
	<rule from="^http://(www\.)?ejunkie\.com/"
		to="https://www.e-junkie.com/" />

		<test url="http://static.e-junkie.com/manifest.json?v=5.0" />

	<rule from="^http:"
		to="https:" />

</ruleset>
