<!--
	Mismatched:
		www.oauth.net
		blog.oauth.net
		oauthwiki.oauth.net
		tokens.oauth.net
		tumble.oauth.net
		wiki.oauth.net

-->
<ruleset name="OAuth">

	<!--	Cert: *.geoloqi.com	-->
	<target host="oauth.net" />
	<target host="www.oauth.net" />
	<target host="blog.oauth.net" />
	<target host="oauthwiki.oauth.net" />
	<target host="tokens.oauth.net" />
	<target host="tumble.oauth.net" />

	<rule from="^http://(www\.|blog|oauthwiki|tokens|tumble)?oauth\.net/"
		to="https://oauth.net/" />

	<test url="http://www.oauth.net/about/introduction/" />
	<test url="http://blog.oauth.net/2/" />
	<test url="http://oauthwiki.oauth.net/code/" />
	<test url="http://tokens.oauth.net/articles/" />
	<test url="http://tumble.oauth.net/about/credits/" />

</ruleset>
