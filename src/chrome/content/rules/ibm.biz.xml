<!--
	For other IBM coverage, see IBM.xml.


	www.ibm.biz: Mismatched

-->
<ruleset name="IBM.biz">

	<!--	Direct rewrites:
				-->
	<target host="ibm.biz" />

	<!--	Complications:
				-->
	<target host="www.ibm.biz" />


	<securecookie host="^\w" name=".+" />


	<rule from="^http://www\.ibm\.biz/"
		to="https://ibm.biz/" />

	<rule from="^http:"
		to="https:" />

</ruleset>
