<!--
	Server sends no certificate chain *

	* See https://whatsmychaincert.com


	Mixed content:

		- Images, from:

			- (www.)?
			- jquerytools.org

-->
<ruleset name="Worldhealth.net" default_off="missing certificate chain">

	<!--	Direct rewrites:
				-->
	<target host="worldhealth.net" />
	<target host="www.worldhealth.net" />


	<securecookie host="^www\.worldhealth\.net$" name=".+" />


	<rule from="^http:"
		to="https:" />

</ruleset>
