<!--
	For other KLDP coverage, see KLDP.org.xml.


	Fully covered domains:

		- kldp.net

		- [\w-]+.kldp.net:

			- drupal
			- emacs
			- gurugio
			- ktug
			- lisp
			- man
			- mutt
			- qmail
			- vi
			- www


	Mixed content:

		- Image on qmail from $self *

	* Secured by us


-->
<ruleset name="KLDP.net">

	<target host="kldp.net" />
	<target host="*.kldp.net" />


	<rule from="^http://([\w-]+\.)?kldp\.net/"
		to="https://$1kldp.net/" />

</ruleset>
