<!--
	Nonfunctional hosts in *.hkqf.gov.hk:

	h: http redirect
	m: certificate mismatch
	r: connection refused
	s: self-signed certificate
	t: timeout on https
-->
<ruleset name="hkqf.gov.hk">
	<target host="hkqf.gov.hk" />
	<target host="www.hkqf.gov.hk" />

	<rule from="^http:" to="https:" />
</ruleset>
