<ruleset name="BT Wi-Fi (partial)">
	<!-- *.btwifi.com -->
	<target host="btwifi.com" />
	<target host="my.btwifi.com" />
	<target host="www.btwifi.com" />

	<!-- *.btwifi.co.uk -->
	<target host="btwifi.co.uk" />
	<target host="www.btwifi.co.uk" />

	<securecookie host=".+" name=".+" />

	<rule from="^http:" to="https:" />
</ruleset>
