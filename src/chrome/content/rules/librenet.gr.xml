<!--
This ruleset is experimental. If you experience problems please
open an issue at https://github.com/kargig/https-everywhere
-->

<ruleset name="librenet.gr">
  <target host="librenet.gr" />
  <target host="www.librenet.gr" />

  <securecookie host="^(?:www\.)?librenet\.gr$" name=".*"/>

  <rule from="^http://(?:www\.)?librenet\.gr/" to="https://librenet.gr/"/>
</ruleset>
