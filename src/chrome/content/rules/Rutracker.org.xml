<ruleset name="Rutracker">
	<target host="rutracker.org" />
	<target host="www.rutracker.org" />
	<target host="rutracker.net" />
	<target host="www.rutracker.net" />

	<rule from="^http://(www\.)?rutracker\.org/"
		to="https://rutracker.org/" />

	<rule from="^http://(www\.)?rutracker\.net/"
		to="https://rutracker.net/" />
</ruleset>
