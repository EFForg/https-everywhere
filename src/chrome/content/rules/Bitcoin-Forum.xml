<!--
	Fully covered hosts in *bitcointalk.org:

		- (www.)?
		- ip

-->
<ruleset name="Bitcoin Talk.org">

	<!--	Direct rewrites:
				-->
	<target host="bitcointalk.org"/>
	<target host="ip.bitcointalk.org"/>
	<target host="www.bitcointalk.org"/>

	<securecookie host="^bitcointalk\.org$" name=".+" />

	<rule from="^http:"
		to="https:" />

</ruleset>
