<!--
	(www.)?: Dropped

-->
<ruleset name="Infamous" default_off="connection dropped">

	<target host="foreverinfamous.com" />
	<target host="*.foreverinfamous.com" />


	<securecookie host="^(?:w*\.)?foreverinfamous\.com$" name=".+" />


	<rule from="^http://(www\.)?foreverinfamous\.com/"
		to="https://$1foreverinfamous.com/" />

</ruleset>
