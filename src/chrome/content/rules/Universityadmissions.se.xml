<!--
	Problematic subdomains:

		- ^	(cert only matches www)

-->
<ruleset name="universityadmissions.se">

	<target host="universityadmissions.se" />
	<target host="www.universityadmissions.se" />


	<securecookie host="^www\.universityadmissions.se$" name=".+" />


	<rule from="^http:" to="https:" />

</ruleset>
