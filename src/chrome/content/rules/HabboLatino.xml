<!--
	Nonfunctional domains:

		- (www.)hlat.me	

-->
<ruleset name="HabboLatino" default_off="mismatch">

	<target host="hlat.us" />
	<target host="*.hlat.us" />


	<securecookie host="^(?:w*\.)?hlat\.us$" name=".+" />


	<rule from="^http://(www\.)?hlat\.us/"
		to="https://$1hlat.us/" />

</ruleset>