<!--
	Nonfunctional hosts in *.nic.ir:
		-w whois.nic.ir (e)

	e: expired certificate
	h: http redirect
	m: certificate mismatch
	r: connection refused
	s: self-signed certificate
	t: timeout on https
-->
<ruleset name="Nic.ir">
	<target host="nic.ir" />
	<target host="www.nic.ir" />
	<target host="tools.nic.ir" />
	<target host="rrmt.nic.ir" />

	<rule from="^http:" to="https:" />
</ruleset>
