<ruleset name="Tor Project">

	<target host="torproject.org" />
	<target host="*.torproject.org" />
		<test url="http://blog.torproject.org/" />
		<test url="http://check.torproject.org/" />
		<test url="http://trac.torproject.org/" />

	<securecookie host=".+" name=".+" />

	<rule from="^http:"
		to="https:" />

</ruleset>
