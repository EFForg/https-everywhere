<ruleset name="Tor Project">

	<target host="torproject.org" />
	<target host="*.torproject.org" />
		<exclusion pattern="^http://torperf\.torproject\.org/" />


	<securecookie host="^(?:.*\.)?torproject\.org$" name=".+" />


	<rule from="^http://([^/:@\.]+\.)?torproject\.org/"
		 to="https://$1torproject.org/" />

</ruleset>
