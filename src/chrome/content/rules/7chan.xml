<!--
	Insecure cookies are set for these domains:

		- .7chan.org

-->
<ruleset name="7chan">

	<!--	Direct rewrites:
				-->
	<target host="7chan.org" />
	<target host="www.7chan.org" />


	<!--	CloudFlare cookies:
					-->
	<!--securecookie host="^\.7chan\.org$" name="^(?:__cfduid|cf_clearance)$" /-->

	<securecookie host="^\.7chan\.org$" name=".+" />


	<rule from="^http:"
		to="https:" />

</ruleset>
