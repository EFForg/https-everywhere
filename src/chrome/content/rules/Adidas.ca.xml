	<target host="www.miteam.fr.adidas.ca" />
	<target host="m.adidas.ca" />
	<target host="www.miteam.adidas.ca" />
	<target host="shop.miteam.adidas.ca" />
	<rule from="^http:" to="https:" />
</ruleset>
