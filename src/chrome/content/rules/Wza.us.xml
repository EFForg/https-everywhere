<!--
	Different content http/https:
		admin.wza.us
		jabber.wza.us

	Invalid certificate:
		www.wza.us
		bootsie.wza.us
		a.mx.wza.us
		b.mx.wza.us
		a.ns.wza.us
		b.ns.wza.us

-->
<ruleset name="wza.us">

	<target host="wza.us" />
	<target host="mail.wza.us" />

	<rule from="^http:"
		to="https:" />

</ruleset>
