<!--
	Invalid certificate:
		cloud.evoliatis.com
		ref.evoliatis.com

	Mixed content from fonts.googleapis.com:
		evoliatis.com
		www.evoliatis.com

-->
<ruleset name="Evoliatis.com" platform="mixedcontent">

	<target host="evoliatis.com" />
	<target host="www.evoliatis.com" />

	<rule from="^http:"
		to="https:" />

</ruleset>
