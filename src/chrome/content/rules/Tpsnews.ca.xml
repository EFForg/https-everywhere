<!--
	Nonfunctional hosts in *.tpsnews.ca:

	h: http redirect
	m: certificate mismatch
	r: connection refused
	s: self-signed certificate
	t: timeout on https
-->
<ruleset name="Tpsnews.ca">
	<target host="tpsnews.ca" />
	<target host="www.tpsnews.ca" />

	<rule from="^http:"
		to="https:" />
</ruleset>
