<!--
	Fully covered hosts in *bitmask.net:

		- ^
		- demo
		- dl


	These altnames don't exist:

		- www.demo.bitmask.net
		- www.dl.bitmask.net
		- www.bitmask.net

-->
<ruleset name="Bitmask.net (partial)">

	<!--	Direct rewrites:
				-->
	<target host="bitmask.net" />
	<target host="demo.bitmask.net" />
	<target host="dl.bitmask.net" />


	<rule from="^http:"
		to="https:" />

</ruleset>
