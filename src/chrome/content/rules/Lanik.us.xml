<!--
	For Lanik.us

		Works:

			- forums
			- www
			- $

		Doesn't Work:
		nil
-->
<ruleset name="Lanik.us">
	<target host="lanik.us" />
	<target host="www.lanik.us" />
	<target host="forums.lanik.us" />

	<rule from="^http:"
			to="https:" />

	<securecookie host="^forums\.lanik\.us$" name=".+" />
	<securecookie host="^\.lanik\.us$" name="^__cfduid$" />
</ruleset>