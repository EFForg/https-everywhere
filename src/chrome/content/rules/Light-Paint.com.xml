<!--
	www: mismatched, CN: secure.light-paint.com


	Mixed content:

		- Images from www *

	* Secured by us

-->
<ruleset name="Light-Paint.com">

	<target host="light-paint.com" />
	<target host="*.light-paint.com" />


	<securecookie host="^light-paint\.com$" name=".+" />


	<rule from="^http://(?:www\.)?light-paint\.com/"
		to="https://light-paint.com/" />

	<rule from="^http://secure\.light-paint\.com/"
		to="https://secure.light-paint.com/" />

</ruleset>
