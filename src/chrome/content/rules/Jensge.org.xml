<!--
	Problematic subdomains:

		- www	(mismatched, CN: www.webstreams.de)

-->
<ruleset name="jensge.org" platform="cacert">

	<!--	Direct rewrites:
				-->
	<target host="jensge.org" />

	<!--	Complications:
				-->
	<target host="www.jensge.org" />


	<rule from="^http://www\.jensge\.org/"
		to="https://jensge.org/" />

	<rule from="^http:"
		to="https:" />

</ruleset>
