<ruleset name="OAG">

	<target host="oag.com"/>
	<target host="*.oag.com"/>


	<securecookie host="^tppro\.oag\.com$" name=".*"/>


	<!--	!www doesn't work.	-->
	<rule from="^http://(?:www\.)?oag\.com/"
		to="https://www.oag.com/"/>

	<rule from="^http://tppro\.oag\.com/"
		to="https://tppro.oag.com/"/>

</ruleset>
