<!--
	Nonfunctional hosts in *.gamerswithjobs.com:

	h: http redirect
	m: certificate mismatch
	r: connection refused
	s: self-signed certificate
	t: timeout on https
-->
<ruleset name="Gamerswithjobs">
	<target host="gamerswithjobs.com" />
	<target host="www.gamerswithjobs.com" />

	<rule from="^http:" to="https:" />
</ruleset>
