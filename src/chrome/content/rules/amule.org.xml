<!--
	Redirect to HTTP:
		forum.amule.org
		test.amule.org
		wiki.amule.org
-->
<ruleset name="amule.org">
	<target host="amule.org" />
	<target host="www.amule.org" />
	<target host="bugs.amule.org" />
	<target host="ftp.amule.org" />
	<target host="imap.amule.org" />
	<target host="mail.amule.org" />
	<target host="smtp.amule.org" />
	<target host="webmail.amule.org" />

	<rule from="^http:" to="https:" />
</ruleset>
