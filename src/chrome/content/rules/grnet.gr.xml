<ruleset name="grnet.gr">
  <target host="grnet.gr" />
  <target host="www.grnet.gr" />

  <securecookie host="^(?:www\.)?grnet\.gr$" name=".*"/>

  <rule from="^http://(?:www\.)?grnet\.gr/" to="https://grnet.gr/"/>
</ruleset>
