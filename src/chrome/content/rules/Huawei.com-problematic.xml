<ruleset name="Huawei.com (missing certificate chain)" default_off="missing certificate chain">

	<target host="uniportal.huawei.com" />


	<securecookie host="^uniportal\.huawei\.com$" name=".+" />


	<rule from="^http:"
		to="https:" />

</ruleset>
