<!--
	Fully covered domains:

		- tum.de subdomains:

			- (www.)
			- adm9.in.tum.de
			- lrr.in
			- sec.in
			- www21.in
			- www.lehre
			- www.lehren
			- www.piwik
			- mediatum.ub
			- www

		- mailmanbroy.informatik.tu-muenchen.de
		- www.tu-muenchen.de


	^tum.de & ^tu-muenchen.de do not exist


	Observed cookie domains:

		- in.tum.de
		- adm9.in.tum.de
		- www.in.tum.de
		- www.tum.de


	Mixed content:

		- Web bugs, on:

			- ias.in.tum.de from adm9.in.tum.de *
			- www.in from www.etracker.de *
			- www.tum.de from www.piwik.tum.de *

	* Secured by us

	Expired certificates:
		- ias.in
		- ias.cs.tum.edu
		- ias.informatik.tu-muenchen.de
-->
<ruleset name="Technical University Munich (partial)">

	<target host="*.tum.de" />
	<target host="*.tu-muenchen.de" />

	<!--	Not secured by server:
					-->
	<!--securecookie host="^www\.sec\.in\.tum\.de$" name="^PHPSESSID$" /-->
	<!--securecookie host="^mediatum\.ub\.tum\.de$" name="^PSESSION$" /-->

	<securecookie host="^(?:in|(?:adm9|ias|sec|www)\.in|mediatum\.ub|www(?:\.lehren?)?)?\.tum\.de$" name=".+" />
	<securecookie host="^(?:www\.informatik)\.tu-muenchen\.de$" name=".+" />


	<rule from="^http://www(\.lehren?|\.piwik)?\.tum\.de/"
		to="https://www$1.tum.de/" />
        <test url='http://www.lehren.tum.de/nc/startseite/' />
        <test url='http://www.piwik.tum.de/' />


	<rule from="^http://((?:(?:sec|www|www21)\.)?in|mediatum\.ub)\.tum\.de/"
		to="https://$1.tum.de/" />
        <test url='http://mediatum.ub.tum.de/' />
        <test url='http://www.in.tum.de/' />


	<rule from="^http://((?:ias|mailmanbroy)\.informatik|www)\.tu-muenchen\.de/"
		to="https://$1.tu-muenchen.de/" />
        <test url='http://mailmanbroy.informatik.tu-muenchen.de/' />


	<rule from="^http://isabelle\.in\.tum\.de/"
		to="https://isabelle.in.tum.de/" />
        <test url='http://isabelle.in.tum.de/index.html' />

	<rule from="^http://www\.lrr\.in\.tum\.de"
	        to="https://www.lrr.in.tum.de"
	/>
	<test url='http://www.lrr.in.tum.de/startseite/' />
</ruleset>
