<!--
	Fully covered domains:

		- tum.de subdomains:

			- (www.)
			- adm9.in.tum.de
			- lrr.in
			- sec.in
			- www21.in
			- www.lehre
			- www.lehren
			- www.piwik
			- mediatum.ub
			- www

		- mailmanbroy.informatik.tu-muenchen.de
		- www.tu-muenchen.de


	^tum.de & ^tu-muenchen.de do not exist


	Observed cookie domains:

		- in.tum.de
		- adm9.in.tum.de
		- www.in.tum.de
		- www.tum.de


	Mixed content:

		- Web bugs, on:

			- ias.in.tum.de from adm9.in.tum.de *
			- www.in from www.etracker.de *
			- www.tum.de from www.piwik.tum.de *

	* Secured by us

	Expired certificates:
		- ias.in
		- ias.cs.tum.edu
		- ias.informatik.tu-muenchen.de
-->
<ruleset name="Technical University Munich (partial)">

	<target host="www.tu-muenchen.de" />

	<target host="www.tum.de" />
	<target host="www.lehre.tum.de" />
	<target host="www.lehren.tum.de" />
	<target host="www.piwik.tum.de" />
	<target host="mediatum.ub.tum.de" />
	<target host="mailmanbroy.informatik.tu-muenchen.de" />
	<target host="adm9.in.tum.de" />
	<target host="www.in.tum.de" />
	<target host="www21.in.tum.de" />
	<target host="www.lrr.in.tum.de" />
	<target host="www.sec.in.tum.de" />
	<target host="isabelle.in.tum.de" />

	<!--	Not secured by server:
					-->
	<!--securecookie host="^www\.sec\.in\.tum\.de$" name="^PHPSESSID$" /-->
	<!--securecookie host="^mediatum\.ub\.tum\.de$" name="^PSESSION$" /-->

	<securecookie host="^(?:in|(?:adm9|ias|sec|www)\.in|mediatum\.ub|www(?:\.lehren?)?)?\.tum\.de$" name=".+" />
	<securecookie host="^(?:www\.informatik)\.tu-muenchen\.de$" name=".+" />

	<rule from="^http://www\.tum\.de"
	        to="https://www.tum.de"/>
	<rule from="^http://www\.tu-muenchen\.de"
	        to="https://www.tu-muenchen.de"/>

	<rule from="^http://www\.lehre\.tum\.de"
	        to="https://www.lehre.tum.de"/>
	<rule from="^http://www\.lehren\.tum\.de"
	        to="https://www.lehren.tum.de"/>
	<rule from="^http://www\.piwik\.tum\.de"
	        to="https://www.piwik.tum.de"/>


	<rule from="^http://mediatum\.ub\.tum\.de"
	        to="https://mediatum.ub.tum.de"/>


	<rule from="^http://mailmanbroy\.informatik\.tu-muenchen\.de/"
	        to="https://mailmanbroy.informatik.tu-muenchen.de/" />


	<!-- *.in.tum.de -->
	<rule from="^http://www\.in\.tum\.de"
	        to="https://www.in.tum.de"/>
	<rule from="^http://adm9\.in\.tum\.de"
	        to="https://adm9.in.tum.de"/>
	<rule from="^http://www\.lrr\.in\.tum\.de"
	        to="https://www.lrr.in.tum.de"/>
	<rule from="^http://www\.sec\.in\.tum\.de"
	        to="https://www.sec.in.tum.de"/>
	<rule from="^http://www21\.in\.tum\.de"
	        to="https://www21.in.tum.de"/>
	<rule from="^http://isabelle\.in\.tum\.de/"
	        to="https://isabelle.in.tum.de/" />
</ruleset>
