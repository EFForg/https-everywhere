<ruleset name="sharetobuy.com">
    <target host="sharetobuy.com" />
    <target host="*.sharetobuy.com" />

    <securecookie host="^(?:.*\.)?sharetobuy\.com$" name=".+" />

    <rule from="^http://sharetobuy\.com/"
        to="https://www.sharetobuy.com/" />
    <rule from="^http://([^/:@]+)?\.sharetobuy\.com/"
        to="https://$1.sharetobuy.com/" />
</ruleset>
