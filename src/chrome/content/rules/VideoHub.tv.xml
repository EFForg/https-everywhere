<!--
	For other Tremor Video coverage, see Tremor_Video.com.xml.


	Nonfunctional subdomains:

		- (www.)	(refused)


	Fully covered subdomains:

		- data
		- marketplace
		- portal
		- publishers

-->
<ruleset name="VideoHub.tv (partial)">

	<!--	Direct rewrites:
				-->
	<target host="data.videohub.tv" />
	<target host="marketplace.videohub.tv" />
	<target host="portal.videohub.tv" />
	<target host="publishers.videohub.tv" />


	<securecookie host="^(?:data|marketplace|\.?portal)\.videohub\.tv$" name=".+" />


	<rule from="^http:"
		to="https:" />

</ruleset>
