<!--
	www: mismatched, CN: newyear.aktivix.org


	These altnames don't exist:

		- www.lists.aktivix.org

-->
<ruleset name="Aktivix.org">

	<target host="aktivix.org" />
	<target host="*.aktivix.org" />


	<rule from="^http://(?:(lists\.|newyear\.)|www\.)?aktivix\.org/"
		to="https://$1aktivix.org/" />

</ruleset>
