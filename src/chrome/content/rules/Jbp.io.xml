<!--
	Insecure cookies are set for these domains:

		- .jbp.io

-->
<ruleset name="jbp.io">

	<!--	Direct rewrites:
				-->
	<target host="jbp.io" />
	<target host="www.jbp.io" />


	<!--	CloudFlare cookies:
					-->
	<!--securecookie host="^\.jbp\.io$" name="^(?:__cfduid|cf_clearance)$" /-->

	<securecookie host="^\.jbp\.io$" name=".+" />


	<rule from="^http:"
		to="https:" />

</ruleset>
