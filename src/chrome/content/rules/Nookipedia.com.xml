<!--
	Nookipedia.com has a wildcard DNS record.
-->

<ruleset name="Nookipedia.com">
	<target host="nookipedia.com" />
	<target host="www.nookipedia.com" />
	<target host="htp.nookipedia.com" />
	<target host="mobile.nookipedia.com" />
	<target host="piwik.nookipedia.com" />
	<target host="play.nookipedia.com" />
	<target host="vwww.nookipedia.com" />
	<target host="wsww.nookipedia.com" />
	<target host="ww3.nookipedia.com" />
	<target host="wwd.nookipedia.com" />
	<target host="wwe.nookipedia.com" />
	<target host="www2.nookipedia.com" />

	<securecookie host=".+" name=".+" />

	<rule from="^http:" to="https:" />
</ruleset>
