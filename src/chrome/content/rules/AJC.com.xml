<!--
	Nonfunctional domains:

		- ajc.com	(Reset)
		- www.ajc.com	(Akamai; 404)

-->
<ruleset name="AJC.com" default_off="504, Akamai">
	<target host="ajc.com" />
	<target host="www.ajc.com" />
	<rule from="^http://ajc\.com/" to="https://www.ajc.com/"/>
	<rule from="^http://www\.ajc\.com/" to="https://www.ajc.com/"/>
</ruleset>

