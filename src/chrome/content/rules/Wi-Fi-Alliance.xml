<!--
	Login required:
		certv1.wi-fi.org
		login.wi-fi.org
		certifications.prod.wi-fi.org
		certifications.staging.wi-fi.org

	Invalid certificate:
		mobile.wi-fi.org

-->
<ruleset name="Wi-Fi Alliance">

	<target host="wi-fi.org" />
	<target host="www.wi-fi.org" />
	<target host="certifications.wi-fi.org" />
	<target host="groups.wi-fi.org" />

	<securecookie host=".+" name=".+" />

	<rule from="^http:"
		to="https:" />

</ruleset>
