<ruleset name="e-activist.com">

	<target host="e-activist.com" />
	<target host="www.e-activist.com" />


	<!-- Fixes https://github.com/EFForg/https-everywhere/issues/7319 -->
	<exclusion pattern="^http://www\.e-activist\.com/ea-action/" />

		<test url="http://www.e-activist.com/ea-action/broadcast.record.message.click.do?ea.url.id=742999&amp;ea.campaigner.email=4DvyaN3Tko8gYHTECbwHV5ku6OPFCMj%2F&amp;ea.campaigner.id=MHk1zxzJpriZkArzVWMSmA==&amp;ea_broadcast_target_id=0" />


	<!--	Not secured by server:
					-->
	<!--securecookie host="^(www\.)?e-activist\.com$" name="^JSESSIONID$" /-->

	<securecookie host="^\w" name="." />


	<rule from="^http:"
		to="https:" />

</ruleset>
