<ruleset name="Cloud Proven.net">

	<target host="cloudproven.net" />
	<target host="*.cloudproven.net" />


	<!--	Server doesn't set Secure for:
						-->
	<securecookie host="^(?:www)?\.cloudproven\.net$" name=".+" />


	<rule from="^http://(www\.)?cloudproven\.net/"
		to="https://$1cloudproven.net/" />

</ruleset>
