<!--
	Everything except "blog.recordbank.be" is HSTS secured, but includes some unsecured cookies.
	The blog subdomain is also not HTTPS enforced.
-->

<ruleset name="Recordbank.be">
  <target host="www.homebank.recordbank.be" />
  <target host="mediaservices.recordbank.be" />
  <target host="blog.recordbank.be" />

  <securecookie host="^(?:www\.homebank|mediaservices|blog)?\.recordbank\.be$" name=".+" />

  <rule from="^http:" to="https:" />
</ruleset>
