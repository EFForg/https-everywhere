<!--
	Invalid certificate:
		mit.gov.tr

	Time-out:
		www.mit.gov.tr/emuze/
 -->

<ruleset name="National Intelligence Organization (partial)">
	<target host="mit.gov.tr" />

	<rule from="^http://mit\.gov\.tr" to="https://www.mit.gov.tr" />

	<target host="www.mit.gov.tr" />
		<exclusion pattern="^http://(?:www\.)?mit\.gov\.tr/emuze" />
		<test url="http://mit.gov.tr/emuze/index.html" />
		<test url="http://www.mit.gov.tr/emuze/index.html" />

	<rule from="^http:" to="https:" />
</ruleset>
