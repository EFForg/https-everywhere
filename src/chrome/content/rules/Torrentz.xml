<!--
	Other Torrentz rulesets:

		- Torrentsmirror.xml
		- Torrentz-Proxy.xml


	Insecure cookies are set for these hosts:

		- torrentz.ch
		- www.torrentz.ch
		- torrentz.com
		- www.torrentz.com
		- torrentz.eu
		- www.torrentz.eu
		- torrentz.in
		- www.torrentz.in
		- torrentz.me
		- www.torrentz.me

-->
<ruleset name="Torrentz">

	<!--	Direct rewrites:
				-->
	<target host="torrentz.ch" />
	<target host="www.torrentz.ch" />
	<target host="torrentz.com" />
	<target host="www.torrentz.com" />
	<target host="torrentz.eu" />
	<target host="www.torrentz.eu" />
	<target host="torrentz.in" />
	<target host="www.torrentz.in" />
	<target host="torrentz.me" />
	<target host="www.torrentz.me" />


	<!--	not secured by server:
					-->
	<!--securecookie host="(?:www\.)?torrentz\.(?:ch|com|eu|in|me)$" name="^sess$" /-->
	
	<securecookie host="(?:www\.)?torrentz\.(?:ch|com|eu|in|me)$" name=".+" />
	

	<rule from="^http:"
		to="https:" />

</ruleset>
