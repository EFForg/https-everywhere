<!--
	Problematic subdomains:

		- brand *

	* Herokuapp


	Fully covered subdomains:

		- (www.)?
		- developer
		- help

-->
<ruleset name="Venmo.com (partial)">

	<target host="venmo.com" />
	<target host="*.venmo.com" />


	<rule from="^http://((?:developer|help|www)\.)?venmo\.com/"
		to="https://$1venmo.com/" />

</ruleset>
