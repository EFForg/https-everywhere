<!--
	For rules not causing false/broken MCB, see UseResponse.com.xml.

-->
<ruleset name="UseResponse.com (false MCB)" platform="mixedcontent">

	<target host="*.useresponse.com" />


	<securecookie host="^(?:www)?\.useresponse\.com$" name=".+" />


	<rule from="^http://www\.useresponse\.com/"
		to="https://www.useresponse.com/" />

</ruleset>
