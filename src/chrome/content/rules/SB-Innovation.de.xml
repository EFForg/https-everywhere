<ruleset name="SB-Innovation.de" default_off="expired, self-signed">

	<target host="sb-innovation.de" />
	<target host="*.sb-innovation.de" />


	<securecookie host="^(?:.*\.)?sb-innovation\.de$" name=".*" />


	<rule from="^http://(?:[\w\-]+\.)?sb-innovation\.de/"
		to="https://www.sb-innovation.de/" />

</ruleset>
