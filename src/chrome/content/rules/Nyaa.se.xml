<!--
	Nonfunctional hosts in *nyaa.se:

		- (www.)? *
		- sukebei *

	* Refused

-->
<ruleset name="Nyaa.se (partial)">

	<!--	Direct rewrites:
				-->
	<target host="files.nyaa.se" />


	<!--	CloudFlare cookies:
					-->
	<!--securecookie host="^\.nyaa\.se$" name="^(?:__cfduid|cf_clearance)$" /-->

	<securecookie host="^\.nyaa\.se$" name="^(?:__cfduid|cf_clearance)$" />


	<rule from="^http:"
		to="https:" />

</ruleset>
