<ruleset name="Nyaa.se (partial)">

	<!--	Direct rewrites:
				-->
	<target host="nyaa.se" />
	<target host="*.nyaa.se" />

	<test url="http://nyaa.se/" />
	<test url="http://www.nyaa.se/" />
	<test url="http://sukebei.nyaa.se/" />
	<test url="http://files.nyaa.se/favicon.png" />


	<!--	CloudFlare cookies:
					-->
	<!--securecookie host="^\.nyaa\.se$" name="^(?:__cfduid|cf_clearance)$" /-->

	<securecookie host="^\.nyaa\.se$" name="^(?:__cfduid|cf_clearance)$" />


	<rule from="^http:"
		to="https:" />

</ruleset>
