<!--
	403 status code (can't find valid test URLs, therefore not including):
		- assets.github.com

	404 status code (can't find valid test URLs, therefore not including):
		- live.github.com

	Other GitHub rulesets:
		- Choosealicense.com.xml
		- Github-Pages.xml
		- Guag.es.xml
		- Speaker_Deck.com.xml

	Redirects:
		- codeload.github.com -> github.com
		- learn.github.com > services.github.com
		- raw-origin.github.com -> github.com
		- teach.github.com > services.github.com
		- training.github.com > services.github.com

	Secure connection failed:
		- ssh.github.com
-->
<ruleset name="GitHub">
	<target host="github.com" />
	<target host="www.github.com" />
	<target host="api.github.com" />
	<target host="codeload.github.com" />
	<target host="gist.github.com" />
	<target host="learn.github.com" />
	<target host="raw-origin.github.com" />
	<target host="teach.github.com" />
	<target host="training.github.com" />

	<target host="github.io" />

	<target host="collector.githubapp.com" />

	<target host="avatars0.githubusercontent.com" />
	<target host="avatars1.githubusercontent.com" />
	<target host="avatars2.githubusercontent.com" />
	<target host="avatars3.githubusercontent.com" />
	<target host="camo.githubusercontent.com" />
	<target host="raw.githubusercontent.com" />
	<target host="render.githubusercontent.com" />

	<securecookie host=".+" name=".+" />

	<rule from="^http:" to="https:" />
</ruleset>
