<!--
	Fully covered subdomains:

		- (www.)?
		- beta
		- live
		- support

-->
<ruleset name="StartMail.com">

	<target host="startmail.com" />
	<target host="*.startmail.com" />


	<securecookie host="^www\.startmail\.com$" name=".+" />


	<rule from="^http://((?:beta|live|support|www)\.)?startmail\.com/"
		to="https://$1startmail.com/" />

</ruleset>
