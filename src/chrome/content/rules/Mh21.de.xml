<!--
	Nonfunctional hosts in *mh21.de:

		- mail *
		- owncloud *

	* 404


	www.mh21.de: 404

-->
<ruleset name="mh21.de (partial)">

	<!--	Direct rewrites:
				-->
	<target host="mh21.de" />

	<!--	Complications:
				-->
	<target host="www.mh21.de" />


	<rule from="^http://www\.mh21\.de/"
		to="https://mh21.de/" />

	<rule from="^http:"
		to="https:" />

</ruleset>
