<!--
	Fully covered domains:

		- gfycat.com

		- *.gfycat.com:

			- zippy

-->
<ruleset name="Gfycat.com">

	<target host="gfycat.com" />
	<target host="*.gfycat.com" />


	<securecookie host="^\.gfycat\.com$" name=".+" />


	<rule from="^http://([^/:@\.]+\.)?gfycat\.com/"
		to="https://$1gfycat.com/" />

</ruleset>
