<!--
	Server sends no certificate chain *

	* See https://whatsmychaincert.com

-->
<ruleset name="waymoot.org" default_off="missing certificate chain">

	<!--	Direct rewrites:
				-->
	<target host="waymoot.org" />
	<target host="www.waymoot.org" />


	<rule from="^http:"
		to="https:" />

</ruleset>
