<ruleset name="JBox.com">

	<target host="jbox.com" />
	<target host="www.jbox.com" />


	<!--	Not secured by server:
					-->
	<!--securecookie host="^\.jbox\.com$" name="^frontend$" /-->

	<securecookie host="^\w" name="." />
	<securecookie host="^\." name="^frontend$" />


	<rule from="^http:"
		to="https:" />

</ruleset>
