<!--
	www.nycmesh.net: Mismatched

-->
<ruleset name="NYC Mesh.net">

	<!--	Direct rewrites:
				-->
	<target host="nycmesh.net" />

	<!--	Complications:
				-->
	<target host="www.nycmesh.net" />


	<securecookie host="^\w" name=".+" />


	<rule from="^http://www\.nycmesh\.net/"
		to="https://nycmesh.net/" />

	<rule from="^http:"
		to="https:" />

</ruleset>
