<ruleset name="Bodybuilding.com">
	<!--
		*.bbcomcdn.com

		Invalid certificate:
			- trackers.bbcomcdn.com
	-->
	<target host="artifacts.bbcomcdn.com" />
	<target host="common.bbcomcdn.com" />
	<target host="store.bbcomcdn.com" />

	<!--
		*.bodybuilding.com

		Invalid certificate:
			- cschat.bodybuilding.com
			- n.bodybuilding.com: equivalent to bodybuilding-com.112.2o7.net
	-->
	<target host="bodybuilding.com" />
	<target host="*.bodybuilding.com" />
		<test url="http://www.bodybuilding.com/" />
		<test url="http://api.bodybuilding.com/" />
		<test url="http://assets.bodybuilding.com/" />
		<test url="http://bodyspace.bodybuilding.com/" />
		<test url="http://ca.bodybuilding.com/" />
		<test url="http://forum.bodybuilding.com/" />
		<test url="http://imagecdn.bodybuilding.com/" />
		<test url="http://marketing.bodybuilding.com/" />
		<test url="http://my.bodybuilding.com/" />
		<test url="http://n.bodybuilding.com/" />
		<test url="http://reviews.bodybuilding.com/" />
		<test url="http://s.bodybuilding.com/" />
	
	<rule from="^http://n\.bodybuilding\.com/"
		  to="https://bodybuilding-com.112.2o7.net/" />

	<rule from="^http:" to="https:" />
</ruleset>
