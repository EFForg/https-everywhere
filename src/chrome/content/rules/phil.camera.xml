<ruleset name="phil.camera">
	<target host=    "phil.camera" />
	<target host="www.phil.camera" />

  	<securecookie host="^phil\.camera$" name=".+" />

	<!-- cert only matches www -->
	<rule from="^http://www\.phil\.camera/"
		to="https://phil.camera/" />

	<rule from="^http:"
		to="https:" />
</ruleset>
