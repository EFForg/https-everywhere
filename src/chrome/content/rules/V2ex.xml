<!--

	Other v2ex rulesets:

		- V2ex.co.xml


	Fully covered subdomains:

		- (www.)
		- cdn
		- livid
		- workspace
	

	Mismatch:
	
		- openssl
		- pypi
		- ubuntu


	Insecure cookies are set for these hosts:

		- v2ex.com
		- workspace.v2ex.com
		- www.v2ex.com


	These altnames don't exist:

		- www.cdn.v2ex.com
		- www.workspace.v2ex.com

-->

<ruleset name="v2ex.com (partial)">

	<target host="v2ex.com" />
	<target host="cdn.v2ex.com" />
	<target host="cn.v2ex.com" />
	<target host="fast.v2ex.com" />
	<target host="livid.v2ex.com" />
	<target host="sjc.v2ex.com" />
	<target host="workspace.v2ex.com" />
	<target host="www.v2ex.com" />


	<securecookie host="^(?:workspace\.|www\.)?v2ex\.com$" name=".+" />


	<rule from="^http:" to="https:" />

</ruleset>
