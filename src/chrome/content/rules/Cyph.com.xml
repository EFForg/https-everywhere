<!--
	Other Cyph rulesets:

		- Cyphdbyhiddenbhs.onion.xml


	Insecure cookies are set for these domains:

		- .cyph.com

-->
<ruleset name="Cyph.com">

	<!--	Direct rewrites:
				-->
	<target host="cyph.com" />
	<target host="api.cyph.com" />
	<target host="www.cyph.com" />


	<!--	CloudFlare cookies:
					-->
	<!--securecookie host="^\.cyph\.com$" name="^(__cfduid|cf_clearance)$" /-->

	<securecookie host="^\.cyph\.com$" name=".+" />


	<rule from="^http:"
		to="https:" />

</ruleset>
