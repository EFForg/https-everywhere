<!--
	See TNS-Global.xml for other TNS coverage.

-->
<ruleset name="TNS Info">

	<target host="tnsinfo.com" />
	<target host="*.tnsinfo.com" />


	<securecookie host="^(?:.*\.)?tnsinfo\.com$" name=".*" />


	<!--	Cert doesn't match !www.	-->
	<rule from="^http://(?:www\.)?tnsinfo\.com/"
		to="https://www.tnsinfo.com/" />

</ruleset>
