<ruleset name="DNSSEC-Tools">

	<target host="dnssec-tools.org" />
	<target host="www.dnssec-tools.org" />


	<securecookie host="^(?:www\.)?dnssec-tools\.org$" name=".*" />


	<rule from="^http://(www\.)?dnssec-tools\.org/"
		to="https://$1dnssec-tools.org/" />

</ruleset>
