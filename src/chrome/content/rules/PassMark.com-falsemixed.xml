<!--
	For rules not causing false/broken MCB, see PassMark.com.xml.

-->
<ruleset name="PassMark.com (false MCB)" platform="mixedcontent">

	<target host="passmark.com" />
	<target host="*.passmark.com" />


	<securecookie host="^www\.passmark\.com$" name=".+" />


	<rule from="^http://(www\.)?passmark\.com/"
		to="https://$1passmark.com/" />

</ruleset>
