<!--
	SpaceWeb


	vip-35.sweb.ru: self-signed


	Nonfunctional subdomains:

		- live	(times out)


	These altnames don't exist:

		- shared.sweb.ru

-->
<ruleset name="SWeb.ru">

	<target host="sweb.ru"/>
	<target host="*.sweb.ru"/>


	<securecookie host="^\.sweb\.ru$" name=".*" />


	<rule from="^http://(cp\.|www\.)?sweb\.ru/"
		to="https://$1sweb.ru/" />

</ruleset>
