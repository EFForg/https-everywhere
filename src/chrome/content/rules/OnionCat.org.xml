<!--
	^: Loops

-->
<ruleset name="OnionCat.org" platform="cacert">

	<!--	Direct rewrites:
				-->
	<target host="www.onioncat.org" />

	<!--	Complications:
				-->
	<target host="onioncat.org" />


	<rule from="^http://onioncat\.org/"
		to="https://www.onioncat.org/" />

	<rule from="^http:"
		to="https:" />

</ruleset>
