<ruleset name="CyberGhost">

	<target host="cyberghostvpn.com"/>
	<target host="*.cyberghostvpn.com"/>

	<!--	encountered:
			- cyberghostvpn.com
			- blog.cyberghostvpn.com	-->
	<securecookie host=".+" name=".+" />

	<!--	known matches:
			- blog
			- support
			- www		-->
	<rule from="^http://(\w+\.)?cyberghostvpn\.com/"
		to="https://$1cyberghostvpn.com/"/>

</ruleset>
