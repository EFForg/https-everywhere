<ruleset name="CyberGhost">

	<target host="cyberghostvpn.com"/>
	<target host="*.cyberghostvpn.com"/>

	<!--	encountered:
			- cyberghostvpn.com
			- blog.cyberghostvpn.com	-->
	<securecookie host="^(?:.*\.)?cyberghostvpn\.com$" name=".+" />

	<!--	known matches:
			- blog
			- support
			- www		-->
	<rule from="^http:" to="https:" />

</ruleset>
