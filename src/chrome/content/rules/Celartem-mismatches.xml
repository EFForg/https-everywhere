<ruleset name="Celartem (mismatches)" default_off="expired, self-signed">

	<target host="celartem.com"/>
	<target host="tokage.celartem.com"/>
	<target host="www.celartem.com"/>

	<rule from="^http://celartem\.com/"
		to="https://www.celartem.com/"/>

	<rule from="^http:"
		to="https:" />

</ruleset>
