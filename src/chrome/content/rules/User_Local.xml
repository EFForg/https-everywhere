<!--

	Nonfunctional domains:

		- awasete	*
		- bsap	*
		- fm	*
		- l12	*
		- le	*
		- ap	**
		- b	**
		- f1	**
		- f2	**
		- l	**
		- rt	**
		- s1	**
		- ui	**
		- www2	**
		- rd	⁴

	* refused
	** times out
	⁴ 404

	Problematic subdomains:

		- b[01]\d	(no https)
		- c	(invalid)
		- c(\d+) 	(invalid)

-->
<ruleset name="User Local (partial)">

	<target host="*.nakanohito.jp" />

	<securecookie host="^\.nakanohito\.jp$" name="^AP$" />

	<rule from="^http://b0\d\.nakanohito\.jp/"
		to="https://bs.nakanohito.jp/" />

		<test url="http://b01.nakanohito.jp/" />

	<rule from="^http://(bs|cs|s|ua|uh|y)\.nakanohito\.jp/"
		to="https://$1.nakanohito.jp/" />

		<test url="http://bs.nakanohito.jp/" />
		<test url="http://cs.nakanohito.jp/" />
		<test url="http://s.nakanohito.jp/" />
		<test url="http://ua.nakanohito.jp/" />
		<test url="http://uh.nakanohito.jp/uhj2/uh.js" />
		<test url="http://y.nakanohito.jp/" />

</ruleset>
