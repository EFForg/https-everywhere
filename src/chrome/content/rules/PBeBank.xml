<ruleset name="PBeBank">
	<target host=     "pbebank.com" />
	<target host= "www.pbebank.com" />

	<test url="http://www.pbebank.com/" />

  	<securecookie host="^.*\.pbebank\.com$" name=".*" />

	<rule from="^http://pbebank\.com/"
		to="https://www.pbebank.com/" />
	<rule from="^http:" 
		to="https:" />
</ruleset>
