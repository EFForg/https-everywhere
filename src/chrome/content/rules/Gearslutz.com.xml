<!--
	These altnames don't exist:

		- www.static.gearslutz.com


	Fully covered subdomains:

		- (www.)?
		- static

-->
<ruleset name="Gearslutz.com">

	<target host="gearslutz.com" />
	<target host="*.gearslutz.com" />


	<rule from="^http://(static\.|www\.)?gearslutz\.com/"
		to="https://$1gearslutz.com/" />

</ruleset>
