	<test url="http://asv.nuggad.net/" />
	<test url="http://ci.nuggad.net/ci" />
	<test url="http://gwp.nuggad.net/" />
	<test url="http://ip-s.nuggad.net/" />
	<test url="http://mtm.nuggad.net/" />
	<test url="http://poldk.nuggad.net/rc" />
