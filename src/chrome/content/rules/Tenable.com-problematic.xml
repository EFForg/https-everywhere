<!--
	For rules that are on by default, see Tenable.com.xml.


	Server sends no certificate chain *

	* See https://whatsmychaincert.com

-->
<ruleset name="Tenable.com (missing certificate chain)" default_off="missing certificate chain">

	<!--	Direct rewrites:
				-->
	<target host="tenable.com" />
	<target host="www.tenable.com" />


	<rule from="^http:"
		to="https:" />

</ruleset>
