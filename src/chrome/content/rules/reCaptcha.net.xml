<ruleset name="reCaptcha.net">
	<target host="recaptcha.net" />
	<target host="www.recaptcha.net" />
	<target host="admin.recaptcha.net" />
	<target host="api.recaptcha.net" />
	<target host="api-secure.recaptcha.net" />
