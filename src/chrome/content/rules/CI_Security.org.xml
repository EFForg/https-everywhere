<!--
	Fully covered subdomains:

		- (www.)?
		- alliance
		- benchmarks
		- community
		- iic
		- msisac


	These altnames don't exist:

		- www.blog.cisecurity.org

-->
<ruleset name="CI Security.org">

	<target host="cisecurity.org" />
	<target host="*.cisecurity.org" />


	<rule from="^http://((?:alliance|benchmarks|community|iic|msisac|www)\.)?cisecurity\.org/"
		to="https://$1cisecurity.org/" />

</ruleset>
