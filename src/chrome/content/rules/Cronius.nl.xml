<ruleset name="Cronius.nl">

	<target host="cronius.nl" />
	<target host="*.cronius.nl" />


	<securecookie host="^\.cronius\.nl$" name=".+" />


	<rule from="^http://(www\.)?cronius\.nl/"
		to="https://$1cronius.nl/" />

</ruleset>
