<ruleset name="hackerschool.com">

	<target host="hackerschool.com"/>
	<target host="*.hackerschool.com"/>

	<securecookie host=".+" name=".+"/>

	<!--	cert from redirector.zerigo.net	-->
	<rule from="^http://hackerschool\.com/"
		to="https://www.hackerschool.com/"/>

	<rule from="^http://(\w+)\.hackerschool\.com/"
		to="https://$1.hackerschool.com/"/>

</ruleset>
