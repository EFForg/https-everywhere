<ruleset name="Kryptronic (partial)">

	<target host="kryptronic.com"/>
	<target host="*.kryptronic.com"/>
		<exclusion pattern="^http://(?:forum|wiki)\."/>

	<securecookie host="^(?:.*\.)?kryptronic\.com$" name=".*"/>

	<rule from="^http://(\w+\.)?kryptronic\.com/"
		to="https://$1kryptronic.com/"/>

</ruleset>
