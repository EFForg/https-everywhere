<ruleset name="CICLOPS" default_off="expired, self-signed">

	<target host="ciclops.org"/>
	<target host="www.ciclops.org"/>

	<securecookie host="^(?:.*\.)ciclops\.org$" name=".*"/>

	<!--	cert !match !www	-->
	<rule from="^http://(?:www\.)?ciclops\.org/"
		to="https://www.ciclops.org/"/>

</ruleset>
