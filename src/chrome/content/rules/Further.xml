<!--
	Problematic subdomains:

		- ^	(reset)

-->
<ruleset name="Further">

	<target host="further.co.uk" />
	<target host="*.further.co.uk" />


	<securecookie host="^(?:www)?\.further\.co\.uk$" name=".+" />


	<rule from="^http://(?:www\.)?further\.co\.uk/"
		to="https://www.further.co.uk/" />

</ruleset>
