<!--
	Insecure cookies are set for these domains:

		- .arlen.io

-->
<ruleset name="Arlen.io">

	<target host="arlen.io" />
	<target host="www.arlen.io" />

	<target host="*.arlen.io" />


	<!--	CloudFlare cookies:
					-->
	<!--securecookie host="^\.arlen\.io$" name="^(__cfduid|cf_clearance)$" /-->

	<securecookie host="^\.arlen\.io$" name=".+" />


	<rule from="^http://(www\.)?arlen\.io/"
		to="https://$1arlen.io/" />

</ruleset>
