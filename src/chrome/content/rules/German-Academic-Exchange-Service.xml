<ruleset name="German Academic Exchange Service">

	<target host="daad.de" />
	<target host="ssl.daad.de" />
	<target host="www.daad.de" />


	<securecookie host="^ssl\.daad\.de$" name=".+" />


	<!--	Cert only matches ssl.	-->
	<rule from="^http://(?:(?:ssl|www)\.)?daad\.de/"
		to="https://ssl.daad.de/" />

</ruleset>
