<!--
	Invalid Certificate:
		ip.gitcdn.link
-->
<ruleset name="GitCDN.link">
	<target host="gitcdn.link" />
	<target host="www.gitcdn.link" />
	<target host="cdn.gitcdn.link" />
	<target host="min.gitcdn.link" />

	<securecookie host=".+" name=".+" />

	<rule from="^http:" to="https:" />
</ruleset>
