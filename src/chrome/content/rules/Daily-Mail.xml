<ruleset name="Daily Mail (partial)" default_off="Akamai certificate">

	<target host="f.dailymail.co.uk" />
	<target host="i.dailymail.co.uk" />
	<target host="img.dailymail.co.uk" />
	<target host="scripts.dailymail.co.uk" />


	<rule from="^http://img\.dailymail\.co\.uk/"
		to="https://i.dailymail.co.uk/" />

	<rule from="^http://(f|i|scripts)\.dailymail\.co\.uk/"
		to="https://$1.dailymail.co.uk/" />
</ruleset>
