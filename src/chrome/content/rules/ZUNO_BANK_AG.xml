<ruleset name="ZUNO_BANK_AG">
  <target host="zuno.eu" />
  <target host="www.zuno.eu" />
  <target host="zuno.cz" />
  <target host="www.zuno.cz" />
  <target host="zuno.sk" />
  <target host="www.zuno.sk" />

  <target host="moje.zuno.sk" />
  <target host="moje.zuno.cz" />

  <rule from="^http://(?:www\.)?zuno\.eu/" to="https://www.zuno.eu/" />
  <rule from="^http://(?:www\.)?zuno\.cz/" to="https://www.zuno.cz/" />
  <rule from="^http://(?:www\.)?zuno\.sk/" to="https://www.zuno.sk/" />

  <rule from="^http:" to="https:" />
</ruleset>
