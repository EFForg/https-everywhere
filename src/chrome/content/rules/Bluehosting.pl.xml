<!--
	Problematic domains:

		- (www.)bluehosting.pl	(works; mismatched, CN: *.bho.pl)

-->
<ruleset name="bluehosting.pl (partial)">

	<target host="bho.pl" />
	<target host="*.bho.pl" />


	<securecookie host="^.+\.bho\.pl$" name=".+" />


	<rule from="^http://([\w-]+\.)?bho\.pl/"
		to="https://$1bho.pl/" />

</ruleset>
