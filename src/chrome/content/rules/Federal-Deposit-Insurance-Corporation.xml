<ruleset name="Federal Deposit Insurance Corporation">

	<target host="fdic.gov" />
	<target host="*.fdic.gov" />


	<securecookie host="^.*\.fdic\.gov$" name=".*" />


	<!--	!www: Cert only matches www.
						-->
	<rule from="^http://(?:www(2)?\.)?fdic\.gov/"
		to="https://www$1.fdic.gov/" />

</ruleset>
