<!--
	Nonfunctional domains:

		- (www.)sungardsummit.com	(shows sunguardhe data)

-->
<ruleset name="SunGuard Higher Education" default_off="self-signed">

	<target host="sungardhe.com" />
	<target host="www.sungardhe.com" />


	<securecookie host="^sungardhe\.com$" name=".*" />


	<rule from="^http://(?:www\.)?sungardhe\.com/"
		to="https://sungardhe.com/" />

</ruleset>
