<!--
	Server sends no certificate chain *

	* See https://whatsmychaincert.com


	Nonfunctional domains:

		- news.bofh.it


	^bofh.it doesn't exist.

-->
<ruleset name="bofh.it" platform="cacert" default_off="missing certificate chain">

	<!--	Direct rewrites:
				-->
	<target host="ml.azzurra.org" />

	<target host="blog.bofh.it" />
	<target host="censura.bofh.it" />
	<target host="lists.bofh.it" />
	<target host="www.bofh.it" />


	<rule from="^http:"
		to="https:" />

</ruleset>
