<!--
	www.bitkonan.com: Mismatched

-->
<ruleset name="Bitkonan.com">

	<!--	Direct rewrites:
				-->
	<target host="bitkonan.com" />

	<!--	Complications:
				-->
	<target host="www.bitkonan.com" />


	<rule from="^http://www\.bitkonan\.com/"
		to="https://bitkonan.com/" />

	<rule from="^http:"
		to="https:" />

</ruleset>
