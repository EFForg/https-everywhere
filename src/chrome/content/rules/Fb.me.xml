<!--
	For other Facebook coverage, see Facebook.xml.

-->
<ruleset name="Fb.me">

	<target host="fb.me" />
	<target host="*.fb.me" />

	<test url="http://www.fb.me/" />
	<test url="http://test.fb.me/" />
	
	<!-- in Bitly_branded_short_domains.xml -->
	<exclusion pattern="^http://on\.fb\.me/" />
	<test url="http://on.fb.me/" />

	<rule from="^http:" to="https:" />

</ruleset>
