<!--
	For rules not causing false/broken MCB, see Vidup.me.xml.

-->
<ruleset name="Vidup.me (fales MCB)" platform="mixedcontent">

	<target host="vidup.me" />
	<target host="*.vidup.me" />


	<securecookie host="^\.vidup\.me$" name=".+" />


	<rule from="^http://(www\.)?vidup\.me/"
		to="https://$1vidup.me/" />

</ruleset>
