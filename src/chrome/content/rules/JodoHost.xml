	<target host="lcsrv1.myhsphere.biz" />


	<securecookie host="^(?:www\.)?support\.jodohost\.com$" name=".+" />
	<securecookie host="^lcsrv1\.myhsphere\.biz$" name=".+" />


	<rule from="^http:" to="https:" />

</ruleset>
