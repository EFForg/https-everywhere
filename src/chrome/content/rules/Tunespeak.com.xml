<ruleset name="Tunespeak.com">

	<target host="stagingspeak.com" />
	<target host="*.stagingspeak.com" />
	<target host="tunespeak.com" />
	<target host="*.tunespeak.com" />


	<securecookie host="^\.(?:staging|tune)speak\.com$" name=".+" />


	<rule from="^http://(www\.)?(staging|tune)speak\.com/"
		to="https://$1$2speak.com/" />

</ruleset>
