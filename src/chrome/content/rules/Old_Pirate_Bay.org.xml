<!--
	Redirect:
		- ^
-->
<ruleset name="Old Pirate Bay.org">
	<target host="oldpiratebay.org" />
	<target host="www.oldpiratebay.org" />

	<securecookie host="^\.oldpiratebay\.org$" name="^__cfduid$" />

	<rule from="^http://oldpiratebay\.org/"
		to="https://www.oldpiratebay.org/" />
	<rule from="^http:"
		to="https:" />
</ruleset>
