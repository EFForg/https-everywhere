<!--
	Nonfunctional hosts in *.eldarya.hu:

	h: http redirect
	m: certificate mismatch
	r: connection refused
	s: self-signed certificate
	t: timeout on https
-->
<ruleset name="Eldarya.hu">
	<target host="eldarya.hu" />
	<target host="www.eldarya.hu" />

	<rule from="^http:" to="https:" />
</ruleset>
