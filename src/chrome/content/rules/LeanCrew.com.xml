<!--
	Invalid certificate:
		- www
		- admin
		- eww
		- mail
		- vvv
		- vwww
		- wew
		- wqw
		- wvw
		- ww2
-->
<ruleset name="LeanCrew.com">
	<target host="leancrew.com" />
	<target host="www.leancrew.com" />

	<rule from="^http://www\.leancrew\.com/" to="https://leancrew.com/" />
	<rule from="^http:" to="https:" />
</ruleset>
