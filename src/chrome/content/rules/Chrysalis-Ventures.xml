<ruleset name="Chrysalis-Ventures (partial)" default_off="mismatched">

	<!--	Cert: *.gridserver.com	-->
	<target host="chrysalisventures.com" />
	<target host="www.chrysalisventures.com" />


	<!--	At least the homepage and
		portfolio/ redirect improperly.

		Neither !www nor www redirect to the other, but
		to keep the number of exceptions needed minimal...	-->
