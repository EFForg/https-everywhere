<ruleset name="Victorinox">
    <target host="victorinox.com" />
    <target host="*.victorinox.com" />

    <rule from="^http://victorinox\.com/"
        to="https://www.victorinox.com/" />
    <rule from="^http://(images|static|www)\.victorinox\.com/"
        to="https://$1.victorinox.com/" />
</ruleset>
