<!--
This ruleset is experimental. If you experience problems please
open an issue at https://github.com/kargig/https-everywhere
-->

<ruleset name="gnu.gr">
  <target host="gnu.gr" />
  <target host="www.gnu.gr" />

  <securecookie host="^(?:www\.)?gnu\.gr$" name=".*"/>

  <rule from="^http://(?:www\.)?gnu\.gr/" to="https://gnu.gr/"/>
</ruleset>
