<ruleset name="Pretty in Cash (mismatches)" default_off="mismatched">

	<!--	exgf.teenbff.com	-->
	<target host="join.boozedgfs.com"/>
	<target host="join.exgf.com"/>
	<target host="prettyincash.com"/>
	<target host="www.prettyincash.com"/>
	<target host="discount.teengfs.com"/>

	<rule from="^http://join\.(boozedgfs|exgf)\.com/"
		to="https://join.$1.com/"/>

	<rule from="^http://(?:www\.)?prettyincash\.com/"
		to="https://prettyincash.com/"/>

	<rule from="^http://discount\.teengfs\.com/"
		to="https://discount.teengfs.com/"/>

</ruleset>
