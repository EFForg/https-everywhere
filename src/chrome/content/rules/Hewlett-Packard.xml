	<!--securecookie host="^ssl\.www8\.hp\.com$" name="^JSESSIONID$" /-->

	<!--
		Stats cookie set by met1.hp.com
						-->
	<securecookie host="^\.hp\.com$" name="^s_vi$" />
	<securecookie host="^(community\.dev|(www\.)?register|h2(0141|2166)\.www2|h30(434|499|507)\.www3|h41183\.www4|h71016\.www7|ssl\.www8)\.hp\.com$" name=".+" />


	<rule from="^http://hp\.com/"
		to="https://hp.com/" />

	<rule from="^http://([^/:@]+)\.(austin|europe|www[1-7])\.hp\.com/"
		to="https://$1.austin.hp.com/" />

	<rule from="^http://(ssl\.)?www8\.hp\.com/"
		to="https://ssl.www8.hp.com/" />
		<test url="http://www8.hp.com/" />
		<test url="http://ssl.www8.hp.com/" />
		
	<rule from="^http://([^/:@.]+|community\.dev)\.hp\.com/"
		to="https://$1.hp.com/" />

	<rule from="^http://shopping1?\.hp\.com/"
		to="https://$1.hp.com/" />
</ruleset>
