<ruleset name="Veuwer">
	<target host=    "veuwer.com" />
	<target host="www.veuwer.com" />

  	<securecookie host="^veuwer\.com$" name=".*" />

	<!-- follow server's behaviour -->
	<rule from="^http://www\.veuwer\.com/"
		to="https://veuwer.com/" />

		<test url="http://veuwer.com/" />

	<rule from="^http:"
		to="https:" />
</ruleset>
