<!--
	Invalid certificate:
		www.logout.hu

	Time out:
		r6.logout.hu

-->
<ruleset name="Logout.hu">
	<target host="logout.hu" />
	<target host="www.logout.hu" />
	<target host="m.logout.hu" />

	<rule from="^http://www\.logout\.hu/" to="https://logout.hu/" />
	<rule from="^http:" to="https:" />
</ruleset>

