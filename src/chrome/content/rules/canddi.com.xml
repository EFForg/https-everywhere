<ruleset name="CANDDi.com (partial)">

	<!--	Direct rewrites:
				-->
	<target host="i.canddi.com" />
	<target host="www.canddi.com" />

	<!--	Complications:
				-->
	<target host="canddi.com" />


	<securecookie host="^\w" name=".+" />


	<rule from="^http://canddi\.com/"
		to="https://www.canddi.com/" />

	<rule from="^http:"
		to="https:" />

</ruleset>
