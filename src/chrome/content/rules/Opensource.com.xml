<ruleset name="opensource.com">

	<target host="opensource.com" />
	<target host="*.opensource.com" />


	<securecookie host="^\.opensource\.com$" name=".+" />


	<rule from="^http://(www\.)?opensource\.com/"
		to="https://$1opensource.com/" />

</ruleset>
