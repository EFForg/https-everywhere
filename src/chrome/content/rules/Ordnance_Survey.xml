<ruleset name="Ordnance Survey">
    <target host="ordnancesurvey.co.uk" />
    <target host="*.ordnancesurvey.co.uk" />

    <rule from="^http://ordnancesurvey\.co\.uk/"
        to="https://www.ordnancesurvey.co.uk/"/>
    <rule from="^http://([^/:@]+)?\.ordnancesurvey\.co\.uk/"
        to="https://$1.ordnancesurvey.co.uk/" />
</ruleset>
