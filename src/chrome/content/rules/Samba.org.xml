<ruleset name="Samba.org">

	<target host="samba.org" />
	<target host="*.samba.org" />

		<test url="http://bugzilla.samba.org/" />
		<test url="http://ccache.samba.org/" />
		<test url="http://download.samba.org/" />
		<test url="http://git.samba.org/" />
		<test url="http://gitweb.samba.org/" />
		<test url="http://jcifs.samba.org/" />
		<test url="http://linux-cifs.samba.org/" />
		<test url="http://lists.samba.org/" />
		<test url="http://ppp.samba.org/" />
		<test url="http://pserver.samba.org/" />
		<test url="http://www.samba.org/" />


	<securecookie host="." name="." />


	<rule from="^http:"
		to="https:" />

</ruleset>
