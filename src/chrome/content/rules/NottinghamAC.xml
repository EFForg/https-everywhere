<!--
	www: Interrupted

-->
<ruleset name="NottinghamAC">

	<target host="nottingham.ac.uk" />
	<target host="*.nottingham.ac.uk" />


	<securecookie host="^(?:.+\.)?nottingham\.ac\.uk$" name=".*" />


	<rule from="^http://(?:www\.)?nottingham\.ac\.uk/"
		to="https://nottingham.ac.uk/" />

	<rule from="^http://(email|jobs|owa)\.nottingham\.ac\.uk/"
		to="https://$1.nottingham.ac.uk/" />

</ruleset>
