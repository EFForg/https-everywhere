<!--
    Nonfunctional subdomains:
        - www     -> wrong domain
-->
<ruleset name="0bin.net">
    <target host="0bin.net" />
    <target host="www.0bin.net" />

    <securecookie host="^0bin\.net" name=".+" />

    <rule from="^http://www\.0bin\.net/"
            to="https://0bin.net/" />

    <rule from="^http:"
            to="https:" />
</ruleset>
