<!--
	HTTP 400:
		- www

	Mismatched:
		- blogg (cdon.se)
		- ebook (cdon.se)
-->

<ruleset name="cdon.se">
	<target host="cdon.se" />
	<target host="www.cdon.se" />
	<target host="preprod.cdon.se" />

	<securecookie host=".+" name=".+" />

	<rule from="^http://www\.cdon\.se/" to="https://cdon.se/" />
	<rule from="^http:" to="https:" />
</ruleset>
