<!--
	Nonfunctional hosts in *.shaneclaiborne.com:

	h: http redirect
	m: certificate mismatch
	r: connection refused
	s: self-signed certificate
	t: timeout on https
-->
<ruleset name="Shaneclaiborne.com">
	<target host="shaneclaiborne.com" />
	<target host="www.shaneclaiborne.com" />

	<rule from="^http:" to="https:" />
</ruleset>
