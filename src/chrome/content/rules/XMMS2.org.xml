<!--
	Problematic subdomains:

		- www	(shows bugs)

-->
<ruleset name="XMMS2.org">

	<!--	Direct rewrites:
				-->
	<target host="xmms2.org" />
	<target host="bugs.xmms2.org" />

	<!--	Complications:
				-->
	<target host="www.xmms2.org" />


	<securecookie host="^bugs\.xmms2\.org$" name=".+" />


	<rule from="^http://www\.xmms2\.org/"
		to="https://xmms2.org/" />

	<rule from="^http:"
		to="https:" />

</ruleset>
