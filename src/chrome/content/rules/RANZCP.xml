<!--
	For ranzcp.org
	(Royal Australian and New Zealand College of Psychiatrists)

		Works:
			- www

		Doesn't Work:
			- $ (mismatch)

-->
<ruleset name="RANZCP">
  <target host="ranzcp.org" />
  <target host="www.ranzcp.org" />

  <rule from="^http://(?:www\.)?ranzcp\.org/"
          to="https://www.ranzcp.org/" />
</ruleset>