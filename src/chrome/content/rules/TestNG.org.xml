<!--
	Invalid certificate:
		- forum.testng.org, equivalent to testng.org
		- mail.testng.org
-->

<ruleset name="TestNG.org">
	<target host="testng.org" />
	<target host="www.testng.org" />
	<target host="forum.testng.org" />

	<rule from="^http://forum\.testng\.org/"
		to="https://testng.org/" />

	<rule from="^http:" to="https:" />
</ruleset>
