<!--
	^: cert only matches www

-->
<ruleset name="Defora.org" default_off="self-signed">

	<target host="defora.org" />
	<target host="*.defora.org" />


	<securecookie host="^\.(?:www\.)?defora\.org$" name=".+" />


	<rule from="^http://(?:www\.)?defora\.org/"
		to="https://www.defora.org/" />

</ruleset>
