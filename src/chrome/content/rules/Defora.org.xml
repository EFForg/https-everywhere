<!--
	^defora.org: Cert only matches www

-->
<ruleset name="Defora.org" default_off="untrusted root">

	<!--	Direct rewrites:
				-->
	<target host="www.defora.org" />

	<!--	Complications:
				-->
	<target host="defora.org" />


	<rule from="^http://defora\.org/"
		to="https://www.defora.org/" />

	<rule from="^http:"
		to="https:" />

</ruleset>
