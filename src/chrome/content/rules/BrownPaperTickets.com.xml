<!--
	Non-functional hosts in *.bpt.me
		Certificate mismatched:
		- www.bpt.me
-->
<ruleset name="BrownPaperTickets.com (partial)">

	<target host="bpt.me" />
	<target host="www.bpt.me" />
	<target host="community.bpt.me" />
	<target host="m.bpt.me" />
	
	<target host="brownpapertickets.com" />
	<target host="www.brownpapertickets.com" />
	<target host="community.brownpapertickets.com" />
	<target host="es.brownpapertickets.com" />
	<target host="fr.brownpapertickets.com" />
	<target host="stats.brownpapertickets.com" />

	<securecookie host=".+" name=".+" />

	<rule from="^http://www\.bpt\.me/"
		to="https://bpt.me/" />
	
	<rule from="^http:"
		to="https:" />

</ruleset>
