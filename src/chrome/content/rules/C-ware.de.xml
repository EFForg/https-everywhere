<!--
	(www.)?c-ware.de: Mismatched

-->
<ruleset name="c-ware.de">

	<!--	Direct rewrites:
				-->
	<target host="dev.c-ware.de" />

	<!--	Complications:
				-->
	<target host="c-ware.de" />
	<target host="www.c-ware.de" />


	<securecookie host="^\w" name=".+" />


	<rule from="^http://(?:www\.)?c-ware\.de/"
		to="https://dev.c-ware.de/" />

	<rule from="^http:"
		to="https:" />

</ruleset>
