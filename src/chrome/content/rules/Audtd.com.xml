<!--
	For other Auditorius coverage, see Auditorius.ru.xml.


	^: Mismatched
	www: Parked


	Insecure cookies are set for these domains:

		- .audtd.com

-->
<ruleset name="Audtd.com (partial)">

	<!--	Direct rewrites:
				-->
	<target host="sync.audtd.com" />
	<target host="sync2.audtd.com" />

	<!--	Complications:
				-->
	<target host="audtd.com" />


	<!--	Not secured by server:
					-->
	<!--securecookie host="^\.audtd\.com$" name="^(?:chk|pid)$" /-->

	<securecookie host="^\.audtd\.com$" name=".+" />


	<!--	Redirect keeps path and args:
						-->
	<rule from="^http://audtd\.com/"
		to="https://auditorius.ru/" />

	<rule from="^http:"
		to="https:" />

</ruleset>
