<!--
	www.adactio.com: Refused

-->
<ruleset name="Adactio.com">

	<!--	Direct rewrites:
				-->
	<target host="adactio.com" />

	<!--	Complications:
				-->
	<target host="www.adactio.com" />


	<rule from="^http://www\.adactio\.com/"
		to="https://adactio.com/" />

	<rule from="^http:"
		to="https:" />

</ruleset>
