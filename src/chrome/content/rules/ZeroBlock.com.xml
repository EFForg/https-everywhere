<!--
	www.zeroblock.com: Dropped

-->
<ruleset name="ZeroBlock.com">

	<!--	Direct rewrites:
				-->
	<target host="zeroblock.com" />

	<!--	Complications:
				-->
	<target host="www.zeroblock.com" />


	<securecookie host=".+" name=".+" />


	<rule from="^http://www\.zeroblock\.com/"
		to="https://zeroblock.com/" />

	<rule from="^http:"
		to="https:" />

</ruleset>
