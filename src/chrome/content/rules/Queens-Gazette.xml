<ruleset name="Queens Gazette" default_off="mismatch">

	<!--	Cert: *.our-hometown.com	-->
	<target host="qgazette.com" />
	<target host="*.qgazette.com" />


	<securecookie host="^\.qgazette\.com$" name=".*" />


	<!--	!www 301s to www.
					-->
	<rule from="^http://(?:www\.)?qgazette\.com/"
		to="https://www.qgazette.com/" />

</ruleset>
