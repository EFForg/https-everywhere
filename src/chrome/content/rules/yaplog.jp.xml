<!--

	For other GMO coverage, see GMO_Internet.xml

	Nonfunctional domains:

		- cl	(times out)
		- help	(times out)

	Problematic domains:

		- log	(invalid cert)

-->
<ruleset name="yaplog.jp">

	<target host="img.yaplog.jp" />
	<target host="static.yaplog.jp" />
	<target host="www.yaplog.jp" />

	<exclusion pattern="^http://static\.yaplog\.jp/$" />

	<test url="http://static.yaplog.jp/static/css/common/notify.css" />

	<rule from="^http:"
		to="https:" />

</ruleset>
