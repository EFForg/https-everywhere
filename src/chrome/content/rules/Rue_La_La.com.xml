<!--
	Problematic subdomains:

		- ^	(works; cert only matches www)

-->
<ruleset name="Rue La La.com">

	<target host="ruelala.com" />
	<target host="www.ruelala.com" />


	<securecookie host="^www\.ruelala\.com$" name=".+" />


	<rule from="^http://(?:www\.)?ruelala\.com/"
		to="https://www.ruelala.com/" />

</ruleset>
