<ruleset name="linutronix">

	<target host="linutronix.de"/>
	<target host="www.linutronix.de"/>
	<!--	* for cross-domain cookie	-->
	<target host="*.www.linutronix.de"/>


	<securecookie host="^(?:.*\.)?linutronix\.de$" name=".*"/>


	<rule from="^http://(www\.)?linutronix\.de/"
		to="https://$1linutronix.de/"/>

</ruleset>
