<!--
	www.ibutsu.org: Mismatched

-->
<ruleset name="ibutsu.org">

	<!--	Direct rewrites:
				-->
	<target host="ibutsu.org" />
	<target host="blog.ibutsu.org" />

	<!--	Complications:
				-->
	<target host="www.ibutsu.org" />


	<securecookie host="^\w" name=".+" />


	<rule from="^http://www\.ibutsu\.org/"
		to="https://ibutsu.org/" />

	<rule from="^http:"
		to="https:" />

</ruleset>
