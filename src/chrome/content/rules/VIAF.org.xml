<!--
	^viaf.org: Mismatched

-->
<ruleset name="VIAF.org">

	<!--	Direct rewrites:
				-->
	<target host="viaf.org" />

	<!--	Complications:
				-->
	<target host="www.viaf.org" />


	<rule from="^http://www\.viaf\.org/"
		to="https://viaf.org/" />

	<rule from="^http:"
		to="https:" />

</ruleset>
