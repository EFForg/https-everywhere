<ruleset name="KnightSwarm">

	<target host="knightswarm.com" />
	<target host="*.knightswarm.com" />


	<securecookie host="^(?:.*\.)?knightswarm\.com$" name=".*" />


	<!--	!www cert is self-signed.
					-->
	<rule from="^http://(?:(order\.)|www\.)?knightswarm\.com/"
		to="https://$1knightswarm.com/" />

</ruleset>
