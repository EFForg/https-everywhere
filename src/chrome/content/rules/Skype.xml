<!--

	For other Microsoft coverage, see Microsoft.xml.


	Nonfunctional hosts in *.skype.com:

		- beta.skype.com (r)
		- shop.skype.com (r)

	h: http redirect
	m: certificate mismatch
	r: connection refused
	s: self-signed certificate
	t: timeout on https

-->
<ruleset name="Skype.com">

	<target host="skype.com" />
	<target host="www.skype.com" />
	<target host="blogs.skype.com" />
	<target host="download.skype.com" />
	<target host="downloads.skype.com" />
	<target host="go.skype.com" />
	<target host="login.skype.com" />
	<target host="secure.skype.com" />
	<target host="static.skype.com" />
	<target host="support.skype.com" />

	<test url="http://download.skype.com/favicon.ico" />
	<test url="http://downloads.skype.com/favicon.ico" />


	<securecookie host=".+" name=".+" />


	<rule from="^http:"
		to="https:" />

</ruleset>
