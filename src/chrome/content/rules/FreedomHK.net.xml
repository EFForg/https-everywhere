<!--
	Non-functional hosts:
		SSL errors:
		- freedomhk.net
-->
<ruleset name="FreedomHK.net">
	<target host="freedomhk.net" />
	<target host="www.freedomhk.net" />
	
	<securecookie host=".+" name=".+" />
	
	<rule from="^http://freedomhk\.net/"
			to="https://www.freedomhk.net/" />
	
	<rule from="^http:" to="https:" />
</ruleset>
