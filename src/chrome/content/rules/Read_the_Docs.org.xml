<!--
	Fully covered subdomains:

		- (www.)
		- media
		- *	(per-project subdomains)

-->
<ruleset name="Read the Docs.org">

	<target host="readthedocs.org" />
	<target host="*.readthedocs.org" />


	<securecookie host="^readthedocs\.org$" name=".+" />


	<rule from="^http://([\w-]+\.)?readthedocs\.org/"
		to="https://$1readthedocs.org/" />

</ruleset>
