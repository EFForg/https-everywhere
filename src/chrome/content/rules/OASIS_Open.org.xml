<!--
	Fully covered subdomains:

		- (www.)?
		- lists

-->
<ruleset name="OASIS Open.org">

	<target host="oasis-open.org" />
	<target host="*.oasis-open.org" />


	<securecookie host="^\.oasis-open\.org$" name=".+" />


	<rule from="^http://(lists\.|www\.)?oasis-open\.org/"
		to="https://$1oasis-open.org/" />

</ruleset>
