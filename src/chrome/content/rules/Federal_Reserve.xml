<ruleset name="Federal Reserve">
    <target host="federalreserve.gov" />
    <target host="*.federalreserve.gov" />

    <rule from="^http://federalreserve\.gov/"
        to="https://www.federalreserve.gov/"/>
    <rule from="^http://([^/:@]+)?\.federalreserve\.gov/"
        to="https://$1.federalreserve.gov/" />
</ruleset>
