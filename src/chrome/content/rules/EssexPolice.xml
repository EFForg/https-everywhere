<!--
  For other UK Police rules, see PoliceUK.xml

  Works:
    - www

  Mismatch:
    - $
-->

<ruleset name="Essex Police">
  <target host="essex.police.uk" />
  <target host="www.essex.police.uk" />

  <rule from="^http://(www\.)?essex\.police\.uk/" to="https://www.essex.police.uk/" />
</ruleset>
