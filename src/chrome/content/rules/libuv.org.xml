<!--
	Non-functional hosts
		Couldn't connect to server:
		- logs.libuv.org

		SSL peer certificate was not OK:
		- www.libuv.org
		- docs.libuv.org
-->
<ruleset name="libuv.org">
	<target host="libuv.org" />
	<target host="www.libuv.org" />
	<target host="dist.libuv.org" />

	<rule from="^http://www\.libuv\.org/"
		to="https://libuv.org/" />

	<rule from="^http:"
		to="https:" />
</ruleset>
