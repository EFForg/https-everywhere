<!--

	Certificate only has entry for www., not the default domain.
	
	Nonfunctional domains:
		- jobboerse (no response)
		- d (no response)
		- studiengangstest (self signed)

-->
<ruleset name="Th-nuernberg.de">
    <target host="th-nuernberg.de"/>
    <target host="www.th-nuernberg.de"/>
    <target host="fachschaft.mb.th-nuernberg.de"/>
    <target host="fachschaft.in.th-nuernberg.de"/>
    <target host="fachschaft.bw.th-nuernberg.de"/>
    <target host="fachschaft.efi.th-nuernberg.de"/>
    <target host="fachschaften.th-nuernberg.de"/>
    <target host="www.in.th-nuernberg.de"/>
    <target host="seeyou.th-nuernberg.de"/>
    <target host="asta.th-nuernberg.de"/>
    <target host="blendedlearning.th-nuernberg.de"/>
    
    <rule from="^http://(www\.)?th-nuernberg\.de/" to="https://www.th-nuernberg.de/"/>
    <rule from="^http:" to="https:" />
</ruleset>
