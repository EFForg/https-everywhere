	Invalid certificate:
		heka.barvaux.org
		mail.barvaux.org
		mout.barvaux.org

-->
<ruleset name="Barvaux.org" default_off="untrusted root">

	<target host="barvaux.org" />
	<target host="www.barvaux.org" />
	<target host="photos.barvaux.org" />
	<target host="webmail.barvaux.org" />

	<rule from="^http:"
		to="https:" />

</ruleset>
