<!--
	Other sovrn rulesets:

		- Lijit.com.xml

-->
<ruleset name="sovrn.com">

	<target host="sovrn.com" />
	<target host="meridian.sovrn.com" />
	<target host="www.sovrn.com" />


	<securecookie host="^\." name="(?:^_gat?$|^_gat_|^mp_|mixpanel$|^optimizely)" />
	<securecookie host="^\w" name=".+" />


	<rule from="^http:"
		to="https:" />

</ruleset>
