<!--
	Mismatch:
		- www	(equal to ^)
-->

<ruleset name="yotuku.cn">

	<target host="yotuku.cn" />
	<target host="www.yotuku.cn" />

	<rule from="^http://(www\.)?yotuku\.cn/" to="https://yotuku.cn/" />

		<test url="http://www.yotuku.cn/#" />
		<test url="http://yotuku.cn/#!/uploads" />

</ruleset>
