<!--
	Mismatch:
		- www	(equal to ^)
-->
<ruleset name="yotuku.cn">

	<target host="yotuku.cn" />
	<target host="www.yotuku.cn" />
	
	<rule from="^http://(www\.)?yotuku\.com/" to="https://yotuku.com/" />

		<test url="http://yotuku.com/" />
		<test url="http://www.yotuku.com/" />
		<test url="http://yotuku.cn/#!/uploads" />
</ruleset>
