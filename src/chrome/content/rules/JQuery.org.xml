<ruleset name="jQuery.org (partial)">

	<target host="jquery.org" />
	<target host="*.jquery.org" />


	<rule from="^http://(?:(contribute\.|irc\.)|www\.)?jquery\.org/"
		to="https://$1jquery.org/" />

</ruleset>
