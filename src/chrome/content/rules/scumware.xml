<!--
	Non-functional hostnames:
		- scumware.org : certificate is for home.pl
-->
<ruleset name="Scumware.org">
	<target host="scumware.org" />
	<target host="www.scumware.org" />
	
	<rule from="^http://scumware\.org/"
		  to="https://www.scumware.org/" />
	
	<rule from="^http:" to="https:" />
</ruleset>
