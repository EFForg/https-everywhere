<!--
	For other Telegram coverage, see Telegram.me.xml.

	Mismatch:
		ip
		data
		www
-->
<ruleset name="Telegram Wiki">
	<target host="telegram.wiki" />
	<target host="www.telegram.wiki" />

	<rule from="^http://www\.telegram\.wiki/"
		to="https://telegram.wiki/" />
	<rule from="^http:"
		to="https:" />
</ruleset>
