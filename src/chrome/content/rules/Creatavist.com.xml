<!--
	Nonfunctional subdomains:

		- blog *

	* Tumblr


	Fully covered subdomains:

		- (www.)?
		- tcsm

-->
<ruleset name="Creatavist.com (partial)">

	<target host="creatavist.com" />
	<target host="*.creatavist.com" />


	<rule from="^http://(tcsm\.|www\.)?creatavist\.com/"
		to="https://$1creatavist.com/" />

</ruleset>
