<!--
	For other Hertz coverage, see Hertz-US.xml


	Non-functional subdomain:
		- hertz.ca		(connection error)
		- car-rental	(connection error)
-->
<ruleset name="Hertz Canda">
	<target host="hertz.ca" />
	<target host="www.hertz.ca" />
	<target host="fr.hertz.ca" />

	<!-- connection error -->
	<rule from="^http://hertz\.ca/"
		to="https://www.hertz.ca/" />

	<rule from="^http:"
		to="https:" />
</ruleset>
