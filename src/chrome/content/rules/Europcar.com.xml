<!--
	Other related rulesets:
		- Click4Wheels.cn.xml
		- Click4Wheels.co.kr.xml
		- Click4Wheels.com.xml
		- Click4Wheels.jp.xml
		- Europcar.be.xml
		- Europcar.co.nz.xml
		- Europcar.co.uk.xml
		- Europcar.com.au.xml
		- Europcar.de.xml
		- Europcar.es.xml
		- Europcar.fr.xml
		- Europcar.it.xml
		- Europcar.pt.xml
		- Europcar-Group.com.xml


	Non-functional subdomains:
		- api	(t)
		- sandbox.api	(t)
		- applications-bck	(f)
		- applications-pp	(f)
		- applications-ptn	(f)
		- applications-rls	(f)
		- applications-tst	(f)
		- faq-test	(m)
		- roadtrip	(m)
		- service	(i)
		- sf-ptn	(t)
		- (www.)stats	(m)

	e: expired certificate
	f: ssl connection error
	h: http redirect
	i: invalid certificate chain
	m: certificate mismatch
	r: connection refused
	s: self-signed certificate
	t: timeout on https
-->
