<!--
	For RawGit.com

		Works:

			- $
			- cdn

		Doesn't Work:
			- www (mismatch)
-->
<ruleset name="RawGit">
	<target host="rawgit.com" />
	<target host="cdn.rawgit.com" />
	<target host="www.rawgit.com" />

	<rule from="^http://www\.rawgit\.com/"
          	to="https://rawgit.com/" />

	<rule from="^http:"
			to="https:" />
</ruleset>