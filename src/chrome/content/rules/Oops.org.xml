<!--
	Fully covered domains:

		- oops.org

		- [\w-]+.oops.org:

			- cdn
			- annyung
			- devel
			- ftp
			- mirror
			- pear

-->
<ruleset name="Oops.org">

	<target host="oops.org" />
	<target host="*.oops.org" />


	<rule from="^http://([\w-]+\.)?oops\.org/"
		to="https://$1oops.org/" />

</ruleset>
