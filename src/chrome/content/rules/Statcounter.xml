<ruleset name="Statcounter">
  <target host="statcounter.com" />
  <target host="*.statcounter.com" />
  <exclusion pattern="^http://gs\.statcounter\.com/" />
<!-- cert warning -->
<!--  <target host="www.statcounter.com" /> -->

	<securecookie host="^(.*\.)?statcounter\.com$" name=".*"/>

  <rule from="^http://(?:www\.)?statcounter\.com/" to="https://statcounter.com/"/>
  <rule from="^http://([^/:@\.]+)\.statcounter\.com/" to="https://$1.statcounter.com/"/>
</ruleset>

