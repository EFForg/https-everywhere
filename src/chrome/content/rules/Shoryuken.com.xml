<!--
	Nonfunctional hosts in *.shoryuken.com:

	h: http redirect
	m: certificate mismatch
	r: connection refused
	s: self-signed certificate
	t: timeout on https
-->
<ruleset name="Shoryuken.com">
	<target host="shoryuken.com" />
	<target host="www.shoryuken.com" />
	<target host="evo.shoryuken.com" />

	<securecookie host="^evo\.shoryuken\.com$" name=".+" />

	<rule from="^http:" to="https:" />

	<test url="http://shoryuken.com" />
</ruleset>
