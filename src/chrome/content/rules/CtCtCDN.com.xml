<!--
	For other Constant-Contact coverage, see Constant-Contact.xml.


	Fully covered hosts in *.ctctcdn.com:

		- static
		- origin.static

-->
<ruleset name="CtCtCDN.com">

	<!--	Direct rewrites:
				-->
	<target host="static.ctctcdn.com" />
	<target host="origin.static.ctctcdn.com" />


	<rule from="^http:"
		to="https:" />

</ruleset>
