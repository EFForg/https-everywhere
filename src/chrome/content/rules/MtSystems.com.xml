<!--
	Mismatch:
		- mtsystems.ch
		- www.mtsystems.ch
-->
<ruleset name="mtSystems.com">

	<target host="mtsystems.com" />
	<target host="www.mtsystems.com" />

	<target host="mtsystems.ch" />
	<target host="www.mtsystems.ch" />


	<rule from="^http://(www\.)?mtsystems\.ch/"
		to="https://www.mtsystems.com/" />
	<rule from="^http:"
		to="https:" />

</ruleset>
