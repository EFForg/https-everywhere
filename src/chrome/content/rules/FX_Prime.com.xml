<!--
	For other GMO coverage, see GMO_Internet.xml.


	^: refused

-->
<ruleset name="FX Prime.com">

	<target host="fxprime.com" />
	<target host="*.fxprime.com" />


	<rule from="^http://(?:www\.)?fxprime\.com/"
		to="https://www.fxprime.com/" />

	<rule from="^http://mirror\.fxprime\.com/"
		to="https://mirror.fxprime.com/" />

</ruleset>
