<ruleset name="Vidahost (partial)">

	<target host="vidahost.com" />
	<target host="my.vidahost.com" />
	<target host="www.vidahost.com" />


	<securecookie host="^(?:my|www)\.vidahost\.com$" name=".+" />


	<rule from="^http:" to="https:" />

</ruleset>
