<!--
	www.4sysops.com: Mismatched

-->
<ruleset name="4sysops.com">

	<!--	Direct rewrites:
				-->
	<target host="4sysops.com" />

	<!--	Complications:
				-->
	<target host="www.4sysops.com" />


	<rule from="^http://www\.4sysops\.com/"
		to="https://4sysops.com/" />

	<rule from="^http:"
		to="https:" />

</ruleset>
