<ruleset name="United Internet">

	<target host=  "united-internet.de" />
	<target host="*.united-internet.de" />

	<target host=  "united-internet-for-unicef-stiftung.de" />
	<target host="*.united-internet-for-unicef-stiftung.de" />

	<!-- Cookies -->

	<securecookie host=".*\.united-internet\.de$" name=".+" />

	<!-- Rules -->

	<rule from="^http:"
		  to="https:" />

	<!-- Tests -->

	<test url="http://www.united-internet.de" />
	<test url="http://www.united-internet-for-unicef-stiftung.de" />

</ruleset>