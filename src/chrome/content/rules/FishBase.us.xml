<!--
	(www.)?: Handshake fails

-->
<ruleset name="FishBase.us" default_off="handshake failure">

	<target host="fishbase.us" />
	<target host="www.fishbase.us" />


	<securecookie host="^www\.fishbase\.us$" name=".+" />


	<rule from="^http://(?:www\.)?fishbase\.us/"
		to="https://www.fishbase.us/" />

</ruleset>
