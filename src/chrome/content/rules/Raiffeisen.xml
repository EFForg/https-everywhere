<ruleset name="Raiffeisen.ch">
        <target host="raiffeisen.ch" />
        <target host="www.raiffeisen.ch" />
        <target host="tb.raiffeisendirect.ch" />

        <securecookie host="^(?:.*\.)?raiffeisen\.ch$" name=".*" />
        <securecookie host="^(?:.*\.)?raiffeisendirect\.ch$" name=".*" />

        
        <rule from="^http:"
                to="https:" />
</ruleset>
