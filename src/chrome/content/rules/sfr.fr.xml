	<target host="assistance.sfr.fr" />
	<target host="news.sfr.fr" />
	<target host="espace-client-red.sfr.fr" />
	<target host="forum.sfr.fr" />
	<target host="messagerie-11.sfr.fr" />
	<target host="sport.sfr.fr" />
	<target host="sfrcloud.sfr.fr" />
	<target host="boutique.sfr.fr" />
	<target host="home.sfr.fr" />
	<target host="messagerie-12.sfr.fr" />
	<target host="play.sfr.fr" />


	<rule from="^http:" to="https:" />
</ruleset>
