<!--
	W3C


	Nonfunctional subdomains:

		- dev *

	* Dropped


	Nonfunctional www paths:

		- icons/ *

	* 404


	Fully covered subdomains:

		- dvcs
		- jigsaw
		- lists
		- validator
		- validator-suite

-->
<ruleset name="W3.org (partial)">

	<!--target host="w3.org" /-->
	<target host="dvcs.w3.org" />
	<target host="jigsaw.w3.org" />
	<target host="lists.w3.org" />
	<target host="validator.w3.org" />
	<target host="validator-suite.w3.org" />
	<!--target host="www.w3.org" /-->

		<!--	Redirects to http:
						-->
		<!--exclusion pattern="^http://www\.w3\.org/$" /-->
		<!--exclusion pattern="^http://(?:www\.)?w3\.org/+(?:.+(?:/+|\.html)(?:$|\?)|icons/)" /-->


	<!--rule from="^http://(?:www\.)?w3\.org/(?=.+/)"
		to="https://www.w3.org/" /-->

	<rule from="^http:"
		to="https:" />

</ruleset>
