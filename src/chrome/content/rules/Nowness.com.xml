<ruleset name="Nowness.com">

	<!--	Direct rewrites:
				-->
	<target host="www.nowness.com" />

	<!--	Complications:
				-->
	<target host="nowness.com" />


	<securecookie host="^\." name="^_gat?$" />
	<securecookie host="^\w" name="." />


	<rule from="^http://nowness\.com/"
		to="https://www.nowness.com/" />

	<rule from="^http:"
		to="https:" />

</ruleset>
