<!--
	Non-functional hosts in *.bpt.me
		Certificate mismatched:
		- www.bpt.me
-->
<ruleset name="BPT.bz (partial)">

	<target host="bpt.me" />
	<target host="www.bpt.me" />
	<target host="community.bpt.me" />
	<target host="m.bpt.me" />

	<rule from="^http://www\.bpt\.me/"
		to="https://bpt.me/" />
	
	<rule from="^http:"
		to="https:" />

</ruleset>
