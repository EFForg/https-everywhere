<ruleset name="QualityAgent (partial)" default_off="mismatch">

	<target host="qualityagent.com" />
	<target host="*.qualityagent.com" />


	<securecookie host="^.+\.qualityunit\.com$" name=".+" />


	<rule from="^http://(support\.|www\.)?qualityagent\.com/"
		to="https://$1qualityagent.com/" />

</ruleset>