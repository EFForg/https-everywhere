<!--
	Problematic domains:

		- www..kali.org

	Fully covered domains:

		- kali.org subdomains:

			- forums

-->
<ruleset name="kali">

	<target host="forums.kali.org" />

	<rule from="^http://(forums)\.kali\.org/" to="https://$1.kali.org/" />
	<securecookie host="^forums\.kali\.org$" name=".*" />

</ruleset>
