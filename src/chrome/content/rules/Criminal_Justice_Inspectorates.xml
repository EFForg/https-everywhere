<ruleset name="Criminal Justice Inspectorates">
    <target host="justiceinspectorates.gov.uk" />
    <target host="*.justiceinspectorates.gov.uk" />

    <rule from="^http://justiceinspectorates\.gov\.uk/"
        to="https://www.justiceinspectorates.gov.uk/" />
