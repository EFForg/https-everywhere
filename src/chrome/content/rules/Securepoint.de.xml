<ruleset name="Securepoint">
  <target host="securepoint.de" />
  <target host="www.securepoint.de" />

  <target host="spdns.de" />
  <target host="www.spdns.de" />

  <rule from="^http://(www\.)?securepoint\.de/" to="https://www.securepoint.de/" />

  <rule from="^http://spdns\.de/" to="https://spdns.de/" />
  <rule from="^http://www\.spdns\.de/" to="https://www.spdns.de/" />
</ruleset>
