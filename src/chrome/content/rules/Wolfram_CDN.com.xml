<!--
	For other Wolfram Research coverage, see Wolfram.com.xml

	Problematic hosts in *wolframcdn.com:

		- ^ (https timeout)

-->
<ruleset name="Wolfram CDN.com">

	<target host="www.wolframcdn.com" />
	<target host="dl.wolframcdn.com" />

	<securecookie host=".+" name=".+" />

	<rule from="^http:"
		to="https:" />

</ruleset>
