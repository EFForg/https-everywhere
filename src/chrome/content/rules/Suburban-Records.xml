<ruleset name="Suburban Records" default_off="self-signed">

	<target host="suburban.nl"/>
	<target host="www.suburban.nl"/>
	<target host="*.www.suburban.nl"/>

	<securecookie host="^(?:.*\.)?suburban\.nl$" name=".*"/>

	<rule from="^http://(?:www\.)?suburban\.nl/"
		to="https://www.suburban.nl/"/>

</ruleset>
