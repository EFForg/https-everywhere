<!--
	Problematic:
	 - https://iciciprulife.com (certificate invalid)
           (even the non-ssl version 404s and redirects to https://www.iciciprulife.com using Javascript)

	 - https://blog.iciciprulife.com (certificate fails to validate in the HTTPSE tests)
-->

<ruleset name="Iciciprulife.com">
	<target host="customer.iciciprulife.com" />
	<target host="hrapps.iciciprulife.com" />
	<target host="lms.iciciprulife.com" />
	<target host="onlinelifeinsurance.iciciprulife.com" />
	<target host="webservices.iciciprulife.com" />
	<target host="www.iciciprulife.com" />

	<exclusion pattern="^http://hrapps\.iciciprulife\.com/$" />

	<test url="http://hrapps.iciciprulife.com/CareerPortal/why-join-us.aspx" />
	<test url="http://hrapps.iciciprulife.com/CareerPortal/home.aspx" />

	<rule from="^http:" to="https:" />
</ruleset>
