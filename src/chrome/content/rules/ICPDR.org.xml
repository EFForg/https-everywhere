<!--
	Invalid certificate:
		- icpdr.org, equivalent to www.icpdr.org
		- danubis.icpdr.org
-->

<ruleset name="ICPDR.org">
	<target host="icpdr.org" />
	<target host="www.icpdr.org" />

	<rule from="^http://icpdr\.org/"
		to="https://www.icpdr.org/" />

	<rule from="^http:" to="https:" />
</ruleset>
