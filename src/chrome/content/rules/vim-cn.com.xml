<!--
	Invalid Certificate:
		- vim-cn.com
		- www.vim-cn.com
		- dimg.vim-cn.com
		- p.vim-cn.com
-->
<ruleset name="vim-cn.com">
	<target host="cfp.vim-cn.com" />
	<target host="chat.vim-cn.com" />
	<target host="img.vim-cn.com" />

	<securecookie host="^chat\.vim-cn\.com$" name=".+" />

	<rule from="^http:" to="https:" />
</ruleset>
