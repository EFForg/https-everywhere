<!--
	Invalid certificate:
		bancaintesa.rs
		meet.bancaintesa.rs

	Redirect to HTTP:
		www.bancaintesa.rs
-->
<ruleset name="Banca Intesa Beograd">

	<target host="online.bancaintesa.rs" />
	<target host="secure.bancaintesa.rs" />
	<target host="with.bancaintesa.rs" />

	<rule from="^http:" to="https:" />

</ruleset>
