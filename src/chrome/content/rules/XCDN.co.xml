<!--
	For other MAGIX coverage, see MAGIX.xml.


	Insecure cookies are set for these domains:

		- .xcdn.co

-->
<ruleset name="XCDN.co">

	<!--	Direct rewrites:
				-->
	<target host="xcdn.co" />


	<!--	CloudFlare cookies:
					-->
	<securecookie host="^\.xcdn\.co$" name="^(?:__cfduid|cf_clearance)$" />


	<rule from="^http:"
		to="https:" />

</ruleset>
