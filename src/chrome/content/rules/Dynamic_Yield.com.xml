<!--
	Nonfunctional subdomains:

		- ^	(shows st)
		- www	(shows s1a)


	Fully covered subdomains:

		- adm
		- s1a
		- st

-->
<ruleset name="Dynamic Yield.com (partial)">

	<target host="*.dynamicyield.com" />


	<rule from="^http://(adm|s1a|st)\.dynamicyield\.com/"
		to="https://$1.dynamicyield.com/" />

</ruleset>
