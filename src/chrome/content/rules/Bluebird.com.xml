<!--
	For other American Express coverage, see AmericanExpress.xml.


	Fully covered subdomains:

		- (www.)?
		- secure

-->
<ruleset name="Bluebird.com">

	<target host="bluebird.com" />
	<target host="*.bluebird.com" />


	<rule from="^http://(secure\.|www\.)?bluebird\.com/"
		to="https://$1bluebird.com/" />

</ruleset>
