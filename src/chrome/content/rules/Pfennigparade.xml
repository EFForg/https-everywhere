<ruleset name="Pfennigparade">

	<target host="pfennigparade.de"/>
	<target host="www.pfennigparade.de"/>

	<rule from="^http://pfennigparade\.de/"
		to="https://www.pfennigparade.de/" />

	<rule from="^http:" to="https:"/>

	<test url="http://pfennigparade.de/"/>
	<test url="http://www.pfennigparade.de/"/>

</ruleset>
