<!--
	For other Union Pacific Railroad Company coverage, see up.com.xml.


	^upds.com: refused

-->
<ruleset name="UPDS.com">

	<!--	Direct rewrites:
				-->
	<target host="www.upds.com" />

	<!--	Complications:
				-->
	<target host="upds.com" />


	<securecookie host="^\w" name=".+" />


	<rule from="^http://upds\.com/"
		to="https://www.upds.com/" />

	<rule from="^http:"
		to="https:" />

</ruleset>
