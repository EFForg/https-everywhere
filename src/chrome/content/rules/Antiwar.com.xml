<!--
	Nonfunctional hosts in *.antiwar.com:

	h: http redirect
	m: certificate mismatch
	r: connection refused
	s: self-signed certificate
	t: timeout on https
-->
<ruleset name="Antiwar.com">
	<target host="antiwar.com" />
	<target host="www.antiwar.com" />

	<rule from="^http:" to="https:" />
</ruleset>
