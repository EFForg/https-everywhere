<!--
	^thunderbird-mail.de: Mismatched

-->
<ruleset name="Thunderbird-Mail.DE">

	<!--	Direct rewrites:
				-->
	<target host="www.thunderbird-mail.de" />

	<!--	Complications:
				-->
	<target host="thunderbird-mail.de" />


	<rule from="^http://thunderbird-mail\.de/"
		to="https://www.thunderbird-mail.de/" />


	<rule from="^http:"
		to="https:" />

</ruleset>
