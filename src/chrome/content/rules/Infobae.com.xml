<!--
	Nonfunctional hosts in *.infobae.com:

	h: http redirect
	m: certificate mismatch
	r: connection refused
	s: self-signed certificate
	t: timeout on https
-->
<ruleset name="Infobae.com">
	<target host="www.infobae.com" />

	<securecookie host="^\w" name=".+" />

	<rule from="^http:" to="https:" />
</ruleset>
