<ruleset name="Crate.io">
	<target host="crate.io" />
	<target host="*.crate.io" />

	<test url="http://cdn.crate.io/" />
	<test url="http://www.crate.io/" />
	<test url="http://play.crate.io/" />

	<!--	Not secured by server: -->
	<securecookie host="^\.?crate\.io$" name=".+" />

	<rule from="^http:"
		to="https:" />
</ruleset>

