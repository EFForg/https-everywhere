<!--
	Mixed content:
		www.netnow.co (css,js and img from ^)

	Invalid certificate:
		asi.netnow.co
-->
<ruleset name="NetNow.co">

	<target host="netnow.co" />
	<target host="www.netnow.co" />

	<securecookie host=".+" name=".+" />

	<!-- Avoid mixed content -->
	<rule from="^http://www\.netnow\.co/"
		to="https://netnow.co/" />

	<rule from="^http:"
		to="https:" />

</ruleset>
