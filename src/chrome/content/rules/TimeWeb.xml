<!--
	Other TimeWeb rulesets:

		- TimeWeb.com.xml

-->
<ruleset name="TimeWeb.ru">

	<target host="timeweb.ru" />
	<target host="cp.timeweb.ru" />
	<target host="webmail.timeweb.ru" />
	<target host="www.timeweb.ru" />


	<securecookie host="^(?:.*\.)?timeweb\.ru$" name=".+" />


	<rule from="^http:"
		to="https:" />

</ruleset>
