<!--
	Problematic subdomains:

		- blog ¹
		- status ²

	¹ Pagely
	² StatusPage

-->
<ruleset name="Parse.com (partial)">

	<target host="parse.com" />
	<target host="*.parse.com" />


	<rule from="^http://(www\.)?parse\.com/"
		to="https://$1parse.com/" />

	<rule from="^http://status\.parse\.com/"
		to="https://parse.statuspage.io/" />

</ruleset>
