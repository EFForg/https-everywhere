<!--
	Nonfunctional subdomains:

		- blog *

	* Redirects to www


	These altnames don't exist:

		- www.beta.incoin.io

-->
<ruleset name="incoin.io (partial)">

	<target host="incoin.io" />
	<target host="*.incoin.io" />


	<rule from="^http://(beta\.|www\.)?incoin\.io/"
		to="https://$1incoin.io/" />

</ruleset>
