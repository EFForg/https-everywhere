<!--
	For other bigmir coverage, see Bigmirnet.xml.

	Mismatched hosts in *mport.ua:

		- www

-->
<ruleset name="MPort.ua">

	<target host="mport.ua" />
	<target host="www.mport.ua" />


	<securecookie host=".+" name=".+" />


	<rule from="^http://www\.mport\.ua/"
		to="https://mport.ua/" />

	<rule from="^http:"
		to="https:" />

</ruleset>
