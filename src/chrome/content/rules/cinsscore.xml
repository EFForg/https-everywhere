<ruleset name="cinsscore">
    <target host="ciarmy.com" />
    <target host="www.ciarmy.com" />
    <target host="cinsscore.com" />
    <target host="www.cinsscore.com" />

    <rule from="^https?://(www\.)?ciarmy.com/"
            to="https://cinsscore.com/" />

    <rule from="^https://www.cinsscore.com/"
            to="https://cinsscore.com/" />

    <rule from="^http:"
            to="https:" />
    
</ruleset>