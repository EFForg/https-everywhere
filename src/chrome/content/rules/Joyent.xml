	<rule from="^http://(\w+)\.joyent\.com/"
		to="https://$1.joyent.com/"/>

	<rule from="^http://([\w\-]*api|my)\.joyentcloud\.com/"
		to="https://$1.joyentcloud.com/"/>

	<rule from="^http://(?:www\.)?no\.de/"
		to="https://no.de/"/>

