<!--

	403 Forbidden:
		- swebooktest.swebus.se
		- wstest2.swebus.se
		- wwwtest.swebus.se

	503 Service Unavailable:
		- mobil.swebus.se
		- ws.swebus.se
		- wstest.swebus.se

	Invalid certificate:
		- Lyncdiscover.swebus.se
		- www.express.swebus.se
		- framstegsresan.swebus.se
		- media.swebus.se
		- media2.swebus.se
		- ombord.swebus.se

-->
<ruleset name="Swebus.se">
	<target host="www.swebus.se" />
	<target host="swebook.swebus.se" />
	<target host="swebus.se" />
	<target host="fia.swebus.se" />

	<securecookie host=".+" name=".+" />

	<!-- The domain swebus.se is redirected because the secure version sends an invalid redirect response. -->
	<rule from="^http://swebus\.se/" to="https://www.swebus.se/" />
	<rule from="^http://fia\.swebus\.se/" to="https://fia.nobina.com/sv" />

	<rule from="^http:" to="https:" />
</ruleset>
