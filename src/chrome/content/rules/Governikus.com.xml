<ruleset name="Governikus.com">
	<target host="www.governikus.com" />
	<target host=    "governikus.com" />
	<target host="www.governikus.de" />
	<target host=    "governikus.de" />
	<target host="www.bos-bremen.de" />
	<target host=    "bos-bremen.de" />

	<securecookie host="^www\.governikus\.com$" name=".+" />

	<!-- cert only matches www -->
	<rule from="^http://governikus\.com/"
		to="https://www.governikus.com/" />
	
	<!-- cert only matches www -->	
	<rule from="^http://?bos-bremen\.de/"
		to="https://www.bos-bremen.de/" />
	
	<!-- hosname mismatch, CN: www.bos-bremen.de -->	
	<rule from="^http://(www\.)?governikus\.de/"
		to="https://www.governikus.com/" />

	<rule from="^http:"
		to="https:" />
</ruleset>
