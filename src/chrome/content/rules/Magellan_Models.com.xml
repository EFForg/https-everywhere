<ruleset name="Magellan Models.com (false MCB)" platform="mixedcontent">

	<target host="magellanmodels.com" />
	<target host="www.magellanmodels.com" />


	<securecookie host="^(?:www)?\.magellanmodels\.com$" name=".+" />


	<rule from="^http:" to="https:" />

</ruleset>