<!--
	Problematic domains:

		- avuedigitalservices.com	(cert only matches www)

-->
<ruleset name="Avue Digital Services">

	<target host="avuedigitalservices.com" />
	<target host="www.avuedigitalservices.com" />


	<rule from="^http://(?:www\.)?avuedigitalservices\.com/"
		to="https://www.avuedigitalservices.com/" />

</ruleset>
