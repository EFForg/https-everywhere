<ruleset name="Vungle.com (false MCB)" platform="mixedcontent">

	<target host="*.vungle.com" />


	<securecookie host="^\.vungle\.com$" name=".+" />


	<rule from="^http://(blog|www)\.vungle\.com/"
		to="https://$1.vungle.com/" />

</ruleset>
