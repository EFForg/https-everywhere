<!--
	Mixed content:
		- blog.maxdome.de
	
	Redirect to HTTP:
		- faq.maxdome.de

	Mismatching cert:
		- presse.maxdome.de
-->
<ruleset name="Maxdome.de">
	<target host="maxdome.de" />
	<target host="www.maxdome.de" />
	<target host="store.maxdome.de" />
	<target host="developer.maxdome.de" />

	<rule from="^http:" to="https:" />
</ruleset>
