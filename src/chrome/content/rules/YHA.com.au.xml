<!--
	Non-functional subdomains:
		- enights		(t)
		- secure		(t)
		- wp.test		(ssl connection error)
		- www-wd		(i)

	e: expired certificate
	h: http redirect
	i: invalid certificate chain
	m: certificate mismatch
	r: connection refused
	s: self-signed certificate
	t: timeout on https
-->
<ruleset name="YHA.com.au">

	<target host="yha.com.au" />
	<target host="www.yha.com.au" />
	<target host="auth.yha.com.au" />
	<target host="auth-dev.yha.com.au" />
	<target host="autodiscover.yha.com.au" />
	<target host="bitbucket.yha.com.au" />
	<target host="book.yha.com.au" />
	<target host="cn.yha.com.au" />
	<target host="greylog.yha.com.au" />
	<target host="jp.yha.com.au" />
	<target host="kr.yha.com.au" />
	<target host="mail.yha.com.au" />
	<target host="portal.yha.com.au" />
	<target host="portalauthor.yha.com.au" />
	<target host="preprod.yha.com.au" />
	<target host="bookings.test.yha.com.au" />
	<target host="webapi.test.yha.com.au" />
	<target host="wim.yha.com.au" />

	<securecookie host=".+" name=".+" />

	<rule from="^http:"
		to="https:" />
</ruleset>
