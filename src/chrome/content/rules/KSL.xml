<ruleset name="KSL">

	<target host="ksl.com" />
	<target host="*.ksl.com" />


	<securecookie host="^\.ksl\.com$" name=".+" />


	<rule from="^http://ksl\.com/"
		to="https://www.ksl.com/" />
		<test url="http://ksl.com/" />
	<rule from="^http://((?:img|local|static|www)\.)ksl\.com/"
		to="https://$1ksl.com/" />
		<test url="http://www.ksl.com/" />
		<test url="http://local.ksl.com/" />
		<test url="http://static.ksl.com/" />

</ruleset>