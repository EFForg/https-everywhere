<ruleset name="ShoutWiki.com" platform="mixedcontent">

	<target host="shoutwiki.com" />
	<target host="*.shoutwiki.com" />
		<exclusion pattern="^http://.*\..*\.shoutwiki\.com/" />
			<test url="http://fi.starwars.shoutwiki.com/" />

	<test url="http://es.shoutwiki.com/" />
	<test url="http://www.shoutwiki.com/" />
	<test url="http://blog.shoutwiki.com/" />
	<test url="http://bugzilla.shoutwiki.com/" />

	<rule from="^http:"
		to="https:" />

</ruleset>
