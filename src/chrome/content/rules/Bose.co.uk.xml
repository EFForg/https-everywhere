<!--
	Invalid certificate:
		bose.co.uk
		campaign.bose.co.uk
		campus.bose.co.uk
		m.bose.co.uk

-->
<ruleset name="Bose">

    <target host="bose.co.uk" />
    <target host="www.bose.co.uk" />

    <securecookie host=".+" name=".+" />

    <rule from="^http://bose\.co\.uk/"
        to="https://www.bose.co.uk/" />

    <rule from="^http:"
        to="https:" />

</ruleset>
