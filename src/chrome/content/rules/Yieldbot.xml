<!--
	CDN buckets:

		- dqckmuwjiytix.cloudfront.net

			- cdn.yb0t.com


	Nonfunctional domains:

		- (www.)yieldbot.com	(times out)


	i.yldbt.com sets __ybot\w cookies on
	whichever domain it is fetched from

-->
<ruleset name="YieldBot (partial)">

	<target host="cdn.yb0t.com" />
	<target host="i.yldbt.com" />
	<target host="cdn.yldbt.com" />

	<rule from="^http://cdn\.yb0t\.com/"
		to="https://dqckmuwjiytix.cloudfront.net/" />

	<rule from="^http:"
		to="https:" />

</ruleset>
