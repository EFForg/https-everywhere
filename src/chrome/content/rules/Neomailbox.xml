<!--
	Invalid certificate:
		support.neomailbox.com

		www.neomailbox.net

-->
<ruleset name="neomailbox">

	<target host="neomailbox.com" /><!-- (→ http://www.neomailbox.com) -->
	<target host="www.neomailbox.com" />
	<target host="secure.neomailbox.com" />

	<target host="neomailbox.net" /><!-- (→ http://www.neomailbox.com) -->
	<target host="www.neomailbox.net" /><!-- (→ http://www.neomailbox.com) -->

	<securecookie host="^(?:w*\.)?neomailbox\.(?:com|net)$" name=".+" />

	<rule from="^http://(www\.)?neomailbox\.(com|net)/"
		to="https://www.neomailbox.com/" />

	<rule from="^http:"
		to="https:" />

</ruleset>
