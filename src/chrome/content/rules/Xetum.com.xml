<!--
	Some pages redirect to http.


	Mixed content:

		- favicon from $self *

	* Secured by us

-->
<ruleset name="Xetum.com (partial)">

	<target host="xetum.com" />
	<target host="www.xetum.com" />


	<rule from="^http://(www\.)?xetum\.com/(?=[^?/]+/+[^?/]+|checkout(?:$|[?/])|favicon\.ico|static/)"
		to="https://$1xetum.com/" />

</ruleset>
