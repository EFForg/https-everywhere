<!--
	Invalid certificate:
		mx.umgum.com
		ns.umgum.com

	Refused:
		ns2.umgum.com
		static.umgum.com
-->
<ruleset name="umgum.com">
	<target host="umgum.com" />
	<target host="www.umgum.com" />
	<target host="dir.umgum.com" />
	<target host="mail.umgum.com" />
	<target host="map.umgum.com" />
	<target host="pma.umgum.com" />

	<rule from="^http:" to="https:" />
</ruleset>
