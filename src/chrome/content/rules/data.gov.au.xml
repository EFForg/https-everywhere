<!--

	Problematic hosts in *data.gov.au:

		- linked ᵐ
		- wiki ᵐ

	ᵐ Mismatched

-->
<ruleset name="data.gov.au">

	<target host="data.gov.au" />
	<target host="www.data.gov.au" />
	<target host="blog.data.gov.au" />
	<target host="search.data.gov.au" />
	<target host="toolkit.data.gov.au" />

	<rule from="^http:"
		to="https:" />

</ruleset>
