<!--
Enable HTTPS on the nyaa.eu domain

-->
<ruleset name="Nyaa.eu">

	<!--	Direct rewrites:
				-->
  <target host="nyaa.eu" />
  <target host="www.nyaa.eu" />
  <target host="sukebei.nyaa.eu" />

	<!--	CloudFlare cookies:	
	-->
				
   <securecookie host="^\.nyaa\.eu$" name="^(?:__cfduid|cf_clearance)$" />


   <rule from="^http:"
    to="https:" />


</ruleset>
