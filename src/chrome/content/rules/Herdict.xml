<ruleset name="Herdict">

	<target host="herdict.org" />
	<target host="*.herdict.org" />
	<target host="nardikt.ru" />
	<target host="*.nardikt.ru" />


	<securecookie host="^www\.(herdic|nardik)t\.org" name=".+" />


	<rule from="^http://(?:www\.)?(herdic|nardik)t\.org/"
		to="https://www.$1t.org/" />

	<rule from="^http://dev\.(herdic|nardik)t\.org/"
		to="https://dev.$1t.org/" />

</ruleset>
