<ruleset name="Accellion" platform="mixedcontent">

	<target host="accellion.com"/>
	<!--	* for cross-domain cookie	-->
	<target host="*.accellion.com"/>

	<securecookie host="^(?:.*\.)accellion\.com$" name=".*"/>

	<rule from="^http://(www\.)?accellion\.com/"
		to="https://$1accellion.com/"/>

</ruleset>
