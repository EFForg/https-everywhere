    <rule from="^http://www\.jonkoping\.polisen\.se" to="https://polisen.se/" />
    <rule from="^http://www\.kronoberg\.polisen\.se" to="https://polisen.se/" />
    <rule from="^http://www\.ostergotland\.polisen\.se" to="https://polisen.se/" />
    <rule from="^http://www\.skane\.polisen\.se" to="https://polisen.se/" />
    <rule from="^http://www\.stockholm\.polisen\.se" to="https://polisen.se/" />
    <rule from="^http://www\.varmland\.polisen\.se" to="https://polisen.se/" />
    <rule from="^http://www\.vasternorrland\.polisen\.se" to="https://polisen.se/" />
    <rule from="^http://www\.vastmanland\.polisen\.se" to="https://polisen.se/" />
    <rule from="^http://www\.nfc\.polisen\.se" to="https://nfc.polisen.se/" />
    <rule from="^http://www\.skl\.polisen\.se" to="https://nfc.polisen.se/" />

    <rule from="^http:" to="https:" />
</ruleset>
