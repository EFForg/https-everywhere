<!--

    Wrong certificate:
        - www.polismuseet.polisen.se
        - www.svenskpolis.polisen.se

    Other unknown SSL error:
        - volontar.polisen.se

-->
<ruleset name="Polismyndigheten Sverige">
    <target host="polisen.se" />
    <target host="www.polisen.se" />
    <target host="mobil.polisen.se" />
    <target host="nfc.polisen.se" />
    <target host="wwww.polisen.se" />
    <target host="skl.polisen.se" />

    <securecookie host=".+" name=".+" />

    <rule from="^http://www\.blekinge\.polisen\.se" to="https://polisen.se/" />
    <rule from="^http://www\.dalarna\.polisen\.se" to="https://polisen.se/" />
