<!--
	Fully covered domains:

		- \w+.fetchback.com:

			- pixel

-->
<ruleset name="FetchBack">

	<target host="fetchback.com" />
	<target host="*.fetchback.com" />

	<securecookie host="^(?:.*\.)?fetchback\.com$" name=".+" />

	<rule from="^http://(\w+\.)?fetchback\.com/"
		to="https://$1fetchback.com/" />

</ruleset>
