<ruleset name="Kubieziel.de" platform="cacert">

	<!--	Direct rewrites:
				-->
	<target host="kubieziel.de" />
	<target host="www.kubieziel.de" />


	<!--	Server doesn't set Secure:
						-->
	<securecookie host="^(?:www\.)?kubieziel\.de$$" name=".+" />


	<rule from="^http:"
		to="https:" />

</ruleset>
