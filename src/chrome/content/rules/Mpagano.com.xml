<!--
	CN: localhost

-->
<ruleset name="mpagano.com" default_off="mismatch, self-signed">

	<target host="mpagano.com"/>
	<target host="www.mpagano.com"/>

	<!--	cert !valid for www, !www redirects to www	-->
	<rule from="^http://(?:www\.)?mpagano\.com/"
		to="https://www.mpagano.com/"/>

</ruleset>
