<ruleset name="KRAXNET s.r.o.">
    <target host="xnet.cz" />
    <target host="www.xnet.cz" />
    <target host="admin.xnet.cz" />

    <rule from="^http://xnet\.cz/"
		    to="https://xnet.cz/" />
    <rule from="^http://www\.xnet\.cz/"
		    to="https://xnet.cz/" />
    <rule from="^http://admin\.xnet\.cz/"
            to="https://admin.xnet.cz/" />
</ruleset>
