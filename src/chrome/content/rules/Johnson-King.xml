<ruleset name="Johnson King" default_off="mismatched">

	<!--	hostcentric.com	-->
	<target host="johnsonking.com"/>
	<target host="*.johnsonking.com"/>

	<securecookie host="^(?:.*\.)?johnsonking\.com$" name=".+" />

	<rule from="^http://(?:www\.)?johnsonking\.com/"
		to="https://www.johnsonking.com/"/>

</ruleset>
