<!--
	Server error:
		- matcc.company, equivalent to www.matcc.company
-->

<ruleset name="Matcc.company" platform="mixedcontent">
	<target host="matcc.company" />
	<target host="www.matcc.company" />

	<rule from="^http://matcc\.company/"
		to="https://www.matcc.company/" />

	<rule from="^http:" to="https:" />
</ruleset>
