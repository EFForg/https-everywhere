<!--
	HTTP redirect:
		- www.welt.de

	Mismatch:
		- gutscheine.welt.de
		- m.welt.de
		- mba.welt.de
		- service.welt.de

	Mixed content:
		- stepstone.welt.de

	Refused:
		- biowetter.welt.de
		- donnerunddoria.welt.de
		- drakensang.welt.de
		- investigativ.welt.de
		- klassiker.welt.de
		- kompakt.welt.de
		- kosmos.welt.de
		- luxus.welt.de
		- luxusuhren.welt.de
		- medien.welt.de
		- ps.welt.de
		- reisewetter.welt.de
		- to.welt.de
		- wetter.welt.de

	Timeout:
		- ^welt.de
-->
<ruleset name="Welt.de (partial)">
	<target host="beta.welt.de" />
	<target host="codewidget.welt.de" />
	<target host="css.welt.de" />
	<target host="img.welt.de" />

	<!-- redirect to http 404 -->
	<exclusion pattern="^http://css\.welt\.de/$" />
	<exclusion pattern="^http://img\.welt\.de/$" />

	<test url="http://codewidget.welt.de/beta-promotion/css/betapromotion.css" />
	<test url="http://css.welt.de/skins/welt/css/693/reset.css" />
	<test url="http://img.welt.de/img/config_master_header/orig149433181/3270004236/welt-logo-trans.png" />

	<rule from="^http:" to="https:" />
</ruleset>
