<!-- Do not change -> approved by 1and1 staff -->
<ruleset name="Profiseller.de">

    <!--
		The following URI's get redirected to profiseller.de by the corresponding server:
		- 1und1-webpartner.de
	-->
    <target host=  "profiseller.de" />
    <target host="*.profiseller.de" />

    <!-- Cookies -->

    <securecookie host=".*\.profiseller\.de$" name=".+" />

    <!-- Rules -->

    <rule from="^http:"
          to="https:" />

    <!-- Tests -->

    <test url="http://www.profiseller.de" />

</ruleset>