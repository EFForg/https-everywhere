		- beacon	(times out)
		- mrd	(404)
		- remoraimage	(times out)

-->

<ruleset name="xlisting.jp">

	<target host="cv.xlisting.jp" />
	<target host="kwsearch.xlisting.jp" />
	<target host="pulsar.xlisting.jp" />
	<target host="rainbow.xlisting.jp" />
	<target host="remora.xlisting.jp" />
	<target host="srt-dmp.xlisting.jp" />
	<target host="startup.xlisting.jp" />
	<target host="xrecommend.xlisting.jp" />

	<test url="http://eihwaz.xlisting.jp/cookieoptout/do?mode=optout" />
	<test url="http://pulsar.xlisting.jp/xsync" />
	<test url="http://rainbow.xlisting.jp/xtrail" />
	<test url="http://startup.xlisting.jp/client/application/selfplan_entry" />

	<rule from="^http:"
		to="https:" />

</ruleset>
