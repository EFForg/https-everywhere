<!--
	Mismatch:
		www.json-schema.org
-->
<ruleset name="JSON-Schema.org">
	<target host="json-schema.org" />
	<target host="www.json-schema.org" />

	<securecookie host=".+" name=".+" />

	<rule from="^http://www\.json-schema\.org/" to="https://json-schema.org/" />
	<rule from="^http:" to="https:" />
</ruleset>
