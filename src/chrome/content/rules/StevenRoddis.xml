<!--
	For stevenroddis.com
	Strict-Transport-Security:max-age=157680000; includeSubDomains

		Works:

			- www
			- $

		Doesn't Work:
-->
<ruleset name="Steven Roddis">
  <target host="stevenroddis.com" />
  <target host="*.stevenroddis.com" />

  <rule from="^http://(?:www\.)?stevenroddis\.com/"
          to="https://www.stevenroddis.com/" />
</ruleset>
