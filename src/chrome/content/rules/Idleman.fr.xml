<!--
	Nonfunctional hosts in *.idleman.fr:

	h: http redirect
	m: certificate mismatch
	r: connection refused
	s: self-signed certificate
	t: timeout on https
-->
<ruleset name="Idleman.fr">
	<target host="blog.idleman.fr" />
	<target host="projet.idleman.fr" />
	<target host="leed.idleman.fr" />

	<rule from="^http:" to="https:" />

	<test url="http://blog.idleman.fr/contacts/" />
        <test url="https://leed.idleman.fr/?page=Accueil" />
        <test url="http://projet.idleman.fr/blazekiss/" />
</ruleset>
