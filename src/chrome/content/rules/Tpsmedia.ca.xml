<!--
	Nonfunctional hosts in *.tpsmedia.ca:

	h: http redirect
	m: certificate mismatch
	r: connection refused
	s: self-signed certificate
	t: timeout on https
-->
<ruleset name="Tpsmedia.ca">
	<target host="tpsmedia.ca" />
	<target host="www.tpsmedia.ca" />

	<rule from="^http:"
		to="https:" />
</ruleset>
