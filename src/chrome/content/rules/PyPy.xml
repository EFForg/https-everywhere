<!--
	Connection refused:
		- speed.pypy.org

	Invalid certificate:
		- bugs.pypy.org
		- buildbot.pypy.org
		- doc.pypy.org: equivalent to pypy.readthedocs.io
		- packages.pypy.org
-->

<ruleset name="PyPy">
	<target host="pypy.org" />
	<target host="www.pypy.org" />
	<target host="doc.pypy.org" />
	
	<rule from="^http://doc\.pypy\.org/"
		  to="https://pypy.readthedocs.io/" />
	<rule from="^http:" to="https:" />
</ruleset>
