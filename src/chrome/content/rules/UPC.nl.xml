<!--
	Nonfunctional subdomains:

		- assets	(refused)


	Fully covered subdomains:

		- (www.)
		- business
		- preprod.business
		- grootzakelijk
		- m
		- tvgids
		- overupc
		- service
		- upcmail
		- vragen
		- preprod.www

-->
<ruleset name="UPC">
  <target host="upc.nl" />
  <target host="*.upc.nl" />


	<securecookie host="^(?:service|upcmail|vragen|www)?\.upc\.nl$" name=".+" />


  <rule from="^http://((?:business|preprod\.(?:business|www)|grootzakelijk|m|tvgids|overupc|service|upcmail|vragen|www)\.)?upc\.nl/" to="https://$1upc.nl/"/>
</ruleset>