<!--
	Other BareMetal rulesets:

		- Domain-DNS.com.xml

-->
<ruleset name="BareMetal" platform="mixedcontent">

	<target host="baremetal.com" />
	<target host="*.baremetal.com" />


	<securecookie host="^swww\.baremetal\.com$" name=".+" />


	<rule from="^http://(proxy\.|s?www\.)?baremetal\.com/"
		to="https://$1baremetal.com/" />

</ruleset>
