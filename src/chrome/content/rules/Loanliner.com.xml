<!--
	^: cert only matches www

-->
<ruleset name="Loanliner.com">

	<target host="loanliner.com" />
	<target host="www.loanliner.com" />


	<securecookie host="^(?:www\.)?loanliner\.com$" name=".+" />


	<rule from="^http://(?:www\.)?loanliner\.com/"
		to="https://www.loanliner.com/" />

</ruleset>
