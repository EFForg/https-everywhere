<!--
	For other Movie Database coverage, see The_Movie_Db.org.xml.


	Insecure cookies are set for these domains:

		- .tmdb.org

-->
<ruleset name="TMDb.org">

	<!--	Direct rewrites:
				-->
	<target host="tmdb.org" />
	<target host="image.tmdb.org" />
	<target host="www.tmdb.org" />


	<!--	CloudFlare cookies:
					-->
	<!--securecookie host="^\.tmdb\.org$" name="^(?:__cfduid|cf_clearance)$" /-->

	<securecookie host="^\.tmdb\.org$" name=".+" />


	<rule from="^http:"
		to="https:" />

</ruleset>
