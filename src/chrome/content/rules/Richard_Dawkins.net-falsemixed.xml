<!--
	For rules not causing false/broken MCB, see Richard-Dawkins-Foundation.xml.

-->
<ruleset name="Richard Dawkins.net (false MCB)" platform="mixedcontent">

	<target host="rdfrs.com" />
	<target host="www.rdfrs.com" />
	<target host="richarddawkins.net" />
	<target host="www.richarddawkins.net" />


	<securecookie host="^www\.richarddawkins\.net$" name=".+" />


	<rule from="^http://(?:www\.)?r(?:dfs\.com|icharddawkins\.net)/"
		to="https://www.richarddawkins.net/" />

</ruleset>
