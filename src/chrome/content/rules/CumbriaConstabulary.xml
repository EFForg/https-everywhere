<!--
	For Cumbria Constabulary

		Works:

			- www

		Doesn't Work:

			- $ (gives a 404)

-->
<ruleset name="Cumbria Constabulary">
	<target host="cumbria.police.uk" />
	<target host="www.cumbria.police.uk" />

	<rule from="^http://(?:www\.)?cumbria\.police\.uk/" to="https://www.cumbria.police.uk/"/>
</ruleset>
