<ruleset name="Northeast Credit Union" platform="mixedcontent">
  <target host="necuhb.org" />
  <target host="necu.org" />
  <target host="www.necu.org" />
  <target host="*.necuhb.org" />

  <rule from="^http://([^/:@]*)\.necuhb\.org/"
          to="https://$1.necuhb.org/"/>

  <rule from="^https://(www\.)?necu\.org/"
          to="https://$1necu.org/"/>
</ruleset>
