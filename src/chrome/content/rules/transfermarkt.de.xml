<!--
		404:
			transfermarkt.de
-->
<ruleset name="Transfermarkt.de">

	<target host="transfermarkt.de" />
	<target host="*.transfermarkt.de" />
		<test url="http://www.transfermarkt.de/" />
		<test url="http://www.transfermarkt.de/live/counter" />
		<test url="http://www.transfermarkt.de/navigation/hauptnavi" />
	
	<rule from="^http://transfermarkt\.de/"
			to="https://www.transfermarkt.de/" />
			<test url="http://transfermarkt.de/" />

	<rule from="^http:"
			to="https:" />

</ruleset>
