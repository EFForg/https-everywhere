<!--
	For problematic rules, see Cryptalloy.de-problematic.xml.


	Problematic subdomains:

		- (www.) *
		- shop *

	* Self-signed, CN: webserver.ispgateway.de

-->
<ruleset name="Cryptalloy.de (partial)">

	<target host="shop.cryptalloy.de" />
	<target host="www.shop.cryptalloy.de" />


	<securecookie host="^www\.shop\.cryptalloy\.de$" name=".+" />


	<rule from="^http://(?:www\.)?shop\.cryptalloy\.de/"
		to="https://www.shop.cryptalloy.de/" />

</ruleset>
