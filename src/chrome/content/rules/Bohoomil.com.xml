<!--
	Problematic domains:
		- ^ ¹
		- www ²
		- ams2 ¹

	¹: Expired
	²: Mismatched
-->
<ruleset name="bohoomil.com (bad cert)" default_off="Expired certificate">

	<target host="bohoomil.com" />
	<target host="www.bohoomil.com" />
	<target host="ams2.bohoomil.com" />

	<rule from="^http:"
		to="https:" />

</ruleset>
