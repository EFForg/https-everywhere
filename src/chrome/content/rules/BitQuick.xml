<!--
	Fully covered domains:

		- (www.)?bitquick.co
		- (www.)?bitquick.me
		- (www.)?bitquick.tw

-->
<ruleset name="BitQuick">

	<target host="bitquick.co" />
	<target host="www.bitquick.co" />
	<target host="bitquick.in" />
	<target host="www.bitquick.in" />
	<target host="bitquick.me" />
	<target host="www.bitquick.me" />
	<target host="bitquick.tw" />
	<target host="www.bitquick.tw" />


	<!--	Not secured by server:
					-->
	<!--securecookie host="^www\.bitquick\.co$" name="^PHPSESSID$" /-->

	<securecookie host="^www\.bitquick\.co$" name=".+" />


	<rule from="^http:"
		to="https:" />

</ruleset>
