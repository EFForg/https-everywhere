<!--
	Other Skrill rulesets:

		- Moneybookers.xml


	Fully covered subdomains:

		- (www.)
		- account
		- sso
		- help

-->
<ruleset name="Skrill.com">

	<target host="skrill.com" />
	<target host="*.skrill.com" />


	<securecookie host=".*\.skrill\.com$" name=".+" />


	<rule from="^http://((?:account|help|sso|www)\.)?skrill\.com/"
		to="https://$1skrill.com/" />

</ruleset>
