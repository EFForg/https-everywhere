<!--
	Open Directory


	(www.)?: Plaintext reply

-->
<ruleset name="ODir.org" default_off="plaintext reply">

	<!--	Direct rewrites:
				-->
	<target host="odir.org" />
	<target host="www.odir.org" />


	<securecookie host="^\.odir\.org$" name=".+" />


	<rule from="^http:"
		to="https:" />

</ruleset>
