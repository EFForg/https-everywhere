<!--

	Nonfunctional subdomains:

		- ^	(redirected to http)
		- lab	(connection closed)
		- news	(redirected to http)

	Problematic subdomains:

		- style	(invalid cert)

-->
<ruleset name="prican">

	<target host="api.prcm.jp" />
	<target host="img.prcm.jp" />

	<exclusion pattern="^http://api\.prcm\.jp/$" />

	<test url="http://api.prcm.jp/pics/word/recommend" />

	<rule from="^http:"
		to="https:" />

</ruleset>