<!--
	Fully covered hosts:

		- (www.)?


	Insecure cookies are set for these hosts:

		- zsim.de
		- www.zsim.de

-->
<ruleset name="zsim.de">

	<!--	Direct rewrites:
				-->
	<target host="zsim.de" />
	<target host="www.zsim.de" />


	<!--	Not secured by server:
					-->
	<!--securecookie host="^(www\.)?zsim\.de$" name="^csrftoken$" /-->

	<securecookie host="^(?:www\.)?zsim\.de$" name=".+" />


	<rule from="^http:"
		to="https:" />

</ruleset>
