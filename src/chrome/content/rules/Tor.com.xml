<!--

	Problematic hosts in *tor.com:

		- submissions (self-signed)

-->
<ruleset name="Tor.com">

	<target host="tor.com" />
	<target host="www.tor.com" />
	<target host="ebookclub.tor.com" />
	<target host="giveaway.tor.com" />
	<target host="publishing.tor.com" />

	<rule from="^http:"
		to="https:" />

</ruleset>
