<!--
	For other SLI Systems coverage, see SLI-Systems.com.xml.

	MCB:
		poolandspawarehouse.resultspage.com

	Mismatched:
		^resultspage.com

	Not exist:
		www.resultspage.com

	Redirects to http:
		lenovo-outlet.resultspage.com
-->
<ruleset name="Resultspage.com">
	<target host="*.resultspage.com" />
		<test url="http://sli-systems.resultspage.com/" />

	<exclusion pattern="^http://(lenovo-outlet|poolandspawarehouse)\.resultspage\.com/" />
		<test url="http://lenovo-outlet.resultspage.com/autocomplete/sli-rac.stub.js" />
		<test url="http://poolandspawarehouse.resultspage.com/poolandspawarehouse/Sp400" />

	<securecookie host=".+" name=".+" />

	<rule from="^http:" to="https:" />
</ruleset>
