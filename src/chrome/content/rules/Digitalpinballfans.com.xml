<!--
	Nonfunctional hosts in *.digitalpinballfans.com:

	h: http redirect
	m: certificate mismatch
	r: connection refused
	s: self-signed certificate
	t: timeout on https
-->
<ruleset name="Digitalpinballfans.com">
	<target host="digitalpinballfans.com" />
	<target host="www.digitalpinballfans.com" />

	<rule from="^http:" to="https:" />
</ruleset>
