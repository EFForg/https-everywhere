<!--
	Invalid Server Name:
		www.bronxzoo.com
		pages.bronxzoo.com
	Redirect off-site:
		apps.bronxzoo.com

-->

<ruleset name="Bronxzoo.com">
	<target host="bronxzoo.com" />
	<target host="app.bronxzoo.com" />

	<securecookie host="(app\.)?bronxzoo\.com$" name=".+" />

	<rule from="^http:" to="https:" />
</ruleset>
