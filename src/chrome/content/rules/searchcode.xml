<ruleset name="searchcode">
    <target host="searchcode.com" />
    <target host="*.searchcode.com" />

    <securecookie host="^(.*\.)?searchcode\.com$" name=".+" />

    <rule from="^https?://searchcode\.com/"
        to="https://www.searchcode.com/" />
    <rule from="^http://([^/:@]+)?\.searchcode\.com/"
        to="https://$1.searchcode.com/" />
</ruleset>
