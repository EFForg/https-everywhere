<!--
	Problematic subdomains:

		- go *

	* Herokuapp


	Fully covered subdomains:

		- (www.)?
		- new
		- staging

-->
<ruleset name="Typesafe.com (partial)">

	<target host="typesafe.com" />
	<target host="*.typesafe.com" />


	<rule from="^http://((?:new|staging|www)\.)?typesafe\.com/"
		to="https://$1typesafe.com/" />

</ruleset>
