<!--
	Fully covered hosts in *vescudero.net:
		- (www.)
	Insecure cookies are set for these hosts:
		- www.vescudero.net
	CloudFlare sets insecure __cfduid and cf_clearance wildcard cookies for every client domain
-->
<ruleset name="vescudero.net">

	<target host="vescudero.net" />
	<target host="www.vescudero.net" />

	<securecookie host="^www\.vescudero\.net$" name=".+" />
	<securecookie host="^vescudero\.net$" name=".+" />
	
	<rule from="^http:"
		to="https:" />

</ruleset>