<!--
	For rules not causing false/broken MCB, see Coop.ch.xml
-->
<ruleset name="Coop" platform="mixedcontent">
	<target host="coop.ch"/>  
	<target host="www.coop.ch"/>
	<target host="jamadu.ch"/>  
	<target host="www.jamadu.ch"/>

	<rule from="^http://jamadu\.ch/"
		to="https://www.jamadu.ch/"/>
	<rule from="^http:"
		to="https:"/>
</ruleset>
