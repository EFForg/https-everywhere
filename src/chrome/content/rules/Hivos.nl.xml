<ruleset name="Hivos.nl">

	<target host="hivos.nl" />
	<target host="www.hivos.nl" />


	<!--	Not secured by server:
					-->
	<!--securecookie host="^(www\.)?hivos.nl$" name="^eZSESSID$" /-->

	<securecookie host="^(?:www\.)?hivos.nl$" name=".+" />


	<rule from="^http://(www\.)?hivos\.nl/"
		to="https://$1hivos.nl/" />

</ruleset>
