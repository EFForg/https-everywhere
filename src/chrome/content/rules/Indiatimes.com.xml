<!--
	Nonfunctional hosts in *.indiatimes.com:

	h: http redirect
	m: certificate mismatch
	r: connection refused
	s: self-signed certificate
	t: timeout on https
-->
<ruleset name="Indiatimes.com">
	<target host="indiatimes.com" />
	<target host="www.indiatimes.com" />

	<rule from="^http:" to="https:" />
</ruleset>
