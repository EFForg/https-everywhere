<ruleset name="Meetrics">

<target host="*.meetrics.com"/>
<target host="*.meetrics.de"/>
<target host="*.meetrics.net"/>

<target host="meetrics.com"/>
<target host="meetrics.de"/>
<target host="meetrics.net"/>

<target host="*.mxcdn.net"/>

<target host="*.s400.meetrics.net"/>

<target host="*.research.de.com"/>
<target host="research.de.com"/>

<rule from="^http://www\.meetrics\.(com|de|net)/" to="https://www.meetrics.$1/"/>
<rule from="^http://s62\.research\.de\.com/" to="https://s62.research.de.com/"/>
<rule from="^http://s400\.meetrics\.net/" to="https://s400.meetrics.net/"/>
<rule from="^http://s400\.mxcdn\.net/" to="https://s400.mxcdn.net/"/>
</ruleset>

<rule from="^http://(dc115|dc107)\.s400.meetrics\.net/" to="https://$2.s400.meetrics.net/"/>

