<!--
	Nonfunctional subdomains:

		- dir *
		- hosts *

	* Shows www, mismatched

-->
<ruleset name="Linux Forums (partial)">

	<target host="linuxforums.org" />
	<target host="www.linuxforums.org" />


	<securecookie host="^(?:w*\.)?linuxforums\.org$" name=".+" />


	<rule from="^http:" to="https:" />

</ruleset>
