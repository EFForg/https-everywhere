<ruleset name="WikiSpooks">
    <target host="wikispooks.com" />
    <target host="*.wikispooks.com" />

    <securecookie host="^(.*\.)?wikispooks\.com$" name=".+" />

    <rule from="^https?://wikispooks\.com/"
        to="https://www.wikispooks.com/" />
    <rule from="^http://([^/:@]+)?\.wikispooks\.com/"
        to="https://$1.wikispooks.com/" />
</ruleset>
