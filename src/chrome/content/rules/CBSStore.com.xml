<!--
	Non-functional hosts:
		SSL error:
		- cbsstore.com
-->
<ruleset name="CBSStore.com">

	<target host="cbsstore.com" />
	<target host="www.cbsstore.com" />
	<target host="secure.cbsstore.com" />


	<rule from="^http://cbsstore\.com/"
		to="https://www.cbsstore.com/" />

	<rule from="^http:"
		to="https:" />

</ruleset>
