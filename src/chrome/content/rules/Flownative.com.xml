<!--
	NB: Server sends no certificate chain, see https://whatsmychaincert.com

-->
<ruleset name="Flownative.com" default_off="missing certificate chain">

	<!--	Direct rewrites:
				-->
	<target host="flownative.com" />
	<target host="www.flownative.com" />


	<rule from="^http:"
		to="https:" />

</ruleset>
