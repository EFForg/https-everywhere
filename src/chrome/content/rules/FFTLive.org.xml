<!--
	^fftlive.org: Mismatched


	Insecure cookies are set for these hosts:

		- www.fftlive.org

-->
<ruleset name="FFTLive.org">

	<!--	Direct rewrites:
				-->
	<target host="www.fftlive.org" />

	<!--	Complications:
				-->
	<target host="fftlive.org" />


	<!--	Not secured by server:
					-->
	<!--securecookie host="^www\.fftlive\.org$" name="^ASP\.NET_SessionId$" /-->

	<securecookie host="^\w" name="." />


	<rule from="^http://fftlive\.org/"
		to="https://www.fftlive.org/" />

	<rule from="^http:"
		to="https:" />

</ruleset>
