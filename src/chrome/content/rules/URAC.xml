<!--
	For other Matrix Group coverage, see Matrix_Group.xml.


	Problematic subdomains:

		- ^	(cert only matches www)

-->
<ruleset name="URAC">

	<target host="urac.org" />
	<target host="www.urac.org" />


	<securecookie host="^www\.urac\.org$" name=".+" />


	<rule from="^http://urac\.org/"
		to="https://www.urac.org/" />
	<rule from="^http:"
		to="https:" />

</ruleset>
