<!--
	For other Expedia coverage, see Expedia.com.xml


	Non-functional subdomains:

		- expedia.de	(t)
		- amerikanischesabenteuer	(m)
		- autan	(m)
		- www.booking	(m)
		- business	(t)
		- charter	(i)
		- deals	(r)
		- entdecker	(r)
		- ferienwohnung	(i)
		- findedeinmelia	(m)
		- gruppen	(r)
		- inside	(t)
		- jordan	(r)
		- traveliq.lufthansa	(m)
		- maps	(t)
		- marketing	(r)
		- meeting	(r)
		- msn	(t)
		- partner		- (r)
		- partners	(r)
		- pauschal	(i)
		- pioneer	(m)
		- press	(t)
		- reisen	(t)
		- reisewende	(m)
		- rundreisen	(t)
		- strand-inspirator	(r)
		- support	(i)
		- www.vangogh2015	(m)
		- werbung	(t)
		- widgets	(t)

	e: expired certificate
	h: http redirect
	i: invalid certificate chain
	m: certificate mismatch
	r: connection refused
	s: self-signed certificate
	t: timeout on https
-->
<ruleset name="Expedia.de">

	<target host="expedia.de" />
	<target host="www.expedia.de" />
