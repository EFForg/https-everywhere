<!--
	For rules not causing false/broken MCB, see Defense_One.com.xml.


	s://d.../.+ redirects to s://w.../$

-->
<ruleset name="Defense One.com (false MCB)" platform="mixedcontent">

	<target host="defenseone.com" />
	<target host="www.defenseone.com" />


	<rule from="^http://(?:www\.)?defenseone\.com/"
		to="https://www.defenseone.com/" />

</ruleset>
