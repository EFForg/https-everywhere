<!--
	!www: cert only matches www.


	Mixed images from graph.digiseller.ru

-->
<ruleset name="Plati.ru" default_off="self-signed">

	<target host="exaccess.ru" />
	<target host="www.exaccess.ru" />
	<target host="plati.ru" />
	<target host="www.plati.ru" />


	<securecookie host="^www\.plati\.ru$" name=".+" />


	<rule from="^http://(?:www\.)?(?:exaccess|plati)\.ru/"
		to="https://www.plati.ru/" />

</ruleset>