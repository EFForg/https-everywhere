<!--
	^metachris.com: Mismatched, self-signed

-->
<ruleset name="metachris.com">

	<!--	Direct rewrites:
				-->
	<target host="www.metachris.com" />

	<!--	Complications:
				-->
	<target host="metachris.com" />


	<securecookie host="^\w" name="." />


	<rule from="^http://metachris\.com/"
		to="https://www.metachris.com/" />

	<rule from="^http:"
		to="https:" />

</ruleset>
