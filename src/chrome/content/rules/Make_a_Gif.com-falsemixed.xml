<!--
	For rules not causing false/broken MCB, see Make_a_Gif.com.xml.

-->
<ruleset name="Make a Gif.com (false MCB)" platform="mixedcontent">

	<target host="makeagif.com" />
	<target host="*.makeagif.com" />


	<securecookie host="^\.makeagif\.com$" name=".+" />


	<rule from="^http://makeagif\.com/"
		to="https://makeagif.com/" />

</ruleset>
