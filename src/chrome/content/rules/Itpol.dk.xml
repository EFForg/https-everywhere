<!--
	Problematic domains

		- admin ¹²
		- pad ¹³
		- wiki ¹³

	¹: Redirects to ^, yet HSTS includeSubDomains is specified for itpol.dk.
	²: 'Unknown protocol' on HELLO
	³: Bad CN in cert.
-->
<ruleset name="Itpol.dk">

	<target host="it-pol.dk" />
	<target host="www.it-pol.dk" />

	<rule from="^http://(www\.)?it-?pol\.dk/"
		to="https://$1itpol.dk/" />

</ruleset>
