<!--
	For rules causing false/broken MCB, see IAB.org-falsemixed.xml.

-->
<ruleset name="IAB.org (partial)">

	<target host="iab.org" />
	<target host="www.iab.org" />
		<!--
			Avoid broken MCB:
						-->
		<exclusion pattern="^http://(?:www\.)?iab\.org/+(?!favicon\.ico|wp-content/|wp-includes/)" />


	<rule from="^http://(www\.)?iab\.org/"
		to="https://$1iab.org/" />

</ruleset>
