<ruleset name="opendatacity">
	<target host="opendatacity.de"/>
	<target host="www.opendatacity.de"/>
	<target host="apps.opendatacity.de"/>
	<target host="labs.opendatacity.de"/>

	<rule from="^http://(?:www\.)?opendatacity\.de/"
			to="https://opendatacity.de/"/>
	<rule from="^http://apps\.opendatacity\.de/"
			to="https://apps.opendatacity.de/"/>
	<rule from="^http://labs\.opendatacity\.de/"
			to="https://labs.opendatacity.de/"/>
</ruleset>
