<ruleset name="Vidling" default_off="mismatch">

	<!--	Cert: *.heroku.com	-->
	<target host="vidling.com" />
	<target host="www.vidling.com" />


	<securecookie host="^vidling\.com$" name=".*" />


	<!--	www redirects to !www.	-->
	<rule from="^http://(?:www\.)?vidling\.com/"
		to="https://vidling.com/" />

</ruleset>
