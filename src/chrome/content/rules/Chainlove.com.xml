<!--
	Problematic subdomains:

		- ^ *
		- images *

	* Akamai

-->
<ruleset name="Chainlove.com">

	<!--	Direct rewrites:
				-->
	<target host="www.chainlove.com" />

	<!--	Complications:
				-->
	<target host="chainlove.com" />
	<target host="images.chainlove.com" />


	<rule from="^http://(?:images\.)?chainlove\.com/"
		to="https://www.chainlove.com/" />

	<rule from="^http:"
		to="https:" />

</ruleset>
