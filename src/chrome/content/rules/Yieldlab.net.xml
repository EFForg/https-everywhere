<!--
	For other Yieldlab coverage, see Yieldlab.de.xml.

-->
<ruleset name="Yieldlab.net">

	<target host="*.yieldlab.net" />


	<!--	Tracking cookie set by ad:
						-->
	<securecookie host="^\.yieldlab\.net$" name="^id$" />

	<rule from="^http://(ad|probe)\.yieldlab\.net/"
		to="https://$1.yieldlab.net/" />

</ruleset>