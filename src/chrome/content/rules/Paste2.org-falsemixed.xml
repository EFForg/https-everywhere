<!--
	Fpr rules not causing false/broken MCB, see Paste2.org.xml.

-->
<ruleset name="Paste2.org (false MCB)" platform="mixedcontent">

	<!--	Direct rewrites:
				-->
	<target host="paste2.org" />
	<target host="www.paste2.org" />


	<!--	(avoiding cf_clearance <= mixed content)
								-->
	<securecookie host="^\.paste2\.org$" name="^(?:__cfduid|__qca)$" />


	<rule from="^http:"
		to="https:" />

</ruleset>
