<!--
	Problematic subdomains:

		- static

-->
<ruleset name="WebStarts">

	<target host="webstarts.com" />
	<target host="*.webstarts.com" />


	<securecookie host="^(?:www\.)?webstarts\.com$" name=".+" />


	<rule from="^http://(?:static\.|(www\.))?webstarts\.com/"
		to="https://$1webstarts.com/" />

</ruleset>
