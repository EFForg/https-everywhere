<ruleset name="Florida Institute of Technology (partial)" platform="mixedcontent mixedcontent">

	<target host="fit.edu" />
	<target host="alumni.fit.edu" />
	<target host="assets.fit.edu" />
	<target host="cas.fit.edu" />
	<target host="cs.fit.edu" />
	<target host="events.fit.edu" />
	<target host="lists.fit.edu" />
	<target host="media.fit.edu" />
	<target host="online.fit.edu" />
	<target host="pantherpass.fit.edu" />
	<target host="services.fit.edu" />
	<target host="spam.fit.edu" />
	<target host="tracks.fit.edu" />
	<target host="webmail.fit.edu" />
	<target host="www.fit.edu" />

	<rule from="^http://(www\.)?fit\.edu/"
		to="https://www.fit.edu/" />

	<rule from="^http://(411|(asset|ca|c|event|list|pantherpas|service|track)s|alumni|media|online|spam|webmail)\.fit\.edu/"
		to="https://$1.fit.edu/" />

	<rule from="^http://(go|(portal\.)?my)\.fit\.edu/"
		to="https://portal.my.fit.edu/" />

</ruleset>
