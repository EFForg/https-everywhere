<ruleset name="JustPaste.it">
	<target host=    "justpaste.it" />
	<target host="www.justpaste.it" />

  	<securecookie host="^justpaste\.it$" name=".*" />

	<rule from="^http://www\.justpaste\.it/"
		to="https://justpaste.it/" />

		<test url="http://justpaste.it/" />

	<rule from="^http:"
		to="https:" />
</ruleset>
