<!--
	.nl is a simple redirect to .com.

-->
<ruleset name="Blendle.com">

	<target host="blendle.com" />
	<target host="www.blendle.com" />
	<target host="blendle.nl" />
	<target host="www.blendle.nl" />


	<rule from="^http://(www\.)?blendle\.(com|nl)/"
		to="https://$1blendle.$2/" />

</ruleset>
