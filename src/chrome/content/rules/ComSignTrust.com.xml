<!--
	^: blank

-->
<ruleset name="ComSignTrust.com">

	<target host="comsigntrust.com" />
	<target host="*.comsigntrust.com" />


	<!--	Not secured by server:
					-->
	<!--securecookie host="^(blog|www)\.comsigntrust\.com$" name="^PHPSESSID$" /-->

	<securecookie host="^(?:blog|www)?\.comsigntrust\.com$" name=".+" />


	<rule from="^http://(?:www\.)?comsigntrust\.com/"
		to="https://www.comsigntrust.com/" />

	<rule from="^http://blog\.comsigntrust\.com/"
		to="https://blog.comsigntrust.com/" />

</ruleset>
