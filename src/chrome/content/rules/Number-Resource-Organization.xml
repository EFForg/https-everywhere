<!--
	Number Resource Organization


	^nro.net: Mismatched

-->
<ruleset name="NRO.net">

	<!--	Direct rewrites:
				-->
	<target host="www.nro.net"/>

	<!--	Complications:
				-->
	<target host="nro.net"/>


	<securecookie host="^(?:.*\.)nro\.net$" name=".*"/>


	<rule from="^http://nro\.net/"
		to="https://www.nro.net/"/>

	<rule from="^http:"
		to="https:" />

</ruleset>
