<!--
     Deutsche Börse
-->

<ruleset name="Deutsche Börse Group">
    <target host="deutsche-boerse.com" />
    <target host="www.deutsche-boerse.com" />

    <rule from="^http://deutsche-boerse\.com/"
        to="https://www.deutsche-boerse.com/" />
    <rule from="^http://([^/:@]+)?\.deutsche-boerse\.com/"
        to="https://$1.deutsche-boerse.com/" />
</ruleset>
