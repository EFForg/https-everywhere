<ruleset name="Man Group">
    <target host="man.com" />
    <target host="*.man.com" />

    <securecookie host="^(?:.*\.)?man\.com$" name=".+" />

    <rule from="^http://man\.com/"
        to="https://www.man.com/" />
    <rule from="^http://([^/:@]+)?\.man\.com/"
        to="https://$1.man.com/" />
</ruleset>
