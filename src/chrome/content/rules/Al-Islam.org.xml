<!--
	Invalid certificate:
	- beta.al-islam.org
	- blog.al-islam.org
	- quran.al-islam.org
	- supplications.al-islam.org

-->
<ruleset name="Al-Islam.org">
	<target host="al-islam.org" />
	<target host="www.al-islam.org" />

	<securecookie host="^www\.al-islam\.org$" name=".+" />

	<rule from="^http:" to="https:" />
</ruleset>
