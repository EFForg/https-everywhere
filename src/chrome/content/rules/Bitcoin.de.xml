<!--
	Insecure cookies are set for these domains:

		- .bitcoin.de

-->
<ruleset name="Bitcoin.de">

	<target host="bitcoin.de" />
	<target host="blog.bitcoin.de" />
	<target host="forum.bitcoin.de" />
	<target host="www.bitcoin.de" />


	<!--	CloudFlare cookies:
					-->
	<!--securecookie host="^\.bitcoin\.de$" name="^(__cfuid|cf_clearance)$" /-->

	<securecookie host="^.*\.bitcoin\.de$" name=".+" />


	<rule from="^http:"
		to="https:" />

</ruleset>
