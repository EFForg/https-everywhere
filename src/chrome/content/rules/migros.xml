<!--
HTTP redirect:
	- migros.ch
	- www.migros.ch
	- captormania.migros.ch
-->
<ruleset name="Migros">
	<target host="melectronics.ch"/>
	<target host="www.melectronics.ch"/>
	<target host="migrosbank.ch"/>
	<target host="www.migrosbank.ch"/>
	<target host="100ideen.migros.ch"/>
	<target host="migros-service.migros.ch"/>
	<target host="minimania.migros.ch"/>
	<target host="filialen.migros.ch"/>
	<target host="shop.m-budget.migros.ch"/>
	<target host="m-fanshop.ch"/>
	<target host="www.m-fanshop.ch"/>
	<target host="migros-shop.de"/>
	<target host="www.migros-shop.de"/>
	<target host="migrosmagazin.ch"/>
	<target host="www.migrosmagazin.ch"/>
	<target host="migrosmagazine.ch"/>
	<target host="www.migrosmagazine.ch"/>
	<target host="brueckenbauer.ch"/>
	<target host="www.brueckenbauer.ch"/>

	<rule from="^http://(?:www\.)?migros\.ch/"
		to="https://www.migros.ch/"/>
	<rule from="^http://(?:www\.)?melectronics\.ch/"
		to="https://www.melectronics.ch/"/>
	<rule from="^http://(?:www\.)?migrosbank\.ch/"
		to="https://www.migrosbank.ch/"/>
	<rule from="^http://(?:www\.)?m-fanshop\.ch/"
		to="https://www.m-fanshop.ch/"/>
	<rule from="^http://(?:www\.)?migros-shop\.de/"
		to="https://www.migros-shop.de/"/>
	<rule from="^http://(?:www\.)?migrosmagazin\.ch/"
		to="https://www.migrosmagazin.ch/"/>
	<rule from="^http://(?:www\.)?migrosmagazine\.ch/"
		to="https://www.migrosmagazine.ch/"/>
	<rule from="^http://(?:www\.)?brueckenbauer\.ch/"
		to="https://www.migrosmagazin.ch/"/>
	<rule from="^http:"
		to="https:"/>
</ruleset>
