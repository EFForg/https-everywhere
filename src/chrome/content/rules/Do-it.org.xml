<!--
	www.do-it.org: Mismatched

-->
<ruleset name="Do-it.org">

	<!--	Direct rewrites:
				-->
	<target host="do-it.org" />

	<!--	Complications:
				-->
	<target host="www.do-it.org" />


	<securecookie host="^\w" name="." />
	<securecookie host="^\." name="^_gat?$" />


	<rule from="^http://www\.do-it\.org/"
		to="https://do-it.org/" />

	<rule from="^http:"
		to="https:" />

</ruleset>
