<ruleset name="LiveNation.fi (partial)">
	<target host="livenation.fi" />
	<target host="www.livenation.fi" />

	<rule from="^http://livenation\.fi/"
			to="https://www.livenation.fi/" />

	<rule from="^http:"
			to="https:" />
</ruleset>
