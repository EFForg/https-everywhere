	<!--	Not secured by server:
					-->
	<!--securecookie host="^\.www\.karwansaraypublishers\.com$" name="^(epc-initiated|frontend)$" /-->


	<rule from="^http://(?:www\.)?karwansaraypublishers\.com/"
		to="https://karwansaraypublishers.com/"/>

</ruleset>
