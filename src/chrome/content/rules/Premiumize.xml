<ruleset name="Premiumize.me">

	<target host="premiumize.me" />
	<target host="*.premiumize.me" />


	<securecookie host="^secure\.premiumize\.me$" name=".*" />


	<!--	Server redirects as so.		-->
	<rule from="^http://(?:secure\.|www\.)?premiumize\.me/"
		to="https://secure.premiumize.me/" />

</ruleset>
