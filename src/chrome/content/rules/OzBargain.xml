<ruleset name="OzBargain">
  <target host="ozbargain.com.au" />
  <target host="www.ozbargain.com.au" />
  <target host="ozb.me" />
  <target host="*.ozb.me" />

  <rule from="^https?://(?:www\.)?ozbargain\.com\.au/"
  		  to="https://www.ozbargain.com.au/"/>
  <rule from="^http://cdn\.ozb\.me/"
  		  to="https://cdn.ozb.me/" />
  <!-- nonfunctioning:
		- files.ozbargain.com.au
 	-->
</ruleset>
