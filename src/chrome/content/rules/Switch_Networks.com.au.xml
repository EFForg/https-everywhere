<!--
	Fully covered subdomains:

		- (www.)?
		- whmcs

-->
<ruleset name="Switch Networks.com.au">

	<target host="switchnetworks.com.au" />
	<target host="*.switchnetworks.com.au" />


	<rule from="^http://(whmcs\.|www\.)?switchnetworks\.com\.au/"
		to="https://$1switchnetworks.com.au/" />

</ruleset>
