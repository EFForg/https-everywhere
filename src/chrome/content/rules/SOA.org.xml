<!--
	Nonfunctional hosts in *soa.org:

		- jobs ʰ

	ʰ Redirects to http

-->
<ruleset name="SOA.org (partial)">

	<target host="soa.org" />
	<target host="www.soa.org" />


	<securecookie host="^\w" name=".+" />
	<securecookie host="^\." name="^(?:incap_ses_\d+|visid_incap)_\d+$" />


	<rule from="^http:"
		to="https:" />

</ruleset>
