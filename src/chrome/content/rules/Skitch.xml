<!--
	CDN buckets:

		- skitch-img.s3.amazonaws.com

-->
<ruleset name="Skitch">
  <target host="skitch.com" />
  <target host="www.skitch.com" />
  <target host="img.skitch.com" />

	<securecookie host=".+" name=".+"/>

  <rule from="^http://(?:www\.)?skitch\.com/"
          to="https://skitch.com/" />
  <rule from="^http:" to="https:" />
</ruleset>
