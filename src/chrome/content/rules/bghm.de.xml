<!--
	This domain contains a wildcard DNS and a wildcard certificate.

	Not found:
		entwicklung.extranet.bghm.de
		rollin.extranet.bghm.de
		www.receiver.bghm.de
-->
<ruleset name="BGHM.de">

	<target host="bghm.de" />
	<target host="*.bghm.de" />

	<test url="http://www.bghm.de/" />

	<!-- Invalid certificate -->
	<exclusion pattern="^http://av\.bghm\.de/" />
	<test url="http://av.bghm.de/" />
	<exclusion pattern="^http://www\.fa-mfs\.bghm\.de/" />
	<test url="http://www.fa-mfs.bghm.de/" />
	<exclusion pattern="^http://lswebdir\.bghm\.de/" />
	<test url="http://lswebdir.bghm.de/" />
	<exclusion pattern="^http://meetings\.bghm\.de/" />
	<test url="http://meetings.bghm.de/" />
	<exclusion pattern="^http://webext2013\.bghm\.de/" />
	<test url="http://webext2013.bghm.de/" />
	<exclusion pattern="^http://www\.zielnull\.bghm\.de/" />
	<test url="http://www.zielnull.bghm.de/" />

	<!-- Refused -->
	<exclusion pattern="^http://av2013\.bghm\.de/" />
	<test url="http://av2013.bghm.de/" />
	<exclusion pattern="^http://webcon2013\.bghm\.de/" />
	<test url="http://webcon2013.bghm.de/" />

	<!-- Timeout -->
	<exclusion pattern="^http://corporate-access\.bghm\.de/" />
	<test url="http://corporate-access.bghm.de/" />

	<securecookie host=".+" name=".+" />

	<rule from="^http:" to="https:" />

</ruleset>
