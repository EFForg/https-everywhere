<ruleset name="Bundeskanzlerin">
    <target host="bundeskanzlerin.de" />
    <target host="*.bundeskanzlerin.de" />

    <rule from="^http://bundeskanzlerin\.de/"
        to="https://www.bundeskanzlerin.de/"/>
    <rule from="^http://([^/:@]+)?\.bundeskanzlerin\.de/"
        to="https://$1.bundeskanzlerin.de/" />
</ruleset>
