<ruleset name="ETHZ.ch (problematic)" default_off="expired, missing certificate chain">

	<!--	Direct rewrites:
				-->
	<target host="www.ethlife.ethz.ch" />
	<target host="wiki.se.inf.ethz.ch" />

	<!--	Complications:
				-->
