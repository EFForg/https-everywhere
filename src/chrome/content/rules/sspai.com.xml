<!--
	InvalidCert:
		cdn.sspai.com	( equal to ^ )
		matrix.sspai.com
		test.sspai.com

	Different content between http and https:
		www.sspai.com	( equal to ^ )

	MCB:
		^	( images from cdn.sspai.com )
-->

<ruleset name="sspai.com">

	<target host="sspai.com" />
	<target host="cdn.sspai.com" />
	<target host="www.sspai.com" />
	
	<rule from="^http://(cdn\.|www\.)?sspai\.com/" to="https://sspai.com/" />

</ruleset>
