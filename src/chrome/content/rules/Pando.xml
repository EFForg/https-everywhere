	Insecure cookies are set for these domains:

		- .pando.com

-->
<ruleset name="Pando.com">

	<target host="pando.com" />
	<target host="www.pando.com" />


	<!--	CloudFlare cookies:
					-->
	<!--securecookie host="^\.pando\.com$" name="^(?:__cfuid|cf_clearance)$" /-->

	<securecookie host="^(?:.*\.)?pando\.com$" name=".*" />


	<rule from="^http:"
		to="https:" />

</ruleset>
