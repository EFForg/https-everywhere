<!--
	For other MODX coverage, see MODX.xml.


	Problematic domains:

		- www.modxcloud.com	(cert only matches ^)

-->
<ruleset name="MODX Cloud">

	<target host="modxcloud.com" />
	<target host="www.modxcloud.com" />


	<rule from="^http://(?:www\.)?modxcloud\.com/"
		to="https://modxcloud.com/" />

</ruleset>
