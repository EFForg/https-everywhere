<!--
	Apple China

	For other Apple coverage, see Apple.xml.


	- !www: no https
	- www:	akamai

-->
<ruleset name="Apple.com.cn" default_off="mismatched">

	<!--	Direct rewrites:
				-->
	<target host="www.apple.com.cn" />

	<!--	Complications:
				-->
	<target host="apple.com.cn" />


	<rule from="^http://apple\.com\.cn/"
		to="https://www.apple.com.cn/" />

	<rule from="^http:"
		to="https:" />

</ruleset>
