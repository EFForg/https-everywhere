<!--
	(www.)?: Handshake fails

-->
<ruleset name="InTechnology.co.uk" default_off="handshake failure">

	<target host="intechnology.co.uk" />
	<target host="www.intechnology.co.uk" />


	<securecookie host="^(?:www\.)?intechnology\.co\.uk$" name=".+" />


	<rule from="^http:" to="https:" />

</ruleset>
