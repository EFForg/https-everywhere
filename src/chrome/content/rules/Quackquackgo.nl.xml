<!--
	www: cert only matches ^quackquackgo.nl


	Expired 2014.

-->
<ruleset name="quackquackgo.nl" default_off="expired" platform="cacert">

	<target host="quackquackgo.nl" />
	<target host="www.quackquackgo.nl" />


	<rule from="^http://(?:www\.)?quackquackgo\.nl/"
		to="https://quackquackgo.nl/" />

</ruleset>
