<!--
	For rules not causing false/broken MCB, see The_Atlantic.com.xml.

-->
<ruleset name="The Atlantic.com (false MCB)" platform="mixedcontent">

	<!--	Direct rewrites:
				-->
	<target host="www.theatlantic.com" />

	<!--	Complications:
				-->
	<target host="theatlantic.com" />


	<rule from="^http://theatlantic\.com/"
		to="https://www.theatlantic.com/" />

	<rule from="^http:"
		to="https:" />

</ruleset>
