<ruleset name="XMPP.ru.net">

	<!--	Direct rewrites:
				-->
	<target host="www.xmpp.ru.net" />

	<!--	Complications:
				-->
	<target host="xmpp.ru.net" />


	<!--	Not secured by server:
					-->
	<!--securecookie host="^\.xmpp\.ru\.net$" name="^SESS[\da-f]{32}$" /-->

	<securecookie host="^\.xmpp\.ru\.net$" name=".+" />


	<!--	Redirect keeps forward
		slash, path, and args:
					-->
