<!--

	For other eBay coverage, see ebay.com.xml


	Other eBay Classifieds rulesets:

		- alaMaula.com.xml
		- BilBasen.dk.xml
		- Close5.com.xml
		- DBA.dk.xml
		- 2dehands.be.xml
		- Gumtree.com.xml
		- Kijiji.ca.xml
		- Marktplaats.nl.xml
		- mobile.de.xml
		- Vivanuncios.com.mx.xml
		

	Non-functional subdomains:

		- treebay	(HTTP 404 error)
		- sandbox.treebay	(t)

	e: expired certificate
	h: http redirect
	i: invalid certificate chain
	m: certificate mismatch
	r: connection refused
	s: self-signed certificate
	t: timeout on https
-->
<ruleset name="eBay Classifieds Group">

	<target host="ebayclassifiedsgroup.com" />
	<target host="www.ebayclassifiedsgroup.com" />
	<target host="static.ebayclassifiedsgroup.com" />

	<rule from="^http:"
		to="https:" />
</ruleset>