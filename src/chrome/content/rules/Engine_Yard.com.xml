<!--
	Fully covered subdomains:

		- (www.)?
		- blog
		- support.cloud
		- login

-->
<ruleset name="Engine Yard.com">

	<target host="engineyard.com" />
	<target host="*.engineyard.com" />


	<rule from="^http://((?:blog|support\.cloud|login|www)\.)?engineyard\.com/"
		to="https://$1engineyard.com/" />

</ruleset>
