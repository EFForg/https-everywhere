<!--
	Fully covered subdomains:

		- (www.)?
		- blog
		- developers
		- panel
		- status

-->
<ruleset name="Get Mango.com">

	<target host="getmango.com" />
	<target host="*.getmango.com" />


	<rule from="^http://((?:blog|developers|panel|status|www)\.)?getmango\.com/"
		to="https://$1getmango.com/" />

</ruleset>
