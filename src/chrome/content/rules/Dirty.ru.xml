<!--
	Problematic hosts in *dirty.ru:

		- pit *

	* Server sends no certificate chain, see https://whatsmychaincert.com


	Fully covered hosts in *dirty.ru:

		- pit

-->
<ruleset name="dirty.ru" default_off="missing certificate chain">

	<!--	Direct rewrites:
				-->
	<target host="pit.dirty.ru" />


	<rule from="^http:"
		to="https:" />

</ruleset>
