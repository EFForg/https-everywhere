<!--
	Nonfunctional subdomains:

		- bugzilla *
		- lists *
		- packages *

	* Redirects to ^atrpms.net; mismatched, CN: atrpms.net

-->
<ruleset name="ATrpms.net" default_off="self-signed">

	<target host="atrpms.net" />
	<target host="www.atrpms.net" />


	<rule from="^http://(?:www\.)?atrpms\.net/"
		to="https://atrpms.net/" />

</ruleset>
