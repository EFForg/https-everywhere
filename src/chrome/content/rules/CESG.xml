<ruleset name="CESG">
    <target host="cesg.gov.uk" />
    <target host="www.cesg.gov.uk" />

    <rule from="^http://cesg\.gov\.uk/"
        to="https://www.cesg.gov.uk/" />
    <rule from="^http://([^/:@]+)?\.cesg\.gov\.uk/"
        to="https://$1.cesg.gov.uk/" />
</ruleset>
