<!--
	Non-functional domains:
		- censurfridns.dk
		- www.censurfridns.dk
-->

<ruleset name="Censurfridns (self-signed)" default_off="self-signed">
	<target host="censurfridns.dk" />
	<target host="*.censurfridns.dk" />

	<rule from="^http://blog\.censurfridns\.dk/"
		to="https://blog.censurfridns.dk/"/>
</ruleset>
