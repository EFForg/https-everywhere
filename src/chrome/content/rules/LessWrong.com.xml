<!--
	Invalid certificate:
		www.lesswrong.com
		images.lesswrong.com

-->
<ruleset name="LessWrong.com">

	<target host="lesswrong.com" />
	<target host="www.lesswrong.com" />
	<target host="wiki.lesswrong.com" />

	<rule from="^http://www\.lesswrong\.com/"
		to="https://lesswrong.com/" />

	<rule from="^http:"
		to="https:" />

</ruleset>
