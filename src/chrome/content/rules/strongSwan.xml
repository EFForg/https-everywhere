<ruleset name="strongSwan">
    <target host="strongswan.org" />
    <target host="*.strongswan.org" />

    <!--    Observed cookie domains:
        - ^/
        - ^wiki
        - ^www
        -->
    <securecookie host=".+" name=".+" />


    <!--    Observed subdomains:
            - download
            - git
            - wiki
            - www
    -->
    <rule from="^http://(\w+\.)?strongswan\.org/"
        to="https://$1strongswan.org/" />
</ruleset>
