<!--
	Nonfunctional hosts in *.148apps.com:

	h: http redirect
	m: certificate mismatch
	r: connection refused
	s: self-signed certificate
	t: timeout on https
-->
<ruleset name="148apps">
	<target host="148apps.com" />
	<target host="www.148apps.com" />

	<rule from="^http:" to="https:" />
</ruleset>
