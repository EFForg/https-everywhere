<!--
	Invalid certificate:
		- whoisjuan.me, equivalent to www.whoisjuan.me
-->

<ruleset name="WhoIsJuan.me">
	<target host="whoisjuan.me" />
	<target host="www.whoisjuan.me" />

	<rule from="^http://whoisjuan\.me/"
		to="https://www.whoisjuan.me/" />

	<rule from="^http:" to="https:" />
</ruleset>
