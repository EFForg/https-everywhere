<!--
	Partially covered subdomains:

		- (www.)

		- support	(same as www)


	Fully covered subdomains:

		- owa
		- webmail2

-->
<ruleset name="Space2u (partial)">

	<target host="space2u.com" />
	<target host="www.space2u.com" />
	<target host="support.space2u.com" />
	<target host="webmail.space2u.com" />
	<target host="webmail01.space2u.com" />


	<rule from="^http:"
		to="https:" />

</ruleset>
