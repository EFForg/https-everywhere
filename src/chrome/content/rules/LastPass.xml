<ruleset name="LastPass.com">
  <target host="lastpass.com" />
  <target host="*.lastpass.com" />

  <securecookie host="^(?:.+\.)?lastpass\.com$" name=".*"/>

  <rule from="^http:" to="https:" />
  
  <test url="http://helpdesk.lastpass.com/" />
  <test url="http://blog.lastpass.com/" />
  <test url="http://forums.lastpass.com/" />
  <test url="http://enterprise.lastpass.com/" />
</ruleset>
