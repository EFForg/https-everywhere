<ruleset name="LastPass.com" default_off="Breaks addon ( https://www.eff.org/r.3Dq )">
  <target host="lastpass.com" />
  <target host="*.lastpass.com" />

  <securecookie host="^(?:.+\.)?lastpass\.com$" name=".*"/>

  <rule from="^http://(?:www\.)?lastpass\.com/" to="https://lastpass.com/"/>

	<rule from="^http://forums\.lastpass\.com/"
		to="https://forums.lastpass.com/" />
</ruleset>
