<!--
	Nonfunctional hosts in *.eldarya.es:

	h: http redirect
	m: certificate mismatch
	r: connection refused
	s: self-signed certificate
	t: timeout on https
-->
<ruleset name="Eldarya.es">
	<target host="eldarya.es" />
	<target host="www.eldarya.es" />

	<rule from="^http:" to="https:" />
</ruleset>
