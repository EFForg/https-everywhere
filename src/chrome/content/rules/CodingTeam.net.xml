<!--

	Problematic hosts in *codingteam.net:

		- www (mismatched)

-->
<ruleset name="CodingTeam.net">

	<target host="codingteam.net" />
	<target host="www.codingteam.net" />

	<rule from="^http://www\.codingteam\.net/"
		to="https://codingteam.net/" />

	<rule from="^http:"
		to="https:" />

</ruleset>

