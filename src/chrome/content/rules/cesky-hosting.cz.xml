    <!-- Mixed content
    <test url="http://cesky-hosting.cz" />
    <test url="http://www.cesky-hosting.cz" />
    -->
    <test url="http://webmail.cesky-hosting.cz" />
    <test url="http://webmail2.cesky-hosting.cz" />
    <test url="http://webftp.cesky-hosting.cz" />
    <test url="http://mysql.cesky-hosting.cz" />
    <test url="http://pgsql.cesky-hosting.cz" />
    <test url="http://statistiky.cesky-hosting.cz" />
    <test url="http://chat.cesky-hosting.cz" />

    <rule from="^http:"
            to="https:" />
