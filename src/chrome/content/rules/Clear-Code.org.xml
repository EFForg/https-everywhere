<!--
	^: cert only matches www

-->
<ruleset name="Clear-Code.org" platform="cacert">

	<!--	Direct rewrites:
				-->
	<target host="www.clear-code.com" />

	<!--	Complications:
				-->
	<target host="clear-code.com" />


	<rule from="^http://clear-code\.com/"
		to="https://www.clear-code.com/" />

	<rule from="^http:"
		to="https:" />

</ruleset>
