  <securecookie host="skanetrafiken\.se$" name=".*"/>
  <securecookie host="\.skanetrafiken\.se$" name=".*"/>
  
  <rule from="^https?://(?:www\.)?([^\.]+\.)?skanetrafiken.se/" to="https://www.$1skanetrafiken.se/"/>
</ruleset>