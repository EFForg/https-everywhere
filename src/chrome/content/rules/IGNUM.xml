<!--
	Other IGNUM rulesets:

		- Domena.cz.xml


	Self-signed domains:

		- peklo.core.ignum.cz	(presented by client sites, 403)

-->
<ruleset name="IGNUM.cz (partial)">

	<target host="ignum.cz" />
	<target host="www.ignum.cz" />


	<securecookie host="^(?:www\.)?ignum\.cz$" name=".*" />


	<rule from="^http:"
		to="https:" />

</ruleset>
