<ruleset name="Torrentz Proxy">
	<target host="torrentz-proxy.com" />
	<target host="www.torrentz-proxy.com" />
	
	<securecookie host="torrentz-proxy\.com$" name=".*"/>
	<securecookie host="www.torrentz-proxy\.com$" name=".*"/>
	
	<rule from="^http://(www\.)?torrentz-proxy\.com/" to="https://$1torrentz-proxy.com/" />
</ruleset>
