<!--
	For other Torrentz coverage, see Torrentz.xml.


	Insecure cookies are set for these hosts:

		- torrentz-proxy.com
		- www.torrentz-proxy.com

-->
<ruleset name="Torrentz-Proxy.com">

	<!--	Direct rewrites:
				-->
	<target host="torrentz-proxy.com" />
	<target host="www.torrentz-proxy.com" />
	

	<!--	Not secured by server:
					-->
	<!--securecookie host="(?:www\.)?torrentz-proxy\.com$" name="^sess$" /-->

	<securecookie host="(?:www\.)?torrentz-proxy\.com$" name=".+" />
	

	<rule from="^http:"
		to="https:" />

</ruleset>
