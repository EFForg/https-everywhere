<!--
	These altnames don't exist:

		- store.eager.io

-->
<ruleset name="eager.io">

	<target host="eager.io" />
	<target host="bus.eager.io" />
	<target host="embedded.eager.io" />
	<target host="dev.eager.io" />
	<target host="www.eager.io" />
	<target host="status.eager.io" />



	<rule from="^http://status\.eager\.io/"
		to="https://eager.statuspage.io/" />

	<rule from="^http:" to="https:" />
</ruleset>
