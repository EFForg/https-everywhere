	<target host="www.dashlane.com" />

	<!--	Complications:
				-->
	<target host="dashlane.com" />


	<!--	Not secured by server:
					-->
	<!--securecookie host="^www\.dashlane\.com$" name="^(?:kw_ab_download_full|webanonid)$" /-->

	<securecookie host="^\." name="^_ga(?:t?$|t_)" />
