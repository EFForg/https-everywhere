<!--
	HTTP-only content:
		- www.teamliquid.net

	Invalid certificatE:
		- wiki.teamliquid.net, equivalent to liquipedia.net
-->

<ruleset name="Team Liquid">
	<!-- liquipedia.net -->
	<target host="liquipedia.net" />
	<target host="www.liquipedia.net" />


	<!-- teamliquid.net -->
	<target host="teamliquid.net" />
	<target host="wiki.teamliquid.net" />

	<rule from="^http://wiki\.teamliquid\.net/"
		to="https://liquipedia.net/" />


	<rule from="^http:" to="https:" />
</ruleset>
