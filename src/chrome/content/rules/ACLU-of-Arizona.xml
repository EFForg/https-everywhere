<ruleset name="ACLU of Arizona">

	<target host="acluaz.org" />
	<target host="www.acluaz.org" />

	<securecookie host="^www\.acluaz\.org$" name=".+" />

	<rule from="^http:" to="https:" />

</ruleset>
