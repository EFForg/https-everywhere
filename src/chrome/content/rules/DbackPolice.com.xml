<!--
	(www.)?: Refused

-->
<ruleset name="DbackPolice.com" default_off="connection refused">

	<target host="dbackpolice.com" />
	<target host="*.dbackpolice.com" />


	<securecookie host="^\.(?:www\.)?dbackpolice\.com$" name=".+" />


	<rule from="^http://(www\.)?dbackpolice\.com/"
		to="https://$1dbackpolice.com/" />

</ruleset>
