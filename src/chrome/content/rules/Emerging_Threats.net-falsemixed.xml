<!--
	For rules that are on by default, see Emerging_Threats.net.xml.

-->
<ruleset name="Emerging Threats.net (mismatched)" default_off="mismatched">

	<target host="emergingthreats.net" />
	<target host="www.emergingthreats.net" />


	<rule from="^http://emergingthreats\.net/"
		to="https://www.emergingthreats.net/" />

	<rule from="^http:"
		to="https:" />

</ruleset>
