<!--
	Cert only matches *.bookshare.org

-->
<ruleset name="Bookshare">

	<target host="bookshare.org" />
	<target host="www.bookshare.org" />


	<securecookie host="^www\.bookshare\.org$" name=".+" />


	<rule from="^http://(?:www\.)?bookshare\.org/"
		to="https://www.bookshare.org/" />

</ruleset>
