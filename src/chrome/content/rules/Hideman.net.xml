<!--
	Other Hideman rulesets:

		- Hidepay.net.xml


	Fully covered subdomains:

		- (www.)
		- cdn
		- static

-->
<ruleset name="Hideman.net">

	<target host="hideman.net" />
	<target host="*.hideman.net" />


	<securecookie host="^\.hideman\.net$" name=".+" />


	<rule from="^http://((?:cdn|static|www)\.)?hideman\.net/"
		to="https://$1hideman.net/" />

</ruleset>
