<!--
	^rocksbox.com: refused

-->
<ruleset name="Rocksbox.com">

	<!--	Direct rewrites:
				-->
	<target host="www.rocksbox.com" />
	
	<!--	Complications:
				-->
	<target host="rocksbox.com" />


	<securecookie host="^\w" name=".+" />


	<rule from="^http://rocksbox\.com/"
		to="https://www.rocksbox.com/" />

	<rule from="^http:"
		to="https:" />

</ruleset>
