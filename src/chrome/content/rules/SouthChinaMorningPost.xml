<!--
	Redirect to http:
		^scmp.com
		www.scmp.com
-->

<ruleset name="SouthChinaMorningPost" default_off="incomplete certificate chain">

	<target host="cdn1.i-scmp.com" />
	<target host="cdn2.i-scmp.com" />
		<test url="http://cdn2.i-scmp.com/sites/all/libraries/autotrack/autotrack.js" />
	<target host="cdn3.i-scmp.com" />
	<target host="cdn4.i-scmp.com" />

	<rule from="^http:" to="https:" />
</ruleset>
