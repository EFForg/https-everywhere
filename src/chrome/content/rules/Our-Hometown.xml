<ruleset name="Our Hometown">

	<target host="our-hometown.com" />
	<target host="*.our-hometown.com" />


	<securecookie host="^\.our-hometown\.com$" name=".*" />


	<!--	Cert only matches *.our-hometown.com.
							-->
	<rule from="^http://(?:www\.)?our-hometown\.com/"
		to="https://www.our-hometown.com/" />

</ruleset>
