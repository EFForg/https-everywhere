<ruleset name="jtlebi.fr">

	<target host="jtlebi.fr" />
	<target host="*.jtlebi.fr" />


	<!--	Not secured by server:
					-->
	<!--securecookie host="^(blog\.|www\.)?jtlebi\.fr$" name="^PHPSESSID$" /-->

	<securecookie host="^(?:blog\.|www\.)?jtlebi\.fr$" name=".+" />


	<!--	(www.) redirects to blog.
						-->
	<rule from="^http://(?:blog\.|www\.)?jtlebi\.fr/"
		to="https://blog.jtlebi.fr/" />

</ruleset>
