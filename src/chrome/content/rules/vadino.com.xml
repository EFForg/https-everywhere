<!--
	Other Vadino rulesets:

		- seomon.com.xml

-->
<ruleset name="Vadino.com">

	<target host="vadino.com" />
	<target host="www.vadino.com" />


	<securecookie host="^\." name="^(?:__cfduid|cf_clearance)$" />
	<securecookie host=".+" name=".+" />


	<rule from="^http:"
		to="https:" />

</ruleset>
