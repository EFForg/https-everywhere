<!-- gsfacket is a swedish labour union

	Problematic subdomains; "unable to get local issuer certificate":
		- bladet.gsfacket.se
		- gsbox.gsfacket.se
		- webmail.gsfacket.se

	Timout:
		- kaggen.gsfacket.se
		- smtp.gsfacket.se

	http and https not returning same content:
		- beta.gsfacket.se
		- office.gsfacket.se
-->
<ruleset name="Gsfacket.se">
	<target host="gsfacket.se" />
	<target host="www.gsfacket.se" />
	<target host="besok.gsfacket.se" />
	<target host="enkat.gsfacket.se" />
	<target host="mail.gsfacket.se" />

	<rule from="^http:" to="https:"/>
</ruleset>
