<ruleset name="data.gov.hk">

	<target host="data.gov.hk" />
	<target host="www.data.gov.hk" />
	<target host="api.data.gov.hk" />

	<rule from="^http://www\.data\.gov\.hk/"
	to="https://data.gov.hk/" />

	<rule from="^http:" to="https:" />

</ruleset>
