<!--
	Fully covered hosts in *bidswitch.net:

		- x


	Insecure cookies are set for these domains:

		- .bidswitch.net

-->
<ruleset name="bidswitch.net">

	<!--	Direct rewrites:
				-->
	<target host="apac-jp-sync.bidswitch.net" />
	<target host="apac-jp.bidswitch.net" />
	<target host="staging-as-jp-sync.bidswitch.net" />
	<target host="staging-as-jp.bidswitch.net" />
	<target host="x.bidswitch.net" />

	<!--	Not secured by server:
					-->
	<!--securecookie host="^\.bidswitch\.net$" name="^tuuid$" /-->

	<securecookie host="^\.bidswitch\.net$" name=".+" />

	<test url="http://apac-jp-sync.bidswitch.net/sync" />


	<rule from="^http:"
		to="https:" />

</ruleset>
