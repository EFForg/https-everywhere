<!--
	^ourcommonplace.com: 522 over http & https

-->
<ruleset name="OurCommonPlace.com">

	<!--	Direct rewrites:
				-->
	<!--target host="ourcommonplace.com" /-->
	<target host="www.ourcommonplace.com" />


	<securecookie host="^\.ourcommonplace.com$" name=".+" />


	<rule from="^http:"
		to="https:" />

</ruleset>
