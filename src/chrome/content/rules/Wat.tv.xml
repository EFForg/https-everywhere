<ruleset name="Wat.tv platform=mixedcontent">

<target host="*.wat.tv"/>
<target host="wat.tv"/>
<target host="*.wat.fr"/>

<rule from="^http://(www\.)?wat\.tv/" to="https://www.wat.tv/"/>
<rule from="^http://(s4|s5|s6)\.wat\.fr/" to="https://$1.wat.fr/"/>
<rule from="^http://(s|s0|s1|s2|s3)\.wat\.fr/" to="https://s5.wat.fr/"/>
<!-- s+ s1-3  : cert only valid for akamai -->
</ruleset>