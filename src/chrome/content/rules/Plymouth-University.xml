<ruleset name="Plymouth University" platform="mixedcontent">

	<target host="plymouth.ac.uk" />
	<target host="*.plymouth.ac.uk" />


	<securecookie host="^(.*\.)?plymouth\.ac\.uk$" name=".*" />


	<!--
		Encountered subdomains:

			- intranet
			- student
			- www

				-->
	<rule from="^http://(\w+\.)?plymouth\.ac\.uk/"
		to="https://$1plymouth.ac.uk/" />

</ruleset>
