<!--
	For rules that are on by default, see University_of_Potsdam.xml.

-->
<ruleset name="Uni-Potsdam.de (self-signed)" default_off="self-signed">

	<target host="epic.hpi.uni-potsdam.de" />
	<target host="ares.epic.hpi.uni-potsdam.de" />


	<rule from="^http://(ares\.)?epic\.hpi\.uni-potsdam\.de/"
		to="https://$1epic.hpi.uni-potsdam.de/" />

</ruleset>
