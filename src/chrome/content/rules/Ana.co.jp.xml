<!--
	Nonfunctional hosts in *.ana.co.jp:

	h: http redirect
	m: certificate mismatch
	- ana.co.jp
	r: connection refused
	s: self-signed certificate
	t: timeout on https
-->
<ruleset name="Ana.co.jp">
	<target host="www.ana.co.jp" />
	<target host="aswbe-i.ana.co.jp" />
	<target host="ape0.ana.co.jp" />

	<test url="http://www.ana.co.jp/" />
	<test url="http://ape0.ana.co.jp/skyweb/F/ape/snslink?request=snslinkSetting001&country=ot&lang=en" />
	<test url="http://aswbe-i.ana.co.jp/rei12c/international_asw/pages/static/common/sessionerror/session_error_pc_default.xhtml" />
	<rule from="^http:" to="https:" />
</ruleset>
