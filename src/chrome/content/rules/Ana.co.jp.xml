<!--
	Nonfunctional hosts in *.ana.co.jp:

	h: http redirect
	m: certificate mismatch
	- ana.co.jp
	r: connection refused
	s: self-signed certificate
	t: timeout on https
-->
<ruleset name="Ana.co.jp">
	<target host="www.ana.co.jp" />
	<target host="aswbe-i.ana.co.jp" />
	<target host="ape0.ana.co.jp" />

	<rule from="^http:" to="https:" />
</ruleset>
