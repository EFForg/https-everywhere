<ruleset name="OpenIKED.org">

	<target host="openiked.org" />
	<target host="www.openiked.org" />

	<securecookie host=".+" name=".+" />

	<!-- Invalid certificate on https://openiked.org,
		http://openiked.org/ redirects to http://www.openiked.org/ -->
	<rule from="^http://openiked\.org/"
		to="https://www.openiked.org/" />

	<rule from="^http:" to="https:" />

</ruleset>
