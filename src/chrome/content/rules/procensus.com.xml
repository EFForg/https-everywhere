<ruleset name="Procensus">

	<target host="*.procensus.com" />
	<target host="procensus.com" />

	<securecookie host="^.*\.?procensus\.com$" name=".+" />

	<rule from="^http:"
		to="https:" />

	<test url="http://www.procensus.com/" />
	<test url="http://app.procensus.com/" />
	<test url="http://procensus.com/" />

</ruleset>
