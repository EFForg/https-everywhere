	<rule from="^http://(am[mp]oc0\d|autodisover|circportal|cwa|www\.ganweb01|webmail(?:\.\w+)?)\.gannett\.com/"
		to="https://$1.gannett.com/" />

	<rule from="^http://benefits\.gannett\.com/[^\?]*(\?.*)?"
