<!--
	For rules not causing false/broken MCB, see OpenVZ.org.xml.


	Server sends no certificate chain *

	* See https://whatsmychaincert.com

-->
<ruleset name="OpenVZ.org (false MCB)" platform="mixedcontent" default_off="missing certificate chain">

	<!--	Direct rewrites:
				-->
	<target host="forum.openvz.org" />


	<securecookie host="^\.forum\.openvz\.org$" name=".+" />


	<rule from="^http:"
		to="https:" />

</ruleset>
