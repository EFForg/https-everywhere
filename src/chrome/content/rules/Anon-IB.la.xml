<!--
	Insecure laokies are set for these domains:

		- .anon-ib.la

-->
<ruleset name="Anon-IB.la">

	<!--	Direct rewrites:
				-->
	<target host="anon-ib.la" />
	<target host="archive.anon-ib.la" />
	<target host="int.anon-ib.la" />
	<target host="usa.anon-ib.la" />
	<target host="www.anon-ib.la" />


	<!--	CloudFlare laokies:
					-->
	<!--securecookie host="^\.anon-ib\.la$" name="^(?:__cfduid|cf_clearance)$" /-->

	<securecookie host="^\.anon-ib\.la$" name=".+" />


	<rule from="^http:"
		to="https:" />

</ruleset>
