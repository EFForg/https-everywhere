<!--
	For other Epoch coverage, see Epoch.com.xml.


	(www.): configured for rc4 only

-->
<ruleset name="EpochStats.com">

	<target host="epochstats.com" />
	<target host="*.epochstats.com" />


	<rule from="^http://((?:hits|sales|www)\.)?epochstats\.com/"
		to="https://$1epochstats.com/" />

</ruleset>
