<!--
	www.drupalwatchdog.com: Mismatched

-->
<ruleset name="Drupal Watchdog.com">

	<!--	Direct rewrites:
				-->
	<target host="drupalwatchdog.com" />

	<!--	Complications:
				-->
	<target host="www.drupalwatchdog.com" />


	<rule from="^http://www\.drupalwatchdog\.com/"
		to="https://drupalwatchdog.com/" />

	<rule from="^http:"
		to="https:" />

</ruleset>
