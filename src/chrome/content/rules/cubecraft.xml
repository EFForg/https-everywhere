<ruleset name="CubeCraft">
        <target host="cubecraft.net" />
        <target host="*.cubecraft.net" />

        <test url="https://www.cubecraft.net" />
        <test url="https://store.cubecraft.net" />
        <test url="https://cdn.cubecraft.net" />
        <test url="https://cubecraft.statuspage.io" />

        <rule from="^http://status\.cubecrtaft\.net/"
                to="https://cubecraft.statuspage.io/" />

        <rule from="^http:"
                to="https:" />
</ruleset>
