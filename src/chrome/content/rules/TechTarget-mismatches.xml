<!--	See also TechTarget.xml
-->
<ruleset name="TechTarget (mismatches)" default_off="self-signed">

	<target host="searchcloudcomputing.de"/>
	<target host="www.searchcloudcomputing.de"/>
	<target host="searchdatacenter.de"/>
	<target host="www.searchdatacenter.de"/>
	<target host="searchnetworking.de"/>
	<target host="www.searchnetworking.de"/>
	<target host="searchsecurity.de"/>
	<target host="www.searchsecurity.de"/>
	<target host="searchstorage.de"/>
	<target host="www.searchstorage.de"/>

	<rule from="^http://(www\.)?search(cloudcomputing|datacenter|networking|security|storage)\.de/" to="https://www.search$2.de/"/>

	<securecookie host="^www\.search(?:cloudcomputing|datacenter|networking|security|storage)\.de$" name=".*"/>

</ruleset>
