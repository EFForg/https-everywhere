<!--
	For other New York Times coverage, see NYTimes.xml.

	Too many redirects:
		^
		www
-->
<ruleset name="NewYorkTimes.com">
	<target host="newyorktimes.com" />
	<target host="www.newyorktimes.com" />

	<rule from="^http://(www\.)?newyorktimes\.com/"
		to="https://www.nytimes.com/" />

</ruleset>
