<ruleset name="Get Safe Online">
    <target host="getsafeonline.org" />
    <target host="*.getsafeonline.org" />

    <securecookie host="^(?:.*\.)?getsafeonline\.org$" name=".*" />

    <rule from="^http://getsafeonline\.org/"
        to="https://www.getsafeonline.org/" />
    <rule from="^http://([^/:@]+)\.getsafeonline\.org/"
        to="https://$1.getsafeonline.org/" />
</ruleset>
