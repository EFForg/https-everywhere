	STS header includes includeSubdomains


	Insecure cookies are set for these hosts:

		- www.startupsort.com

-->
<ruleset name="StartupSort.com">

	<target host="startupsort.com" />
	<target host="*.startupsort.com" />

		<test url="http://static.startupsort.com/" />
		<test url="http://www.startupsort.com/" />


	<!--	Not secured by server:
					-->
	<!--securecookie host="^www\.startupsort\.com$" name="^connect\.sid$" /-->

	<securecookie host="." name="." />


	<rule from="^http:"
		to="https:" />

</ruleset>
