<!--
	For other Open Doors coverage, see Open_Doors.org.xml.


	Fully covered hosts in *od.org:

		- shared

-->
<ruleset name="OD.org">

	<!--	Direct rewrites:
				-->
	<target host="shared.od.org" />


	<!--	Not secured by server:
					-->
	<securecookie host="^shared\.od\.org$" name=".+" />


	<rule from="^http:"
		to="https:" />

</ruleset>
