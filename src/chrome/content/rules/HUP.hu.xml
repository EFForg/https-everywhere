<!--
	Nonfunctional hosts in *.hup.hu:

	h: http redirect
	m: certificate mismatch
		- wiki.hup.hu
	r: connection refused
	s: self-signed certificate
	t: timeout on https
-->
<ruleset name="HUP">
	<target host="hup.hu" />
	<target host="www.hup.hu" />
	<!-- <target host="wiki.hup.hu" /> m -->

	<rule from="^http:" to="https:" />
</ruleset>
