<!--
    Nonfunctional subdomains:
        - www
-->
<ruleset name="Xdebug.org">
    <target host="xdebug.org" />
    <target host="bugs.xdebug.org" />
    <target host="www.xdebug.org" />

    <securecookie host="^(bugs\.)?xdebug\.org" name=".+" />

    <rule from="^http://www\.xdebug\.org/"
            to="https://xdebug.org/" />

    <rule from="^http:"
            to="https:" />
</ruleset>
