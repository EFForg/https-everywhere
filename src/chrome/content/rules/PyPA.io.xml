<!--
	Fully covered hosts in *pypa.io:

		- (www.)?
		- bootstrap
		- virtualenv

-->
<ruleset name="PyPA.io">

	<!--	Direct rewrites:
				-->
	<target host="pypa.io" />
	<target host="bootstrap.pypa.io" />
	<target host="virtualenv.pypa.io" />
	<target host="www.pypa.io" />


	<rule from="^http:"
		to="https:" />

</ruleset>
