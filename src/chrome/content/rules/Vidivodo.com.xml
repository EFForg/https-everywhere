<ruleset name="Vidivodo.com" default_off="mixedcontent">
<target host="www.vidivodo.com"/>
<!-- asset.vidivodo.com uses a cert that is only valid for other edgecastcdn domains , effectively breaks the page layout & makes it unusable -->
<rule from="^http://www\.vidivodo\.com/" to="https://www.vidivodo.com/"/>
</ruleset>
