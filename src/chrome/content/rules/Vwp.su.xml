<!--
	Insecure cookies are set for these domains:

		- .vwp.su

-->
<ruleset name="vwp.su">

	<!--	Direct rewrites:
				-->
	<target host="vwp.su" />
	<target host="www.vwp.su" />


	<!--	CloudFlare cookies:
					-->
	<!--securecookie host="^\.vwp\.su$" name="^(?:__cfduid|cf_clearance)$" /-->

	<securecookie host="^\.vwp\.su$" name=".+" />


	<rule from="^http:"
		to="https:" />

</ruleset>
