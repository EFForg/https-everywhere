<!--
	Other Pydio rulesets:

		- Pyd.io.xml


	www: Prints "Pydio Apis"

-->
<ruleset name="Pydio.com">

	<!--	Direct rewrites:
				-->
	<target host="pydio.com" />

	<!--	Complications:
				-->
	<target host="www.pydio.com" />


	<rule from="^http://www\.pydio\.com/"
		to="https://pydio.com/" />

	<rule from="^http:"
		to="https:" />

</ruleset>
