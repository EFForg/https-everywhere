<ruleset name="Alipay.com (javascript loop)" default_off="js redirect to http">

	<target host="fun.alipay.com" />


	<securecookie host="^fun\.alipay\.com$" name=".+" />


	<rule from="^http://fun\.alipay\.com/"
		to="https://fun.alipay.com/" />

</ruleset>
