<!--
	Other Uniregistry rulesets:

		- Uniregistry.net.xml


	Insecure cookies are set for these hosts:

		- uniregistry.com

-->
<ruleset name="Uniregistry.com">

	<!--	Direct rewrites:
				-->
	<target host="uniregistry.com" />
	<target host="mail.uniregistry.com" />
	<target host="www.uniregistry.com" />


	<!--	Not secured by server:
					-->
	<!--securecookie host="^uniregistry\.com$" name="^session$" /-->

	<securecookie host="^uniregistry\.com$" name=".+" />


	<rule from="^http:"
		to="https:" />

</ruleset>
