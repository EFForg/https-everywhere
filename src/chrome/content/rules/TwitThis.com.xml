<!--
	www.twitthis.com: Mismatched


	Insecure cookies are set for these domains:

		- .twitthis.com


	Mixed content:

		- css from $self
		
		- Images, from:
		
			- $self
			- www.shoemoneysystem.com ᵈ

	ᵈ Unsecurable <= dropped

-->
<ruleset name="TwitThis.com" default_off="expired" platform="mixedcontent">

	<!--	Direct rewrites:
				-->
	<target host="twitthis.com" />

	<!--	Complications:
				-->
	<target host="www.twitthis.com" />


	<!--	Not secured by server:
					-->
	<!--securecookie host="^\.twitthis\.com$" name="^TwitThis2$" /-->

	<securecookie host="." name="." />


	<rule from="^http://www\.twitthis\.com/"
		to="https://twitthis.com/" />

	<rule from="^http:"
		to="https:" />

</ruleset>
