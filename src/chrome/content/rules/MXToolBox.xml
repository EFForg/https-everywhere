<ruleset name="MXToolBox" default_off="redirect loop">

	<target host="mxtoolbox.com" />
	<target host="www.mxtoolbox.com" />


	<securecookie host="^(?:www\.)?mxtoolbox\.com$" name=".*" />


	<rule from="^http://(www\.)?mxtoolbox\.com/"
		to="https://$1mxtoolbox.com/" />

</ruleset>
