<!--
	Clients have unique subdomains.

	Web bugs.

-->
<ruleset name="intelli-direct.com">

	<target host="intelli-direct.com" />
	<target host="*.intelli-direct.com" />


	<securecookie host="^\.intelli-direct\.com$" name=".+" />


	<rule from="^http://([\w-]+\.)?intelli-direct\.com/"
		to="https://$1intelli-direct.com/" />

</ruleset>