<!--
	^: cert only matches www

-->
<ruleset name="BestOfferBuy.com">

	<target host="bestofferbuy.com" />
	<target host="*.bestofferbuy.com" />


	<securecookie host="^\.(?:www\.)?bestofferbuy\.com$" name=".+" />


	<rule from="^http://(?:www\.)?bestofferbuy\.com/"
		to="https://www.bestofferbuy.com/" />

</ruleset>
