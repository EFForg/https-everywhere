<!--
	For othe Silent Circle coverage, see Silent_Circle.xml.


	Problematic domains:

		- (www.)secureinthecircle.com	(CN: silentcircle.com)

-->
<ruleset name="Secure in the Circle" default_off="mismatched">

	<target host="secureinthecircle.com" />
	<target host="www.secureinthecircle.com" />


	<rule from="^http://(?:www\.)?secureinthecircle\.com/"
		to="https://secureinthecircle.com/" />

</ruleset>
