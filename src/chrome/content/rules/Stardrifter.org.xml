<!--
	^stardrifter.org: Mismatched

-->
<ruleset name="Stardrifter.org" default_off="self-signed">

	<!--	Direct rewrites:
				-->
	<target host="www.stardrifter.org" />

	<!--	Complications:
				-->
	<target host="stardrifter.org" />


	<rule from="^http://stardrifter\.org/"
		to="https://www.stardrifter.org/" />

	<rule from="^http:"
		to="https:" />

</ruleset>
