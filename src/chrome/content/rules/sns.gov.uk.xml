<ruleset name="SNS.gov.uk">

	<!--	Direct rewrites:
				-->
	<target host="admin.sns.gov.uk" />

	<!--	Complications:
				-->
	<target host="www.sns.gov.uk" />


	<securecookie host="^\w" name="." />


	<rule from="^http://www\.sns\.gov\.uk/"
		to="https://admin.sns.gov.uk/" />

	<rule from="^http:"
		to="https:" />

</ruleset>
