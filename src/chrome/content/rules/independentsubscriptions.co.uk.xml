<ruleset name="Independent Subscriptions.co.uk" default_off="Invalid certificate, timeout">

	<target host="independentsubscriptions.co.uk" />
	<target host="www.independentsubscriptions.co.uk" />


	<securecookie host="^\." name="^(?:__cfduid|cf_clearance)$" />
	<securecookie host="^\w" name="." />


	<rule from="^http:"
		to="https:" />

</ruleset>
