<ruleset name="Findbuch.de">
	<target host="findbuch.de" />
   <rule from="^http://findbuch\.de/"
		to="https://www.findbuch.de/" />   <!-- findbuch.de uses an mismatched cerificate (www.findbuch.de) so redirect manually --->
  
	<target host="www.findbuch.de" />
	<target host="beacon.findbuch.de" />
	<rule from="^http:" to="https:" />
</ruleset>
