<ruleset name="CLAS Consultants">
    <target host="clasconsultants.net" />
    <target host="*.clasconsultants.net" />

    <securecookie host="^(?:.*\.)?clasconsultants\.net$" name=".+" />

    <rule from="^http://clasconsultants\.net/"
        to="https://www.clasconsultants.net/" />
    <rule from="^http://([^/:@]+)?\.clasconsultants\.net/"
        to="https://$1.clasconsultants.net/" />
</ruleset>
