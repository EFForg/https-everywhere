<ruleset name="Wildfiregames.com">
    <target host="wildfiregames.com" />
    <target host="www.wildfiregames.com" />
    <target host="releases.wildfiregames.com" />

    <securecookie host="^(www\.|releases\.)?wildfiregames\.com" name=".+" />

    <rule from="^http:"
            to="https:" />
</ruleset>
