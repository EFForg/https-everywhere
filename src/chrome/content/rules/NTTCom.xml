<!--

	For other NTTCom coverage, see tracer.jp.xml.

	Problematic subdomains:

		- ^	(invalid)
		- blog	(invalid)
		- invalid	(invalid)

-->
<ruleset name="NTTCom">

	<target host="www.nttcoms.com" />
	<target host="research.nttcoms.com" />

	<securecookie host="^www\.nttcoms\.com$" name=".+" />

	<exclusion pattern="^http://research\.nttcoms\.com/$" />

	<rule from="^http:"
		to="https:" />

</ruleset>