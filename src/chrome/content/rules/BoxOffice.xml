<!--
	CDN buckets:

		- s3.amazonaws.com/bo-assets/


	$ redirects to http

-->
<ruleset name="BoxOffice (partial)">

	<target host="boxoffice.com" />
	<target host="www.boxoffice.com" />
		<exclusion pattern="^http://(?:www\.)?boxoffice\.com/(?:$|\?|/)" />


	<rule from="^http:" to="https:" />

</ruleset>
