<ruleset name="Pandora">
<target host="pandora.com"/>
<target host="www.pandora.com"/>
<target host="*.pandora.com"/>
<exclusion pattern="^http://investor\.pandora\.com/"/>
<exclusion pattern="^http://advertising\.pandora\.com/"/>
<test url="http://help.pandora.com/"/>
<test url="http://submit.pandora.com/"/>
<test url="http://amp.pandora.com/"/>
<test url="http://stats.pandora.com/"/>
<test url="http://investor.pandora.com/"/>
<test url="http://advertising.pandora.com/"/>
<securecookie host=".+" name=".+" />
<rule from="^http:" to="https:"/>
</ruleset>
