<ruleset name="Qualcomm (partial)">

	<target host="qca.qualcomm.com"/>
	<target host="www.qca.qualcomm.com"/>

	<securecookie host="^.*\.qca\.qualcomm\.com$" name=".*"/>

	<rule from="^http://(www\.)?qca\.qualcomm\.com/"
		to="https://$1qca.qualcomm.com/"/>

</ruleset>
