<!--
        Problematic domains:
            - ^ (timeout)
            - futureleaders.amica.com (timeout)

-->
<ruleset name="Amica.com">
	<target host="careers.amica.com" />
	<target host="m.amica.com" />
	<target host="securemail.amica.com" />
	<target host="www.amica.com" />

	<test url="http://careers.amica.com/" />
	<test url="http://m.amica.com/" />
	<test url="http://securemail.amica.com/" />
	<test url="http://www.amica.com/" />

	<rule from="^http:" to="https:" />
</ruleset>
