<!--
	Invalid certificate:
		hm.clonezilla.org
		osdn.clonezilla.org

	Unreachable:
		events.clonezilla.org
-->
<ruleset name="clonezilla.org">
	<target host="clonezilla.org" />
	<target host="www.clonezilla.org" />
	<target host="mail.clonezilla.org" />

	<rule from="^http:" to="https:" />
</ruleset>
