<!--
	CDN buckets:

		- scubblyimages.s3.amazonaws.com


	At least some pages redirect to http.

-->
<ruleset name="Scubbly (partial)">

	<target host="scubbly.com" />
	<target host="www.scubbly.com" />


	<rule from="^http://(www\.)?scubbly\.com/(bot-trap|css|images|js)/"
		to="https://$1scubbly.com/$2/" />

</ruleset>
