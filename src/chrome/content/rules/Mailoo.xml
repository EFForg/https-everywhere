<!--
	^mailoo.org: Mismatched, missing certificate chain

-->
<ruleset name="Mailoo">

	<!--	Direct rewrites:
				-->
  <target host="www.mailoo.org" />

	<!--	Complications:
				-->
  <target host="mailoo.org" />

	<rule from="^http://mailoo\.org/"
		to="https://www.mailoo.org/" />

	<rule from="^http:"
		to="https:" />
</ruleset>
