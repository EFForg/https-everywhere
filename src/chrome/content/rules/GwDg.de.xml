	Gesellschaft fuer wissenschaftliche Datenverarbeitung


	These altnames don't exist:

		- www.piwik.gwdg.de

-->
<ruleset name="GwDg.de">

	<target host="piwik.gwdg.de" />

		<test url="http://piwik.gwdg.de/piwik.php?idsite=" />


	<securecookie host="^\w" name=".+" />


	<rule from="^http:"
		to="https:" />

</ruleset>
