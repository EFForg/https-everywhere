<!--
	Nonfunctional hosts in *.viazul.com:

	h: http redirect
	m: certificate mismatch
	r: connection refused
	s: self-signed certificate
	t: timeout on https
-->
<ruleset name="Viazul.com">
	<target host="viazul.com" />
	<target host="www.viazul.com" />

	<rule from="^http:" to="https:" />
</ruleset>
