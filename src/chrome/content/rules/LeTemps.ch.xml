<!--
	HTTP redirect:
		- boutique.letemps.ch

	Mismatch:
		- beta.letemps.ch
		- epaper.letemps.ch

	Refused:
		- parlement.letemps.ch
		- pub.letemps.ch
-->
<ruleset name="LeTemps.ch">
	<target host="letemps.ch" />
	<target host="www.letemps.ch" />
	<target host="assets.letemps.ch" />
	<target host="labs.letemps.ch" />
	<target host="blogs.letemps.ch" />

	<rule from="^http:" to="https:" />
</ruleset>
