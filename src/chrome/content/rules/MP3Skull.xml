<ruleset name="MP3Skull">

    <target host="mp3skull.is" />
    <target host="www.mp3skull.is" />

    <target host="mp3skull.wtf" />
    <target host="www.mp3skull.wtf" />

    <securecookie host=".+" name=".+" />

    <rule from="^http://(?:www\.)?mp3skull\.is/"
            to="https://mp3skull.wtf/" />

    <rule from="^http://(?:www\.)?mp3skull\.wtf/"
            to="https://mp3skull.wtf/" />

</ruleset>
