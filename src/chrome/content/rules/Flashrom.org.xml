<ruleset name="flashrom (CAcert mismatch)" default_off="mismatch" platform="cacert" f="Flashrom.org.xml">

	<!--	Cert: www.coreboot.org	-->
	<target host="flashrom.org" />
	<target host="www.flashrom.org" />


	<securecookie host="^flashrom\.org$" name=".*" />


	<rule from="^http:"
		to="https:" />

</ruleset>
