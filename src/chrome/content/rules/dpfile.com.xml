	<rule from="^http://(i|j|m|si|t)(\d+)\.(s1\.|s2\.|s3\.)?dpfile\.com/" to="https://i2.dpfile.com/" />
	<exclusion pattern="^http://m\d+\.dpfile\.com/" />
	<exclusion pattern="^http://si\d+\.dpfile\.com/" />

