<!--
	Other SAS Institute rulesets:

		- Aimatch.com.xml

-->
<ruleset name="SAS Institute">

	<target host="sas.com" />
	<target host="*.sas.com" />


	<!--	Observed cookie domains:

			- ^
			- .
			- login
			- www
				-->
	<securecookie host="^(?:.*\.)?sas\.com$" name=".+" />


	<rule from="^http;//(login\.|www\.)?sas\.com/"
		to="https://$1sas.com/" />

</ruleset>