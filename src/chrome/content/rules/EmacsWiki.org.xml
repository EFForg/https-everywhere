<!--
	^emacswiki.org: Mismatched

-->
<ruleset name="EmacsWiki.org">

	<!--	Direct rewrites:
				-->
	<target host="www.emacswiki.org" />

	<!--	Complications:
				-->
	<target host="emacswiki.org" />


	<securecookie host="^\w" name="." />


	<rule from="^http://emacswiki\.org/"
		to="https://www.emacswiki.org/" />

	<rule from="^http:"
		to="https:" />

</ruleset>
