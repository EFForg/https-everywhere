<ruleset name="Bundespolizei">
    <target host="bundespolizei.de" />
    <target host="*.bundespolizei.de" />

    <rule from="^http://bundespolizei\.de/"
        to="https://www.bundespolizei.de/"/>
    <rule from="^http://([^/:@]+)?\.bundespolizei\.de/"
        to="https://$1.bundespolizei.de/" />
</ruleset>
