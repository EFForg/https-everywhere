<!--
	Nonfunctional subdomains:

		- wiki *

	* Redirects to http


	Fully covered hosts in *opencsw.org:

		- (www.)?
		- lists


	These altnames don't exist:

		- www.lists.opencsw.org

-->
<ruleset name="OpenCSW.org (partial)">

	<!--	Direct rewrites:
				-->
	<target host="opencsw.org" />
	<target host="lists.opencsw.org" />
	<target host="www.opencsw.org" />

		<!--exclusion pattern="^http://wiki\.opencsw\.org/" /-->


	<rule from="^http:"
		to="https:" />

</ruleset>
