<!--

	Unsupported domains:

	- ajrp.awm.gov.au (connection interrupted)

-->
<ruleset name="Australian War Memorial (partial)">

	<target host="awm.gov.au" />
	<target host="cas.awm.gov.au" />
	<target host="static.awm.gov.au" />
	<target host="www.awm.gov.au" />

	<securecookie host="^(cas|static|www)\.awm\.gov\.au$"
			name=".+" />

	<rule from="^http:"
		to="https:" />

</ruleset>