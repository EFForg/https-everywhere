<!--
	For other Ikoula coverage, see Ikoula.com.xml.


	Fully covered subdomains:

		- (www.)?
		- blog

-->
<ruleset name="iKeepinCloud.com">

	<target host="ikeepincloud.com" />
	<target host="*.ikeepincloud.com" />


	<rule from="^http://(blog\.|www\.)?ikeepincloud\.com/"
		to="https://$1ikeepincloud.com/" />

</ruleset>
