<!--
	For rules not causing false/broken MCB, see Evolve_Socially.org.xml.

-->
<ruleset name="Evolve Socially.org (false MCB)" platform="mixedcontent">

	<target host="evolvesocially.org" />
	<target host="*.evolvesocially.org" />
		<!--
			Handled in Evolve_Socially.org.xml:
								-->
		<!--exclusion pattern="^http://(www\.)?evolvesocially\.org/(favicon\.ico|wp-content/|wp-includes/)" /-->


	<securecookie host="^(?:w*\.)?evolvesocially\.org$" name=".+" />


	<rule from="^http://(www\.)?evolvesocially\.org/(?!favicon\.ico|wp-content/|wp-includes/)"
		to="https://$1evolvesocially.org/" />

</ruleset>
