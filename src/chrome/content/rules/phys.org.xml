<ruleset name="Phys.org">

	<target host="phys.org" />
	<target host="m.phys.org" />
	<target host="www.phys.org" />
	<target host="physorg.com" />

	<securecookie host="^\." name="^__qca" />
	<securecookie host="^\w" name=".+" />

	<rule from="^http://physorg\.com/"
		to="https://phys.org/" />

	<rule from="^http:"
		to="https:" />

</ruleset>
