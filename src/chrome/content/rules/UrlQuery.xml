<ruleset name="urlQuery" default_off="SSL error">
	<target host="urlquery.net" />
	<target host="www.urlquery.net" />

	<securecookie host="^urlquery\.net$" name=".+" />

	<!-- Certificate only matches "urlquery.net". -->
	<rule from="^http://(?:www\.)?urlquery\.net/" to="https://urlquery.net/" />
</ruleset>
