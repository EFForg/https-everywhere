<!--
	Chapman University


	Fully covered subdomains:

		- (www.)
		- blogs
		- inside
		- social

-->
<ruleset name="Chapman.edu">

	<target host="chapman.edu" />
	<target host="*.chapman.edu" />


	<rule from="^http://((?:blogs|inside|social|www)\.)?chapman\.edu/"
		to="https://$1chapman.edu/" />

</ruleset>
