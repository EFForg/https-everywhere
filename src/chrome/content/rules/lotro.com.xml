<!--
	lotro.com


	Fully covered subdomains:

		- www
		- store
		- signup
		- transfers

-->
<ruleset name="lotro.com">

	<target host="lotro.com" />
	<target host="*.lotro.com" />

	<rule from="^http://(?:www\.)?lotro\.com/"
		to="https://www.lotro.com/"/>

	<rule from="^http://(store|signup|transfers)\.lotro\.com/"
		to="https://$1.lotro.com/" />

</ruleset>
