<!--
	^insinuator.net: Mismatched


	Mixed content:

		- Images from $self *

	* Secured by us

-->
<ruleset name="Insinuator.net">

	<!--	Direct rewrites:
				-->
	<target host="*.insinuator.net" />

	<!--	Complications:
				-->
	<target host="insinuator.net" />

		<test url="http://www.insinuator.net/" />


	<securecookie host="." name="." />


	<rule from="^http://insinuator\.net/"
		to="https://www.insinuator.net/" />

	<rule from="^http:"
		to="https:" />

</ruleset>
