<!--
	Cert only matches *.edx.org

-->
<ruleset name="edX">

	<target host="edx.org" />
	<target host="*.edx.org" />


	<securecookie host="^(?:courses|www)\.edx\.org$" name=".+" />


	<rule from="^http://(?:www\.)?edx\.org/"
		to="https://www.edx.org/" />

	<rule from="^http://courses\.edx\.org/"
		to="https://courses.edx.org/" />

</ruleset>
