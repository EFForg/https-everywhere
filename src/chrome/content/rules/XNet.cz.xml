<ruleset name="XNet.cz">
    <target host="xnet.cz" />
    <target host="www.xnet.cz" />
    <target host="admin.xnet.cz" />

    <test url="http://xnet.cz/" />
    <test url="http://www.xnet.cz/" />
    <test url="http://admin.xnet.cz/" />

    <rule from="^http://xnet\.cz/"
		    to="https://xnet.cz/" />
    <rule from="^http://www\.xnet\.cz/"
		    to="https://xnet.cz/" />
    <rule from="^http://admin\.xnet\.cz/"
            to="https://admin.xnet.cz/" />
</ruleset>
