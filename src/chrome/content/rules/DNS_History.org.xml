<!--
	www: Cert only matches ^dnshistory.org


	Insecure cookies are set for these hosts:

		- dnshistory.org

-->
<ruleset name="DNS History.org">

	<target host="dnshistory.org" />
	<target host="www.dnshistory.org" />


	<!--	Not secured by server:
					-->
	<!--securecookie host="^dnshistory\.org$" name="^DNSHistory$" /-->

	<securecookie host="^dnshistory\.org$" name=".+" />


	<rule from="^http://(?:www\.)?dnshistory\.org/"
		to="https://dnshistory.org/" />

</ruleset>
