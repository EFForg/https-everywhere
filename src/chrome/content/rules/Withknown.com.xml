<ruleset name="Withknown.com">
  <target host="withknown.com" />
  <target host="*.withknown.com" />

  <exclusion pattern="^http://assets\.withknown\.com/"/>
  <exclusion pattern="^http://docs\.withknown\.com/"/>
  <exclusion pattern="^http://stream\.withknown\.com/"/>
  <rule from="^http://([a-zA-Z0-9\-\_]+\.)?withknown\.com/" to="https://$1withknown.com/" />
</ruleset>
