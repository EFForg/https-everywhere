<!--
    Other Seznam.cz rulesets:
        - Stream.cz.xml
        - Mapy.cz.xml
        - Sklik.cz.xml
        - ProZeny.cz.xml
        - Lide.cz.xml
        - Mixer.cz.xml
-->
<ruleset name="Seznam.cz">
  <target host="seznam.cz" />
  <target host="www.seznam.cz" />

  <target host="registrace.seznam.cz" />
  <target host="email.seznam.cz" />
  <target host="lab.email.seznam.cz" />
  <target host="post.cz" />
  <target host="www.post.cz" />

  <target host="login.szn.cz" />
  <target host="login.seznam.cz" />

  <rule from="^http://(?:www\.)?seznam\.cz/" to="https://www.seznam.cz/" />

  <rule from="^http://registrace\.seznam\.cz/" to="https://registrace.seznam.cz/" />
  <rule from="^http://email\.seznam\.cz/" to="https://email.seznam.cz/" />
  <rule from="^http://lab\.email\.seznam\.cz/" to="https://lab.email.seznam.cz/" />
  <rule from="^http://(?:www\.)?post\.cz/" to="https://email.seznam.cz/" />

  <rule from="^http://login\.szn\.cz/" to="https://login.szn.cz/" />
  <rule from="^http://login\.seznam\.cz/" to="https://login.seznam.cz/" />

</ruleset>
