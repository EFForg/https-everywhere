<!--
	For other Getsatisfaction coverage, see Getsatisfaction.com.xml.


	Problematic subdomains:

		- ^	(mismatched, CN: *.getsatisfaction.com)


	Serves web bugs.

-->
<ruleset name="gsfn.us">

	<target host="gsfn.us" />
	<target host="loader.engage.gsfn.us" />


	<rule from="^http://gsfn\.us/"
		to="https://getsatisfaction.com/" />

	<rule from="^http://loader\.engage\.gsfn\.us/"
		to="https://loader.engage.gsfn.us/" />

</ruleset>
