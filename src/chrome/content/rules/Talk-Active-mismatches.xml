<!--
	For rules that are on by default, see Talk-Active.xml.

-->
<ruleset name="Talk Active (mismatches)" default_off="mismatch, self-signed">

	<!--	All cert: web12.talkactive.net	-->
	<target host="talkactive.net" />
	<target host="www.talkactive.net" />
	<target host="web10.dk" />
	<target host="www.web10.dk" />


	<securecookie host="^web10\.dk$" name=".*" />


	<rule from="^http://(?:www\.)?talkactive\.net/"
		to="https://talkactive.net/" />

	<rule from="^http://(?:www\.)?web10\.dk/"
		to="https://web10.dk/" />

</ruleset>
