<ruleset name="JabRef">

	<target host="jabref.org" />
	<target host="*.jabref.org" />

	<rule from="^http:" to="https:" />

    <test url="http://blog.jabref.org/" />
    <test url="http://builds.jabref.org/" />
	<test url="http://discourse.jabref.org/" />
    <test url="http://www.jabref.org/" />
</ruleset>
