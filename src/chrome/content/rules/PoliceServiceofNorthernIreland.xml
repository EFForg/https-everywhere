<!--
  For other UK Police rules, see PoliceUK.xml

  Works:
    - www

  Doesn't Work:
    - $ (gives a 404)
-->

<ruleset name="Police Service of Northern Ireland">
  <target host="psni.police.uk" />
  <target host="www.psni.police.uk" />

  <rule from="^http://(?:www\.)?psni\.police\.uk/" to="https://www.psni.police.uk/"/>
</ruleset>
