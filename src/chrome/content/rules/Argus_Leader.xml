<ruleset name="The Argus Leader">

	<target host="argusleader.com" />
	<target host="*.argusleader.com" />


	<securecookie host="^www\.argusleader\.com$" name=".+" />


	<rule from="^http://(?:cmsimg\.|www\.)argusleader\.com/"
		to="https://www.argusleader.com/" />

</ruleset>
