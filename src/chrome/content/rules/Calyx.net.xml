<!--
	www: cert only matches ^calyx.net

-->
<ruleset name="calyx.net">

	<!--	Direct rewrites:
				-->
	<target host="calyx.net" />

	<!--	Complications:
				-->
	<target host="www.calyx.net" />


	<rule from="^http://www\.calyx\.net/"
		to="https://calyx.net/" />

	<rule from="^http:"
		to="https:" />

</ruleset>
