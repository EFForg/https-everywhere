<!--
	Invalid certificate:
		admin.ez.afsp.org

	Private:
		mail.afsp.org
		rocket.afsp.org

-->
<ruleset name="American Foundation for Suicide Prevention">

	<target host="afsp.org" />
	<target host="www.afsp.org" />
	<target host="store.afsp.org" />

	<rule from="^http://www\.afsp\.org/"
		to="https://afsp.org/" />

	<rule from="^http:" to="https:" />

</ruleset>
