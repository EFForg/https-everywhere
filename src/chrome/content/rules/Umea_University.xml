	<securecookie host="^(?:(?:www\.)?anstalld|www\.cambro|cas|(?:www\.)?servicedesk\.its)\.umu\.se$" name=".+" />


	<rule from="^http://((?:www\.cambro|cas|intra\.ub|www)\.?)umu\.se/"
