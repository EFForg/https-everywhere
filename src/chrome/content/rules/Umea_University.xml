	<!--	Domains for which both ^foo and www.foo
		exist, and both work without caveat:
							-->
	<rule from="^http://(www\.)?(anstalld|servicesdesk\.its|student)\.umu\.se/"
		to="https://$1$2.umu.se/" />

</ruleset>
