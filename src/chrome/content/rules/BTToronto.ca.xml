<!--
	Invalid certificate:
		- live.bttoronto.ca

	Timed out:
		- bttoronto.ca, equivalent to www.bttoronto.ca
		- blogs.bttoronto.ca
-->

<ruleset name="BTToronto.ca">
	<target host="bttoronto.ca" />
	<target host="www.bttoronto.ca" />

	<rule from="^http://bttoronto\.ca/"
		  to="https://www.bttoronto.ca/" />

	<rule from="^http:" to="https:" />
</ruleset>
