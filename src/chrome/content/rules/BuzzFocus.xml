<ruleset name="BuzzFocus" default_off="expired, mismatched, self-signed">

	<target host="buzzfocus.com" />
	<target host="www.buzzfocus.com" />


	<securecookie host="^www\.buzzfocus\.com$" name=".+" />


	<rule from="^http://(?:www\.)?buzzfocus\.com/"
		to="https://www.buzzfocus.com/" />

</ruleset>
