<ruleset name="Think Up Themes.com" platform="mixedcontent">

	<target host="thinkupthemes.com" />
	<target host="www.thinkupthemes.com" />


	<securecookie host="^\w" name="." />


	<rule from="^http:"
		to="https:" />

</ruleset>
