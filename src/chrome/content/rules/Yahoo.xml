		<exclusion pattern="^http://(?:www\.)?totaltravel\.com/images/" />
	<target host="yahoo.com" />
	<target host="*.yahoo.com" />
		<exclusion pattern="^http://developer\.yahoo\.com/(?:$|\?)" />
		<exclusion pattern="^http://mail\.yahoo\.com/+(?:$|\?)" />
		<!--
