	<rule from="^http://([\w-]+)\.c\.yom\.mail\.yahoo\.(com|net)/"
		to="https://$1.c.yom.mail.yahoo.$2/" />

	<rule from="^http://(ar|mx)\.(?:lifestyle|mujer)\.yahoo\.com/"
		to="https://$1.mujer.yahoo.com/" />

	<rule from="^http://ca\.(?:lifestyle|shine)\.yahoo\.com/"
