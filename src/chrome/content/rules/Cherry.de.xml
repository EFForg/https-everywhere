<ruleset name="Cherry.de">

	<target host="cherry.de" />
	<target host="*.cherry.de" />


	<securecookie host="^(?:www\.)?cherry\.de$" name=".+" />


	<rule from="^http://(wt\.|www\.)?cherry\.de/"
		to="https://$1cherry.de/" />

</ruleset>
