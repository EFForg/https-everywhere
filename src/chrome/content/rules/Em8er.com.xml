<!--
	Known subdomains:

	em8er.com (enforces HTTPS)
	www.em8er.com (supports HTTPS doesn't enforce it, redirects to PLAIN http://em8er.com even from HTTPS!!!)
	forums.em8er.com (supports HTTPS but doesn't enforce it)
-->
<ruleset name="Em8er.com">
	<target host="em8er.com" />
	<target host="*.em8er.com" />

	<securecookie host=".+" name=".+" />

	<rule from="^http:" to="https:" />
</ruleset>
