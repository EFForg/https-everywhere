<ruleset name="WikiFur">
  <target host="wikifur.com" />
  <target host="*.wikifur.com" />
  
  <securecookie host=".+" name=".+"/>
  <rule from="^http:" to="https:" />
  
  <testurl="http://en.wikifur.com/" />
  <testurl="http://wikifur.com/" />
  <testurl="http://fursuit.wikifur.com/" />
</ruleset>
