<ruleset name="Hackpad">

	<target host="hackpad.com" />
	<target host="*.hackpad.com" />


	<!--	Secured by server:
					-->
	<!--securecookie host="^\.hackpad\.com$" name="^(ES2|ET)$" /-->

	<securecookie host=".*\.hackpad\.com$" name=".+" />


	<rule from="^http://([\w-]+\.)?hackpad\.com/"
		to="https://$1hackpad.com/" />

</ruleset>
