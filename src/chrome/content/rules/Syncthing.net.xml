<!--
	Fully covered hosts in *syncthing.net:

		- (www.)?
		- discourse

-->
<ruleset name="Syncthing.net">

	<!--	Direct rewrites:
				-->
	<target host="syncthing.net" />
	<target host="discourse.syncthing.net" />
	<target host="www.syncthing.net" />


	<rule from="^http:"
		to="https:" />

</ruleset>
