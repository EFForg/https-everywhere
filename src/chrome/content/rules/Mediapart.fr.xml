  <test url="http://blogs.mediapart.fr" />
  <test url="http://blogs.mediapart.fr/blog/jose-bove" />
  <test url="http://static.mediapart.fr/sites/all/themes/mediapart/mediapart_v5/images/logo-multimedias.svg" />

</ruleset>
