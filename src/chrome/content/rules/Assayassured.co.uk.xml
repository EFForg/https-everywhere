	Serves widgets on 3rd-party websites.

-->
<ruleset name="Assay Assured.co.uk (partial)">

	<target host="assayassured.co.uk" />
	<target host="www.assayassured.co.uk" />
		<!--
			Pages redirect to http:
						-->
		<!--exclusion pattern="^http://(www\.)?assayassured\.co\.uk/+($|\?|index\.php|site/*($|[?/]))" /-->


	<rule from="^http://(www\.)?assayassured\.co\.uk/(?!$|\?|index\.php|site/*(?:$|[?/]))"
		to="https://$1assayassured.co.uk/" />

</ruleset>
