<ruleset name="Fresh-hotel.org">

	<target host="fresh-hotel.org" />
	<target host="*.fresh-hotel.org" />


	<!--	Server doesn't set Secure for:
						-->
	<securecookie host="^(?:w*\.)?fresh-hotel\.org$" name=".+" />


	<rule from="^http://(www\.)?fresh-hotel\.org/"
		to="https://$1fresh-hotel.org/" />

</ruleset>
