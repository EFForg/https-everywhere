<!--
	Insecure netokies are set for these domains:

		- .anon-ib.net

-->
<ruleset name="Anon-IB.net">

	<!--	Direct rewrites:
				-->
	<target host="anon-ib.net" />
	<target host="archive.anon-ib.net" />
	<target host="int.anon-ib.net" />
	<target host="usa.anon-ib.net" />
	<target host="www.anon-ib.net" />


	<!--	CloudFlare netokies:
					-->
	<!--securecookie host="^\.anon-ib\.net$" name="^(?:__cfduid|cf_clearance)$" /-->

	<securecookie host="^\.anon-ib\.net$" name=".+" />


	<rule from="^http:"
		to="https:" />

</ruleset>
