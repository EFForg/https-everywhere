<!--
	Host is HSTS preloaded with all subdomains on some browsers,
	even if it does not send a HSTS header.
-->
<ruleset name="Drupal.org">

	<target host="drupal.org" />
	<target host="*.drupal.org" />

	<wtest url="http://amsterdam2014.drupal.org/" />
	<wtest url="http://api.drupal.org/" />
	<wtest url="http://assoc.drupal.org/" />
	<wtest url="http://association.drupal.org/" />
	<wtest url="http://chicago2011.drupal.org/" />
	<wtest url="http://cph2010.drupal.org/" />
	<wtest url="http://denver2012.drupal.org/" />
	<wtest url="http://docs.drupal.org/" />
	<wtest url="http://groups.drupal.org/" />
	<wtest url="http://jobs.drupal.org/" />
	<wtest url="http://latinamerica2015.drupal.org/" />
	<wtest url="http://lists.drupal.org/" />
	<wtest url="http://localize.drupal.org/" />
	<wtest url="http://london2011.drupal.org/" />
	<wtest url="http://losangeles2015.drupal.org/" />
	<wtest url="http://munich2012.drupal.org/" />
	<wtest url="http://portland2013.drupal.org/" />
	<wtest url="http://qa.drupal.org/" />
	<wtest url="http://security.drupal.org/" />
	<wtest url="http://sf2010.drupal.org/" />
	<wtest url="http://sydney2013.drupal.org/" />
	<wtest url="http://testing.drupal.org/" />
	<wtest url="http://www.drupal.org/" />


	<rule from="^http:"
		to="https:" />

</ruleset>
