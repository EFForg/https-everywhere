<ruleset name="Beobank.be">
  <target host="beobank.be" />
  <target host="www.beobank.be" />
  <target host="ident.beobank.be" />
  <target host="online.beobank.be" />
  
  <!-- Many cookies are unsecured -->
  <securecookie host="beobank\.be$" name=".+" />
  <securecookie host="^(?:www|ident|online)?\.beobank\.be$" name=".+" />
  
  <rule from="^http:" to="https:" />
</ruleset>
