<ruleset name="Cheshire Constabulary">

	<target host="cheshire.police.uk" />
	<target host="www.cheshire.police.uk" />


	<securecookie host="^\." name="^_ga(?:t?$|t_)" />
	<securecookie host="^\w" name=".+" />


	<rule from="^http:"
		to="https:" />

</ruleset>
