<ruleset name="Tutorialspoint (partial)">

	<!--
	
	Mismatch: ^tutorialspoint.com
	
	Refused: dev.tutorialspoint.com
	
	-->
	
	<target host="www.tutorialspoint.com"/>

	<!--	Mixed content	-->
	<exclusion pattern="^http://www\.tutorialspoint\.com/(?![\w/-]+\.(css|js|gif|ico|jpg|png|woff))" />
		<test url="http://www.tutorialspoint.com/" />
		<test url="http://www.tutorialspoint.com/index.htm" />
		<test url="http://www.tutorialspoint.com/codingground.htm" />
		<test url="http://www.tutorialspoint.com/tutor_connect/index.php" />
		<test url="http://www.tutorialspoint.com/html" />
		<test url="http://www.tutorialspoint.com/html5" />
		<test url="http://www.tutorialspoint.com/nodejs" />
		<test url="http://www.tutorialspoint.com/ipython_terminal_online.php" />
		
		<test url="http://www.tutorialspoint.com/theme/css/style-min.css" />
		<test url="http://www.tutorialspoint.com/theme/js/custom-min.js" />
		<test url="http://www.tutorialspoint.com/images/loading-cg.gif" />
		<test url="http://www.tutorialspoint.com/favicon.ico" />
		<test url="http://www.tutorialspoint.com/images/apple_store.jpg" />
		<test url="http://www.tutorialspoint.com/green/images/logo.png" />
		<test url="http://www.tutorialspoint.com/theme/css/fonts/font-awesome/fonts/fontawesome-webfont.woff" />
	
	<rule from="^http:" to="https:" />

</ruleset>
