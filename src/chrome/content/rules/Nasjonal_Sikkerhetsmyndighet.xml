<ruleset name="Nasjonal Sikkerhetsmyndighet (NSM)">
    <target host="nsm.stat.no" />
    <target host="*.nsm.stat.no" />

    <securecookie host="^(.*\.)?nsm\.stat\.no$" name=".+" />

    <rule from="^https?://nsm\.stat\.no/"
        to="https://www.nsm.stat.no/" />
    <rule from="^http://([^/:@]+)?\.nsm\.stat\.no/"
        to="https://$1.nsm.stat.no/" />
</ruleset>
