<!--
	For rules not causing false/broken MCB, see Tango.me.xml.

-->
<ruleset name="Tango.me (false MCB)" platform="mixedcontent">

	<target host="tango.me" />
	<target host="*.tango.me" />


	<!--	Server doesn't set Secure for:
						-->
	<securecookie host="^(?:www)?\.tango\.me$" name=".+" />


	<rule from="^http://(www\.)?tango\.me/"
		to="https://$1tango.me/" />

</ruleset>
