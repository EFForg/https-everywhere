<!--
	www: cert only matches ^keybase.io

-->
<ruleset name="Keybase.io">

	<target host="keybase.io" />
	<target host="*.keybase.io" />


	<!--	Secured by server:
					-->
	<!--securecookie host="^\.keybase\.io$" name="^guest$" /-->


	<rule from="^http://(?:www\.)?keybase\.io/"
		to="https://keybase.io/" />

</ruleset>
