<ruleset name="WePromise.EU" default_off="connection dropped">

	<!--	Direct rewrites:
				-->
	<target host="wepromise.eu" />
	<target host="www.wepromise.eu" />


	<!--	Not secured by server:
					-->
	<!--securecookie host="^(www\.)?wepromise\.eu$" name="^ci_session$" /-->

	<securecookie host="^(?:www\.)?wepromise\.eu$" name=".+" />


	<rule from="^http:"
		to="https:" />

</ruleset>
