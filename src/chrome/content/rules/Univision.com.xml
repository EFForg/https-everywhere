<!--
	Nonfunctional hosts in *.univision.com:

	h: http redirect
	m: certificate mismatch
	r: connection refused
	s: self-signed certificate
	t: timeout on https
-->
<ruleset name="Univision.com">
	<target host="univision.com" />
	<target host="www.univision.com" />

	<securecookie host="^\w" name=".+" />

	<rule from="^http:" to="https:" />
</ruleset>
