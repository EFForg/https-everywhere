<!--
	This domains have moderate false MCB which causes some breakage.
	
	For secured domains, see ugent.xml
!-->

<ruleset name="Ghent University (false MCB)" platform="mixedcontent">

  <target host="minerva.ugent.be" />
  <target host="zephyr.ugent.be" />
  
  <securecookie host="^minerva\.ugent\.be$" name=".+" />
  <securecookie host="^zephyr\.ugent\.be$" name=".+" />
  
  <rule from="^http:" to="https:" />
  
</ruleset>
