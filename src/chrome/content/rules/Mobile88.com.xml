<!--
	Other related ruleset:
		- iPay88.com.xml


	Non-functional subdomains:

		- $host		(HTTP 403 error)
		- approute2	(t)
		- blog	(t)
		- cms	(t)
		- cms2	(t)
		- content	(m)
		- content-cms	(m)
		- forum	(s)
		- media	(m)
		- on	(ssl connection error)
		- play	(m)
		- content.staging	(m)

	e: expired certificate
	h: http redirect
	i: invalid certificate chain
	m: certificate mismatch
	r: connection refused
	s: self-signed certificate
	t: timeout on https
-->
<ruleset name="Mobile88.com">

	<target host="sandbox.mobile88.com" />

	<rule from="^http:"
		to="https:" />
</ruleset>
