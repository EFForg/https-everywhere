<ruleset name="GHBtns.com">
        <target host="ghbtns.com" />
        <target host="www.ghbtns.com" />
  
        <!-- https://www.ghbtns.com/ -> https://ghbtns.com/ -->
        <rule from="^http://www.ghbtns.com"
                to="https://ghbtns.com" />
                
        <!-- http://ghbtns.com/ -> https://ghbtns.com/ -->
        <rule from="^http://ghbtns.com"
                to="https://ghbtns.com" />
</ruleset>
