<!--
	For other American Express coverage, see AmericanExpress.xml.


	Fully covered subdomains:

		- icm
		- qwww
		- web
		- www

-->
<ruleset name="AeXp-static.com">

	<target host="*.aexp-static.com" />


	<rule from="^http://(icm|web|q?www)\.aexp-static\.com/"
		to="https://$1.aexp-static.com/" />

</ruleset>
