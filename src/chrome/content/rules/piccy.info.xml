<!--
	Redirect to HTTP:
		ns3.piccy.info

	SSL protocol error:
		ns5.piccy.info
		piccy.piccy.info
		wrld.piccy.info

	Time out:
		ns2.piccy.info
-->
<ruleset name="piccy.info">
	<target host="piccy.info" />
	<target host="www.piccy.info" />
	<target host="i.piccy.info" />

	<rule from="^http://www\.piccy\.info/" to="https://piccy.info/" />
	<rule from="^http:" to="https:" />
</ruleset>
