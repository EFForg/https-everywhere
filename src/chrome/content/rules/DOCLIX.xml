<!--
	Nonfunctional domains:

		- (www.)adside.com
		- (www.)doclix.com

-->
<ruleset name="DOCLIX (partial)">

	<target host="ads.doclix.com" />
	<target host="track.doclix.com" />
	<target host="advertiser.doclix.com" />
	<target host="publisher.doclix.com" />


	<!--	- Cert doesn't match ads
		- Ads included on 3rd-party websites
							-->
	<rule from="^http://(?:ads|track)\.doclix\.com/"
		to="https://track.doclix.com/" />


	<rule from="^http:" to="https:" />
</ruleset>
