<!--
	www.blackarch.org: Mismatched

-->
<ruleset name="BlackArch.org" default_off="self-signed">

	<!--	Direct rewrites:
				-->
	<target host="blackarch.org" />

	<!--	Complications:
				-->
	<target host="www.blackarch.org" />


	<rule from="^http://www\.blackarch\.org/"
		to="https://blackarch.org/" />

	<rule from="^http:"
		to="https:" />

</ruleset>
