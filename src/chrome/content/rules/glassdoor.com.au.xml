	<!--securecookie host="^www\.glassdoor\.com\.au$" name="^(?:_uac|ARPNTS|gdId)$" /-->

	<securecookie host="^\." name="^(?:__cfduid$|_gat?$|_gat_|cf_clearance$)" />
