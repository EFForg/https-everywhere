<!--
No certificate:
  data.caf.fr
  e*.emailing.caf.fr
-->
<ruleset name="caf.fr">
        <target host="caf.fr" />
        <target host="www.caf.fr" />
        <target host="www2.caf.fr" />
        <target host="wwwd.caf.fr" />

        <rule from="^http://caf\.fr/"
                to="https://www.caf.fr/" />

        <rule from="^http:"
                to="https:" />
</ruleset>
