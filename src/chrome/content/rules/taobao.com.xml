<ruleset name="taobao.com">
	<target host="taobao.com"/>
	<target host="*.taobao.com"/>

	<test url="http://www.taobao.com/" />
	<test url="http://store.taobao.com/" />
	<test url="http://login.taobao.com/" />
	<test url="http://service.taobao.com/" />

	<rule from="^http:"
		to="https:"/>
</ruleset>
