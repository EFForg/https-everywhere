<!--
	For other Bunchball coverage, see Bunchball.com.xml.


	Fully covered hosts in *bunchball.net:

		- (www.)?
		- assets

-->
<ruleset name="Bunchball.net">

	<!--	Direct rewrites:
				-->
	<target host="bunchball.net" />
	<target host="assets.bunchball.net" />
	<target host="www.bunchball.net" />


	<rule from="^http:"
		to="https:" />

</ruleset>
