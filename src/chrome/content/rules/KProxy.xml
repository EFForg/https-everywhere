<ruleset name="KProxy.com">

	<target host="kproxy.com" />
	<!--	* for cross-domain cookie.	-->
	<target host="*.kproxy.com" />


	<!--	Observed cookie domains:

			- /
			- ^.
			- www
				-->
	<securecookie host="^(?:.*\.)?kproxy\.com$" name=".*" />


	<rule from="^http://(www\.)?kproxy\.com/"
		to="https://$1kproxy.com/" />

</ruleset>
