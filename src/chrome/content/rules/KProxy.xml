<ruleset name="KProxy">

	<target host="kproxy.com" />
	<target host="*.kproxy.com" />
	<target host="hidedoor.com" />
	<target host="www.hidedoor.com" />


	<!--	Observed cookie domains:

			- /
			- ^.
			- www
				-->
	<securecookie host="^(.*\.)?kproxy\.com$" name=".*" />


	<rule from="^http://([^/:@]*\.)?kproxy\.com/"
		to="https://$1kproxy.com/" />
	<rule from="^http://(www\.)?hidedoor\.com/"
		to="https://$1hidedoor.com/" />

</ruleset>
