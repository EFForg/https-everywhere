<!--
	STS header includes includeSubdomains

-->
<ruleset name="NCIP Hub.org">

	<target host="nciphub.org" />
	<target host="*.nciphub.org" />

		<test url="http://webdav.nciphub.org/" />
		<test url="http://www.nciphub.org/" />


	<securecookie host="." name="." />


	<rule from="^http:"
		to="https:" />

</ruleset>
