<!--
	Advena.io is the primary content serving domain of the Advena CM network.
	advena.io and advenacm.com are owned by the same company, but are separate
	websites, no aliases. Advenacm.com's https-everywhere rule can be found at
	https://github.com/EFForg/https-everywhere/pull/4818
-->

<ruleset name="Advena">
        <target host="advenacm.com" />
        <target host="www.advenacm.com" />
        <target host="bootstrap.advenacm.com" />
        <target host="repository.advenacm.com" />
        <target host="support.advenacm.com" />
        <target host="blog.advenacm.com" />
        
        <rule from="^http:"
                to="https:" />
</ruleset>
