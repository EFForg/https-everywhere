<ruleset name="MediaRoots.org">

	<target host="mediaroots.org" />
	<target host="www.mediaroots.org" />


	<securecookie host="^\." name="^(?:__cfduid|cf_clearance)$" />
	<securecookie host="^\w" name=".+" />


	<rule from="^http:"
		to="https:" />

</ruleset>
