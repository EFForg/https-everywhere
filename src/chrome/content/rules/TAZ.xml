<!--
	Nonfunctional domains:

		-bewegung.taz.de
			-forward to http
-->
<ruleset name="TAZ">
	<target host="taz.de"/>
	<target host="*.taz.de"/>

	<rule from="^http://(?:www\.)?taz\.de/"
		to="https://www.taz.de/"/>
	<rule from="^http://(blogs|buchmesse|dl|download|m|shop)\.taz\.de/"
		to="https://$1.taz.de/"/>
	<rule from="^http://www\.buchmesse\.taz\.de/"
		to="https://buchmesse.taz.de/"/>
</ruleset>
