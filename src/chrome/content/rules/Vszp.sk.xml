<!--
	Invalid certificate:
		- vszp.sk
		- gw.vszp.sk
		- old.vszp.sk
		- rezervacia.vszp.sk
-->
<ruleset name="Vszp.sk">
	<target host="vszp.sk" />
	<target host="www.vszp.sk" />
	<target host="portal.vszp.sk" />

	<rule from="^http://vszp\.sk/"
		to="https://www.vszp.sk/" />

	<rule from="^http:" to="https:" />
</ruleset>
