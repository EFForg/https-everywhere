<ruleset name="InterWorx (partial)">

	<target host="interworx.com"/>
	<target host="*.interworx.com"/>

	<!--	encountered cookies:
			- ^\.
			- ^my
			- ^support	-->
	<securecookie host="^(?:my|support)?\.interworx\.com$" name=".*"/>

	<rule from="^http://((my|support|www)\.)?interworx\.com/"
		to="https://$1interworx.com/"/>

</ruleset>
