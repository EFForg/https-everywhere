<!--
	For rules not causing false/broken MCB, see Invasion.com.xml.

-->
<ruleset name="Invasion.com (false MCB)" platform="mixedcontent">

	<target host="invasion.com" />
	<target host="*.invasion.com" />


	<securecookie host="^\.invasion\.com$" name=".+" />


	<rule from="^http://(www\.)?invasion\.com/"
		to="https://$1invasion.com/" />

</ruleset>
