<!--
	Invalid certificate:
		lecontrepied.org

	Login required:
		crm.lecontrepied.org

-->
<ruleset name="LeContrepied.org">

	<target host="lecontrepied.org" />
	<target host="www.lecontrepied.org" />

	<rule from="^http://lecontrepied\.org/"
		to="https://www.lecontrepied.org/" />

	<rule from="^http:"
		to="https:" />

</ruleset>
