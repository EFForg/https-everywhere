<!--
	Nonfunctional hosts in *.fatface.com:

	h: http redirect
	m: certificate mismatch
	r: connection refused
	s: self-signed certificate
	t: timeout on https
-->
<ruleset name="Fatface.com">
	<target host="fatface.com" />
	<target host="www.fatface.com" />

	<rule from="^http:" to="https:" />
</ruleset>
