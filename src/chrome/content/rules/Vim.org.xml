<!--
	http://www.vim.org/ advises to visit https://vim.sourceforge.io/
	for the HTTPS version of this page.

	Refused:
		www.vim.org

	Invalid certificate:
		www.[lang].vim.org

-->
<ruleset name="Vim.org">

	<target host="www.vim.org" />

	<rule from="^http://www\.vim\.org/"
		to="https://vim.sourceforge.io/" />

</ruleset>
