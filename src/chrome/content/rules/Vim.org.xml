<!--
	Nonfunctional hosts in *.vim.org:

	h: http redirect
	m: certificate mismatch
	r: connection refused
	s: self-signed certificate
	t: timeout on https
-->
<ruleset name="Vim.org">
	<target host="vim.org" />
	<target host="www.vim.org" />

	<rule from="^http://(www\.)?vim\.org/"
		to="https://vim.sourceforge.io/" />


</ruleset>
