<!--
	www.ggdns.de: Mismatched

-->
<ruleset name="ggdns.de">

	<!--	Direct rewrites:
				-->
	<target host="ggdns.de" />

	<!--	Complications:
				-->
	<target host="www.ggdns.de" />


	<securecookie host="^\w" name=".+" />


	<rule from="^http://www\.ggdns\.de/"
		to="https://ggdns.de/" />

	<rule from="^http:"
		to="https:" />

</ruleset>
