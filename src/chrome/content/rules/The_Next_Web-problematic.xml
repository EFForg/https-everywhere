<ruleset name="The Next Web (problematic)" default_off="mismatched">

	<target host="guides.thenextweb.com" />
	        <exclusion pattern="^http://guides\.thenextweb\.com/(?:av|cs|image)s/" />
	<target host="index.thenextweb.com" />
	<target host="jobs.thenextweb.com" />
		<exclusion pattern="^http://jobs\.thenextweb\.com/(?:Areas|Content|i)/" />


	<rule from="^http:" to="https:" />

</ruleset>