<!--
	CDN buckets:

		- dmuuhinfkryqs.cloudfront.net

		- 8tracks.imgix.net

			- imgix.8tracks.com

-->
<ruleset name="8tracks.com">

	<!--	Direct rewrites:
				-->
	<target host="8tracks.com" />
	<target host="www.8tracks.com" />

	<!--	Complications:
				-->
	<target host="imgix.8tracks.com" />


	<rule from="^http://imgix\.8tracks\.com/"
		to="https://8tracks.imgix.net/" />

	<rule from="^http:"
		to="https:" />

</ruleset>
