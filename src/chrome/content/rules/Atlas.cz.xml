<!--
	Other Centrum.cz rulesets:
		- Atlas.sk
		- Centrum.sk

	Nonfunctional hosts in *.atlas.cz:

	timeout on https:
		- aukce.atlas.cz
		- pomoc.atlas.cz
		- registrace.atlas.cz
-->
<ruleset name="Atlas.cz">
	<target host="atlas.cz" />
	<target host="www.atlas.cz" />

	<rule from="^http:" to="https:" />
</ruleset>
