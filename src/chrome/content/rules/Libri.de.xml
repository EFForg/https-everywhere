<ruleset name="Libri.de">

	<target host="libri.de" />
	<target host="*.libri.de" />


	<securecookie host="^www\.libri\.de$" name=".*" />


	<!--	Cert only matches www.	-->
	<rule from="^http://(?:www\.)?libri\.de/"
		to="https://www.libri.de/" />

	<rule from="^http://media\.libri\.de/"
		to="https://media.libri.de/" />

</ruleset>
