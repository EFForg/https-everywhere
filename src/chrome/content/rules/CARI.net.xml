<!--
	For problematic rules, see CARI.net-self-signed.xml.

-->
<ruleset name="CARI.net (partial)">

	<target host="cari.net" />
	<target host="*.cari.net" />


	<securecookie host="^(?:.*\.)?cari\.net$" name=".*" />


	<rule from="^http://(chat\.|www\.)?cari\.net/"
		to="https://$1cari.net/" />

</ruleset>
