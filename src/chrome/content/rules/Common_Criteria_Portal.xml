<ruleset name="Common Criteria Portal">
    <target host="commoncriteriaportal.org" />
    <target host="*.commoncriteriaportal.org" />

    <securecookie host="^(?:.*\.)?commoncriteriaportal\.org$" name=".+" />

    <rule from="^http://commoncriteriaportal\.org/"
        to="https://www.commoncriteriaportal.org/" />
    <rule from="^http://([^/:@]+)?\.commoncriteriaportal\.org/"
        to="https://$1.commoncriteriaportal.org/" />
</ruleset>
