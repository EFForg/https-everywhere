<!--
	For rules that are on by default, see Zugaina.xml.

-->
<ruleset name="Zugaina (mismatches)" default_off="expired, mismatch" platform="cacert mixedcontent">

	<target host="zugaina.com" />
	<target host="www.zugaina.com" />
	<target host="gentoo-overlays.zugaina.org" />


	<!--	Cert: *.zugaina.org	-->
	<rule from="^https?://(?:www\.)?zugaina\.com/"
		to="https://zugaina.com/" />

	<!--	Expired.	-->
	<rule from="^http://gentoo-overlays\.zugaina\.org/"
		to="https://gentoo-overlays.zugaina.org/" />

</ruleset>
