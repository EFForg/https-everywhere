<ruleset name="Zugaina (mismatches)" default_off="mismatched" platform="cacert mixedcontent">

	<target host="zugaina.com" />
	<target host="www.zugaina.com" />


	<rule from="^http://(?:www\.)?zugaina\.com/"
		to="https://zugaina.com/" />

</ruleset>
