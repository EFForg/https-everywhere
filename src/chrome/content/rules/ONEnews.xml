<!--
	For other ONEsolution coverage, see ONEsolution.xml.


	CN: www.oneadmin.cz

-->
<ruleset name="ONEnews" default_off="mismatched">

	<target host="onenews.cz" />
	<target host="www.onenews.cz" />


	<!--	Server drops path like so:
						-->
	<rule from="^http://onenews\.cz/[^\?](\?.*)"
		to="https://www.onenews.cz/$1" />

	<rule from="^http://www\.onenews\.cz/"
		to="https://www.onenews.cz/" />

</ruleset>
