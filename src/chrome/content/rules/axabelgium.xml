<ruleset name="AXA Belgium">
  <target host="axabank.be" />
  <target host="www.axabank.be" />
  <target host="www.axa.be" />
  <target host="www.fe.axa.be" />
  
  <!-- The cookies of ".axa.be", "www.axa.be" and "www.fe.axa.be" are unsecured. -->
  <securecookie host="^(?:www\.fe|www)?\.axa\.be$" name=".+" />
  
  <rule from="^http://axa\.be/" to="https://www.axa.be/" />
  <rule from="^http:" to="https:" />
</ruleset>
