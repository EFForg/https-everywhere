<!--
	^ CN: www.as112.net

-->
<ruleset name="DNS-OARC.net">

	<target host="dns-oarc.net" />
	<target host="*.dns-oarc.net" />


	<rule from="^http://(?:www\.)?dns-oarc\.net/"
		to="https://www.dns-oarc.net/" />

	<rule from="^http://(indico|lists)\.dns-oarc\.net/"
		to="https://$1.dns-oarc.net/" />

</ruleset>
