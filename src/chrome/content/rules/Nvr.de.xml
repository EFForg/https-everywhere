<!--
	Nonfunctional hosts in *.nvr.de:
		- auskunft1.nvr.de (m)
		- auskunft2.nvr.de (t)

	h: http redirect
	m: certificate mismatch
	r: connection refused
	s: self-signed certificate
	t: timeout on https
-->
<ruleset name="Nvr.de">
	<target host="nvr.de" />
	<target host="www.nvr.de" />
	<target host="sdnet.nvr.de" />
	<target host="test.nvr.de" />

	<rule from="^http:" to="https:" />
</ruleset>
