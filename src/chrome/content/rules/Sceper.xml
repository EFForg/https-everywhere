<!--
	Nonfunctional subdomains:

		- img
		- static
		- store

-->
<ruleset name="Sceper (partial)" default_off="self-signed">

	<!--	Scene Peer
				-->
	<target host="sceper.eu" />
	<target host="www.sceper.eu" />
	<target host="sceper.ws" />
	<target host="www.sceper.ws" />


	<securecookie host="^sceper\.ws$" name=".+" />


	<rule from="^http://(?:www\.)?sceper\.(?:eu|ws)/"
		to="https://sceper.ws/" />

</ruleset>
