<!--
  No HTTPS support:
    * proxy.opennicproject.org
    * wiki.opennicproject.org
-->
<ruleset name="OpenNIC project">
  <target host="opennicproject.org" />
  <target host="www.opennicproject.org" />
  <target host="servers.opennicproject.org" />

  <rule from="^http:"
          to="https:" />
</ruleset>
