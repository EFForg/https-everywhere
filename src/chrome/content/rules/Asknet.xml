<!--
	Nonfunctional domains:

		- (www.)asknet.com *
		- (www.)asknet.de *

	* No https

-->
<ruleset name="asknet (partial)">

	<target host="srv.asknet.com" />
	<target host="srv.asknet.de" />
	<target host="srv.cdn-asknet.com" />
	<target host="srv.cdn-asknet.de" />


	<rule from="^http:" to="https:" />

</ruleset>