<!--
	Handled in Bitly_branded_short_domains.xml:
		- on.omaha.com

	Invalid certificate:
		- ballot.omaha.com
		- studio.omaha.com

	Redirects to HTTP:
		- www.omaha.com
-->

<ruleset name="Omaha.com">
	<target host="omaha.com" />
	<target host="checkout.omaha.com" />
	<target host="myaccount.omaha.com" />

	<rule from="^http:" to="https:" />
</ruleset>
