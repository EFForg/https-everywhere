<!--
	Nonfunctional subdomains:

		- blog *

	* tumblr

	archive.is was the old domain, which now redirects to archive.today

-->
<ruleset name="archive.today (partial)">

	<target host="archive.today" />
	<target host="www.archive.today" />
	<target host="archive.is" />
	<target host="www.archive.is" />
		<!--exclusion pattern="http://blog\.archive\.today/" /-->


	<rule from="^http://(www\.)?archive\.today/"
		to="https://$1archive.today/" />

	<rule from="^http://(www\.)archive\.is/"
		to="https://$1archive.is/" />

</ruleset>
