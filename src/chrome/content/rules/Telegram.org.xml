<!--
	Other Telegram rulesets:

		- Tdesktop.com.xml
		- Telegram.me.xml


	Fully covered subdomains:

		- (www.)?
		- core
		- desktop
		- web

-->
<ruleset name="Telegram.org">

	<target host="telegram.org" />
	<target host="*.telegram.org" />


	<rule from="^http://((?:core|desktop|web|www)\.)?telegram\.org/"
		to="https://$1telegram.org/" />

</ruleset>
