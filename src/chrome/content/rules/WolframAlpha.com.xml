	<rule from="^http://wolframalpha\.com/"
		to="https://www.wolframalpha.com/" />

	<rule from="^http://api-(cn|maps|tw)\.wolframalpha\.com/"
		to="https://www.wolframalpha.com/" />

	<rule from="^http:"
		to="https:" />

</ruleset>
