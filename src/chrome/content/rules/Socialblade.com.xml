<!--
	See also Socialblade-falsemixed.xml.

	Mixed content:
	- /blog (Wordpress)
	- /chat (chatabout.net iframe without https support)
-->
<ruleset name="Socialblade.com">
	<target host="socialblade.com" />
	<target host="www.socialblade.com" />
	<target host="analytics.socialblade.com" />
	<target host="support.socialblade.com" />

	<exclusion pattern="^http://socialblade\.com/blog/(?!wp-content/|wp-includes/)" />
	<exclusion pattern="^http://socialblade\.com/chat/" />

	<test url="http://socialblade.com/blog/" />
	<test url="http://socialblade.com/blog/category/socialblade" />
	<test url="http://socialblade.com/blog/category/twitch" />
	<test url="http://socialblade.com/blog/category/youtube-news" />
	<test url="http://socialblade.com/blog/wp-content/" />
	<test url="http://socialblade.com/blog/wp-includes/" />
	<test url="http://socialblade.com/chat/" />

	<rule from="^http:"
		to="https:" />
</ruleset>
