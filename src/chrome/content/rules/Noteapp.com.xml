<!--
	Ruleset for noteapp.com.

	Not supporting HTTPS or using an invalid certificate:
		* cdn.noteapp.com
		* git.noteapp.com
		* rtc.noteapp.com
-->
<ruleset name="NoteApp.com">

	<target host="noteapp.com" />

	<target host="images.noteapp.com" />
	<target host="www.noteapp.com" />

	<rule from="^http:"
		to="https:" />

</ruleset>
