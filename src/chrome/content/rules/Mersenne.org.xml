<!--
	Cert only matches *.mersenne.org

-->
<ruleset name="Mersenne.org" default_off="self-signed">

	<target host="mersenne.org" />
	<target host="*.mersenne.org" />


	<securecookie host="^\.mersenne\.org$" name=".+" />


	<rule from="^http://(?:www\.)?mersenne\.org/"
		to="https://www.mersenne.org/" />

</ruleset>
