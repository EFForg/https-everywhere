<!--
	www.copia.is: Mismatched

-->
<ruleset name="Copia.Is">

	<!--	Direct rewrites:
				-->
	<target host="copia.is" />

	<!--	Complications:
				-->
	<target host="www.copia.is" />


	<securecookie host="^\w" name="." />


	<rule from="^http://www\.copia\.is/"
		to="https://copia.is/" />

	<rule from="^http:"
		to="https:" />

</ruleset>
