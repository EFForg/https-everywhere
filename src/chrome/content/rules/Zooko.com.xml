<!--
	Nonfunctional hosts in *zooko.com:

		- lists *

	* Refused


	(www.)?: Dropped over http & https

-->
<ruleset name="Zooko.com" default_off="refused">

	<target host="zooko.com" />
	<target host="www.zooko.com" />


	<securecookie host="^zooko\.com$" name=".+" />


	<rule from="^http:"
		to="https:" />

</ruleset>
