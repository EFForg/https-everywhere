<!--
	Other Hearst Corporation rulesets:

		- Hearst-Corporation.xml

-->
<ruleset name="Delish (partial)" default_off="mismatched">

	<!--	Cert: subscribe.hearstmags.com	-->
	<target host="delish.com" />
	<!--	Akamai	-->
	<target host="answerology.delish.com" />
	<target host="www.delish.com" />

	<!--	!www redirects to www.	-->
	<rule from="^http://(?:www\.)?delish\.com/"
		to="https://delish.com/" />

	<rule from="^http://answerology\.delish\.com/"
		to="https://answerology.delish.com/" />

</ruleset>
