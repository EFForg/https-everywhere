<!--
	For rules not causing false/broken MCB, see Hollywood_Outlaws.com.xml.

-->
<ruleset name="Hollywood Outlaws.com (false MCB)" platform="mixedcontent">

	<target host="hollywoodoutlaws.com" />
	<target host="*.hollywoodoutlaws.com" />
		<!--
			Handled in Hollywood_Outlaws.com.xml:
								-->
		<!--exclusion pattern="^http://(www\.)?hollywoodoutlaws.com/+(favicon\.ico|wp-content/|wp-includes/)" /-->


	<securecookie host="^\.hollywoodoutlaws\.com$" name=".+" />


	<rule from="^http://(?:www\.)?hollywoodoutlaws\.com/(?!favicon\.ico|wp-content/|wp-includes/)"
		to="https://www.hollywoodoutlaws.com/" />

</ruleset>
