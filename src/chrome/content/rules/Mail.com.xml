<!-- Do not change -> approved by 1and1 staff -->
<ruleset name="Mail.com">

	<target host=  "mail.com" />
	<target host="*.mail.com" />

	<!-- Cookies -->

	<securecookie host="^(?:.*\.)?mail\.com$" name=".+" />

	<!-- Rules -->

	<rule from="^http:"
		  to="https:" />

	<!-- Tests -->

	<test url="http://www.mail.com" />
	<test url="http://service.mail.com" />

</ruleset>