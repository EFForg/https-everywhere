<!--
	For rules not causing mixed content, see Weibo.com.xml.


	Mixed css on weihr.sinaapp.com from img.t.sinajs.cn

-->
<ruleset name="Weibo.com (mixed content)" platform="mixedcontent">

	<!--	Special cases:
				-->
	<target host="food.weibo.com" />
	<target host="hr.weibo.com" />
	<target host="widget.weibo.com" />


	<!--	Not secured by server:
					-->
	<!--securecookie host="^weihr\.sinaapp\.com$" name="^saeut$" /-->

	<securecookie host="^\w" name="." />


	<rule from="^http://hr\.weibo\.com/"
		to="https://weihr.sinaapp.com/" />

	<rule from="^http:"
		to="https:" />

</ruleset>
