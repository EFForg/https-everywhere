<!--
	For mentalhealth.asn.au

		Works:

			- www
			- $

		Doesn't Work:
-->
<ruleset name="Mentalhealth.asn.au">
  <target host="mentalhealth.asn.au" />
  <target host="*.mentalhealth.asn.au" />

  <rule from="^http://(www\.)?mentalhealth\.asn\.au/"
          to="https://$1mentalhealth.asn.au/" />
</ruleset>