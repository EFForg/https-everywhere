<ruleset name="Borsen.dk (partial)">

    <target host="borsen.dk" />
    <target host="pleasure.borsen.dk" />
    <target host="annonce.borsen.dk" />
    <target host="play.borsen.dk" />
    <target host="img.borsen.dk" />

    <securecookie host="^(?:pleasure|annonce|play|img)\.borsen\.dk$" name=".+" />

    <rule from="^http:"
        to="https:" />

</ruleset>
