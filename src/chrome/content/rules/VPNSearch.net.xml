<ruleset name="VPNSearch.net">

	<target host="vpnsearch.net" />
	<target host="*.vpnsearch.net" />


	<securecookie host="^(?:w*\.)?vpnsearch\.net$" name=".+" />


	<!--	www redirects to ^ over http,
		so copy that behavior:
					-->
	<rule from="^http://(?:www\.)?vpnsearch\.net/"
		to="https://vpnsearch.net/" />

</ruleset>
