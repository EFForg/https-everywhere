<!--
	HTTP redirects to NXDOMAIN:
		- secure.elgas.com.au

	Invalid certificate:
		- lng.elgas.com.au
		- trade.elgas.com.au

	Wrong server:
		- testserver.elgas.com.au
-->

<ruleset name="Elgas.com.au">
	<target host="elgas.com.au" />
	<target host="www.elgas.com.au" />
	<target host="emap.elgas.com.au" />

	<rule from="^http:" to="https:" />
</ruleset>
