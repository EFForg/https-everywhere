	<!--	Not secured by server:
					-->
	<!--securecookie host="^\.tribalfusion\.com$" name="^(?:ANON_ID|RealLoginName|SAccess|Tkt1|copyFlightId|tfre)$" /-->

	<securecookie host=".+" name=".+" />


	<rule from="^http://tribalfusion\.com/"
		to="https://www.tribalfusion.com/" />

	<rule from="^http:"
