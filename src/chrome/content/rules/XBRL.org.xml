<!--
	Nonfunctional hosts in *.xbrl.org:

	h: http redirect
	m: certificate mismatch
	r: connection refused
	s: self-signed certificate
	t: timeout on https
-->
<ruleset name="xbrl.org">
	<target host="www.xbrl.org" />
                <test url="http://www.xbrl.org/Specification/XBRL-2.1/REC-2003-12-31/XBRL-2.1-REC-2003-12-31+corrected-errata-2013-02-20.html" />
	<target host="specifications.xbrl.org" />
                <test url="http://specifications.xbrl.org/faq.html" />

	<rule from="^http:" to="https:" />
</ruleset>
