<!--
	Problematic domains:

		- geef.nl	(times out)


	Some pages redirect to http.

-->
<ruleset name="Geef.nl">

	<target host="geef.nl" />
	<target host="www.geef.nl" />


	<rule from="^http://(?:www\.)?geef\.nl/(commons/|css/|externalMod\.php|favicon\.ico|images/|js/|login|upload/)"
		to="https://www.geef.nl/$1" />

</ruleset>
