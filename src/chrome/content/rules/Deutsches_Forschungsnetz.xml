<ruleset name="Deutsches Forschungsnetz">
    <target host="dfn.de" />
    <target host="*.dfn.de" />

    <securecookie host="^(?:.*\.)?dfn\.de$" name=".+" />

    <rule from="^http://dfn\.de/"
        to="https://www.dfn.de/" />
    <rule from="^http://([^/:@]+)?\.dfn\.de/"
        to="https://$1.dfn.de/" />
</ruleset>
