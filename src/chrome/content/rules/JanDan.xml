	mismatch:
		www.jandan.com	( equal to ^ )

	Redirect to http:
		s.jandan.com
-->

<ruleset name="JanDan (partial)">

	<target host="www.jandan.net" />
	<rule from="^http://www\.jandan\.net/" to="https://jandan.net/" />
