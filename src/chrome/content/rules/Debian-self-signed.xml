<!--
	For rules that are on by default, see Debian.net.xml.


	Fully covered domains:

		- live.debian.net
		- mentors.debian.net
-->
<ruleset name="Debian.net (self-signed)" default_off="self-signed">

	<target host="live.debian.net" />
	<target host="mentors.debian.net" />


	<rule from="^http:"
		to="https:" />

</ruleset>
