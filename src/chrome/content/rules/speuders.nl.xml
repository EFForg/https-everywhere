<ruleset name="Speuders">
  <target host="speuders.nl" />
  <target host="www.speuders.nl" />
  <target host="static-speuders.nl" />
  <target host="*.images-speuders.nl" />

  <rule from="^http://(?:www\.)?speuders\.nl/" to="https://www.speuders.nl/"/>
  <rule from="^http://static-speuders\.nl/" to="https://static-speuders.nl/"/>
  <rule from="^http://([^/:@]*)\.images-speuders\.nl/" to="https://$1.images-speuders.nl/"/>
</ruleset>




