 <!--
	Nonfunctional hosts in *.bashy.io:
		- clippy
		- dokku
-->
<ruleset name="Bashy.io">
	<target host="www.bashy.io" />
	<target host="canvas.bashy.io" />
	<target host="ghost.bashy.io" />

	<rule from="^http://bashy\.io/" to="https://www.bashy.io/" />
	<rule from="^http:" to="https:" />
</ruleset>
