<ruleset name="Asia-Pacific Network Information Centre (mismatches)" default_off="mismatch">

	<target host="conference.apnic.net" />


	<!--	Cert: www.apnic.net	-->
	<rule from="^http://conference\.apnic\.net/"
		to="https://conference.apnic.net/" />

</ruleset>
