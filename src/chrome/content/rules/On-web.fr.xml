<ruleset name="on-web.fr">

    <target host="on-web.fr" />
    <target host="*.on-web.fr" />

    <test url="http://www.on-web.fr/" />
    <test url="http://on-web.fr/" />
    <test url="http://planet-work.on-web.fr/" />

    <rule from="^http://(?:www\.)?on-web\.fr/"
        to="https://www.planet-work.com/" />

</ruleset>
