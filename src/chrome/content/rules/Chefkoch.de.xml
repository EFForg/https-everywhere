<ruleset name="Chefkoch (partial)">

	<target host="cdn.chefkoch.de" />
	<target host="css.chefkoch.de" />
	<target host="emskr.chefkoch.de" />
	<target host="img.chefkoch.de" />
	<target host="js.chefkoch.de" />
	<target host="static.chefkoch.de" />

	<target host="css.chefkoch-cdn.de" />
	<target host="emskr.chefkoch-cdn.de" />
	<target host="img.chefkoch-cdn.de" />
	<target host="js.chefkoch-cdn.de" />
	<target host="static.chefkoch-cdn.de" />

	<target host="www.chefkoch-blog.de" />

	<rule from="^http://cdn\.chefkoch\.de/"
		to="https://static.chefkoch-cdn.de/" />

	<exclusion pattern="^http://js\.chefkoch\-cdn.de/js/vendor/jwplayer6" />

	<rule from="^http:"
		to="https:" />

	<test url="http://cdn.chefkoch.de/ck.de/cms-uploads/wmf2/wmf-teaserlogo.gif" />
	<test url="http://js.chefkoch-cdn.de/js/vendor/jwplayer6/skins/chefkoch.xml" />

</ruleset>
