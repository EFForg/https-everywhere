<ruleset name="Chefkoch (partial)">

	<target host="*.chefkoch.de" />
	<target host="*.chefkoch-cdn.de" />
	<target host="*.chefkoch-blog.de" />

	<rule from="^http://(css|emskr|img|js|static)\.chefkoch-cdn\.de/"
		to="https://$1.chefkoch-cdn.de/" />
		
	<rule from="^http://(css|emskr|img|js|static)\.chefkoch\.de/"
                to="https://$1.chefkoch.de/" />	
		
	<rule from="^http://cdn\.chefkoch\.de/"	
		to="https://static.chefkoch-cdn.de/" />
	
	<rule from="^http://www\.chefkoch-blog\.de/"	
		to="https://www.chefkoch-blog.de/" />
		
</ruleset>
