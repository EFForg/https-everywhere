
<ruleset name="Chefkoch (partial)">

	<target host="chefkoch.de" />
	<target host="*.chefkoch.de" />
		<test url="https://www.chefkoch.de/layout/bundles/app/css/main.min.css" />
		<test url="https://www.chefkoch.de/layout/bundles/app/js/ck.ramen.main.min.js" />
		<test url="https://api.chefkoch.de/v2/users/ee75466fb02f82b7b6cf32e69a758d3c/avatar/crop-80x80" />
	
	<target host="*.chefkoch-cdn.de" />
		<test url="https://img.chefkoch-cdn.de/img/default/icons/icon-rss.png" />
		<test url="https://img.chefkoch-cdn.de/gujAd.min.js" />
		<test url="https://static.chefkoch-cdn.de/ck.de/faded-end.min.js" />

	<target host="chefkoch-blog.de" />
	<target host="www.chefkoch-blog.de" />

	<rule from="^http:"
		to="https:" />

</ruleset>
