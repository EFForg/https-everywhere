<!--
	For rules not causing false/broken MCB, see Rosetta_Code.org.xml.

-->
<ruleset name="Rosetta Code.org (false MCB)" platform="mixedcontent">

	<target host="rosettacode.org" />
	<target host="*.rosettacode.org" />


	<securecookie host="^\.?rosettacode\.org$" name=".+" />


	<rule from="^http://rosettacode\.org/"
		to="https://rosettacode.org/" />

</ruleset>
