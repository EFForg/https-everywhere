<!--
	NB: Server sends no certificate chain, see https://whatsmychaincert.com

-->
<ruleset name="JSBi.org" default_off="missing certificate chain">

	<target host="jsbi.org" />
	<target host="www.jsbi.org" />


	<securecookie host="^\w" name=".+" />


	<rule from="^http:"
		to="https:" />

</ruleset>
