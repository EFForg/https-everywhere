<!--
	    Non-HTTPS hosts in *.sumo.com:
      
            giveaways.sumo.com
                slack.sumo.com
                 page.sumo.com
                  gtm.sumo.com
                  
-->
<ruleset name="sumo.com">
	<target host="sumo.com" />
	<target host="www.sumo.com" />
	<target host="load.sumo.com" />

	<rule from="^http:" to="https:" />
</ruleset>
