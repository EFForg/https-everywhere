<!--
	Nonfunctional hosts in *.mic.com:

	h: http redirect
	m: certificate mismatch
	r: connection refused
	s: self-signed certificate
	t: timeout on https
-->
<ruleset name="Mic.com">
	<target host="mic.com" />
	<target host="www.mic.com" />

	<securecookie host="^\w" name=".+" />

	<rule from="^http:" to="https:" />
</ruleset>
