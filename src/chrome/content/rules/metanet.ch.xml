<ruleset name="metanet.ch (partial)">

        <target host="metanet.ch"/>
        <target host="*.metanet.ch"/>

        <rule from="^http://(?:www\.)?metanet\.ch/"
                to="https://www.metanet.ch/"/>
        <rule from="^http://(support).metanet\.ch/"
                to="https://$1.metanet.ch/"/>

</ruleset>
