<!--
	Nonfunctional hosts:

		- (www.)?sweflix.net ¹
		- (www.)?sweflix.to ²

	¹ 521
	² Redirects to http

-->
<ruleset name="Sweflix" default_off="redirects to http">
	<target host="sweflix.net" />
	<target host="www.sweflix.net" />
	<target host="sweflix.to" />
	<target host="www.sweflix.to" />


	<rule from="^http:"
		to="https:" />
</ruleset>
