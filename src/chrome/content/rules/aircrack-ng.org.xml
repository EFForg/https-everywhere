<ruleset name="aircrack-ng.org (mismatched)" default_off="mismatched">

	<target host="aircrack-ng.org" />
	<target host="www.aircrack-ng.org" />


	<securecookie host="^aircrack-ng\.org$" name=".+" />
	<securecookie host="^.+\.aircrack-ng\.org$" name=".+" />


	<rule from="^http:"
		to="https:" />

</ruleset>