<!--
	Wildcard DNS record, all other subdomains are mismatched.
-->
<ruleset name="VGChartz.com">
	<target host="vgchartz.com" />
	<target host="www.vgchartz.com" />
	<target host="gamrconnect.vgchartz.com" />
	<target host="hub.vgchartz.com" />
	<target host="pro.vgchartz.com" />

	<rule from="^http:" to="https:" />
</ruleset>
