<!--
	^: Expired
	www: Cert only matches ^railscloud.de

-->
<ruleset name="Railscloud.de" default_off="expired">

	<!--	Direct rewrites:
				-->
	<target host="railscloud.de" />

	<!--	Complications:
				-->
	<target host="www.railscloud.de" />


	<rule from="^http://www\.railscloud\.de/"
		to="https://railscloud.de/" />

	<rule from="^http:"
		to="https:" />

</ruleset>
