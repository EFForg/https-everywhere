	<target host="donate.unhcr.org" />
	<target host="tracks.unhcr.org" />


	<!--	Not secured by server:
					-->
	<!--securecookie host="^donate\.unhcr\.org$" name="^(AWSELB|CONCRETE5)$" /-->

	<securecookie host="^donate\.unhcr\.org$" name=".+" />


	<rule from="^http:"
		to="https:" />

</ruleset>
