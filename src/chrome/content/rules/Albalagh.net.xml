<!--
	Note that https://www.albalagh.net/bookstore is a subsite of the main site
	so be sure to test changes at both "/" and "/bookstore".

-->
<ruleset name="Albalagh.net">
	<target host="albalagh.net" />
	<target host="www.albalagh.net" />
	<!-- Purchase checkout subdomain from bookstore -->
	<target host="secure.albalagh.net" />

	<securecookie host=".+" name=".+" />

	<rule from="^http:" to="https:" />
</ruleset>
