	<target host="www.adcap.biz" />


	<!--	CloudFlare cookies:
					-->
	<!--securecookie host="^\.adcap\.biz$" name="^(?:__cfduid|cf_clearance)$" /-->

	<securecookie host="^\.adcap\.biz$" name=".+" />


	<rule from="^http:"
		to="https:" />

</ruleset>
