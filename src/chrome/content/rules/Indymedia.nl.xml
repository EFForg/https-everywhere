<!--
	For other Indymedia coverage, see Indymedia.xml.


	Problematic hosts in *indymedia.nl:

		- irc *

	* Expired


	STS header includes includeSubDomains

-->
<ruleset name="Indymedia.nl">

	<!--	Direct rewrites:
				-->
	<target host="indymedia.nl" />
	<target host="*.indymedia.nl" />

		<exclusion pattern="^http://irc\.indymedia\.nl/" />

			<!--	+ve:
					-->
			<test url="http://irc.indymedia.nl/" />

		<test url="http://www.indymedia.nl/" />


	<securecookie host="." name="." />


	<rule from="^http:"
		to="https:" />

</ruleset>
