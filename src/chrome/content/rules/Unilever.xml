<ruleset name="Unilever" default_off="mismatched">

	<!--	Akamai	-->
	<target host="unilever.com" />
	<target host="*.unilever.com" />


	<!--	!www times out.	-->
	<rule from="^http://(?:www\.)?unilever\.com/"
		to="https://www.unilever.com/" />

	<rule from="^http://uimg\.unilever\.com/"
		to="https://uimg.unilever.com/" />

</ruleset>
