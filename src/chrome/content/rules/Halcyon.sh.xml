<!--
	Time out:
		www.halcyon.sh

	Refused:
		callback.halcyon.sh

-->
<ruleset name="Halcyon.sh">

	<target host="halcyon.sh" />
	<target host="www.halcyon.sh" />
	<target host="s3.halcyon.sh" />

	<rule from="^http://www\.halcyon\.sh/"
		to="https://halcyon.sh/" />

	<rule from="^http:"
		to="https:" />

</ruleset>
