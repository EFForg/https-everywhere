<ruleset name="oVPN.to (partial)">

	<target host="*.ovpn.to" />
		<!--	(www.) doesn't work over https.	-->
		<exclusion pattern="^http://www\." />


	<!--	Observed cookie domains:

			- board
			- cp
				-->
	<securecookie host=".+" name=".+" />


	<!--	Observed subdomains:

			- anomail
			- board
			- cp
			- static
				-->
	<rule from="^http://(\w+)\.ovpn\.to/"
		to="https://$1.ovpn.to/" />

</ruleset>
