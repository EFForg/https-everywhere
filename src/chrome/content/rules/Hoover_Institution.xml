<ruleset name="Hoover Institution" default_off="mismatched">

	<target host="hoover.org" />
	<target host="*.hoover.org" />


	<rule from="^http://(?:www\.)?hoover\.org/"
		to="https://www.hoover.org/" />

	<rule from="^http://media\.hoover\.org/"
		to="https://media.hoover.org/" />

</ruleset>
