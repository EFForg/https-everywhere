<!--
	^: Mismatched

-->
<ruleset name="spkcn.com">

	<target host="spkcn.com" />
	<target host="ideas.spkcn.com" />
	<target host="www.spkcn.com" />


	<!--	Redirect preserves forward
		slash, path, and args:
					-->
	<rule from="^http://spkcn\.com/"
		to="https://www.spkcn.com/" />

	<rule from="^http:"
		to="https:" />

</ruleset>
