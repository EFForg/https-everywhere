<!--
	For rules that are on by default, see Ansible.com.xml.

-->
<ruleset name="Ansible.com (mismatched)" default_off="mismatched">

	<target host="ansible.com" />
	<target host="www.ansible.com" />


	<rule from="^http://ansible\.com/"
		to="https://www.ansible.com/" />

	<rule from="^http:"
		to="https:" />

</ruleset>
