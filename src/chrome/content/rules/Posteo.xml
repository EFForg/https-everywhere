<!--
	Fully covered hosts in *posteo.de:

		- (www.)?
		- lists
		- m

-->
<ruleset name="Posteo.de">

	<!--	Direct rewrites:
				-->
  <target host="posteo.de" />
  <target host="m.posteo.de" />
  <target host="lists.posteo.de" />
  <target host="www.posteo.de" />


  <rule from="^http:" to="https:" />
</ruleset>
