<!--
	Nonfunctional subdomains:

		- webinfo

-->
<ruleset name="Reformal (partial)">

	<target host="reformal.ru" />
	<target host="*.reformal.ru" />
		<exclusion pattern="^http://webinfo\." />


	<securecookie host="^\.reformal\.ru$" name=".+" />


	<rule from="^http://([\w-]+\.)?reformal\.ru/"
		to="https://$1reformal.ru/" />

</ruleset>