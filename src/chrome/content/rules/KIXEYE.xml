<!--
	bym-netdna.s3.amazonaws.com

		- Equivalent to bym-fb4-s.cdn.kixeye.com

	dd-fb-cdn.s3.amazonaws.com

		- Equivalent to dd-fb-cdn4.kixeye.com


	Nonfunctional domains:

		- (www.)casualcollective.com
		- old.casualcollective.com
		- (www.)kixeye.com
		- forums.kixeye.com	(ssl_error_rx_record_too_long)

-->
<ruleset name="KIXEYE (partial)">

	<target host="cdn.casualcollective.com" />
	<target host="bym-vx4-s.kixeye.com" />
	<target host="bp-fb4-s.cdn.kixeye.com" />
	<target host="bym-fb4-s.cdn.kixeye.com" />
	<target host="dd-fb-cdn4.kixeye.com" />
	<target host="di-fb-cdn4.kixeye.com" />
	<target host="gp-fb-cdn4-s.kixeye.com" />
	<target host="wc-fb4-s.kixeye.com" />
	<target host="wc-fb4-s.cdn.kixeye.com" />


	<rule from="^http://cdn\.casualcollective\.com/"
		to="https://s3.amazonaws.com/cdn.casualcollective.com/" />

		<!--
				- bym-vx4-s
				- dd-fb-cdn4
				- di-fb-cdn4
				- gp-fb-cdn4-s
				- wc-fb4-s
				- bp-fb4-s.cdn
				- bym-fb4-s.cdn
				- wc-fb4-s.cdn
						-->

	<rule from="^http:" to="https:" />
</ruleset>
