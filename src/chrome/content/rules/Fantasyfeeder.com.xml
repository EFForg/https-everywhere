<!--
	Nonfunctional hosts in *.fantasyfeeder.com:

	h: http redirect
	m: certificate mismatch
	r: connection refused
	s: self-signed certificate
	t: timeout on https
-->
<ruleset name="Fantasyfeeder.com">
	<target host="fantasyfeeder.com" />
	<target host="www.fantasyfeeder.com" />

	<rule from="^http:" to="https:" />
</ruleset>
