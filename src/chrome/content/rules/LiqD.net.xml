<!--
	Fully covered hosts in *liqd.net:

		- (www.)?
		- adhocracy-cms
		- fold
		- stats


	Insecure cookies are set for these domains:

		- .liqd.net

-->
<ruleset name="LiqD.net">

	<!--	Direct rewrites:
				-->
	<target host="liqd.net" />
	<target host="adhocracy-cms.liqd.net" />
	<target host="fold.liqd.net" />
	<target host="stats.liqd.net" />
	<target host="www.liqd.net" />


	<!--	Not secured by server:
					-->
	<!--securecookie host="^\.liqd\.net$" name="^qtrans_cookie_test$" /-->

	<securecookie host="^\.liqd\.net$" name=".+" />


	<rule from="^http:"
		to="https:" />

</ruleset>
