<!--
	Nonfunctional hosts in *.businessprophet.co.uk:
		- businessprophet.co.uk (m)

	h: http redirect
	m: certificate mismatch
	r: connection refused
	s: self-signed certificate
	t: timeout on https
-->
<ruleset name="BusinessProphet.co.uk">
	<target host="businessprophet.co.uk" />
	<target host="www.businessprophet.co.uk" />

	<rule from="^http://businessprophet\.co\.uk/" to="https://www.businessprophet.co.uk/" />
	<rule from="^http:" to="https:" />
</ruleset>
