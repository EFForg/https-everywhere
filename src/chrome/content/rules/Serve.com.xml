<!--
	For other American Express coverage, see AmericanExpress.xml.


	Fully covered subdomains:

		- (www.)?
		- secure

-->
<ruleset name="Serve.com">

	<target host="serve.com" />
	<target host="*.serve.com" />


	<rule from="^http://(secure\.|www\.)?serve\.com/"
		to="https://$1serve.com/" />

</ruleset>
