<!--
	For related rules, see OpenBSD_Store.com.xml

	Mismatched:
	- ^
	- www
-->
<ruleset name="OpenBSD Europe">
	<target host="openbsdeurope.com" />
	<target host="www.openbsdeurope.com" />
	<target host="shop.openbsdeurope.com" />

	<securecookie host="^shop\.openbsdeurope\.com$" name=".+" />
	
	<rule from="^http://(www|shop)?openbsdeurope\.com/" to="https://shop.openbsdeurope.com/" />
</ruleset>
