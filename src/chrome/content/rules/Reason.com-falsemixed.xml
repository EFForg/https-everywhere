<!--
	For rules not causing false/broken MCB, see Reason.com.xml.

-->
<ruleset name="Reason.com (false MCB)" platform="mixedcontent">

	<target host="reason.com" />
	<target host="www.reason.com" />


	<securecookie host="^\.reason\.com$" name=".+" />


	<rule from="^http:"
		to="https:" />

</ruleset>
