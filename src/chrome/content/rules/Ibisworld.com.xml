<!--

	Problematic domains:

		- media.ibisworld.com (certificate mismatch)

-->
<ruleset name="Ibisworld.com">
	<target host="ibisworld.com" />
	<rule from="^http://ibisworld\.com" to="https://www.ibisworld.com" />

	<target host="www.ibisworld.com" />
	<target host="secure.ibisworld.com" />

	<exclusion pattern="^http://www\.ibisworld\.com/media/" />
	<test url="http://www.ibisworld.com/media/" />

	<rule from="^http:" to="https:" />
</ruleset>
