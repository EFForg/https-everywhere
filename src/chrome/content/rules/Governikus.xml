<ruleset name="Governikus">
	<target host="www.governikus.com" />
	<target host="governikus.com" />
	<target host="www.governikus.de" />
	<target host="governikus.de" />
	<target host="www.bos-bremen.de" />

	<securecookie host="^(www\.)?governikus\.com$" name=".*" />
	<securecookie host="^(www\.)?governikus\.de$" name=".*" />
	<securecookie host="^www\.bos-bremen\.de$" name=".*" />

	<rule from="^http://(www\.)?governikus\.com/" to="https://www.governikus.com/" />
	<rule from="^http://(www\.)?governikus\.de/" to="https://www.governikus.de/" />
	<rule from="^http://www\.bos-bremen\.de/" to="https://www.bos-bremen.de/" />
</ruleset>
