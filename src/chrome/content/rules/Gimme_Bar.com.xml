<!--
	www: cert only matches ^gimmebar.com


	Mixed content:

		- Flash, from:

			- player.vimeo.com
			- \w.vimeocdn.com

-->
<ruleset name="Gimme Bar.com">

	<target host="gimmebar.com" />
	<target host="www.gimmebar.com" />


	<rule from="^http://(?:www\.)?gimmebar\.com/"
		to="https://gimmebar.com/" />

</ruleset>
