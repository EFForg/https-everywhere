<!--
	Nonfunctional subdomains:

		- (www.)? *

	* Refused

-->
<ruleset name="ThisisColossal.com" default_off="connection refused">

	<target host="colossal.com" />
	<target host="*.colossal.com" />


	<securecookie host="^\.colossal\.com$" name=".+" />


	<rule from="^http://(?:www\.)?colossal\.com/"
		to="https://www.colossal.com/" />

</ruleset>
