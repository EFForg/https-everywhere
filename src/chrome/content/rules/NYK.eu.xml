<!--
	Cert mismatch on ^
-->
<ruleset name="NowYouKnow">

	<target host="nowyouknow.eu" />
	<target host="www.nowyouknow.eu" />

	<target host="cdn.nowyouknow.eu" />

	<securecookie host=".+" name=".+" />

	<rule from="^http://(www\.)?nowyouknow\.eu/" to="https://www.nowyouknow.eu/" />

	<rule from="^http:"
		to="https:" />
</ruleset>
