<!--
	Invalid certificate:
		- svn.freetz.org
		- trac.freetz.org
-->
<ruleset name="Freetz.org">
	<target host="freetz.org" />
	<target host="www.freetz.org" />
	<target host="trac.freetz.org" />

	<rule from="^http://trac\.freetz\.org/" to="https://freetz.org/" />

	<rule from="^http:" to="https:" />
</ruleset>
