				-->
	<target host="tap-cdn.rubiconproject.com" />
	<target host="tap2-cdn.rubiconproject.com" />

		<test url="http://pixel.rubiconproject.com/tap.php?v=" />


	<!--	Not secured by server:
					-->
	<!--securecookie host="^(pixel)?\.rubiconproject\.com$" name="^c$" /-->

