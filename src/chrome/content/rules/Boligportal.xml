<!--
	The following domain uses a bad cert:
		support.boligportal.dk
-->
<ruleset name="Boligportal (partial)">
	<target host="boligportal.dk" />
	<target host="www.boligportal.dk" />
	<target host="static1.boligportal.dk" />

	<rule from="^http://(www\.)?boligportal\.dk/"
		to="https://www.boligportal.dk/" />

	<rule from="^http://static1\.boligportal\.dk/"
		to="https://static1.boligportal.dk/" />
</ruleset>
