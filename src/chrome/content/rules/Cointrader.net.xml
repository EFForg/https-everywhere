<!--
	Fully covered subdomains:

		- (www.)?
		- news

-->
<ruleset name="Cointrader.net">

	<target host="cointrader.net" />
	<target host="news.cointrader.net" />
	<target host="www.cointrader.net" />


	<rule from="^http://(news\.|www\.)?cointrader\.net/"
		to="https://$1cointrader.net/" />

</ruleset>
