<!--
	Other PJ Media rulesets:

		- PJTV.com.xml


	Problematic subdomains:

		- ^	(cert only matches ^pjmedia.com)
		- cdn	(404, CN: *.netdna-ssl.com)

-->
<ruleset name="PJ Media">

	<target host="pjmedia.com" />
	<target host="*.pjmedia.com" />


	<rule from="^https?://(?:cdn\.|www\.)?pjmedia\.com/"
		to="https://pjmedia.com/" />

</ruleset>