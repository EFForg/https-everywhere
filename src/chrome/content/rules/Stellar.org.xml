<!--
	Fully covered subdomains:

		- (www.)?
		- launch

-->
<ruleset name="Stellar.org">

	<target host="stellar.org" />
	<target host="launch.stellar.org" />
	<target host="www.stellar.org" />


	<securecookie host="^\.stellar\.org$" name="^__cfduid$" />


	<rule from="^http:" to="https:" />

</ruleset>
