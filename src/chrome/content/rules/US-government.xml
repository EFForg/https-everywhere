<ruleset name="US government (partial)">

	<target host="research.archives.gov" />
	<target host="mtrics.cdc.gov" />
	<target host="wwwn.cdc.gov" />
	<target host="census.gov" />
	<target host="www.census.gov" />
	<target host="cms.gov" />
	<target host="questions.cms.gov" />
	<target host="www.cms.gov" />
	<target host="safeharbor.export.gov" />
	<target host="genome.gov" />
	<target host="www.genome.gov" />
	<target host="cms.hhs.gov" />
	<target host="www.cms.hhs.gov" />
	<target host="intelligence.house.gov" />
	<target host="wwws.loc.gov" />
	<target host="medicare.gov" />
	<target host="plancompare.medicare.gov" />
	<target host="www.medicare.gov" />
	<target host="mymedicare.gov" />
	<target host="www.mymedicare.gov" />
	<target host="usajobs.gov" />
	<target host="help.usajobs.gov" />
	<target host="login.usajobs.gov" />
	<target host="my.usajobs.gov" />
	<target host="www.usajobs.gov" />
	<target host="postalinspectors.uspis.gov" />
	<target host="uspsoig.gov" />
	<target host="www.uspsoig.gov" />


	<securecookie host="^research\.archives\.gov$"
			name=".*" />
	<securecookie host="^(questions|www)\.cms\.gov$"
			name=".*" />
	<securecookie host="^safeharbor\.export\.gov$"
			name=".*" />
	<securecookie host="^(?:.*\.)?(?:census|medicare|mymedicare|usajobs)\.gov$"
			name=".*" />
	<securecookie host="^intelligence\.house\.gov$"
			name=".*" />
	<securecookie host="^wtr\.nhtsa\.gov$"
			name=".+" />


	<test url="http://plancompare.medicare.gov/pfdn/Popup/CommonDrugAbbriviations?Language=English" />


	<rule from="^http://mtrics\.cdc\.gov/"
		to="https://cdcgov.112.2o7.net/" />

	<rule from="^http://(?:www\.)?(census|genome|medicare|mymedicare)\.gov/"
		to="https://www.$1.gov/"/>

	<rule from="^http://(?:www\.)?cms(?:\.hhs)?\.gov/"
		to="https://www.cms.gov/"/>

	<rule from="^http:"
		to="https:" />

</ruleset>