<ruleset name="CodeAurora">

	<target host="codeaurora.org" />
	<!--	* for cross-domain cookies.	-->
	<target host="*.codeaurora.org" />


	<securecookie host="^\.codeaurora\.org$" name=".*" />


	<!--	Cert only matches www.	-->
	<rule from="^http://(?:www\.)?codeaurora\.org/"
		to="https://www.codeaurora.org/" />

</ruleset>
