	<target host="yuedust.yuedu.126.net" />
		<exclusion pattern="^http://yuedust\.yuedu\.126\.net/(?!book/?$|client/?$|contentcenter/?$|index/?$|manhua/?$|rank/?$|yc/?$|ycmm/?$|book_reader/|css/|images/|js/|$)" />
		<test url="http://yuedust.yuedu.126.net/book" />
		<test url="http://yuedust.yuedu.126.net/book/" />
		<test url="http://yuedust.yuedu.126.net/client" />
		<test url="http://yuedust.yuedu.126.net/client/" />
		<test url="http://yuedust.yuedu.126.net/contentcenter" />
		<test url="http://yuedust.yuedu.126.net/contentcenter/" />
		<test url="http://yuedust.yuedu.126.net/index" />
		<test url="http://yuedust.yuedu.126.net/index/" />
		<test url="http://yuedust.yuedu.126.net/manhua" />
		<test url="http://yuedust.yuedu.126.net/manhua/" />
		<test url="http://yuedust.yuedu.126.net/rank" />
		<test url="http://yuedust.yuedu.126.net/rank/" />
		<test url="http://yuedust.yuedu.126.net/yc" />
		<test url="http://yuedust.yuedu.126.net/yc/" />
		<test url="http://yuedust.yuedu.126.net/ycmm" />
		<test url="http://yuedust.yuedu.126.net/ycmm/" />
