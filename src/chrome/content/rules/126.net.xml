		<test url="http://yuedust.yuedu.126.net/css/sns-plugin.css" />
		<test url="http://yuedust.yuedu.126.net/images/bookwdg.png" />
		<test url="http://yuedust.yuedu.126.net/images/down/QR_All.png" />
		<test url="http://yuedust.yuedu.126.net/js/art-template.js" />

	<rule from="^http:" to="https:" />
</ruleset>
