<ruleset name="BeWelcome">
  <target host="bewelcome.com" />
  <target host="*.bewelcome.com" />
  <target host="bewelcome.org" />
  <target host="*.bewelcome.org" />
  <rule from="^http://(.+(@|.))?bewelcome.(org|com)/" to="https://$1bewelcome.org/" />
  <securecookie host="^(.+.)?bewelcome.(com|org)$" name=".*" />
</ruleset>
