<!--
	^teletrust.de: Refused

-->
<ruleset name="TeleTrusT.de">

	<!--	Direct rewrites:
				-->
	<target host="www.teletrust.de" />

	<!--	Complications:
				-->
	<target host="teletrust.de" />


	<securecookie host="^\w" name=".+" />


	<rule from="^http://teletrust\.de/"
		to="https://www.teletrust.de/" />

	<rule from="^http:"
		to="https:" />

</ruleset>
