<!--
	Nonfunctional hosts in *.evote-ch.ch:
		- int1.evote-ch.ch (s)
		- int2.evote-ch.ch (s)
		- int3.evote-ch.ch (s)
		- int4.evote-ch.ch (s)
		- test.evote-ch.ch (f)

	f: secure connection failed
	h: http redirect
	m: certificate mismatch
	r: connection refused
	s: self-signed certificate
	t: timeout on https
-->
<ruleset name="Evote-ch.ch">
	<target host="evote-ch.ch" />
	<target host="www.evote-ch.ch" />
	<target host="demo.evote-ch.ch" />
	<target host="doc.evote-ch.ch" />
	<target host="wahl.evote-ch.ch" />

	<rule from="^http:" to="https:" />
</ruleset>
